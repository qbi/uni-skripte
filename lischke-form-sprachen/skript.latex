\chapter{Einf"uhrung und Wiederholung}

\begin{defini}
  $L$ hei"st \emph{(formale) Sprache} "uber dem Alphabet $X$ (d.\,h. $X$
  ist endl. Menge) $:\Longleftrightarrow L\subseteq X^{\ast}$

  \begin{tabular}{ll}
    $X^{\ast}$\ldots & Vereinigung aller Teilmengen\\
    $e$\ldots & leeres Wort\\
    $X^{+}=$& $X^{\ast}\setminus\{e\}$
  \end{tabular}
\end{defini}

\begin{defini}
  $G=[X,N,S,R]$ hei"st \emph{(generative) Grammatik} $:\Longleftrightarrow$
  \begin{enumerate}
   \item $X$ endl. Menge (Menge terminaler Symbole)
   \item $N$ endl. Menge (Menge nichtterminaler oder Hilfsymbole), $X\cap N=\emptyset$
   \item $S\in N$ Anfangs- oder Startsymbol
   \item $R\subseteq (X\cup N)^{\ast}\times(X\cup N)^{\ast}$ ($R$ endl.
    Regelmenge)
  \end{enumerate}
  Die Paare $[p,q]\in R$ hei"sen Regel und werden oft auch als
  $p\longrightarrow q$ geschrieben.
\end{defini}

\begin{defini}
  $w'$ hei"st unmittelbare Ableitung von $w$ bezgl. einer Grammatik
  $G=[X,N,S,R]$ $w\longrightarrow w :\Leftrightarrow$

  \begin{gather*}
      w,w'\in (X\cup N)^{\ast} \wedge \exists p\exists q\exists p_{1}\exists p_{2}
       (p,q,p_{1},p_{2}\in (X\cup N)^{\ast}\wedge [p,q] \in R \wedge
       w=p_{1}pp_{2}\wedge w'=p_{1}qp_{2})
  \end{gather*}

  $w'$ hei"st Ableitung aus $w$ bezgl. $G$, $w\longrightarrow*w'$
  $\Leftrightarrow$ oder es gibt endl. viele W"orter $w_{0},
  w_{1},\ldots w_{r}$
  mit $w_{0}=w, w_{i-1}\xrightarrow[G]{} w_{i}$ f"ur $i=1,\ldots,r$ und
  $w_{r} = w'$. Man nennt dies auch eine Ableitung der L"ange $r$ und
  schreibt daf"ar $w_{0} \Rightarrow w_{1} \Rightarrow
  \ldots \Rightarrow w_{r}$ oder $w_{0} \xrightarrow{r}
  w_{r}$
\end{defini}

\begin{defini}
  $L$ hei"st die von der Grammatik $G[\ldots]$ erzeugte Sprache, $L= L(G)
  :\Leftrightarrow L=\{w: S\xrightarrow[G]{*} w\}\cap
  X^{\ast}$
\end{defini}

\begin{defini}
  Zwei Grammatiken $G_{1}$ und $G_{2}$ hei"sen "aquivalent $G_{1}\sim
  G_{2}$  $:\Leftrightarrow$ $L(G_{1}) = L(G_{2})$
\end{defini}

\begin{defini}
  Eine Grammatik $G=\gramma$ hei"st \emph{Normalfomgrammatik}
  $:\Leftrightarrow$ $R\subseteq N^{+}\times(X\cup N)^{*}$
\end{defini}

\begin{satz}
  Zu jeder Grammtik gibt es eine "aquivalente Normalformgramatik.

  \begin{proof}
    Es sei $G=\gramma$ belibige Grammatik und zun"achst
    $[\alpha,\beta]\not\in R$ f"ur jedes Wort $p$

    $X':={x':x'\in X}$ wobei $X'\cap X=X'\cap N=\emptyset$

    $R'$\ldots Menge aller Regeln, die aus der Regel der Menge $R$
    entstehen, in dem darin alle Buchstaben $x\in X$ durch die
    entspechenden Buchstaben $x'\in X'$ ersetzt werden.

    $R'' := \{[x',x]: x\in X\}$ und $G' := [X, N\cup X', S', R'\cup R'']$
    $\longrightarrow$ $G'\sim G$

    Ist $[e,p]\in R$ f"ur gewisse $p$, so sei $R'$ wie oben mit Ausnahme
    aller Regeln $[e,p']$.

    Sei $E\not\in X'\cup N$ und $R_{E} := \{[B,BE]: B\in X\cup N\} \cup \{[B,EB]:
    B\in \in N\} \cup \{[E,p']: [e,p]\in R \wedge p' \text{ entsteht aus p durch
    Ersetzen aller } x \text{ in } X \text{ durch } x\in X'\}$,

    $G_{E}:= [N, N\cup X' \cup\{E\}, S, R'\cup R''\cup R_{E}]$
    $\longrightarrow$ $G_{E}\sim G$.
  \end{proof}
\end{satz}

Avram Noam Chomsky

\begin{defini}
  Eine Grammatik $G=\gramma$ (ohne Einschr"ankung an die Regelm"enge $R$)
  hei"st Grammatik vom \emph{Typ 0}

  $G$ hei"st Grammatik vom \emph{Typ 1} oder nicht verk"urzende Grammtik
  $:\Leftrightarrow$ $\forall p\forall q ([p,q]\in R
  \rightarrow l(p) \le l(q))$

  $G$ hei"st Grammatik vom Typ 1' oder Kontextgrammtik $\Longleftrightarrow$
  $\forall p\forall q ([p,q])\in R \rightarrow \exists p_{1}\exists
  p_{2}\exists u\exists B(p_{1},p_{2},u \le (X\cup N)*\wedge B\in N\wedge
  u\not= e\wedge p=p_{1}Bp_{2}\wedge q=p_{1}\cup p_{2}$

  $G$ hei"st Grammtik vom Type 2 oder kontextfreie Grammtik
  $:\Leftrightarrow$ $\forall p\forall q([p,q]\in R) \rightarrow p\in N
  \wedge q(X\cup N)^{+}$

  G ehi"st Grammtik vom Typ 3, Automaten- oder rechtslineare Grammtik
  $:\Leftrightarrow$ $\forall p,q ([p,q]\in R
  \rightarrow p\in N \wedge q \in X\cdot N\cup X)$

  Eine Sprache L hei"st vom Typ~$i$, wenn sie von einer Grammtik vom
  Typ~$i$ erzeugt wird. $rundL_{i}$ ist die Klasse aller Sprachen vom
  Typ~$i$ ($i\in\{0,1,1',2,3\}$).
\end{defini}

\begin{defini}
  Ist $L$ eine Sprache von Typ~$i$, so hei"st auch $L\cup \{e\}$ Sprache vom
  Typ $i$ ($i\in{0,1,1',2,3}$)

  Ist $G=\gramma$ eine Grammtik vom Typ $i$ und $S'\not\in X\cap N$, so
  hei"st auch $G' := [X,N\cup {S}, S, \{[S', e], [S',S]\}\cup R]$ Grammatik
  vom type $i$ ($i\in \{0,1,1',2,3\}$)
\end{defini}

