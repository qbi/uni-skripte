%lemma 1.11
\begin{lem}\label{lem:111}
  \begin{enumerate}[(i)]
   \item $A\subset B\Rightarrow\overline{A}\subset\overline{B}$
   \item Sei $I\neq\emptyset$ beliebig. Dann ist $\bigcup_{i\in I}
    \overline{A}_{i}\subset\overline{\bigcup_{i\in I} A_{i}}$ und
    $\overline{\bigcap_{i\in I} A_{i}} \subset\bigcap_{i\in I}
    \overline{A}_{i}$
   \item $\bigcup_{i=1}^{n} \overline{A}_{i}= \overline{\bigcup_{i=1}^{n}
    A_{i}}$. Eine analoge Aussage für Durchschnitte gilt nicht.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}[(i)]
     \item folgt sofort aus der Definition des Abschlusses.
     \item $\overline{A}_{i}\subset\overline{\bigcup_{j\in I} A_{j}}$ (folgt
      aus obigem Punkt) für alle $i\in
      I\Rightarrow\overline{A}_{i}\subset\overline{\bigcup_{j\in I} A_{j}}$.
      Weiter ist $\bigcap_{i\in I} A_{i} \subset\bigcap_{i\in
      I}\overline{A_{i}}\Rightarrow\overline{\bigcap_{i\in I} A_{i}}\subset
      \bigcap_{i\in I} \overline{A_{i}}$
     \item folgt aus dem untenstehenden dritten Punkt
    \end{enumerate}
  \end{proof}

  \begin{enumerate}[(i)]
   \item $A\subset B\Rightarrow \accentset{\circ}{A}\subset \accentset{\circ}{B}$
   \item Sei $I\neq\emptyset$ beliebig. Dann ist $(\bigcap_{i\in I}
    A_{i})^{o}\subset\bigcap_{i\in I} \accentset{\circ}{A}_{i}, \bigcup_{i\in I}
    \accentset{\circ}{A}_{i}\subset (\bigcup_{i\in I} A_{i})^{o}$
   \item $(\bigcap_{i=1}^{n} A_{i})^{o}= \bigcap_{i=1}^{n} \accentset{\circ}{A}_{i}$.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}[(i)]
     \item folgt aus der Definition und der Übungsaufgabe
     \item folgt aus der Definition und der Übungsaufgabe
     \item $x\in\bigcap_{i=1}^{n} \accentset{\circ}{A}_{i}\Rightarrow\bigcap_{i=1}^{n}
      \accentset{\circ}{A}_{i}$ ist eine Umgebung von $x$ in $\bigcap_{i=1}^{n} A_{i}$.
      Daraus folgt, $x$ ist innerer Punkt von $\bigcap_{i=1}^{n} A_{i},
      x\in(\bigcap_{i=1}^{n} A_{i})^{o}\Rightarrow \bigcap_{i=1}^{n} \accentset{\circ}{A}_{i}
      \subset (\bigcap_{i=1}^{n} A_{i})^{o}$. Umgekehrte Inklusion
      folgt aus dem zweiten Punkt.
    \end{enumerate}
  \end{proof}
\end{lem}


