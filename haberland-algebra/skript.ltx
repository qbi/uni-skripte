\begin{fakt}
  \begin{enumerate}
   \item Der Durchschnitt von Normalteilern ist wieder ein Normalteiler.
   \item Sei $N\subseteq H\subseteq G$ Untergruppen, ist $N$ ein Normalteiler
    in $G\Rightarrow N$ ist Normalteiler in $H$.
   \item Ist $f\colon G\rightarrow H$ Homomorphismus und $N$ eine Normalteiler
    in $H$, so ist $f^{-1}(N)$ Normalteiler in $G$.
   \item Ist $f\colon G\rightarrow H$ ein Homomorphismus und $N$ ein
    Normalteiler in $G$, so ist $f(N)$ ein Normalteiler in $f(G)$.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item Seien $H_{i}, i\in I$ Normalteiler in $G$, also
      $sH_{i}=H_{i}s\Rightarrow \bigcap_{i\in I} sH_{i}=\bigcap_{i\in I}
      H_{i}s \Rightarrow s\bigcap_{i\in I} H_{i}= (\bigcap_{i\in I}H_{i})s$.
     \item trivial
     \item Sei $x\in f^{-1}(N), s\in G, f(sxs^{-1})=f(s)f(x)f(s^{-1}), f(x)\in
      N, \Rightarrow f(sxs^{-1})\in N\Rightarrow sxs^{-1}\in f^{-1}(N)$.
     \item Sei $x\in f(N), t \in f(G)\Rightarrow y=f(x)$ für ein $x\in N,
      t=f(s)$ für ein $s\in G$. Dann ist $tyt^{-1}=f(sxs^{-1}), sxs^{-1}\in
      N\Rightarrow tyt^{-1}\in f(N)$.
    \end{enumerate}
  \end{proof}
\end{fakt}

\paragraph{Konstruktion der Faktorgruppe $G/_{H}$ einer Gruppe $G$ nach einem
  Normalteiler $H$}

Wir kopieren das Verfahren der Konstruktion der Restklassen modulo $m$. Sei
$G/_{H}\coloneqq\{xH\colon x\in G\}$ eine Menge. Wir definieren eine
Multiplikation auf $G/_{H}$ mit $(xH)(yH)\coloneqq xyH$.
\begin{enumerate}
 \item (Korrektheit der Definition): Sei $xH=x'H, yH=y'H$. Dann ist zu zeigen,
  dass $xyH=x'y'H$. Es gilt: $x'=xh_{1}, y'=yh_{2}, h_{i}\in H\Rightarrow
  x'y'= xh_{1}yh_{2}=xyh_{3}h_{2}$ wegen $Hy=yH$ mit $h_{3}\in H$. Es folgt,
  $x'y'H=xyH$.
 \item (Gruppeneigenschaft): $(xHyH)zH=(xyH)zH=xyzH$, analog
  $xH(yHzH)=xHyzH=xyzH$, Das neutrale Element ist $H$ und invers zu $xH$ ist
  $x^{-1}H$.
\end{enumerate}

\begin{defini}
  Wir bezeichnen $G/_{H}$ als \highl{Faktorgruppe} von $G$ nach $H$.
\end{defini}

\begin{bemerk}
  Der kanonische Homomorphismus $G\rightarrow G/_{H}, x\mapsto xH$ ist
  surjektiv. Sein Kern ist $H$.
\end{bemerk}

\begin{bsp}[Was ist kanonisch?]
  Ein endlich"=deminsionaler Vektorraum ist nichtkanonisch isomorph zu seinem
  Dualraum, jedoch kanonisch isomorph zu seinem Bidualraum.
\end{bsp}

%satz 2
\begin{satz}[Homomorphiesatz]
  \label{satz:2}
  Sei $f\colon G\rightarrow G'$ ein Homomorphismus von Gruppen, $H=\ker(f)$.
  Dann existiert genau ein Homomorphismus:
  \begin{gather*}
    f_{*}\colon G/_{H}\rightarrow G'
  \end{gather*}
  welcher das folgende Diagramm kommutativ macht:
    \begin{gather*}
    \begin{xy}
      \xymatrix{
      G \ar[rr]^{f} \ar[rd]_{\text{kan}} & & G'\ar@{.>}[dl]^{f_{*}}\\
      & G/_{H} &
	}
    \end{xy}
  \end{gather*}
  \todo{Pfeil rumdrehen}
  Die Abbildung $f_{*}$ verursacht einen Isomorphismus zwischen $G/_{H}$ und
  $f(G)$.
  \begin{proof}
    \begin{enumerate}
     \item (Eindeutigkeit von $f_{*}$): Seien $f_{*}, f_{*}'$ zwei solche
      Homomorphismen. Dann gilt $f_{*}(xH)=f(x)=f_{*}'(xH)\Rightarrow f_{*}'=
      f_{*}$.
     \item (Existenz von $f_{*}$): $f_{*}(xH)=f(x)$ nach Definition.
      Korrektheit: $xH=x'H\Rightarrow x'=xh, h\in H,
      f(x')=f(xh)=f(x)f(h)=f(x)$.\\
      Homomorphie: $f_{*}(xyH)=f(xy)=f(x)f(y)= f_{*}(xH)f_{*}(yH)$.
     \item (Injektivität): $f_{*}(xH)=1\Rightarrow f(x)=1\Rightarrow x\in H$.
     \item $f_{*}(G/_{H})=f(G)$ ist klar.
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bsp}
  \begin{enumerate}
   \item $G=\R$ mit der Addition, $H=\Z$ ist ein Normalteiler. Was ist
    $G/_{H}= \R/_{\Z}$? Antwort: Kreislinie! Sei $\phi\colon\R\rightarrow
    \C_{1}^{*}=\{z\in\C\colon\abs{z}=1\}, \phi(t)=e^{2\pi it}$ ist ein
    Homomorphismus: $e^{z_{1}+z_{2}}= e^{z_{1}}e^{z_{2}}$ und $\ker\phi=\Z,
    \Ima \phi=\C_{1}^{*}$. Nach dem Homomorphiesatz induziert $\phi$ einen
    Isomorphismus $\R/_{\Z}\rightarrow\C_{1}^{*}$.
   \item $G=\C$ mit Addition, $H=\Z+i\Z= \{ a+bi\colon a,b\in\Z\}$. Man
    bezeichnet $H$ auch als die \highl[Zahlen!ganze \textsc{Gauss}sche]{ganzen
    \textsc{Gauss}schen Zahlen}. Damit ist $G/_{H}$ ein Torus.
  \end{enumerate}
\end{bsp}

%satz 3
\begin{satz}[Isomorphiesätze]
  \begin{enumerate}
   \item Seien $K\subseteq H$ zwei Normalteiler in $G$. Dann ist $H/_{K}$ ein
    Normalteiler in $G/_{K}$ und es gibt einen kanonischen Isomorphismus
    \[(G/_{K})/_{(H/_{K})}\rightarrow G/_{H}\]
   \item Sei $K$ ein Normalteiler in $G$ und $H$ eine Untergruppe in $G$. Dann
    ist $HK\coloneqq\{hk\colon h\in H, k\in K\}$ eine Untergruppe in $G$ und
    $H\cap K$ ist Normalteiler in $H$. Weiterhin gibt es einen kanonischen
    Isomorphismus \[H/_{H\cap K}\rightarrow HK/_{K}\]
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item $K$ ist Normalteiler in $H$ und wir haben den kanonischen
      Homomorphismus $G/_{K}\rightarrow G/_{H}, xK\mapsto xH$. Dies ist
      offensichtlich ein surjektiver Homomorphismus. Es ist $\ker=\{ xK\colon
      xH=H\}=\{xK\colon x\in H\}=H/_{K}$. Nun wenden wir \autoref{satz:2} an.
     \item Zuerst zeigen wir, dass $HK$ eine Untergruppe ist: $1\in HK,
      h_{1}k_{1}\cdot h_{2}k_{2}= h_{1}(k_{1}h_{2})k_{2}$. Da $K$ ein
      Normalteiler ist, kann man schreiben: $k_{1}h_{2}=h_{2}k_{3}$ und es
      folgt: $h_{1}h_{2}k_{3}k_{2}\in HK$. Für das Inverse gilt: $(hk)^{-1}=
      k^{-1}h^{-1}= h^{-1}k_{1}\in HK$. Damit ist $HK$ eine Untergruppe.
      Weiter ist $K$ trivialerweise Normalteiler in $HK$, also ist $HK/_{K}$
      wohldefiniert. Der kanonische Homomorphismus ist $H\rightarrow HK/_{K},
      h\mapsto hK$. Die Surjektivität ist nicht ganz offensichtlich: Elemente
      aus $HK/_{K}$ sind der Form $hkK=hK, h\in H, k\in K$. Der Kern ist
      $\{h\in H\colon hK=K\}=\{h\in H\colon h\in K\}=H\cap K$ und die
      Anwendung des Homomorphiesatzes bringt die Behauptung.
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bsp}
  Sei $G=\C$ mit der Addition, $\phi\colon\C\rightarrow\C^{*}, e\mapsto
  e^{2\pi iz}$. Das ist ein surjektiver Homomorphismus mit dem Kern $\Z$. Sei
  $\Gamma=\Z+\Z i=\{a+bi\colon a,b\in\Z\}, \phi(\Gamma)= \{e^{-2\pi b}\colon
  b\in\Z\}$. Es folgt nach dem ersten Isomorphiesatz:
  $\C^{*}/_{\phi(\Gamma)}\cong \C/_{\Z}/_{\Gamma/_{\Z}}\cong \C/_{\Gamma}$.
\end{bsp}

\section{Das direkte Produkt von Gruppen}

\begin{defini}
  Seien $G_{1}, G_{2}$ zwei Gruppen. Dann wird $G_{1}\times G_{2}$ durch
  folgende Multiplikation zu einer Gruppe:
  \begin{gather*}
    (g_{1}, g_{2})(h_{1}, h_{2})= (g_{1}h_{1}, g_{2}h_{2})
  \end{gather*}
  Dies heißt \highl[Produkt!direktes]{direktes Produkt} von $G_{1}$ und
  $G_{2}$.
\end{defini}

\begin{fakt}
  Seien $H, K$ Untergruppen in $G$, so dass
  \begin{enumerate}
   \item $H\cap K=\{1\}$
   \item $HK=\{hk\colon h\in H, k\in K\}= G$
   \item $hk=kh$ für alle $h\in H, k\in K$
  \end{enumerate}
  Dann ist $f\colon H\times K\rightarrow G, (h,k)\mapsto hk$ ein Isomorphismus.
  \begin{proof}
    Dies ist ein Homomorphismus: $f((h_{1}, k_{1})(h_{2}, k_{2}))=
    f(h_{1}h_{2}, k_{1}k_{2})= h_{1}h_{2}k_{1}k_{2}= f(h_{1}, k_{1})f(h_{2},
    k_{2})$. Die Injektivität folgt aus: $f(h,k)=1\Leftrightarrow
    hk=1\Rightarrow h=k=1$ und die Surjektivität folgt aus der zweiten
    Eigenschaft.
  \end{proof}
\end{fakt}

\begin{bsp}
  Die zyklische Gruppe $\Z/_{6\Z}\cong\Z/_{3\Z}\times\Z/_{2\Z}$ wegen
  $H=\{0,2,4\}, K=\{0,3\}$.
\end{bsp}

