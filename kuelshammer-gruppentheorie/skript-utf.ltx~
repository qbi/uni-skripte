\chapter{Einführung}

\section{Zahlbereiche}

\begin{enumerate}
 \item $(\Z,+), (\Q, +)$
 \item $(\Q\setminus{0}, \cdot)$
 \item $\Z/_{n\Z}$ der Restklassenring bzw. die Restklassengruppe modulo $n$
  mit der Addition.
 \item $(\Z/_{n\Z})^{\times}={a+n\Z\colon\ggt(a,n)=1}$ prime Restklassengruppe
  modulo $n$ mit der Multiplikation
\end{enumerate}

\section{Lineare Algebra}

Sei $K$ ein Körper  und $V$ ein $K$"~Vektorraum. Dann haben wir:
\begin{enumerate}
 \item $\GL(n,K)={A\in K^{n\times n}\colon \abs{A}\neq0}$ mit der
  Matrixmultiplikation. Dies ist die allgemeine lineare Gruppe des Grades $n$
  über $K$.
 \item $\GL(V)={f\colon V\rightarrow V\colon f\text{ linear und bijektiv}}$.
 \item $\SL(n,K)={A\in K^{n\times n}\colon\abs{A}=1}$ die spezielle lineare
  Gruppe des Grades $n$ über $K$.
\end{enumerate}

Sei $V$ ein euklidischer Vektorraum mit dem Skalarprodukt
\begin{enumerate}
 \item $O(V)=\{f\colon V\rightarrow V\colon f\text{ Isometrie}\}$ ist die
  orthogonale Gruppe von $V$. Dabei bedeutet Isometrie, dass $f$ linear ist
  und für alle $x,y$ aus $V$ gilt: $\langle f(x), f(y)\rangle=\langle
  x,y\rangle$.
 \item $O(n)=O(n,\R)=\{A\in\R^{n\times n}\colon A^{T}A=1_{n}\}$ orthogonale
  Gruppe des Grades $n$.
\end{enumerate}

Sei $V$ ein unitärer Vektorraum mit dem Skalarprodukt
\begin{enumerate}
 \item $U(V)=\{f\colon V\rightarrow V\colon f\text{ Isometrie}\}$ unitäre
  Gruppe von $V$
 \item $U(n)=U(n,\C)=\{A\in\C^{n\times n}\colon\overline{A}^{T}A=1_{n}\}$
\end{enumerate}

\section{Kombinatorik}
Sei $\Omega$ eine Menge.
\begin{enumerate}
 \item $\Sym(\Omega)= \{f\colon\Omega\rightarrow\Omega\colon f\text{
  bijektiv}\}$ symmetrische Gruppe auf $\Omega$
 \item $\Alt(\Omega)= \{f\in\Sym(\Omega)\colon f\text{ gerade}\}$
  alternierende Gruppe auf $\Omega$. Dabei muss $\Omega$ endlich sein.
\end{enumerate}

\section{Geometrie}

\begin{enumerate}
 \item $\AO(\R^{n})=\{f\colon\R^{n}\rightarrow\R^{n}\colon\norm{f(x)-f(y)=
  \norm{x-y}} \forall x,y\in\R^{n}\}$ Bewegungsgruppe.
 \item \missingfigure{Bild der $P_{6}$"~Gruppe} Symmetriegruppe des
  regelmäßigen $n$"~Ecks $P_{n}$ mit $G=\{f\in\AO(\R^{2})\colon f(P_{n})=
  P_{n}\}$ \textsc{Dieder}gruppe. Die Gruppe hat $2n$~Elemente.
 \item Friesgruppen: Symmetriegruppen von Friesen.
 \item kristallografische Gruppen in der Ebene oder im Raum:
  \missingfigure{Symmetrisches Gitter}
 \item Symmetriegruppen von Tetraeder, Würfel, Ikosaeder und ähnlichen Objekten.
\end{enumerate}

\section{Algebra}

Sei $L|K$ eine Körpererweiterung. Diese besitzt eine \textsc{Galois}gruppe
$\F(L|K)=\{f\colon L\rightarrow L\colon f \text{ Automorphismus von } L,
f|K=\iota_{K}\}$.

\section{Topologie}
\begin{enumerate}
 \item Fundamentalgruppen, Homologiegruppen, \dots{}, 
\end{enumerate}

\section{Zahlentheorie}
Sei $K$ ein algebraischer Zahlkörper und $\CO_{K}$ der Ganzheitsring.
\begin{enumerate}
 \item Einheitengruppe: $\CO_{K}^{\times}= \{a\in \CO_{K}\setminus\{0\}\colon
  \nicefrac{1}{a}\in\CO_{K}\}$
 \item Klassengruppe
\end{enumerate}

\section{Beliebige mathematische Theorie}

Sei $M$ ein Objekt dieser Theorie. Dann hat man mindestens eine Gruppe, die
Automorphismengruppe $\Aut(M)$ von $M$. Beispiele sind Lie"~Algebren,
Codierungstheorie, \textsc{Banach}räume etc.


\chapter{Halbgruppen}

\begin{defini}[Halbgruppe]
  Sei $M$ eine Menge. Darauf legen wir eine \highl{Verknüpfung} fest $M\times
  M\rightarrow M$ mit $(a,b)\mapsto a\times b$ (alternativ auch $a+b, a\cdot
  b$ oder $ab$). Dann ist $(M,\times)$ eine \highl{Monade} oder auch
  \highl{Magma}.
\end{defini}

\begin{bsp}
  \begin{enumerate}[(i)]
   \item Die Addition, Subtraktion und Multiplikation auf den natürlichen,
    rellen und komplexen Zahlen.
   \item Der Durchschnitt oder die Vereinigung auf der Potenzmenge $\FP(X)$.
   \item Der größte gemeinsame Teiler oder das kleinste gemeinsame Vielfache
    auf $\N$.
   \item $\circ$ auf $\Abb(X)=\{f\colon X\rightarrow X\colon f\text{
    Abbildung}\}$
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  Wenn $M$ klein ist, kann man eine Verknüpfungstafel aufstellen:
  \todo{Verknüpfungstafel}
  Ein Beispiel sind die Wahrheitswerte.
\end{bemerk}

\begin{defini}[rechts-, linksneutral, neutral]
  Sei $M$ eine Monade und $e\in M$. Das Element $e$ ist genau dann
  \highl{rechtsneutral}, wenn für alle $a\in M$ gilt: $ae=a$. Man nennt $e$
  genau dann  neutral, wenn es rechts- und linksneutral ist.
\end{defini}

\begin{bemerk}
  Sei $e\in M$ linksneutral und $f\in M$ rechtsneutral. Dann folgt, $e=ef=f$.
  Insbesondere existiert in $M$ höchstens ein neutrales Element.
\end{bemerk}

\begin{bsp}
  Die $0$ ist neutral in $(\Z,+)$ und die $1$ ist neutral in $(\Z,\cdot)$.
\end{bsp}

\begin{defini}[Vertauschbarkeit, Kommutativität]
  Sei $M$ eine Monade und $a,b\in M$. Die Elemente $a$ und $b$ heißen
  \highl{vertauschbar}, wenn gilt: $ab=ba$. Die Menge $M$ heißt
  \highl{kommutativ} oder \highl{abelsch}, wenn alle Elemente vertauschbar sind.
\end{defini}

\begin{defini}[Halbgruppe, Monoid]
  Die Menge $M$ heißt genau dann \highl{Halbgruppe}, wenn $(xy)z= x(yz)$ und
  ein \highl{Monoid}, wenn $M$ eine Halbgruppe mit neutralem Element ist.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item $(\N,+)$ Halbgruppe, $(\N_{0}, +)$ Monoid
   \item $X$ Menge $\Rightarrow\Abb(X)$ Monoid, neutrales Element
    $\id_{X}\colon X\rightarrow X, x\mapsto x$ identische Abbildung
   \item Sei $A\neq\emptyset$ eine Menge (\highl{Alphabet}). Die Elemente von
    $A$ heißen \highl{Buchstaben}. Ein \highl{Wort} über $A$ ist die endliche
    Folge $w=(a_{1},\dotsc, a_{m})=\colon a_{1}\cdot\dotso \cdot a_{m}$. Die
    \highl[Halbgruppe!freie]{freie Halbgruppe} über $A$ ist definiert als
    $W\coloneq\{w\colon w\text{ Wort über } A\}$. Das
    \highl[Wort!leeres]{leere Wort} ist $\epsilon=()\notin W$. Dann können wir
    das \highl[Monoid!freies]{freie Monoid} über $A$ mit $W_{0}\coloneq
    W\cup\{\epsilon\}$ definieren.
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  Die neutralen Elemente werden oft mit $1$ bezeichnet. Falls $+$ die
  Verknüpfung bezeichnet, verwendet man auch $0$ als neutrales Element.
\end{bemerk}

%def 1.4
\begin{defini}[Invertierbarkeit]
  Sei $M$ ein Monoid und $a\in M$. Das Element $a$ heißt genau dann
  \highl{rechtsinvertierbar} (oder \highl{linksinvertierbar}), wenn ein
  Element $b\in M$ mit $ab=1$ ($ba=1$) existiert. Man bezeichnet $b$ als das
  \highl{Rechtsinverse} oder \highl{Linksinverse} zu $a$.
\end{defini}

\begin{bemerk}
  Sei $b\in M$ rechtsinvers und $c\in M$ linksinvers zu $a$. Dann folgt,
  $b=1b=(ca)b= c(ab)= c1=c$. Das Element $a$ heißt dann \highl{invertierbar}
  und $b$ \highl{Inverses} zu $a$. Wir schreiben $b=\colon a^{-1}$ oder bei
  der Addition $b=\colon-a$. Es gilt: $a^{-1}a=1=aa^{-1}$. Damit ist auch
  $a^{-1}$ invertierbar und wir haben $(a^{-1})^{-1}=a$. Wenn zwei Elemente
  $x,y\in M$ invertierbar sind, dann ist $xy$ invertierbar und $(xy)^{-1}=
  y^{-1}x^{-1}$. Denn $xyy^{-1}x^{-1}=x1x^{-1}=1$.
\end{bemerk}

\begin{bsp}
  Sei $K$ ein Körper und $n$ eine natürliche Zahl. Dann folgt für das Monoid
  bezüglich der Matrxmultiplikation $K^{n\times n}$ mit der Einheitsmatrix als
  natürliches Element. Eine Matrix $A\in K^{n\times n}$ ist invertierbar, wenn
  $A$ eine reguläre Matrix ist, d.\,h. die Matrix von $A$ ist ungleich $0$. In
  der linearen Algebra impliziert die Linksinvertierbarkeit auch die
  Rechtsinvertierbarkeit.
\end{bsp}

%def 1.5
\begin{defini}[Potenz]
  Sei $H$ eine Halbgruppe und $a\in H, n\in N$. Die $n$"~te \highl{Potenz} von $a$ ist
  definiert als $a^{n}\coloneq a\cdot\dotso \cdot a$ mit $n$~Faktoren. Wenn
  $H$ ein Monoid ist, gilt: $a^{0}\coloneq1$. Sollte $a$ invertierbar sein,
  dann können wir negative Potenzen festlegen: $a^{-n}= (a^{-1})^{n}$.
\end{defini}

\begin{bemerk}
  Rechenregel: $a^{n}a^{m}= a^{n+m}, (a^{n})^{m}=a^{nm}$, wenn $a,b$
  vertauschbar sind, gilt auch: $(ab)^{n}= a^{n}b^{n}$. Ist $+$ die
  Verknüpfung, so schreibt man: $na$ statt $a^{n}$. Dann sehen die
  Rechenregeln so aus: $(m+n)a= ma+na, m(na)= (mn)a$, wenn $a,b$ vertauschbar
  sind, so gilt noch: $n(a+b)= na+nb$.
\end{bemerk}

%def 1.6
\begin{defini}[Homomorphismus]
  Seien $M,N$ zwei Monaden und wir betrachten die Abbildung $f\colon
  M\rightarrow N$.
  \begin{enumerate}
   \item Die Abbildung $f$ heißt genau dann \highl{Homomorphimus}, wenn für
    alle $a,b\in M$ gilt: $f(ab)=f(a)f(b)$.
   \item $f$ Monomorphismus $\Leftrightarrow f$  injektiver Homomorphimus
   \item $f$ Epimorphismus $\Leftrightarrow f$ surjektiver Homomorphimus
   \item $f$ Isomorphismus $\Leftrightarrow f$ injektiver Homomorphimus
   \item $f$ Endomorphismus $\Leftrightarrow f$ Homomorphimus und $M=N$
   \item $f$ Automorphismus $\Leftrightarrow f$ bijektiver Endomorphismus
  \end{enumerate}
  Wir setzen $\Hom(M,N)\coloneq\{f\colon M\rightarrow N\colon f \text{
  Homomorphimus}\}, \End(M)\coloneq\Hom(M,M),
  \Aut(M)\coloneq\{f\in\End(M)\colon f\text{ bijektiv}\}$.
\end{defini}





