\chapter{Lineare Gruppen}

%satz 14.1
\begin{satz}Lemma von \textsc{Iwasawa}]
  Sei $G$ eine perfekte Gruppe, $\underline{O}$ eine treue, primitive $G$"~Menge,
  $\alpha\in \underline{O}$ und $A$ ein auflösbarer Normalteiler von
  $H\coloneq\Stb_{G}(\alpha)$ mit $G=\langle gAg^{-1}\colon g\in G\rangle$.
  Dann ist $G$ einfach.
  \begin{proof}
    Sei $1\lhd N\unlhd G$. Da $G$ treu und primitiv auf $\underline{O}$
    operiert, ist $N$ transitiv auf $\underline{O}$. Nach dem Argument von
    \textsc{Frattini} ist also $G= NH$. Wegen $A\unlhd H$ ist $H\subseteq
    N_{G}(NA)$. Wegen $NA\subseteq N_{G}(NA)$ ist $G= NH\subseteq
    N_{G}(NA)\subseteq G$, d.\,h. $NA\unlhd G$. Daher:
    \begin{gather*}
      G= \langle gAg^{-1}\colon g\in G\rangle= \langle gNAg^{-1}\colon g\in
	 G\rangle= NA
    \end{gather*}
    Folglich: $G/N= AN/N\cong A(A\cap N)$ auflösbar. Andererseits ist $(G/N)'=
    G'N/N=G/N$. Insgesamt haben wir $G/N=1$, d.\,h. $N=G$.
  \end{proof}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item  Seien $\KK$ ein Körper und $V$ ein endlich"=dimensionaler $\KK$"~Vektorraum.
  Dann gilt:
  \begin{gather*}
    Z\coloneq\{\alpha\id_{V}\colon\alpha\in\KK^{*}\}\subseteq Z(\GL(V))
  \end{gather*}
  Denn für $g\in\GL(V), \alpha\in\KK^{*}, v\in V$ gilt:
  $(g(\alpha\id_{V})g^{-1})(v)= g(\alpha g^{-1}(v))= \alpha g(g^{-1}(v))=
  (\alpha\id_{V})(v)$. Man nennt $\PGL(V)\coloneq\GL(V)/Z$
  \highl[Gruppe!projektive allgemeine lineare]{projektive allgemeine lineare
    Gruppe} von $V$.

   \item Aus (i) folgt: $Z\cap\SL(V)= \{\alpha\id_{V}\colon\alpha\in\KK,
    \alpha^{\dim V}=1\}\leq Z(\SL(V))$. Man nennt
    $\PSL(V)\coloneq\SL(V)/(\SL(V)\cap Z)\cong\SL(V)Z/Z\leq\PGL(V)$ die
    \highl[Gruppe!projektive spezielle lineare Gruppe]{projektive spezielle
      lineare Gruppe} von $V$.
   \item Für $n\in\N$ ist also
    $Z\coloneq\{\alpha1_{n}\colon\alpha\in\KK^{*}\}\leq Z(\GL(n,\KK))$ und
    $\GL(n,\KK)/Z=\colon\PGL(n,\KK)$ heißt \highl[Gruppe!projektive allgemeine
    lineare Gruppe]{projektive allgemeine lineare Gruppe} des Grades $n$ über
    $\KK$.
   \item Daher ist $Z\cap\SL(n,\KK)= \{\alpha1_{n}\colon\alpha\in\KK,
    \alpha^{n}=1\}\leq Z(\SL(n,\KK))$ und
    $\PSL(n,\KK)\coloneq\SL(n,\KK)/\SL(n,\KK)\cap Z$ heißt
    \highl[Gruppe!projektive spezielle lineare]{projektive spezielle lineare
      Gruppe} de Grades $n$ über $\KK$.
   \item Für jeden $\KK$"~Vektorraum $V$ der Dimension $n<\infty$ gilt:
    \begin{align*}
      \GL(V) &\cong\GL(n,\KK) & \SL(V) &\cong\SL(n,\KK)\\
      \PGL(V) &\cong\PGL(n,\KK) & \PSL(V) &\cong\PSL(n,\KK)
    \end{align*}
  \end{enumerate}
\end{bemerk}

%bem 14.2
\begin{bemerk}
  Für jeden Körper $\KK$ und jeden $\KK$"~Vektorraum $V$ mit $1<\dim V<\infty$
  operiert $\GL(V)$ auf der Menge $\underline{O}$ aller eindimensionalen
  Untervektorräume $U\subseteq V$:
  \begin{gather*}
    \vorhoch{g}U\coloneq g(U)\qquad g\in \GL(U), U\in\underline{O}
  \end{gather*}
  Dabei operiert $Z=\{\alpha\id_{V}\colon\alpha\in\KK^{*}\}$ trivial auf
  $\underline{O}$. Daher operiert auch $\PGL(V)=\GL(V)/Z$ und $\PSL(V)=
  \SL(V)/\SL(V)\cap Z$ auf $\underline{O}$:
  \begin{align*}
    \vorhoch{\overline{g}}U &\coloneq \vorhoch{g}U\coloneq g(U) & g &\in\GL(V),
       \overline{g}\coloneq gZ, U\in\underline{O}\\
    \vorhoch{\overline{g}}U &\coloneq \vorhoch{g}U\coloneq g(U) & g &\in\SL(V),
       \overline{g}\coloneq g(\SL(V)\cap Z), U\in\underline{O}
  \end{align*}
\end{bemerk}

\begin{satz}
  Die Operation von $\PSL(V)$ ist treu und 2"~transitiv.
  \begin{proof}
    Seien $U_{1}\coloneq\KK u_{1}, U_{2}\coloneq\KK u_{2}\in\underline{O}$
    verschieden. Dann sind $u_{1}$ und $u_{2}$ linear unabhängig und lassen
    sich zu einer Basis $u_{1},\dotsc, u_{n}$ ergänzen.  Sind auch $W_{1}=\KK
    w_{1}, W_{2}=\KK w_{2}\in\underline{O}$ verschieden, so erhält man analog
    eine Basis $w_{1},\dotsc, w_{n}$ von $V$.

    Dann existiert genau ein $g\in\GL(V)$ mit $g(U_{i})=W_{i}$. Sei
    $\delta_{i}= \det(g)$ für $i=1,\dotsc,n$. Dann existiert genau ein
    $h\in\GL(V)$ mit $h(U_{i})= \delta^{-1}w$ und $h(U_{i})=w_{i}$ für
    $i=2,\dotsc,n$.   Dabei ist $\det(h)=1$, d.\,h. $h\in\SL(V)$. Dann
    $\vorhoch{h}U_{1}= W_{1}, \vorhoch{h}U_{2}= W_{2}$.

    Da $Z$ sowieso trivial auf $\underline{O}$ operiert, operiert $\PSL(V)$
    zweitransitiv auf $V$. Sei $g\in\SL(V)$ im Kern der Operation. Für
    $1=1,\dotsc,n$ existiert dann ein $\alpha_{i}\in\KK$ mit $g(U_{i})=
    \alpha_{i}u_{i}$. Für verschiedene $i,j\in\{1,\dotsc,n\}$ existiert auch
    ein $b_{ij}\in\KK$ mit $g(u_{i}+u_{j})= b_{ij}(u_{i}+u_{j})$. Dann:
    $b_{ij}u_{i}+ b_{ij}u_{j}= g(u_{i}+u_{j})= g(u_{i})+g(u_{j})=
    \alpha_{i}u_{i}+\alpha_{j}u_{j}$. Wegen der Basiseigenschaft folgt damit
    $\alpha_{i}=b_{ij}=\alpha_{j}$.
    Also ist $g= \alpha_{1}\id_{V}\in Z$ und daher ist $Z\cap\SL(V)$ der Kern
    der Operation von $\SL(V)$ auf $\underline{O}$ und $\PSL(V)$ operiert
    treu auf $\underline{O}$.
  \end{proof}
\end{satz}

%bem 14.3
\begin{bemerk}
  Seien $\KK$ ein Körper und $n$ eine natürliche Zahl. Wir bezeichnen die
  Standardbasis von $\KK^{n\times n}$ mit $e_{ij}$ für $i,j=\{1,\dotsc,n\}$.
  Beispielsweise für $n=2$:
  \begin{align*}
    e_{11} &=
       \begin{pmatrix}
	 1 & 0\\
	 0 & 0
       \end{pmatrix} &
       e_{12} &=
       \begin{pmatrix}
	 0 & 1\\
	 0 & 0
       \end{pmatrix}\\
