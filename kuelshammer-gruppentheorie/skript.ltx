\chapter{Einführung}



\section{Zahlbereiche}

\begin{enumerate}
 \item $(\Z,+), (\Q, +)$
 \item $(\Q\setminus\{0\}, \cdot)$
 \item $\Z/_{n\Z}$ der Restklassenring bzw. die Restklassengruppe modulo $n$
  mit der Addition.
 \item $(\Z/_{n\Z})^{\times}=\{a+n\Z\colon\ggt(a,n)=1\}$ prime Restklassengruppe
  modulo $n$ mit der Multiplikation
\end{enumerate}

\section{Lineare Algebra}

Sei $K$ ein Körper  und $V$ ein $K$"~Vektorraum. Dann haben wir:
\begin{enumerate}
 \item $\GL(n,K)=\{A\in K^{n\times n}\colon \abs{A}\neq0\}$ mit der
  Matrixmultiplikation. Dies ist die allgemeine lineare Gruppe des Grades $n$
  über $K$.
 \item $\GL(V)=\{f\colon V\rightarrow V\colon f\text{ linear und bijektiv}\}$.
 \item $\SL(n,K)=\{A\in K^{n\times n}\colon\abs{A}=1\}$ die spezielle lineare
  Gruppe des Grades $n$ über $K$.
\end{enumerate}

Sei $V$ ein euklidischer Vektorraum mit dem Skalarprodukt
\begin{enumerate}
 \item $O(V)=\{f\colon V\rightarrow V\colon f\text{ Isometrie}\}$ ist die
  orthogonale Gruppe von $V$. Dabei bedeutet Isometrie, dass $f$ linear ist
  und für alle $x,y$ aus $V$ gilt: $\langle f(x), f(y)\rangle=\langle
  x,y\rangle$.
 \item $O(n)=O(n,\R)=\{A\in\R^{n\times n}\colon A^{T}A=1_{n}\}$ orthogonale
  Gruppe des Grades $n$.
\end{enumerate}

Sei $V$ ein unitärer Vektorraum mit dem Skalarprodukt
\begin{enumerate}
 \item $U(V)=\{f\colon V\rightarrow V\colon f\text{ Isometrie}\}$ unitäre
  Gruppe von $V$
 \item $U(n)=U(n,\C)=\{A\in\C^{n\times n}\colon\overline{A}^{T}A=1_{n}\}$
\end{enumerate}

\section{Kombinatorik}
Sei $\Omega$ eine Menge.
\begin{enumerate}
 \item $\Sym(\Omega)= \{f\colon\Omega\rightarrow\Omega\colon f\text{
  bijektiv}\}$ symmetrische Gruppe auf $\Omega$
 \item $\Alt(\Omega)= \{f\in\Sym(\Omega)\colon f\text{ gerade}\}$
  alternierende Gruppe auf $\Omega$. Dabei muss $\Omega$ endlich sein.
\end{enumerate}

\section{Geometrie}

\begin{enumerate}
 \item $\AO(\R^{n})=\{f\colon\R^{n}\rightarrow\R^{n}\colon\norm{f(x)-f(y)}=
  \norm{x-y} \forall x,y\in\R^{n}\}$ Bewegungsgruppe.
 \item Symmetriegruppe des
  regelmäßigen $n$"~Ecks $P_{n}$ mit $G=\{f\in\AO(\R^{2})\colon f(P_{n})=
  P_{n}\}$ \textsc{Dieder}gruppe. Die Gruppe hat $2n$~Elemente.\\
