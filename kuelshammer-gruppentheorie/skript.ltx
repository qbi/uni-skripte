\chapter{Halbgruppen}

\begin{defini}[Monade, Magma]
  Sei $M$ eine Menge. Darauf legen wir eine \highl{Verknüpfung} $M\times
  M\rightarrow M$ mit $(a,b)\mapsto a\times b$ (alternativ auch $a+b, a\cdot
  b$ oder $ab$) fest. Dann ist $(M,\times)$ eine \highl{Monade} oder auch
  \highl{Magma}.
\end{defini}

\begin{bsp}
  \begin{enumerate}[(i)]
   \item Die Addition, Subtraktion und Multiplikation auf den natürlichen,
    reellen und komplexen Zahlen.
   \item Der Durchschnitt oder die Vereinigung auf der Potenzmenge $\FP(X)$.
   \item Der größte gemeinsame Teiler oder das kleinste gemeinsame Vielfache
    auf $\N$.
   \item Die Verknüpfung von Abbildungen $\circ$ auf der Menge aller Abbildungen
    $\Abb(X)=\{f\colon X\rightarrow X\colon f\text{ Abbildung}\}$
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  Wenn $M$ klein ist, kann man eine Verknüpfungstafel aufstellen.  Ein
  Beispiel sind die Wahrheitswerte.
\begin{table}[htb]
\centering
\begin{tabular}{c|cc}
  $\wedge$ & $w$ & $f$\\
  \hline
  $w$ & $w$ & $f$\\
  $f$ & $f$ & $f$
\end{tabular}
\caption{Wahrheitswerte für die UND-Verknüpfung}
\end{table}
\end{bemerk}

\begin{defini}[rechts-, linksneutral, neutral] Sei $M$ eine Monade und $e\in
  M$. Das Element $e$ ist genau dann \highl{rechtsneutral} (oder
  \highl{linksneutral}), wenn für alle $a\in M$ gilt: $ae=a$ (oder $ea=a$). Man
  nennt $e$ genau dann \highl{neutral}, wenn es rechts- und linksneutral ist.
\end{defini}

\begin{bemerk}
  Sei $e\in M$ linksneutral und $f\in M$ rechtsneutral. Dann folgt, $e=ef=f$.
  Insbesondere existiert in $M$ höchstens ein neutrales Element.
\end{bemerk}

\begin{bsp}
  Die $0$ ist neutral in $(\Z,+)$ und die $1$ ist neutral in $(\Z,\cdot)$.
\end{bsp}

\begin{defini}[Vertauschbarkeit, Kommutativität]
  Sei $M$ eine Monade und $a,b\in M$. Die Elemente $a$ und $b$ heißen
  \highl{vertauschbar}, wenn gilt: $ab=ba$. Die Menge $M$ heißt
  \highl{kommutativ} oder \highl{abelsch}, wenn alle Elemente vertauschbar sind.
\end{defini}

\begin{defini}[Halbgruppe, Monoid]
  Die Menge $M$ heißt genau dann \highl{Halbgruppe}, wenn $(xy)z= x(yz)$ gilt
  und \highl{Monoid}, wenn $M$ eine Halbgruppe mit neutralem Element ist.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item $(\N,+)$ Halbgruppe, $(\N_{0}, +)$ Monoid
   \item $X$ Menge $\Rightarrow\Abb(X)$ Monoid, neutrales Element
    $\id_{X}\colon X\rightarrow X, x\mapsto x$ identische Abbildung
   \item Sei $A\neq\emptyset$ eine Menge (\highl{Alphabet}). Die Elemente von
    $A$ heißen \highl{Buchstaben}. Ein \highl{Wort} über $A$ ist die endliche
    Folge $w=(a_{1},\dotsc, a_{m})=\colon a_{1}\cdot\dotso \cdot a_{m}$. Die
    \highl[Halbgruppe!freie]{freie Halbgruppe} über $A$ ist definiert als
    $W\coloneq\{w\colon w\text{ Wort über } A\}$. Das
    \highl[Wort!leeres]{leere Wort} ist $\epsilon=()\notin W$. Dann können wir
    das \highl[Monoid!freies]{freie Monoid} über $A$ mit $W_{0}\coloneq
    W\cup\{\epsilon\}$ definieren.
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  Die neutralen Elemente werden oft mit $1$ bezeichnet. Falls $+$ die
  Verknüpfung bezeichnet, verwendet man auch $0$ als neutrales Element.
\end{bemerk}

%def 1.4
\begin{defini}[Invertierbarkeit]
  Sei $M$ ein Monoid und $a\in M$. Das Element $a$ heißt genau dann
  \highl{rechtsinvertierbar} (oder \highl{linksinvertierbar}), wenn ein
  Element $b\in M$ mit $ab=1$ ($ba=1$) existiert. Man bezeichnet $b$ als das
  \highl{Rechtsinverse} oder \highl{Linksinverse} zu $a$.
\end{defini}

\begin{bemerk}
  Sei $b\in M$ rechtsinvers und $c\in M$ linksinvers zu $a$. Dann folgt,
  $b=1b=(ca)b= c(ab)= c1=c$. Das Element $a$ heißt dann \highl{invertierbar}
  und $b$ \highl{Inverses} zu $a$. Wir schreiben $b=\colon a^{-1}$ oder bei
  der Addition $b=\colon-a$. Es gilt: $a^{-1}a=1=aa^{-1}$. Damit ist auch
  $a^{-1}$ invertierbar und wir haben $(a^{-1})^{-1}=a$. Wenn zwei Elemente
  $x,y\in M$ invertierbar sind, dann ist $xy$ invertierbar und $(xy)^{-1}=
  y^{-1}x^{-1}$. Denn $xyy^{-1}x^{-1}=x1x^{-1}=1$.
\end{bemerk}

\begin{bsp}
  Sei $K$ ein Körper und $n$ eine natürliche Zahl. Dann folgt für das
  Monoid bezüglich der Matrixmultiplikation $K^{n\times n}$ mit der
  Einheitsmatrix als natürliches Element, dass eine Matrix $A\in
  K^{n\times n}$ invertierbar ist, wenn $A$ eine reguläre Matrix ist,
  d.\,h. die Determinante von $A$ ist ungleich $0$. In der linearen Algebra
  impliziert die Linksinvertierbarkeit auch die
  Rechtsinvertierbarkeit.
\end{bsp}

%def 1.5
\begin{defini}[Potenz]
  Sei $H$ eine Halbgruppe und $a\in H, n\in \N$. Die $n$"~te \highl{Potenz} von $a$ ist
  definiert als $a^{n}\coloneq a\cdot\dotso \cdot a$ mit $n$~Faktoren. Wenn
  $H$ ein Monoid ist, gilt: $a^{0}\coloneq1$. Sollte $a$ invertierbar sein,
  dann können wir negative Potenzen festlegen: $a^{-n}= (a^{-1})^{n}$.
\end{defini}

\begin{bemerk}
  Rechenregel: $a^{n}a^{m}= a^{n+m}, (a^{n})^{m}=a^{nm}$, wenn $a,b$
  vertauschbar sind, gilt auch: $(ab)^{n}= a^{n}b^{n}$. Ist $+$ die
  Verknüpfung, so schreibt man: $na$ statt $a^{n}$. Dann sehen die
  Rechenregeln so aus: $(m+n)a= ma+na, m(na)= (mn)a$, wenn $a,b$ vertauschbar
  sind, so gilt noch: $n(a+b)= na+nb$.
\end{bemerk}

%def 1.6
\begin{defini}[Homomorphismus]
  Seien $M,N$ zwei Monaden und wir betrachten die Abbildung $f\colon
  M\rightarrow N$.
  \begin{enumerate}
   \item Die Abbildung $f$ heißt genau dann \highl{Homomorphismus}, wenn für
    alle $a,b\in M$ gilt: $f(ab)=f(a)f(b)$.
   \item $f$ Monomorphismus $\Leftrightarrow f$  injektiver Homomorphismus
   \item $f$ Epimorphismus $\Leftrightarrow f$ surjektiver Homomorphismus
   \item $f$ Isomorphismus $\Leftrightarrow f$ bijektiver Homomorphismus
   \item $f$ Endomorphismus $\Leftrightarrow f$ Homomorphismus und $M=N$
   \item $f$ Automorphismus $\Leftrightarrow f$ bijektiver Endomorphismus
  \end{enumerate}
  Wir setzen $\Hom(M,N)\coloneq\{f\colon M\rightarrow N\colon f \text{
  Homomorphismus}\}, \End(M)\coloneq\Hom(M,M)$ und 
  $\Aut(M)\coloneq\{f\in\End(M)\colon f\text{ bijektiv}\}$.
\end{defini}

%Vorlesung vom 2009-04-16
\begin{bsp}
  \begin{enumerate}
   \item Sei $K$ ein Körper und $n$ eine natürliche Zahl. Dann  ist
    $\det\colon(K^{n\times n},\cdot)\rightarrow(K,\cdot)$ ein Homomorphismus.
   \item Die Exponentialfunktion von $(\R,+)$ auf $(\R,\cdot)$.
   \item Sei $W$ die freie Halbgruppe über einem Alphabet $A$ und $w\in W$ mit
    $w=a_{1},\dotsc, a_{n}$ und $a_{1},\dotsc, a_{n}\in A$. Die Funktion
    $l(w)\coloneq n$ ist die Länge des Wortes. Dann ist $l\colon
    W\rightarrow(\N,+)$ ein Homomorphismus.
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  \begin{enumerate}
   \item Seien $L,M,N$ Monaden und $f\in\Hom(L,M), g\in\Hom(M,N)$. Dann ist
    $g\circ f\in\Hom(L,N)$ ein Homomorphismus. Denn $(g\circ f)(ab)= g(f(ab))=
    g(f(a)f(b))= g(f(a))g(f(b))= (g\circ f)(a)\cdot(g\circ f)(b)$ für $a,b\in
    L$.
   \item Sei $f$ ein Isomorphismus. Dann ist $f^{-1}$ ebenfalls ein
    Isomorphismus. Denn $f^{-1}(xy)=f^{-1}(f(f^{-1}(x))\cdot f(f^{-1}(y)))=
    f^{-1}(f(f^{-1}(x)\cdot f^{-1}(y)))= f^{-1}(x)f^{-1}(y)$.
  \end{enumerate}
\end{bemerk}

%def 1.7
\begin{defini}[isomorph]
  Die Monaden $M,N$ heißen \highl{isomorph} ($M\cong N$), falls ein Isomorphismus
  $f\colon M\rightarrow N$ existiert.
\end{defini}

\begin{bsp}
  Es gilt: $(\{w,f\},\vee)\cong(\{0,1\}, \cdot)$. Zum Nachweis kann man die
  Verknüpfungstafel prüfen.
\end{bsp}

\begin{satz}
  Die Isomorphie ist eine Äquivalenzrelation, d.\,h. es gilt:
  \begin{enumerate}
   \item $M\cong M$ (Reflexivität)
   \item $M\cong N\Rightarrow N\cong M$ (Symmetrie)
   \item $L\cong M\wedge M\cong N\Rightarrow L\cong N$ (Transitivität)
  \end{enumerate}
\end{satz}

\chapter{Gruppen}

%def 2.1
\begin{defini}[Gruppe]
  Eine \highl{Gruppe} ist eine Halbgruppe $G$ mit einem linksneutralen Element
  $e$, in der zu jedem Element $g\in G$ ein weiteres $h\in G$ mit $hg=e$
  existiert.
\end{defini}

\begin{satz}
  Eine Gruppe ist ein Monoid, in dem jedes Element invertierbar ist.
  \begin{proof}
    Seien $G,e,g,h$ wie oben. Zu dem Element $h$ existiert ein $k\in G$ mit
    $kh=e$. Dann ist $ke= khg=e g =g$ und weiter $ge=kee=ke=g$. Folglich ist
    $e$ neutral.
    Für den Nachweis der Rechtsinvertierbarkeit sei $g=ke=k$, d.\,h. $gh=e$.
    Somit ist $g$ invertierbar.
  \end{proof}
\end{satz}


\begin{bsp}
  \begin{enumerate}
   \item $(\Z,+), (\Q,+), (\R,+)$ sind abelsch. Dagegen ist $(\N,+)$
    \emph{keine} Gruppe.
   \item $(\Q\setminus\{0\}, \cdot), (\R\setminus\{0\}, \cdot),
    (\C\setminus\{0\}, \cdot)$. Aber $(\Z\setminus\{0\}, \cdot)$ ist
    \emph{keine} Grupe.
   \item Sei $M$ ein Monoid und $U(M)\coloneq\{a\in M\colon a\text{
    invertierbar}\}$. Dann heißt $U(M)$ \highl{Einheitengruppe} von $M$.
   \item Sei $X$ eine Menge und die Einheiten aller Abbildungen von $X$ in
    sich: $U(\Abb(X))=\{f\colon X\rightarrow X\colon f\text{ bijektiv}\}$.
    Dies wird als \highl[Gruppe!symmetrische]{symmetrische Gruppe} $\Sym(X)$
    auf $X$ bezeichnet. Die Elemente der Gruppe heißen \highl{Permutation}en
    auf $X$. Für $X=\{1,\dotsc,n\}$ schreibt man $\Sym(n)\coloneq\Sym(X)$. Die
    Elemente heißen dann Permutationen des Grades $n$. Schreibweise:
    \begin{gather*}
      f=
	 \begin{pmatrix}
	   1 & 2 & \dotso & n\\
	   f(1) & f(2) & \dotso & f(n)
	 \end{pmatrix}
    \end{gather*}
    Dann ist
    \begin{gather*}
      f=
	 \begin{pmatrix}
	   f(1) & f(2) & \dotso & f(n)\\
	   1 & 2 & \dotso & n
	 \end{pmatrix}
    \end{gather*}
   \item Sei $K$ ein Körper und $n\in\N$. Dann ist $U(K^{n\times n},
    \cdot)=\{A\in K^{n\times n}\colon\det A\neq0\}=\colon\GL(n,K)$.
   \item Für jede nichtleere Familie $(G_{i})_{i\in I}$ von Gruppen $G_{i}$.
    Dann ist auch das \highl[Produkt!direktes]{direkte Produkt} $\prod_{i\in
    I} G_{i}= \bigtimes_{i\in I}G_{i}= \{(g_{i})_{i\in I}\colon g_{i}\in G_{i}
    \forall i\in I\}$ eine Gruppe
    mit $(g_{i})(h_{i})\coloneq(g_{i}h_{i})$. Im Fall $I=\{1,\dotsc,n\}$
    schreibt man $\prod_{i=1}^{n} G_{i} = \bigtimes_{i=1}^{n}= G_{1}\times\dotso
    \times G_{n}$.
  \end{enumerate}
\end{bsp}

%def 2.2
\begin{defini}[Ordnung]
  Die \highl{Ordnung} $\abs{G}$ einer Gruppe ist die Anzahl ihrer Elemente.
\end{defini}

\begin{satz}
  \begin{enumerate}
   \item Sei $n\in N$. Dann ist $\abs{\Sym(n)}=n!$.
   \item Sei $K$ ein Körper und $\abs{K}=q<\infty$. Dann ist $\abs{\GL(n,K)}=
    (q^{n}-1)(q^{n}-q)\dotso (q^{n}-q^{n-1})$.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item Sei $f\in\Sym(n)\Rightarrow f(1)\in\{1,\dotsc,n\},
      f(2)\in\{1,\dotsc,n\}\setminus\{f(1)\},
      f(3)\in\{1,\dotsc,n\}\setminus\{f(1), f(2)\},\dotsc$.
     \item Sei $A=(a_{ij})\in\GL(n,K)\Rightarrow a_{1}\coloneq(a_{11},\dotsc,
      a_{1n})\in K^{n}\setminus\{0\}, a_{2}\coloneq(a_{21},\dotsc, a_{2n})\in
      K^{n}\setminus\spn(a_{1}), a_{3}\coloneq(a_{31},\dotsc, a_{3n})\in
      K^{n}\setminus\spn(a_{1},a_{2}),\dotsc$.
    \end{enumerate}
  \end{proof}
\end{satz}

%satz 2.3
\begin{satz}
  Sei $f\colon G\rightarrow H$ ein Gruppenhomomorphismus. Dann ist $f(1_{G})=
  1_{H}$ und $f(g^{-1})=f(g)^{-1}$ für $g\in G$.
  \begin{proof}
    Es gilt: $f(1_{G})= f(1_{G})1_{H}= f(1_{G})f(1_{G})f(1_{G})^{-1}=
    f(1_{G}\cdot1_{G})f(1_{G})^{-1}= 1_{H}$ und weiter: $f(g^{-1})=
    f(g^{-1})1_{H}= f(g^{-1})f(g)f(g)^{-1}= f(g^{-1}g)f(g)^{-1}= f(1_{G})
    f(g)^{-1}= 1_{H}f(g)^{-1}= f(g)^{-1}$.
  \end{proof}
\end{satz}

\begin{bsp}
  Sei $K$ ein Körper und $n$ eine natürliche Zahl. Dann ist
  $\det\colon\GL(n,K)\rightarrow K\setminus\{0\}$ ein Homomorphismus. Somit
  folgt, $\det(1_{n})=1$ und $\det(a^{-1})= \det(a)^{-1}$.
\end{bsp}

%def 2.4
\begin{defini}[Untergruppe]
  Eine Teilmenge $U$ einer Gruppe $G$ heißt \highl{Untergruppe} von $G$, wenn
  gilt:
  \begin{enumerate}
   \item $1_{G}\in U$
   \item $a,b\in U\Rightarrow ab,a^{-1}\in U$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  Gegebenenfalls ist $U$ mit der entsprechend eingeschränkten Verknüpfung
  selbst eine Gruppe. Wir schreiben dann $U\leq G$ oder $U<G$, wenn $U\neq G$
  ist. Man bezeichnet $U$ dann als \highl[Untergruppe!echte]{echte Untergruppe}.
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item $(\Z,+)<(\Q,+)<(\R,+)<(\C,+)$.
   \item In jeder beliebigen Gruppe $G$ sind $G$ selbst und die
    \highl[Untergruppe!triviale]{triviale Untergruppe} $\{1_{G}\}=1$
    Untergruppen.
   \item Für jede nichtleere Familie $(G_{i})_{i\in I}$ von Gruppen bilden die
    Elemente $(g_{i})_{i\in I}$ aus dem direkten Produkt mit $\abs{\{i\in
    I\colon g_{i}\neq1\}}<\infty$ eine Untergruppe von $\prod_{i\in I} G_{i}$.
    Diese heißt \highl[Produkt!direktes eingeschränktes]{direktes
    eingeschränktes Produkt} von $(G_{i})_{i\in I}$. Hierfür nutzen
    wir die Schreibweise: $\coprod_{i\in I} G_{i}$.
    Für $\abs{I}<\infty$ ist $\coprod_{i\in I} G_{i}= \prod_{i\in I} G_{i}$.
   \item Für jede Monade $M$ ist die Automorphismengruppe $\Aut(M)$ eine
    Untergruppe von $\Sym(M)$.
  \end{enumerate}
\end{bsp}

\begin{satz}\label{satz:24}
  Eine nichtleere Teilmenge $U$ einer Gruppe $G$ ist genau dann eine
  Untergruppe von $G$, wenn gilt: $a,b\in U\Rightarrow ab^{-1}\in U$.
  \begin{proof}
    Wir brauchen nur die Rückrichtung zu zeigen. Die andere Richtung ist klar.
    Sei dazu $U\subseteq G$ nichtleer und die obige Bedingung sei erfüllt.
    Dann existiert ein $x\in U$. Folglich $1_{G}=xx^{-1}\in U$. Also
    $x^{-1}=1_{G}x^{-1}\in U$. Andererseits gilt für $y\in U$:
    $x(y^{-1})^{-1}\in U$.
  \end{proof}
\end{satz}

%def 2.5
\begin{defini}
  Für Teilmengen $X,Y$ einer Gruppe $G$ setzt man: $XY\coloneq\{xy\colon x\in
  X,y\in Y\}$ und $X^{-1}\coloneq\{x^{-1}\colon x\in X\}$.
\end{defini}

\begin{bemerk}\label{bem:32}
  Dann ist $(X^{-1})^{-1}=X, (XY)^{-1}= Y^{-1}X^{-1}, (XY)Z=X(YZ)$ für
  $X,Y,Z\subseteq G$. Der \autoref{satz:24} besagt: $X\leq G\Leftrightarrow
  X\neq\emptyset\wedge XX^{-1}\subseteq X$.
\end{bemerk}

\begin{satz}\label{satz:25}
  Für Untergruppen $U,V,W$ einer Gruppe $G$ gilt stets:
  \begin{enumerate}
   \item $U\cup V\leq G\Leftrightarrow U\subseteq V\vee V\subseteq U$
   \item $UV\leq G\Leftrightarrow UV=VU$
   \item $U\subseteq W\Rightarrow UV\cap W=U(V\cap W)$
    (\highl[Identität!\textsc{Dedekind}sche]{\textsc{Dedekind}sche
    Identität})
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item "`$\Rightarrow$"': Sei $U\cup V\leq G$ und $U\nsubseteq V$. Dann
      gibt es ein Element $u\in U\setminus V$. Für $v\in V$ ist $uv\in U\cup
      V$. Im Fall $uv\in V$ wäre $u=uvv^{-1}\in V$ \lightning{} Also muss
      $uv\in U$ sein und $v=u^{-1}uv\in U$. Daher ist $V\subseteq U$.\\
      "`$\Leftarrow$"' ist trivial.
     \item $UV\leq G\Rightarrow(UV)=(UV)^{-1}= V^{-1}U^{-1}=VU$ und für
      $UV=VU$ folgt: $(UV)(UV)^{-1}= UVV^{-1}U^{-1}\subseteq UVU$ nach
      \autoref{bem:32} und wegen der Kommutativität gilt: $UVU=UUV=UV$.
      Somit ist $UV$ eine Untergruppe.
     \item Sei $U\subseteq W$ und $w\in UV\cap W$. Wir schreiben $w=uv$ mit
      $u\in U$ und $v\in V$. Dann ist $v=u^{-1}w\in W$, d.\,h. $w=uv\in
      U(V\cap W)$. Umgekehrt ist $U(V\cap W) \subseteq UV$ $U(V\cap
      W)\subseteq WW\subseteq W$, d.\,h. $U(V\cap W)\subseteq UV\cap W$.
      \todo{Klarer formulieren}
    \end{enumerate}
  \end{proof}
\end{satz}

%def 2.6
\begin{defini}[erzeugte Untergruppe]\label{def:26}
  Für jede nichtleere Familie $(U_{i})_{i\in I}$ von Untergruppen $U_{i}$
  einer Gruppe $G$ gilt stets:
  \begin{gather*}
    \bigcap_{i\in I} U_{i}\leq G
  \end{gather*}
  Insbesondere
  ist für $X\subseteq G$ der Durchschnitt $D$ aller Untergruppen $U\leq G$ mit
  $X\subseteq U$ eine Untergruppe von $G$. Man nennt $D=\colon\langle
  X\rangle$ die von $X$ \highl[Untergruppe!erzeugte]{erzeugte Untergruppe} von
  $G$. Für $X=\{a_{1},\dotsc, a_{n}\}$ schreibt man $\langle a_{1},\dotsc,
  a_{n}\rangle$.
\end{defini}

\begin{satz}
  Sei $G$ eine Gruppe und $X\subseteq G$. Dann besteht $\langle X\rangle$ aus
  den Elementen der Form $x_{1}^{\epsilon_{1}}\dotso x_{n}^{\epsilon_{n}}$ mit
  $n\in\N_{0}, x_{1},\dotsc, x_{n}\in X,
  \epsilon_{1},\dotsc,\epsilon_{n}\in\{\pm 1\}$. Im Fall $n=0$ interpretiert
  man das Produkt als $1$.
  \begin{proof}
    Die Menge $A$ der angegebenen Elemente ist eine Untergruppe von $G$, die
    $X$ enthält. Nach \autoref{def:26} ist also $\langle X\rangle\subseteq A$.
    Ist umgekehrt $U$ eine Untergruppe von $G$, die $X$ enthält, so enthält
    $U$ auch $A$. Nach \autoref{def:26} ist $A\subseteq\langle X\rangle$.
  \end{proof}
\end{satz}

% Vorlesung vom 2009-04-21
\begin{bsp}\label{bsp:26}
  Ist $X=\{x\}$, so heißt $\langle x\rangle=\langle X\rangle=
  \{x^{n}\colon n\in\Z\}$ die von $x$  erzeugte
  \highl[Untergruppe!zyklische]{zyklische Untergruppe} von $G$. Allgemein
  heißt jede Menge $E$ mit $\langle E\rangle=G$ ein \highl{Erzeugendensystem}
  von $G$. Hat die Gruppe $G$ ein endliches Erzeugendensystem, so heißt $G$
  \highl[Gruppe!endlich erzeugte]{endlich erzeugte Gruppe}. Natürlich ist jede
  endliche Gruppe endlich erzeugt.
\end{bsp}

%satz 2.7
\begin{satz}\label{satz:27}
  Für jeden Gruppenhomomorphismus $f\colon G\rightarrow H$ gilt:
  \begin{enumerate}
   \item Wenn $U\leq G$, dann ist $f(U)=\{f(u)\colon u\in U\}\leq H$.
    Insbesondere ist $\Bld(f)\coloneq f(G)\leq H$.
   \item $V\leq H\Rightarrow f^{-1}(V)= \{g\in G\colon f(g)\in V\}\leq G$.
    Insbesondere ist $\ker(f)\coloneq f^{-1}(\{1_{H}\})\leq G$.
   \item $U\leq G\Rightarrow f^{-1}(f(U))= U(\ker f)= (\ker f)U$ oder $V\leq
    H\Rightarrow f(f^{-1}(V))= V\cap\Bld f$.
   \item Wir haben zueinander inverse Bijektionen $\FU=\{U\leq G\colon\ker
    f\subseteq U\}\leftrightarrows \FV\coloneq\{V\leq H\colon V\subseteq\Bld
    f\}$.  Es ist $U\mapsto f(U)$ und $V\mapsto
    f^{-1}(V)$.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item Sei $U\leq G$. Wegen $U\neq\emptyset$ ist auch $f(U)\neq\emptyset$.
      Ferner ist $f(U)f(U)^{-1}= f(U)f(U^{-1})= f(UU^{-1})\subseteq f(U)$.
     \item Sei $V\leq H$. Weil $f(1_{G})= 1_{H}\in V$ ist $1_{G}\in
      f^{-1}(V)$, d.\,h. $f^{-1}(V)\neq\emptyset$. Seien $a,b\in f^{-1}(V)$,
      d.\,h. $f(a), f(b)\in V$. Dann gilt: $f(ab^{-1})= f(a)f(b)^{-1}\in V$,
      d.\,h. $a,b^{-1}\in V$.
     \item Zunächst sei $U\leq G$. Für $x\in f^{-1}(f(U))$ ist $f(x)\in f(U)$.
      Also ist $f(x)=f(u)$ für $u\in U$. Dann ist $f(x)f(u)^{-1}=1=
      f(xu^{-1})$, d.\,h. $xu^{-1}\in\ker f$ und $x=xu^{-1}u\in(\ker f)U$.
      Daher gilt $f^{-1}(f(U))\subseteq (\ker f)U$. Andererseits ist für
      $a\in\ker f, b\in U$: $f(ab)=f(a)f(b)= 1_{H}f(b)= f(b)\in f(U)$,
      d.\,h.
      $ab\in f^{-1}(f(U))$. Also haben wir $f^{-1}(f(U))= (\ker f)U$. Nach den
      obigen beiden Punkten ist $(\ker f)U=f^{-1}(f(U))\leq G$. Mit
      \autoref{satz:25} (ii) folgt: $(\ker f)U= U(\ker f)$.

      Sei nun $V\leq H$ und $x\in f(f^{-1}(V))$. Dann ist $x=f(a)$ für ein
      $a\in f^{-1}(V)$. Folglich ist $x=f(a)\in V\cap\Bld f$. Daher folgt,
      $f(f^{-1}(V))\subseteq V\cap\Bld f$. Sei umgekehrt $v\in V\cap\Bld f$
      und $g\in G$ mit $v=f(g)$. Dann ist $g\in f^{-1}(V)$ und $v=f(g)\in
      f(f^{-1}(V))$. Daher sind beide Mengen gleich.
     \item Sei $U\in\FU$, d.\,h. $\ker f\subseteq U$. Dann ist auf jeden Fall
      $f(U)\subseteq\Bld f$, d.\,h. $f(U)\in \FV$ und $f^{-1}(f(U))= U(\ker
      f)\subseteq UU\subseteq U\subseteq f^{-1}(f(U))$. Also ist
      $f^{-1}(f(U))=U$.

      Sei jetzt $V\in\FV$, d.\,h. $V\subseteq\Bld f$. Dann wissen wir: $\ker
      f= f^{-1}(\{1_{H}\})\subseteq f^{-1}(V)$. Dies bedeutet nun:
      $f^{-1}(V)\in\FU$ und $f(f^{-1}(V))=V\cap\Bld f=V$.
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerk}
  Man bezeichnet $f(U)$ als das \highl{Bild} von $U$ unter $f$, $f^{-1}(V)$
  als das \highl{Urbild} von $V$ unter $f$, $\Bld f$ als das \highl{Bild} von
  $f$ und $\ker f$ als den \highl{Kern} von $f$.
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item Sei $K$ ein Körper und $n$ eine natürliche Zahl. Dann ist die
     spezielle lineare Gruppe $\SL(n,K)=
    \{A\in\GL(n,K)\colon\det A=1\}= \ker(\det\colon\GL(n,K)\rightarrow
    K\setminus\{0\})$.
   \item Für jedes Element $a$ einer Gruppe $G$ ist $\ad_{a}\colon G\rightarrow
    G$ mit $x\mapsto axa^{-1}$ ein Homomorphismus. Denn für $x,y\in G$ gilt:
    $\ad_{a}(x)\ad_{a}(y)= axa^{-1}aya^{-1}= \ad_{a}(xy)$. Außerdem ist
    $(\ad_{a}\circ\ad_{a^{-1}})(x)= a(a^{-1}x(a^{-1})^{-1})a^{-1}=x$. Daher
    ist $\ad_{a}\circ\ad_{a^{-1}}$ die Identität auf $G$ . Analoges gilt auch
    umgekehrt. Somit ist $\ad_{a}\in\Aut G$. Man nennt $\ad_{a}$ den von $a$
    induzierten \highl[Automorphismus!innerer]{inneren Automorphismus} von $G$.

    Die Abbildung $\ad\colon G\rightarrow\Aut G$ mit $a\mapsto\ad_{a}$ ist ein
    Homomorphismus. Denn für $a,b,x\in G$ ist $(\ad_{a}\circ\ad_{b})(x)=
    a(bxb^{-1})a^{-1}= (ab)x(ab)^{-1}= \ad_{ab}x$. Nach \autoref{satz:27} ist
    $\Bld(\ad)= \{\ad_{a}\colon a\in G\}\leq\Aut G$. Man nennt
    $\Inn(G)\coloneq\Bld(\ad)$ die \highl[Automorphismengruppe!innere]{innere
    Automorphismengruppe} von $G$. Analog ist $\ker(\ad)= \{a\in
    G\colon\ad=\id_{G}\}= \{a\in G\colon axa^{-1}=x\,\forall x\in G\}= \{a\in
    G\colon xa=ax\,\forall x\in G\}=\colon Z(G)\leq G$. Man nennt $Z(G)$ das
    \highl{Zentrum} von $G$.
  \end{enumerate}
\end{bsp}

%satz 2.8
\begin{satz}
  Für jeden Gruppenhomomorphismus $f\colon G\rightarrow H$ gilt, dass $f$
  genau dann injektiv ist, wenn der Kern von $f$ nur aus dem trivialen Element
  besteht.
  \begin{proof}
    \begin{itemize}
     \item["`$\Rightarrow$"'] Sei $f$ injektiv. Wegen $f(1)=1$ liegt das
      Einselement im Kern von $f$. Sei umgekehrt $x\in\ker f$. Dann ist
      $f(x)=1=f(1)$. Also ist $x=1$, da $f$ injektiv.
     \item["`$\Leftarrow$"'] Sei jetzt $\ker f=\{1_{G}\}$. Sind $x,y\in G$
      mit $f(x)=f(y)$, so ist $f(x)f(y)^{-1}=1=f(xy^{-1})$. Also ist
      $xy^{-1}\in\ker f=\{1_{G}\}$. Also $xy^{-1}=1$, d.\,h. $x=y$.
    \end{itemize}
  \end{proof}
\end{satz}

\chapter{Nebenklassen}

%def 3.1
\begin{defini}
  Sei $G$ eine Gruppe und $H$ eine Untergruppe von $G$ sowie $a,b\in G$ mit
  $a^{-1}b\in H$. Dann heißt $a$ \highl{linkskongruent} zu $b$ modulo $H$. Man
  schreibt $a\equiv_{l} b\pmod{H}$.
\end{defini}

\begin{satz}\label{satz:31}
  Die Linkskongruenz modulo $H$ ist eine Äquivalenzrelation auf $G$.
  \begin{proof}
    \begin{enumerate}
     \item $a^{-1}a=1_{G}\in H$
     \item $a^{-1}b\in H\Rightarrow(a^{-1}b)^{-1}= b^{-1}(a^{-1})^{-1}=
      b^{-1}a\in H$
     \item $a^{-1}b, b^{-1}c\in H\Rightarrow a^{-1}bb^{-1}c=a^{-1}c\in H$
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerk}
  Für Elemente $a,b\in G$ gilt: $a\equiv_{l}b\pmod{H}\Leftrightarrow
  a^{-1}b\in H\Leftrightarrow b\in aH\coloneq\{ah\colon h\in H\}$. Daher ist
  die Äquivalenzklasse von einem Element $a\in G$ bezüglich
  $\equiv_{l}\pmod{H}$ die \highl{Linksnebenklasse} von $a$ modulo $H$. Wir
  setzen $G/H\coloneq\{aH\colon a\in G\}$. Für $a\in G$ ist $H\rightarrow aH$
  mit $a\mapsto ah$ bijektiv. Die Surjektivität ist klar und $ah=ah'$ folgt,
  $h=a^{-1}ah=a^{-1}ah'=h'$. Insbesondere ist $\abs{aH}=\abs{H}$.
\end{bemerk}

%bem 3.2
\begin{bemerk}
  Seien $G$ eine Gruppe und $H$ eine Untergruppe von $G$ sowie $a,b\in G$ mit
  $ab^{-1}\in H$. Dann heißt $a$ \highl{rechtskongruent} zu $b$ modulo $H$.
  Man schreibt $a\equiv_{r}b\pmod{H}$. Es gilt ein zu \autoref{satz:31}
  analoges Ergebnis. Die Äquivalenzklasse von $a$ bezüglich
  $\equiv_{r}\pmod{H}$ ist die \highl{Rechtsnebenklasse} $Ha$ von $a$ nach
  $H$. Wir setzen $H\setminus G\coloneq \{Ha\colon a\in G\}$. Für $a\in G$
  ist wieder $\abs{Ha}= \abs{H}$.

  Für $R=Ha\in H\setminus G$ ist $R^{-1}= a^{-1}H^{-1}= a^{-1}H\in G/H$.
  Analog ist $L^{-1}\in H\setminus G$ für $L\in G/H$. So erhält man eine
  Bijektion $G/H\rightarrow H\setminus G$. Man
  nennt $\abs{G\colon H}\coloneq\abs{G/H}= \abs{H\setminus G}$ den
  \highl{Index} von $H$ in $G$.
\end{bemerk}

\begin{satz}[Satz von Lagrange]\label{satz:32-lagrange}
  Für jede Untergruppe $H$ einer Gruppe $G$ gilt:
  \begin{gather*}
    \abs{G}= \abs{G\colon H}\abs{H}
  \end{gather*}
  Insbesondere sind $\abs{H}, \abs{G\colon H}$ in endlichen Gruppen Teiler von
  $\abs{G}$.
  \begin{proof}
    Die Gruppe $G$ ist die disjunkte Vereinigung aller Linksnebenklassen nach
    $H$. Es gibt $\abs{G\colon H}$ Linksnebenklassen. Jede enthält $\abs{H}$
    Elemente.
  \end{proof}
\end{satz}

\begin{bsp}
  Gruppen der Ordnung $24$ können keine Untergruppen der Ordnung $7$ enthalten.
\end{bsp}

%bem 3.3
\begin{bemerk}
  Setzen $\PP\coloneq\{p\in\N\colon p\text{ Primzahl}\}$. Weiterhin schreiben
  wir $m\mid n$, falls $m$ ein Teiler von $n$ ist. 
\end{bemerk}

\begin{satz}
  Gruppen von Primzahlordnung sind zyklisch.
  \begin{proof}
    Sei $G$ eine Gruppe, $\abs{G}=p\in\PP$ und $1\neq g\in G$. Nach dem Satz
    von \textsc{Lagrange} $1\neq\abs{\langle g\rangle}\mid\abs{G}=p$. Also
    $\abs{\langle p\rangle}=p$, d.\,h. $G=\langle p\rangle$ zyklisch.
  \end{proof}
\end{satz}

%def 3.4
\begin{defini}
  Für jedes Element $a$ in einer Gruppe $G$ heißt die Anzahl der Elemente in
  der von $a$ erzeugten Gruppe $\abs{\langle a\rangle}$ die \highl{Ordnung}
  von $a$.
\end{defini}

\begin{bemerk}
  Nach dem \autoref{bsp:26} ist $\langle a\rangle=\{a^{n}\colon n\in\Z\}$.
  \begin{enumerate}[1.\,F{a}ll]
   \item Alle $a^{n}$ sind verschieden. Dann ist $\abs{\langle
    a\rangle}=\infty$.
   \item Es existieren ganze Zahlen $m$ und $n$ mit $m<n$ und $a^{m}=a^{n}$.
    Dann ist $n-m\in\N$ mit $a^{n-m}=a^{n}(a^{m})^{-1}=1$. Sei $k\in\N$
    minimal mit $a^{k}=1$. Dann sind $a^{0}=1, a^{1}=a, a^{2},\dotsc, a^{k-1}$
    paarweise verschieden. Denn wären $a^{i}=a^{j}$ mit $0\leq i\leq j\leq
    k-1$, so ist $1=a^{j-i}$ und $j-i=0$ nach der Wahl von $k$. Somit
    ist $i=j$. Für beliebige $i,j\in\{0,\dotsc,k-1\}$ ist $a^{i}a^{j}=
    a^{i+j}$. Dabei ist $a^{i+j}= a^{i+j-k}$, falls $i+j\leq k$. Daher ist
    $a^{i}a^{j}\in U\coloneq\{a^{0},\dotsc, a^{k-1}\}$. Ferner ist
    $(a^{i})^{-1}=a^{-1}=a^{k-i}\in U$. Daher ist $\langle
    a\rangle\leq U\leq\langle a\rangle$. Also ist $\langle a\rangle=U$.
    Insbesondere ist $\abs{\langle a\rangle}=k$.
  \end{enumerate}
  In beiden Fällen ist also $\abs{\langle a\rangle}= \inf\{k\in\N\colon
  a^{k}=1\}$.
\end{bemerk}

%Vorlesung vom 2009-04-23
%satz 3.4
\begin{satz}[Satz von \textsc{Fermat} oder \textsc{Euler}]
  Für jedes Element $a$ einer endlichen Gruppe $G$ gilt: $a^{\abs{G}}=1$.
  \begin{proof}
    Nach \autoref{satz:32-lagrange} gilt: $\abs{G}= \underbrace{\abs{G\colon\langle
    a\rangle}}_{=\colon l}\cdot \underbrace{\abs{\langle a\rangle}}_{=\colon
    k}$. Nach der vorigen Bemerkung haben wir: $a^{k}=1$. Also
    $a^{\abs{G}}=a^{kl}= (a^{k})^{l}=1^{l}=1$.
  \end{proof}
\end{satz}

%satz 3.5
\begin{satz}\label{satz:35}
  Für $U\leq Z$ existiert eine natürliche Zahl $n$ mit $U=\{nz\colon
  z\in\Z\}=\colon n\Z$.
  \begin{proof}
    Für $n\in\N_{0}$ ist $n\Z$ das Bild des Homomorphismus $\Z\rightarrow\Z$ mit
    $z\mapsto nz$. Daher ist $n\Z\leq\Z$. Sei $U\leq Z$ und \OE\footnote{Herr
    Külshammer nutzt dieses als Zeichen für o.\,B.\,d.\,A.} ist
    $U\neq\{0\}=0\Z$. Für $a\in U\setminus\{0\}$ ist auch $-a\in U$. Daher
    $U\cap\N\neq\emptyset$. Wie jede nichtleere Teilmenge von $\N$ enthält auch
    der Durchschnitt ein kleinstes Element $n$. Dann ist $2n=n+n\in U, 3n\in U$
    usw., d.\,h. $kn\in U$ für $k\in\N$. Folglich auch $-kn\in U$ und $0\in U$.
    Also ist $n\Z\subseteq U$.

    Ist $b\in U$ beliebig. Dann liefert die Division mit Rest einen Quotienten
    $q\in\Z$ und einen Rest mit $r\in\Z$. Dabei gilt: $b=qn+r$. Wegen
    $n\Z\subseteq U$ ist $r=b-qn\in U$. Nach der Wahl des $n$ muss $r=0$
    gelten. Folglich: $b=qn\in n\Z$. Dann ist gezeigt: $U=n\Z$.
  \end{proof}
\end{satz}

\begin{bemerk}
  Es ist $\abs{\Z\colon n\Z}=n$ für $n\in\N$. Denn für $z\in\Z$ existieren
  Quotient und Rest aus $\Z$ mit $z=qn+r$ für $0\leq r<n$. Daher ist $z\in
  r+n\Z$. Folglich hat man: $\Z= (0+n\Z)\cup(1+n\Z)\cup\dotsb\cup(n-1+n\Z)$.
  Da $0\mathord{,}1,\dotsc,n-1$ in paarweise verschiedenen Linksnebenklassen
  nach $n\Z$ liegen, folgt die Behauptung. Daher besitzt $\Z$ für jede
  natürliche Zahl $n$ genau eine Untergruppe vom Index $n$.
\end{bemerk}

%satz 3.6
\begin{satz}
  Jede Untergruppe $V$ einer zyklischen Gruppe $G=\langle g\rangle$ ist wieder zyklisch.
  \begin{proof}
    Sei $f$ ein Homomorphismus (Epimorphismus) von $(\Z,+)$ nach $(G,\cdot)$ mit
    $n\mapsto g^{n}$. Nach dem \autoref{satz:27} (Punkt iv) gilt:
    $V=f(f^{-1}(V))$ mit $f^{-1}(V)\leq \Z$ und nach \autoref{satz:35} ist
    $f^{-1}(V)=n\Z$ für ein $n\in\N_{0}$. Daher $V=f(n\Z)=\langle
    g^{n}\rangle$ zyklisch.
  \end{proof}
\end{satz}

%def 3.7
\begin{defini}[Doppelnebenklassen]
  Sei $G$ eine Gruppe und $H$ und $K$ zwei Untergruppen. Weiterhin seien $a$
  und $b$ zwei Elemente aus $G$. Wir schreiben $a\equiv b\pmod{H,K}$, falls
  $h\in H, k\in K$ mit $b=hak$ existieren. 
\end{defini}

\begin{satz}
  Die Kongruenz modulo $H$ und $K$ ist eine Äquivelenzrelation auf $G$.
  \begin{proof}
    \begin{enumerate}
     \item Für $a\in G$ folgt: $a=1a1$ mit $1\in H, 1\in K$. Also ist $a\equiv
      a\pmod{H,K}$.
     \item Sei $a\equiv b\pmod{H,K}\Rightarrow\exists h\in H, k\in K\colon
      b=hak\Rightarrow a=h^{-1}bk^{-1}$ mit $h^{-1}\in H, k^{-1}\in
      K\Rightarrow b\equiv a\pmod{H,K}$.
     \item $a\equiv b\pmod{H,K}$ und $b\equiv c\pmod{H,K}\Rightarrow\exists
      h,h'\in H, k,k'\in K\colon b=hak, c=h'ak'\Rightarrow c=h'hakk'$. Also
      ist $a\equiv c\pmod{H,K}$.
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerk}
  Für jedes Element $a\in G$ ist die Äquivalenzklasse von $a$ bezüglich der
  Äquivalenzrelation die \highl{Doppelnebenklasse} $HaK\coloneq\{hak\colon
  h\in H, k\in K\}$ von $a$ nach $H$ und $K$. Man setzt: $H\setminus
  G/K\coloneq\{HaK\colon a\in G\}$. Es gilt, $H\setminus G=H\setminus G/1$ und
  $G/K=1\setminus G/K$. Im Allgemeinen ist die Anzahl der Elemente einer
  Doppelnebenklasse \emph{kein} Teiler der Gruppenordnung.
\end{bemerk}

\begin{bsp}
  Sei $G\coloneq\Sym(3), H\coloneq\langle b\rangle, K\coloneq\langle c\rangle,
  a\coloneq1$ mit $b\coloneq
  \begin{pmatrix}
    1 & 2 & 3\\
    2 & 1 & 3
  \end{pmatrix}, c\coloneq
  \begin{pmatrix}
    1 & 2 &3\\
    3 & 2 & 1
  \end{pmatrix}$. Dann besteht $H$ aus $\{1,b\}$ und $K$ aus $\{1,c\}$. Es ist
  $HaK=\{1,b,c,bc\}$. Also $\abs{HaK}=4\nmid 6=\abs{G}$.
\end{bsp}

%satz 3.8
\begin{satz}\label{satz:38}
  Seien $G$ eine Gruppe, $H,K\leq G$ und $a\in G$. Dann enthält $HaK$ genau
  $\abs{H\colon H\cap aKa^{-1}}$ Linksnebenklassen nach $K$ und genau
  $\abs{K\colon a^{-1}Ha\cap K}$ Rechtsnebenklassen nach $H$. Insbesondere ist
  $\abs{HaK}= \abs{H\colon H\cap aKa^{-1}}\cdot\abs{K}= \abs{K\colon
  a^{-1}Ha\cap K}\cdot\abs{H}$.
  \begin{proof}
    Es reicht, den Beweis für eine Seite zu führen. Der Rest folgt aus
    Symmetriegründen.  Es ist $HaK=\bigcup_{h\in H} f(h)$ mit $f\colon
    H\rightarrow G/K$ mit $h\mapsto haK$. Dabei gilt für Elemente $h,h'\in H$:
    $f(h)=f(h')\Leftrightarrow haK=h'aK\Leftrightarrow a^{-1}h^{-1}h'a\in
    K\Leftrightarrow h^{-1}h'\in aKa^{-1}\cap H\Leftrightarrow h(aKa^{-1}\cap
    H)= h'(aKa^{-1}\cap H)$.
  \end{proof}
\end{satz}

\begin{bemerk}
  Nach dem \autoref{satz:27}(i) ist $aKa^{-1}=\ad_{a}(K)\leq G$ und analog
  $a^{-1}Ha\leq G$.
\end{bemerk}

\begin{bsp}
  Für $a=1$ ist $HaK=HK$. Im Allgemeinen ist $HK\nleq K$. Nach dem
  \autoref{satz:38} enthält $HK$ genau $\abs{H\colon H\cap K}$
  Linksnebenklassen nach $K$ und genau $\abs{K\colon K\cap H}$
  Rechtsnebenklassen nach $H$. Insbesondere gilt:
  \begin{enumerate}
   \item $\abs{HK}= \abs{H\colon H\cap K}\cdot\abs{K}= \abs{K\colon H\cap
    K}\cdot\abs{H}$
   \item $\abs{H\colon H\cap K}\leq \abs{G\colon K}$
   \item $\abs{H\colon H\cap K}=\abs{G\colon K}<\infty\Rightarrow G=HK=KH$
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  Den Satz von \textsc{Lagrange} (\autoref{satz:32-lagrange}) kann man
  folgendermaßen verallgemeinern: Ist $G$ eine Gruppe und $K\leq H\leq G$, so
  gilt: $\abs{G\colon K}= \abs{G\colon H}\cdot\abs{H\colon K}$. Denn ist das
  $G=\dotcup_{i\in I} g_{i}H$ und $H=\dotcup_{j\in J} h_{j}K$, so ist
  $G=\dotcup_{\substack{i\in I\\ j\in J}} g_{i}h_{j}K$.
\end{bemerk}

\begin{satz}
  Für Untergruppen $H$ und $K$ einer Gruppe $G$ gilt stets:
  \begin{enumerate}
   \item $\abs{G\colon H\cap K}\leq\abs{G\colon H}\cdot\abs{G\colon K}$
   \item $\abs{G\colon H\cap K}= \abs{G\colon H}\cdot\abs{G\colon
    K}<\infty\Rightarrow G=HK=KH$
   \item Seien $\abs{G\colon H}, \abs{G\colon K}$ endlich und teilerfremd.
    Dann ist $\abs{G\colon H\cap K}= \abs{G\colon H}\cdot\abs{G\colon K}$ und
    $G= HK=KH$.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item $\abs{G\colon H\cap K}= \abs{G\colon H}\cdot\abs{H\colon H\cap
      K}\leq\abs{G\colon H}\cdot\abs{G\colon K}$ (letzter Schritt nach
      \autoref{satz:38}(ii))
     \item Sei $\abs{G\colon H\cap K}= \abs{G\colon H}\cdot \abs{G\colon
      K}<\infty$. Dann zeigt der Beweis des ersten Teiles dass $\abs{G\colon
      K}= \abs{H\colon H\cap K}$. Aus \autoref{satz:38}(iii) folgt dann:
      $G=HK=KH$.
     \item Seien $\abs{G\colon H}, \abs{G\colon K}$ endlich und teilerfremd.
      Die obige Bemerkung zeigt, dass $\abs{G\colon H}\mid\abs{G\colon
      H\cap K}$ und $\abs{G\colon K}\mid \abs{G\colon H\colon K}$. Daher
      ist $\abs{G\colon H}\cdot\abs{G\colon K}\mid \abs{G\colon H\cap K}$.
      Mit dem ersten Punkt folgt, dass $\abs{G\colon H\cap K}= \abs{G\colon
      H}\cdot\abs{G\colon K}$ und (ii) liefert $G=HK=KH$.
    \end{enumerate}
  \end{proof}
\end{satz}

%kap 4
\chapter{Normalteiler und Faktorgruppen}

%satz 4.1
\begin{satz}
  Für eine Untergruppe $N$ einer Gruppe $G$ sind die folgenden Aussagen
  äquivalent:
  \begin{enumerate}[(1)]
   \item $gNg^{-1}\subseteq N$ für alle $g\in G$
   \item $gNg^{-1}=N$ für alle $g\in G$
   \item $gN=Ng$ für alle $g\in G$
   \item $G/N$ ist eine Gruppe mit $(gN)(hN)\coloneq ghN$ für alle $g,h\in G$
   \item Es existiert eine Gruppe $H$ und ein Homomorphismus $f\colon
    G\rightarrow H$ mit $N=\ker f$.
  \end{enumerate}
  \begin{proof}
    \begin{itemize}
     \item[(1)$\Rightarrow$(2)] Ist die erste Aussage erfüllt, so ist
      $N=g\underbrace{g^{-1}N(g^{-1})^{-1}}_{\subseteq N}g^{-1}\subseteq
      gNg^{-1}$.
     \item[(2)$\Rightarrow$(3)] Multiplizieren mit $g$ von rechts.
     \item[$3\Rightarrow4$] Sei die dritte Bedingung erfüllt. Für $g,h,k\in G$
      ist dann $(gN)(hN)= gNhN=ghNN=ghN$, d.\,h. die Multiplikation in $G/N$
      ist wohldefiniert. Ferner ist mit $(gN\cdot hN)(kN)= ghN\cdot kN= (gh)kN=
      g(hk)N= gN(hkN)= gN(hNkN)$ das Assoziativgesetz erfüllt. Daher ist $G/N$
      eine Halbgruppe. Außerdem ist $1N\cdot gN= 1gN=gN$ und $(g^{-1}N)(gN)=
      g^{-1}gN=1N$.
     \item[(4)$\Rightarrow$(5)] Sei $H\coloneq G/N$ und $f(g)\coloneq gN$ für alle
      $g\in G$. Dann ist $f(g)f(h)= (gN)(hN)= ghN= f(gh)$ für $g,h\in G$,
      d.\,h. $f$ ist ein Homomorphismus. Insbesondere $1_{G/N}=f(1_{G})=1N$.
      Für $x\in G$ gilt ferner: $x\in\ker f\Leftrightarrow
      f(x)=1\Leftrightarrow xN=1N\Leftrightarrow1^{-1}x=x\in N$. Also $\ker
      f=N$.
     \item[(5)$\Rightarrow$(1)] Zuletzt sei (5) erfüllt und weiter $x\in N=\ker f,
      g\in G$. Dann $f(gxg^{-1})= f(g)f(x)f(g^{-1})=f(g)1f(g^{-1})=
      f(g)f(g^{-1})= f(gg^{-1})= f(1)=1$, d.\,h. $gxg^{-1}\in\ker f=N$.
    \end{itemize}
  \end{proof}
\end{satz}

\begin{defini}[normale Untergruppe, Normalteiler]
  Gegebenenfalls heißt das $N$ \highl{normal}\index{Untergruppe!normale} oder
  \highl{Normalteiler} in $G$. Man schreibt $N\trianglelefteq G$
\end{defini}

\begin{defini}[Faktorgruppe]
  Die Gruppe $G/N$ heißt \highl{Faktorgruppe} von $G$ nach $N$.
\end{defini}

\begin{bemerk}
  Für $N\trianglelefteq G$ ist $f\colon G\rightarrow G/N$ mit $g\mapsto gn$ ein
  Epimorphismus. Dieser heißt \highl[Epimorphismus!kanonischer]{kanonischer
  Epimorphismus} von $G$ auf $G/N$.
% Vorlesung vom 2009-04-28
  Es gilt: $a\equiv_{l} b\pmod{N} \Leftrightarrow aN=bN\Leftrightarrow
  Na=Nb\Leftrightarrow a\equiv_{r} b\pmod{N}$. Daher schreibt man kurz
  $a\equiv b\pmod{N}$ und sagt, "`$a$ ist kongruent zu $b$ modulo $N$"'.
\end{bemerk}

\begin{bsp}\label{bsp:41}
  \begin{enumerate}
   \item In jeder Gruppe $G$ sind $1, G$ normal. Sind dies die einzigen
    Normalteiler und ist $G\neq1$, dann heißt die Gruppe
    \highl[Gruppe!einfache]{einfach}\index{einfach}. Nach dem
    \autoref{satz:32-lagrange} (Satz von \textsc{Lagrange}) sind Gruppen von Primzahlordnung stets einfach.
    Später werden wir weitere einfache Gruppen
    kennenlernen (Siehe \autoref{cha:4-Normalreihen}). Eine
    nichteinfache Gruppe $G\neq1$ stellt man sich aus Normalteiler $N$ und
    Faktorgruppe $G/N$ zusammengesetzt vor:
    \missingfigure{Bauklötze $G/N$ über $N$}
    Auf diese Weise werden einfache Gruppen zu Bausteinen für beliebige
    Gruppen. Die Bestimmung aller endlichen einfachen Gruppen war eines der
    größten Projekte der Mathematik überhaupt. Beteiligt daran waren ca.\,50
    bis 100~Mathematiker. Die entsprechenden Veröffentlichungen haben einen
    Umfang von etwa 10\,000~Seiten. Das Projekt wurde 1980\footnote{Manche
    meinen auch 2000 oder 2005.} erfolgreich abgeschlossen. Das Buch
  \cite{ronan} erzählt einen Teil der Geschichte.
   \item In jeder Gruppe $G$ ist jede Untergruppe $U$ vom Zentrum von $G$
    normal. Denn für $g\in G$ und $u\in U$ ist $gug^{-1}= ugg^{-1}=u\in U$.
    Insbesondere ist das Zentrum einer Gruppe ein Normalteiler der Gruppe.
    Ferner gilt, dass $G$ genau dann abelsch ist, wenn $G=Z(G)$. Daher
    ist in
    einer abelschen Gruppe jede Untergruppe normal.
   \item Seien $G$ eine Gruppe und $H$ eine Untergruppe mit $\abs{H\colon
    G}=2$. Dann ist $H\trianglelefteq G$. Denn $1H=H=H1$ und $G\setminus H$
    sind die einzigen Linksnebenklassen nach $H$.
   \item Sei $n$ eine natürliche Zahl und $K$ ein Körper. Dann ist $\SL (n,K)=
    \ker(\det)\trianglelefteq\GL(n,K)$.
   \item Für jeden Gruppenhomomorphismus $f\colon G\rightarrow H$ und
    $N\trianglelefteq H$ ist $f^{-1}(N)\trianglelefteq G$. Denn für $g\in G$
    und $x\in f^{-1}(N)$ ist $f(gxg^{-1})= f(g)f(x)f(g^{-1})=
    f(x)f(g)f(g^{-1})= f(x)f(gg^{-1})= f(x)\in N$, d.\,h.
    $gxg^{-1}\in f^{-1}(N)$.
   \item Für jeden Gruppenhomomorphismus $f\colon G\rightarrow H$ und jeden
    Normalteiler $M\trianglelefteq G$ ist $f(M)\trianglelefteq f(G)$. Denn für
    $g\in G$ und $m\in M$ ist $f(g)f(m)f(g^{-1})^{-1}= f(gmg^{-1})\in f(M)$.
    Dagegen ist im Allgemeinen $f(M)\ntrianglelefteq H$. (siehe Übung)
   \item Für jede Familie $(N_{i})_{i\in I}$ von Normalteilern $N_{i}$  einer
    Gruppe $G$ sind auch $\bigcap_{i\in I} N_{i}$ und $\langle N_{i}\colon
    i\in I\rangle\coloneq\langle\bigcup_{i\in I} N_{i}\rangle$ normal in $G$.
   \item Für jede Gruppe $G, \alpha\in\Aut(G)$ und $a,x\in G$ gilt:
    $(\alpha\circ\ad_{a}\circ\alpha^{-1})(x)= \alpha(a\alpha^{-1}(x)a^{-1})=
    \alpha(a) \alpha(\alpha^{-1}(x))\alpha(a)^{-1}= \ad_{\alpha(a)} (x)$,
    d.\,h.
    \begin{gather*}
      \alpha\circ\ad_{a}\circ\alpha^{-1}= \ad_{\alpha(a)}\in\Inn(G)
    \end{gather*}
    Daher $\Inn(G)\trianglelefteq\Aut(G)$ und es heißt
    $\Out(G)\coloneq\Aut(G)/\Inn(G)$ die
    \highl[Automorphismengruppe!äußere]{äußere Automorphismengruppe} von $G$.
   \item Aus $H\trianglelefteq G$ und $K\trianglelefteq H$ folgt im
    Allgemeinen \emph{nicht}, dass $K\trianglelefteq G$. Die Relation
    $\trianglelefteq$ ist \emph{nicht} transitiv. (Beispiel siehe Übungen)
  \end{enumerate}
\end{bsp}

%satz 4.2
\begin{satz}[Homomorphiesatz]\label{satz:42}
  Für jeden Gruppenhomomorphismus $f\colon G\rightarrow H$ ist $F\colon G/\ker
  f\rightarrow\Bld(f)$ mit $g\ker f\mapsto f(g)$ wohldefiniert und ein
  Isomorphismus von Gruppen. Insbesondere ist
  \begin{gather*}
    G/\ker f\cong\Bld f
  \end{gather*}
  \begin{proof}
    Für $a,b\in G$ gilt: $f(a)=f(b)\Leftrightarrow1=f(a)^{-1}f(b)=
    f(a^{-1}b)\Leftrightarrow a^{-1}b\in\ker f\Leftrightarrow a\ker f=b\ker
    f$. Daher ist $F$ wohldefiniert und injektiv.  Die Surjektivität von $F$
    ist klar. Für $g,h\in G$ gilt: $F(g\ker f)F(h\ker f)= f(g)f(h)= f(gh)=
    F(gh\ker f)= F(g\ker(f) h\ker(f))$.
  \end{proof}
\end{satz}

\begin{bsp}
  \begin{enumerate}
   \item Sei $H=\langle h\rangle$ zyklisch. Dann ist $f\colon\Z\rightarrow H$
    mit $z\mapsto h^{z}$ ein Epimorphismus. Nach dem \autoref{satz:35} ist
    $\ker f=n\Z$ für ein $n\in\N_{0}$. Daher ist $H\cong\Z/n\Z$. Jede
    zyklische Gruppe ist also zu $(\Z/n\Z, +)$ für ein $n\in\N_{0}$ isomorph.
   \item Für jede Gruppe $G$ ist $\ad\colon G\rightarrow\Aut(G)$ mit
    $a\mapsto\ad_{a}$ ein Homomorphismus mit dem Kern $Z(G)$ und dem Bild
    $\Inn(G)$. Also folgt:
    \begin{gather*}
      G/Z(G)\cong\Inn(G)
    \end{gather*}
   \item Für $n\in \N$ und jeden Körper $K$ ist $\det\colon\GL(n,K)\rightarrow
    K\setminus\{0\}$ ein Epimorphismus mit dem Kern $\SL(n,K)$. Daher ist
    $\GL(n,K)/\SL(n,K)\cong K\setminus\{0\}$. Insbesondere ist die
    Faktorgruppe $\GL(n,K)/\SL(n,K)$ abelsch.
  \end{enumerate}
\end{bsp}

%satz 4.3
\begin{satz}[1.\,Isomorphiesatz]\label{satz:43-1isosatz}
  Seien $G$ eine Gruppe, $H$ eine Untergruppe und $N$ ein Normalteiler in $G$.
  Dann ist $HN\leq G, N\trianglelefteq HN, H\cap N\trianglelefteq H$ und
  \begin{gather*}
  H/(H\cap N)\cong HN/N    
  \end{gather*}
  \begin{proof}
    Der kanonische Epimorphismus $f\colon G\rightarrow G/N$ mit $a\mapsto aN$
    hat den Kern $N$. Nach \autoref{satz:27} ist also $HN=f^{-1}(f(H))$ eine
    Untergruppe von $G$. Wegen $N\trianglelefteq G$ ist sicher
    $N\trianglelefteq NH$. Die Einschränkung $g\colon H\rightarrow H/N$ von
    $f$ ist ein Homomorphismus mit dem Kern $H\cap\ker f=H\cap N$. Daher ist
    $H\cap N\trianglelefteq H$. Aus dem \autoref{satz:42} folgt: $H/(H\cap N)=
    H/\ker g\cong\Bld g=\{aN\colon a\in H\}= \{anN\colon a\in H, n\in N\}=
    HN/N$.
  \end{proof}
\end{satz}

\begin{bemerk}
  Im Fall $H\trianglelefteq G$ ist auch $HN\trianglelefteq G$. Denn
  $aHNa^{-1}= aHa^{-1}aNa^{-1}\subseteq HN$ für alle $a\in G$.
\end{bemerk}

%satz 4.4.
\begin{satz}[2.\,Isomorphiesatz]\label{satz:44-2isosatz}
  Seien $G$ eine Gruppe, $N$ ein Normalteiler in $G$ und $N\leq H\leq G$. Dann
  gilt: $H/N\trianglelefteq G/N\Leftrightarrow H\trianglelefteq G$.
  Gegebenenfalls ist:
  \begin{gather*}
  (G/N)/(H/N)\cong G/H    
  \end{gather*}
  \begin{proof}
    Sei $f\colon G\rightarrow G/N$ mit $a\mapsto aN$ kanonisch. Für
    $H\trianglelefteq G$  ist $H/N= f(H)\trianglelefteq f(G)=G/N$. Sei
    umgekehrt $H/N\trianglelefteq G/N$ und $g\colon G/N\rightarrow(G/N)(H/N)$
    kanonisch. Für $a\in G$ gilt dann: $a\in\ker(g\circ f)\Leftrightarrow
    g(f(a))=1\Leftrightarrow f(a)\in\ker g=H/N\Leftrightarrow a\in
    f^{-1}(H/N)= f^{-1}(f(H))=H$ (letzte Aussage nach dem \autoref{satz:27}).

    Daher ist $H=\ker(g\circ f)\trianglelefteq G$. Der Homomorphiesatz
    liefert: $G/H=G/\ker(g\circ f)\cong\Bld(g\circ f)=(G/N)/(H/N)$.
  \end{proof}
\end{satz}

%satz 4.5
\begin{satz}[3.\,Isomorphiesatz]\label{satz:45} Seien $G$ eine Gruppe,
  $U_{0}\trianglelefteq U\leq G$ und $V_{0}\trianglelefteq V\leq G$. Dann gilt:
  $(U\cap V_{0})U_{0}\trianglelefteq(U\cap V)U_{0}, (V\cap
  U_{0})V_{0}\trianglelefteq(V\cap U)V_{0}, (U_{0}\cap V)(V_{0}\cap
  U)\trianglelefteq U\cap V$ und
  \begin{gather*}
    (U\cap V)U_{0}/(U\cap V_{0})U_{0}\cong(V\cap U)V_{0}/(V\cap
       U_{0})V_{0}\cong (U\cap V)/(U_{0}\cap V)(V_{0}\cap U)
  \end{gather*}
  \begin{proof}
    Sei $f\colon U\rightarrow U/U_{0}$ mit $u\mapsto uU_{0}$ kanonisch. Wegen
    $V_{0}\trianglelefteq V$ ist $(U\cap V)\cap V_{0}\trianglelefteq U\cap V$
    nach \autoref{satz:43-1isosatz}. Aus dem Beispiel \autoref{bsp:41} (vi)
    folgt: $\underbrace{f(U\cap V_{0})}_{(U\cap
    V_{0})U_{0}/U_{0}}\trianglelefteq \underbrace{f(U\cap V)}_{(U\cap
    V)U_{0}/U_{0}}$. Daher gilt nach dem \autoref{satz:44-2isosatz}: $(U\cap
    V_{0})U_{0}\trianglelefteq(U\cap V)U_{0}$. Ferner ist $F\colon U\cap
    V\rightarrow (U\cap V)U_{0}/(U\cap V_{0})U_{0}$ mit $x\mapsto x(U\cap
    V_{0})U_{0}$ ein Epimorphismus mit dem Kern $(U\cap V)\cap(U\cap
    V_{0})U_{0}$. Dies kann man durch Anwendung der
    \textsc{Dedekind}"~Identität vereinfachen: $(U\cap V)\cap(U\cap
    V_{0})U_{0} = (U\cap V_{0})(U\cap V\cap U_{0})= (U\cap V_{0})(V\cap
    U_{0})$. Daher ist $(U\cap V_{0})(V\cap U_{0})\trianglelefteq U\cap V$.
    Der \autoref{satz:42} liefert: $U\cap V/(U\cap V_{0})(U_{0}\cap V)= U\cap
    V/\ker F\cong(U\cap V)U_{0}/(U\cap V_{0})U_{0}$. Die anderen Aussagen
    folgen aus Symmetriegründen.
  \end{proof}
\end{satz}

\begin{bemerk}
  Der \autoref{satz:45} wird manchmal auch als Satz von \textsc{Zassenhaus}
  bezeichnet.
\end{bemerk}

%Vorlesung vom 2009-04-30
%bem 4.6
\begin{bemerk}\label{bem:46}
  Für jede Familie $(N_{i})_{i\in I}$ von Normalteilern $N_{i}$ einer Gruppe
  $G$ ist $G\rightarrow\prod_{i\in I} G/N_{i}$ mit $g\mapsto(gN_{i})_{i\in I}$
  ein Homomorphismus mit dem Kern $N\coloneq\bigcap_{i\in I} N_{i}$. Nach dem
  Homomorphiesatz ist also $G/N\rightarrow\prod_{i\in I} G/N_{i}$ mit
  $gN\mapsto(gN_{i})_{i\in I}$ ein Monom.
\end{bemerk}


\begin{satz}\label{satz:46}
  Seien $G$ eine Gruppe und $M,N\trianglelefteq G$ mit $M\cap N=1$. Dann ist
  $mn=nm$ für alle $m\in M, n\in N$.
  \begin{proof}
    $m(nm^{-1}n^{-1})=(mnm^{-1})n^{-1}\in M\cap N=1\Rightarrow
    mnm^{-1}n^{-1}=1\Rightarrow mn=nm$
  \end{proof}
\end{satz}

%def 4.7
\begin{defini}
  Eine Untergruppe $U$ einer Gruppe $G$ mit $f(U)\subseteq U$ für alle
  $f\in\Aut(G)$ bzw. für alle $f\in\End(G)$ heißt \highl{charakteristisch}
  bzw. \highl{vollinvariant} in $G$.
\end{defini}

\begin{bemerk}\label{bem:47}
  \begin{enumerate}
   \item Für $U\leq G$ gilt: $U\trianglelefteq G\Leftrightarrow f(U)\subseteq
    U$ für alle $f\in\Inn(G)$.
   \item Daher folgt aus vollinvariant die Eigenschaft charakteristisch und
    daraus die Eigenschaft normal.
   \item Für jede charakteristische Untergruppe $U\leq G$ und alle $f\in\Aut
    G$ ist $U=f(f^{-1}(U))\subseteq f(U)$, d.\,h. $f(U)=U$.
  \end{enumerate}
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item Für jede Gruppe $G$ ist das Zentrum von $G$ charakteristisch in $G$.
    Denn für $z\in Z(G), g\in G$ und $f\in\Aut G$ gilt: $f(z)f(g)= f(zg)=
    f(gz)= f(g)f(z)$, d.\,h. $f(z)\in Z(G)$ wegen $f(G)=G$. Im Allgemeinen ist
    $Z(G)$ \emph{nicht} vollinvariant in $G$ (siehe Übung).
   \item Für jede Gruppe $G$ ist $U=\langle g^{2}\colon g\in G\rangle$
    vollinvariant in $G$. Denn für $g\in G$ und $f\in\End G$ ist $f(g^{2})=
    f(g)^{2}\in U$.
  \end{enumerate}
\end{bsp}

\begin{satz}\label{satz:47}
  Für jede Gruppe $G$ und $K\leq H\leq G$ gilt:
  \begin{enumerate}
   \item Wenn $K$ charakteristisch in $H$ und $H$ charakteristisch in $G$,
    dann ist auch $K$ charakteristisch in $G$.
   \item Wenn $K$ vollinvariant in $H$ und $H$ vollinvariant in $G$, dann ist
    $K$  vollinvariant in $G$.
   \item Wenn $K$ charakteristisch in $H$ und $H$ Normalteiler in $G$, dann
    ist $K$ Normalteiler in $G$.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item Sei die Voraussetzung erfüllt. Für $f\in\Aut G$ liegt die
      Einschränkung $g$ von $f$ auf $H$ nach \autoref{bem:47}(iii) in $\Aut
      H$. Daher ist $f(K)=g(K)\subseteq K$.
     \item Sei die Voraussetzung erfüllt. Für $f\in\End G$ liegt die
      Einschränkung $g$ von $f$ auf $H$ in $\End H$. Daher ist
      $f(K)=g(K)\subseteq K$.
     \item Sei die Voraussetzung erfüllt. Für $g\in G$ ist $f\colon
      H\rightarrow H$ mit $x\mapsto gxg^{-1}$ ein Automorphismus von $H$.
      Daher ist $gKg^{-1}=f(K)\subseteq K$.
    \end{enumerate}
  \end{proof}
\end{satz}

%def 4.8
\begin{defini}[$\Omega$"~Gruppe, Operatoren]
  Sei $\Omega$ eine Menge. Eine \highl{$\Omega$"~Gruppe} ist ein Paar, das aus einer
  Gruppe $G$ und einer Abbildung $\Omega\times G\rightarrow G$ mit
  $(\omega,g)\mapsto{}^{\omega}\!g$ mit $\vorhoch{\omega}(gh)=
  (\vorhoch{\omega}g)(\vorhoch{\omega}h)$ für alle $\omega\in\Omega,
  g,h\in G$ besteht. Die Elemente in $\Omega$ heißen \highl{Operatoren}.
\end{defini}

\begin{bemerk}
  Für $\omega\in\Omega$ gehört die Abbildung $G\rightarrow G$ mit $g\mapsto
  \vorhoch{\omega} g$ zu $\End G$. Dabei können verschiedene Elemente in
  $\Omega$ den gleichen Endomorphismus von $G$ liefern.
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item Jeder Vektorraum $V$ über einem Körper $\Omega$ lässt sich als
    $\Omega$"~Gruppe auffassen: $\vorhoch{\omega}v\coloneq\omega v$ für
    $\omega\in\Omega, v\in V$.
   \item Sei $G$ beliebig, $\Omega=\{\End G, \Aut G, \Inn G\}$ und
    $\vorhoch{\omega}g\coloneq\omega(g)$ für $\omega\in\Omega$ und $g\in G$.
   \item Sei $G$ eine beliebige Gruppe und $\Omega\subseteq G$. Wir definieren
    $\vorhoch{\omega}g= \omega g\omega^{-1}$
   \item Für jede Familie $(G_{i})_{i\in I}$ von $\Omega$"~Gruppen $G_{i}$ ist
    auch $\prod_{i\in I} G_{i}$ eine $\Omega$"~Gruppe mit
    $\vorhoch{\omega}(g_{i})_{i\in I}\coloneq(\vorhoch{\omega}g_{i})_{i\in I}$
    für $\omega\in\Omega$.
  \end{enumerate}
\end{bsp}

%def 4.9
\begin{defini}[$\Omega$"~Untergruppe] Seien $\Omega$ eine Menge und $G$ eine
  $\Omega$"~Gruppe. Eine Untergruppe $H\leq G$ mit $\vorhoch{\omega}h\in H$ für
  alle $\omega\in\Omega$ und $h\in H$ heißt \highl{$\Omega$"~Untergruppe} von
  $G$. Ist $H\trianglelefteq G$, so heißt $H$ \highl{$\Omega$"~Normalteiler}.
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item Jede $\Omega$"~Untergruppe kann man wieder als $\Omega$"~Gruppe
    auffassen.
   \item Für jeden $\Omega$"~Normalteiler $N\trianglelefteq G$ wird die
    Faktorgruppe $G/N$ zu einer $\Omega$"~Gruppe mit
    $\vorhoch{\omega}(gN)\coloneq(\vorhoch{\omega}g)N$ für $g\in G$ und
    $\omega\in\Omega$. Dies rechnet man leicht nach.
  \end{enumerate}
\end{bemerk}

\begin{bsp}
  Ist $G$ beliebig und $\Omega=\End G$ (bzw. $\Omega=\Aut G$ bzw. $\Omega=\Inn
  G$), so sind die $\Omega$"~Untergruppen von $G$ genau die vollinvarianten
  (bzw. charakteristischen bzw. normalen) Untergruppen von $G$. 
\end{bsp}

%def 4.10
\begin{defini}
  Sei $\Omega$ eine Menge sowie $G,H$ zwei $\Omega$-Gruppen. Ein
  Gruppenhomomorphismus $f\colon G\rightarrow H$ mit
  $f(\vorhoch{\omega}g)=\vorhoch{\omega} f(g)$ heißt
  \highl{$\Omega$"~Homomorphismus}. Wir üblich hat man auch die anderen Typen
  von Morphismen und den Begriff $\Omega$-isomorph. Die Notationen sind
  $\cong_{\Omega}, \Hom_{\Omega}(G,H), \End_{\Omega}(H)$ und
  $\Aut_{\Omega}(G)$.
\end{defini}

\begin{bsp}
  Seien $G, \Omega$ beliebig. Für jede $\Omega$"~Untergruppe $H\leq G$ ist die
  Inklusionsabbildung $H\rightarrow G$ mit $h\mapsto h$ ein $\Omega$"~Monom.
  Für jeden $\Omega$"~Normalteiler $N\trianglelefteq G$ ist die kanonische
  Abbildung $G\rightarrow G/N$ mit $g\mapsto gN$ ein $\Omega$"~Epimorphismus.
\end{bsp}

\begin{bemerk}
  Viele Aussagen über Gruppen, Untergruppen, Homomorphismen übertragen sich
  problemlos auf $\Omega$"~Gruppen, $\Omega$"~Untergruppen und
  $\Omega$"~Homomorphismen. Beispielsweise sind Bild und Kern von
  $\Omega$"~Homomorphismen   stets $\Omega$"~Untergruppen und jeder
  $\Omega$"~Homomorphismus $f\colon G\rightarrow H$ induziert einen
  $\Omega$"~Isomorphismus $G/\ker f\rightarrow\Bld f$ mit $g\ker f\mapsto
  f(g)$. Den "`Homomorphiesatz für $\Omega$"~Gruppen"' rechnet man schnell
  nach. Analog übertragen sich auch die anderen Isomorphiesätze auf
  $\Omega$"~Gruppen. Wir werden diese im folgenden ohne Kommentar
  verwenden.
\end{bemerk}


\chapter{Normalreihen}
\label{cha:4-Normalreihen}
%def 5.1
\begin{defini}[Subnormalreihe, Länge, Faktor, Normalreihe]
  Eine endliche Folge von Untergruppen
  \begin{gather}
    \label{eq:51}
       G=G_{0}\trianglerighteq G_{1}\trianglerighteq\dotso \trianglerighteq
       G_{l}=1
  \end{gather}
  einer Gruppe $G$ heißt \highl{Subnormalreihe} von $G$ der \highl{Länge} $l$
  mit \highl{Faktor}en $G_{0}/G_{1},\dotsc, G_{l-1}/G_{l}$. Ist
  $G_{i}\trianglelefteq G$ für alle $i$, dann heißt \autoref{eq:51}
  \highl{Normalreihe}. Ist $G_{i-1}\neq G_{i}$ für alle $i$, dann heißt
  \autoref{eq:51} eine \highl[Subnormalreihe!ohne Wiederholung]{(Sub)normalreihe ohne
  Wiederholung}. Eine \highl{Verfeinerung} von \autoref{eq:51} ist eine
  (Sub)Normalreihe
  \begin{gather}
    \label{eq:52}
       G=H_{0}\trianglerighteq H_{1}\trianglerighteq\dotso \trianglerighteq
       H_{m}=1
  \end{gather}
  derart, dass eine Injektion $f\colon\{1,\dotsc,l\}\rightarrow\{1,\dotsc,m\}$
  mit $G_{i}=H_{f(i)}$ für alle $i$ existiert. Im Fall $m>l$ heißt die
  Verfeinerung \highl[Verfeinerung!echte]{echt}\index{echt}.
\end{defini}

\begin{bsp}
  Seien $a\coloneq
  \begin{pmatrix}
    i & 0\\
    0 & -i
  \end{pmatrix}, b\coloneq
  \begin{pmatrix}
    0 & 1\\
    1 & 0
  \end{pmatrix}\in\GL(2,\C)$ und $G\coloneq\langle a,b\rangle$. Dann ist
  $\abs{G}=8$ und $G\trianglerighteq\langle
  a^{2},b\rangle\trianglerighteq\langle b\rangle\trianglerighteq1$ ist eine
  Subnormalreihe, aber $\langle b\rangle\ntrianglelefteq G$ keine Normalreihe.
  Dagegen ist $G\trianglerighteq\langle a^{2},b\rangle\trianglerighteq\langle
  a^{2}\rangle\trianglerighteq1$ eine Normalreihe.
\end{bsp}

%def 5.2
\begin{defini}
  Subnormalreihen
  \begin{gather}
    \label{eq:53}
       G=G_{0}\trianglerighteq G_{1}\trianglerighteq\dotso \trianglerighteq
       G_{l}=1
  \end{gather}
  und
  \begin{gather}
    \label{eq:54}
       G=H_{0}\trianglerighteq H_{1}\trianglerighteq\dotso \trianglerighteq
       H_{m}=1
  \end{gather}
  einer Gruppe $G$ heißen \highl{isomorph}, wenn $l=m$ ist und ein
  $f\in\Sym(l)$ mit $G_{i-1}/G_{i}\cong H_{f(i)-1}/H_{f(i)}$ für alle $i$
  existieren.
  Dies bedeutet, \autoref{eq:53} und \autoref{eq:54} haben die gleiche Länge
  und ihre Faktoren sind bis auf die Reihenfolge isomorph.
\end{defini}

\begin{bsp}
  $\Z/6\Z$ hat isomorphe Normalreihen
  $\Z/6\Z\trianglerighteq2\Z/6\Z\trianglerighteq 6\Z/6\Z$ oder
  $\Z/6\Z\trianglerighteq 3\Z/6\Z\trianglerighteq 6\Z/6\Z$.
\end{bsp}

\begin{satz}[Verfeinerungssatz von \textsc{Schreier}]\label{satz:52}
  Je zwei Subnormalreihen einer Gruppe $G$ haben isomorphe Verfeinerungen.
  \begin{proof}
    Seien \autoref{eq:53} und \autoref{eq:54} zwei Subnormalreihen von $G$.
    Setze $G_{ik}\coloneq G_{i}(G_{i-1}\cap H_{k})$ und $H_{ik}\coloneq
    H_{k}(H_{k-1}\cap G_{i})$ für $i=0,\dotsc,l$ und $k=0,\dotsc,m$. Dabei sei
    $G_{-1}\coloneq G=\colon H_{-1}$. Dann ist jeweils $G_{i0}=G_{i-1}$
    \todo{ausschreiben} und $G_{im}= G_{i}$ sowie $G_{ik}\trianglelefteq
    G_{i,k-1}$ nach dem \autoref{satz:45}. Daher ist $G\trianglerighteq
    G_{00}\trianglerighteq G_{01}\trianglerighteq\dotso \trianglerighteq
    G_{0m}=G_{10}\trianglerighteq G_{11}\trianglerighteq\dotso
    \trianglerighteq G_{1m}=G_{20}\trianglerighteq\dotso \trianglerighteq
    G_{l0}\trianglerighteq\dotso \trianglerighteq G_{lm}=1$ eine
    Subnormalreihe die \autoref{eq:53} verfeinert. Analog ist
    $H=H_{00}\trianglerighteq H_{10}\trianglerighteq\dotso \trianglerighteq
    H_{l0}=H_{01}\trianglerighteq H_{11}\trianglerighteq\dotso
    \trianglerighteq H_{l1}\trianglerighteq\dotso \trianglerighteq
    H_{0m}\trianglerighteq\dotso \trianglerighteq H_{lm}=1$ eine
    Subnormalreihe, die \autoref{eq:54} verfeinert. Dabei gilt jeweils:
    $G_{i,k-1}/G_{ik}\cong H_{i-1,k}/H_{i}$ nach dem \autoref{satz:45}.
  \end{proof}
\end{satz}

%def 5.3
\begin{defini}[Kompositionsreihe]
  Eine \highl{Kompositionsreihe} einer Gruppe $G$ ist eine Subnormalreihe von
  $G$ ohne Wiederholungen, die keine echte Verfeinerung  ohne Wiederholungen
  hat.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item Die Subnormalreihen von $\Z/6\Z$ (siehe oben) sind Kompositionsreihen.
   \item $\Z$ selbst hat keine Kompositionsreihe. Denn jede Subnormalreihe
    $\Z\triangleright n_{1}\Z\triangleright\dotso \triangleright
    n_{l}\Z\triangleright 0$ kann man zu $\Z\triangleright
    n_{1}\Z\triangleright\dotso \triangleright n_{l}\Z\triangleright
    2n_{l}\Z\triangleright 0$ verfeinern.
   \item Jede endliche Gruppe hat eine Kompositionsreihe.
  \end{enumerate}
\end{bsp}


%Vorlesung vom 2009-05-05
%satz 5.3
\begin{satz}[Satz von \textsc{Jordan}"~\textsc{Hölder}]\label{satz:53}
  Je zwei Kompositionsreihen einer Gruppe $G$ sind isomorph.
  \begin{proof}
    Nach  dem Verfeinerungssatz von \textsc{Schreier} (\autoref{satz:46})
    haben je zwei Kompositionsreihen von $G$ isomorphe Verfeinerungen. Da man
    Wiederholungen streichen kann, kann man annehmen, dass die Verfeinerungen
    keine Wiederholungen haben. Andererseits haben Kompositionsreihen keine
    echten Verfeinerungen ohne Wiederholungen. Daher sind bereits die
    ursprünglichen Kompositionsreihen isomorph.
  \end{proof}
\end{satz}

\begin{bemerk}
  Nach dem zweiten Isomorphiesatz (\autoref{satz:44-2isosatz}) ist eine
  Subnormalreihe genau dann eine Kompositionsreihe, wenn ihre Faktoren
  einfache Gruppen sind. Diese heißen dann \highl{Kompositionsfaktoren} von
  $G$ und die Länge einer Kompositionsreihe heißt \highl{Kompositionslänge}
  von der Gruppe $G$.
\end{bemerk}

%def 5.4
\begin{defini}[(charakteristisch) einfache $\Omega$"~Gruppe]
  Sei $\Omega$ eine Menge. Eine $\Omega$"~Gruppe $G\neq1$ heißt
  \highl{einfach}\index{$\Omega$"~Gruppe!einfache}, wenn $1$ und $G$ die einzigen $\Omega$"~Normalteiler von
  $G$ sind. Im Fall $\Omega=\Aut G$ heißt $G$ \highl{charakteristisch
  einfach}\index{$\Omega$"~Gruppe!charakteristisch einfache}.
\end{defini}

\begin{bemerk}
  Die Definition von $\Omega$-(Sub-)Normalreihen und
  $\Omega$"~Kompositionsreihen ist klar. Die Sätze von \textsc{Schreier} und
  \textsc{Jordan}"~\textsc{Hölder} übertragen sich. Im Fall $\Omega=\Inn G$
  heißen $\Omega$"~Kompositionsreihen \highl[Hauptreihe]{Hauptreihen}. Die
  Faktoren heißen \highl[Hauptfaktor]{Hauptfaktoren} und ihre Länge
  \highl{Hauptlänge}. Nach \autoref{satz:47}~(iii) ist jeder Hauptfaktor
  charakteristisch einfach.
\end{bemerk}

\begin{satz}
  Sei $\Omega$ eine Menge und $G$ eine $\Omega$"~Gruppe mit
  $\Omega$"~Subnormalreihe $G=G_{0}\trianglerighteq
  G_{1}\trianglerighteq\dotso \trianglerighteq G_{l}=1$.
  \begin{enumerate}
   \item Für jede $\Omega$"~Untergruppe $H\leq G$ ist $H=H\cap
    G_{0}\trianglerighteq H\cap G_{1}\trianglerighteq\dotso \trianglerighteq
    H\cap G_{l}=1$ eine $\Omega$"~Subnormalreihe von $H$ mit $H\cap
    G_{i-1}/H\cap G_{i}\cong_{\Omega} (H\cap G_{i-1})G_{i}/G_{i}\leq
    G_{i-1}/G_{i}$.
   \item Für jeden $\Omega$"~Normalteiler $N\trianglelefteq G$ ist $G/N=
    G_{0}N/N\trianglerighteq G_{1}N/N\trianglerighteq\dotso \trianglerighteq
    G_{l}N/N=1$ eine $\Omega$"~Subnormalreihe von $G/N$ mit
    $(G_{i-1}N/N)/G_{i}N/N\cong_{\Omega}
    G_{i-1}N/G_{i}N\cong_{\Omega}G_{i-1}/G_{i-1}\cap G_{i}N\cong_{\Omega}
    (G_{i-1}/G_{i})/(G_{i-1})\cap G_{i}N/G_{i}$ für alle $i$.
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item Jeweils gilt: $H\cap G_{i}= (H\cap G_{i-1})\cap
      G_{i}\trianglelefteq H\cap G_{i-1}$ nach dem ersten Isomorphiesatz
      (\autoref{satz:43-1isosatz}). Der Rest des Satzes lässt sich mit dem
      gleichen Satz beweisen.
     \item Wegen $G_{i}\trianglelefteq G_{i-1}$ ist $G_{i}N/N\trianglelefteq
      G_{i-1}N/N$ (Anwendung des Homomorphismus nach \autoref{bsp:41}~(vi)).
      Ferner ist $G_{i-1}N/G_{i}N= G_{i-1}(G_{i}N)/G_{i}N\cong_{\Omega}
      G_{i-1}/G_{i-1}\cap G_{i}N$ nach \autoref{satz:43-1isosatz}.
      \todo{Ausdrücke prüfen}
    \end{enumerate}
  \end{proof}
\end{satz}

%def 5.5
\begin{defini}[Normaler Endomorphismus]
  Ein Endomorphismus $\alpha$ einer Gruppe $G$ mit $\alpha(xyx^{-1})=
  x\alpha(y)x^{-1}$ für alle $x,y\in G$ heißt
  \highl[Endomorphismus!normaler]{normal}. 
\end{defini}

\begin{bemerk}
  Mit $\Omega\coloneq\Inn G$ sind also die normalen Endomorphismen von $G$
  die $\Omega$"~Endomorphismen von $G$. Ferner ist ein $\alpha\in\End G$
  genau dann normal, wenn gilt: $x^{-1}\alpha(x)\alpha(y)=
  \alpha(y)x^{-1}\alpha(x)$ für alle $x,y\in G$ ist\todo{Nachrechnen}, d.\,h.
  wenn $x^{-1}\alpha(x)$ für $x\in G$ mit jedem Element in $\alpha(G)$
  vertauschbar ist. Insbesondere ist ein $\alpha\in\Aut G$ genau dann normal,
  wenn $x^{-1}\alpha(x)$ im Zentrum von $G$ ist.
\end{bemerk}

\begin{bsp}
  Die Identitätsabbildung $\iota_{G}\colon G\rightarrow G$ mit $g\mapsto g$
  und die \highl{Nullabbildung} $0_{G}\colon G\rightarrow G$ mit $g\mapsto1$
  sind stets normal.
\end{bsp}

\begin{satz}[\textsc{Schur}s Lemma]
  Für jede Menge $\Omega$, jede einfache $\Omega$"~Gruppe $G$ und jeden
  normalen $\Omega$"~Endomorphismus $0\neq\alpha\in\End_{\Omega} G$ gilt:
  $\alpha\in\Aut_{\Omega} G$.
  \begin{proof}
    Sicher ist das $\alpha(G)$ ein $\Omega$"~Normalteiler von $G$. Wegen
    $\alpha\neq 0$ ist $\alpha(G)\neq1$. Also ist $\alpha(G)=G$. Analog ist
    der Kern von $\alpha$ ein $\Omega$"~Normalteiler mit $\ker\alpha\neq G$
    (wegen $\alpha\neq0$). Daher ist der Kern von $\alpha$ gleich $1$, d.\,h.
    $\alpha$ ist injektiv.
  \end{proof}
\end{satz}

%kap 6
\chapter{Direkte Zerlegungen}

%def 6.1
\begin{defini}[Direkte Summe]
  Sei $(G_{i})_{i\in I}$ eine nichtleere Familie von Normalteilern $G_{i}$
  einer Gruppe $G$ mit den folgenden Eigenschaften:
  \begin{enumerate}
   \item $G=\langle G_{i}\colon i\in I\rangle$
   \item $i\in I\Rightarrow G_{i}\cap\langle G_{j}\colon i\neq j\in I\rangle=1$
  \end{enumerate}
  Dann heißt $G$ die \highl[Summe!direkte]{direkte Summe} von $(G_{i})_{i\in
  I}$. Man schreibt $G=\bigoplus_{i\in I} G_{i}$. Falls $I=\{1,\dotsc,n\}$ für
  ein $n\in\N$ auch $G=G_{1}\oplus\dotso \oplus G_{n}$.
\end{defini}

\begin{bemerk}\label{bem:61}
  \begin{enumerate}
   \item Für verschiedene Indizes $i,j\in I$ ist dann $G_{i}\cap G_{j}=1$.
    Nach dem \autoref{satz:46} ist also jedes $x_{i}\in G_{i}$ mit jedem
    $x_{j}\in G_{j}$ vertauschbar. Zu jedem $g\in G$ existieren ferner
    $i_{1},\dotsc, i_{n}\in I, g_{i_{1}}\in G_{i_{1}},\dotsc, g_{i_{n}}\in
    G_{i_{n}}$ mit $g= g_{i_{1}}\cdot\dotso \cdot g_{i_{n}}$. \OE haben wir
    $i_{1},\dotsc, i_{n}$ paarweise verschieden. Auf die Reihenfolge der
    Faktoren kommt es dabei nicht an. Wir setzen $g_{i}=1$ für $i\in
    I\setminus\{i_{1},\dotsc, i_{n}\}$ und schreiben auch $g=\prod_{i\in I}
    g_{i}$. Hat man eine weitere Familie $(h_{i})_{i\in I}$ von Elementen
    $h_{i}\in G_{i}$ mit $\abs{\{i\in I\colon h_{1}\neq1\}}<\infty$ und
    $g=\prod_{i\in I} h_{i}$, so ist $g_{i}=h_{i}$ für alle $i$. Denn im Fall
    $g_{i}\neq h_{i}$ für ein $i\in I$ wäre $1\neq g_{i}^{-1}h_{i}=
    \prod_{i\neq j\in I} g_{j}h_{j}^{-1}\in G_{i}\cap\langle G_{j}\colon i\neq
    j\in I\rangle=1$ \lightning{} Jedes Element in $G$ lässt also in der Form
    $g=\prod_{i\in I} g_{i}$ mit eindeutig bestimmten Elementen $g_{i}\in
    G_{i}$ schreiben, von denen nur endlich viele von $1$ verschieden sind.
    Daraus folgt leicht, dass $\coprod_{i\in I} G_{i}\rightarrow G$ mit
    $(g_{i})_{i\in I}\mapsto\prod_{i\in I}g_{i}$ ein Isomorphismus ist. Man
    identifiziert daher oft $G=\bigoplus_{i\in I}G_{i}$ mit $\coprod_{i\in I}
    G_{i}$ und schreibt z.\,B. im Fall $I=\{1,\dotsc,n\}$ auch
    $G_{1}\times\dotso \times G_{n}$ statt $G_{1}\oplus\dotso \oplus G_{n}$.
   \item Sei umgekehrt $(G_{i})_{i\in I}$ eine Familie beliebiger Gruppen. Wir
    setzen $G\coloneq\coprod_{i\in I} G_{i}$ und $\widehat{G_{j}}\coloneq\{(g_{i})_{i\in
    I}\in \coprod_{i\in I}G_{i}\colon g_{i}=1\forall j\neq i\in I\}$. Dann
    folgt leicht, dass $G=\bigoplus_{i\in I} \widehat{G_{j}}$ und
    $\widehat{G_{j}}\cong G_{j}$ für alle $j\in I$. Auch hier identifiziert
    man oft $G_{j}$ mit $\widehat{G_{j}}$ und fasst so $G_{j}$ als Untergruppe
    von $G$ auf.
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:61}
  Seien $G_{1},\dotsc, G_{n}$ Normalteiler einer Gruppe $G$ mit
  $G=G_{1}\cdot\dotso \cdot G_{n}$ und $G_{i}\cap G_{1}\cdot\dotso \cdot
  G_{i-1}=1$ für $i=2,\dotsc,n$. Dann ist $G=G_{1}\oplus\dotso \oplus G_{n}$.
  \begin{proof}
    Sei $i\in\{1,\dotsc,n\}$ und $1\neq g\in G_{i}\cap\langle G_{1},\dotsc,
    G_{i-1}, G_{i+1},\dotsc, G_{n}\rangle= G_{i}\cap G_{1}\cdot\dotso \cdot
    G_{i-1}\cdot G_{i+1}\cdot\dotso \cdot G_{n}$. Dann existieren Elemente
    $g_{1}\in G_{1},\dotsc, g_{i-1}\in G_{i-1}, g_{i+1}\in G_{i+1},\dotsc,
    g_{n}\in G_{n}$ mit $g=g_{1}\cdot\dotso \cdot g_{i-1}\cdot g_{i+1}
    \cdot\dotso \cdot g_{n}$. Für verschiedene $j,h\in\{1,\dotsc,n\}$ ist
    $G_{j}\cap G_{k}=1$, d.\,h. jedes $x_{j}\in G_{j}$ ist mit jedem $x_{k}\in
    G_{k}$    vertauschbar. Daher haben wir $1=g_{1}\cdot\dotso \cdot
    g_{i-1}\cdot g_{i}\cdot g_{i+1}\cdot\dotso \cdot g_{n}$ mit $g_{i}\coloneq
    g^{-1}$. Sei $j\in\{1,\dotsc,n\}$ maxmimal mit $g_{j}\neq1$. Dann $1\neq
    g_{j}^{-1}= g_{1}\cdot\dotso \cdot g_{j-1}\in G_{j}\cap G_{1}\cdot\dotso
    \cdot G_{j-1}=1$ \lightning{}
  \end{proof}
\end{satz}

\begin{bsp}
  Sind $G_{1}, G_{2}$ Normalteiler einer Gruppe $G$ mit $G=G_{1}\cdot G_{2}$
  und $G_{1}\cap G_{2}=1$. Dann ist $G=G_{1}\oplus G_{2}$.
\end{bsp}

%satz 6.2
\begin{satz}
  Seien $G_{1},\dotsc, G_{n}$ Normalteiler einer endlichen Gruppe $G$ mit
  $\abs{G}= \abs{G_{1}}\cdot\dotso \cdot\abs{G_{n}}$ und $\ggt(\abs{G_{i}},
  \abs{G_{j}})=1$ für $i\neq j$. Dann ist $G=G_{1}\oplus\dotso \oplus G_{n}$.
  \begin{proof}
    Der Beweis erfolgt durch Induktion nach $i$: $G_{i}\cap G_{1}\cdot\dotso
    \cdot G_{i-1}=1$ und $\abs{G_{1}\cdot\dotso \cdot G_{i}}=
    \abs{G_{1}}\cdot\dotso \cdot \abs{G_{i-1}}$. Für $i=2$ ist $\abs{G_{2}\cap
    G_{1}} \mid \ggt(\abs{G_{2}}, \abs{G_{1}})=1$. Also $G_{2}\cap G_{1}=1$
    und $\abs{G_{1}G_{2}}= \abs{G_{1}}\cdot\abs{G_{2}}(\cdot\abs{G_{1}\cap
    G_{2}})= \abs{G_{1}}\cdot\abs{G_{2}}$. Sei die Aussage für $i$ schon
    bewiesen. Dann $\abs{G_{i+1}\cap G_{1}\cdot\dotso \cdot G_{i}} \mid
    \ggt(\abs{G_{i+1}}, \abs{G_{1}\cdot\dotso \cdot G_{i}})=
    \ggt(\abs{G_{i+1}}, \abs{G_{1}}\cdot\dotso \cdot\abs{G_{i}})=1$. Also
    $G_{i+1}\cap G_{1}\cdot\dotso \cdot G_{i}=1$ und $\abs{G_{1}\cdot\dotso
    \cdot G_{i}\cdot G_{i+1}}= \abs{G_{1}\cdot\dotso \cdot
    G_{i}}\cdot\abs{G_{i+1}}= \abs{G_{1}}\cdot\dotso
    \cdot\abs{G_{i}}\cdot\abs{G_{i+1}}$. Am Ende hat man $\abs{G}=
    \abs{G_{1}}\cdot\dotso \cdot\abs{G_{n}}= \abs{G_{1}\cdot\dotso \cdot
    G_{n}}$. Also $G=G_{1}\cdot\dotso \cdot G_{n}$. Aus dem \autoref{satz:61}
    folgt die Behauptung.
  \end{proof}
\end{satz}

