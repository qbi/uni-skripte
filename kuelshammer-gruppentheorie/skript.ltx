%def 15.3
\begin{defini}[Fokalgruppe]
  Für jede Untergruppe $H$ einer endlichen Gruppe $G$ heißt
  \begin{gather*}
    \Foc_{G}(H)\coloneq\langle\underbrace{[g,h]}_{ghg^{-1}h^{-1}}\colon g\in
       G, h\in H,[g,h]\in H\rangle= \langle xy^{-1}\colon x,y\in H,x\sim_{G}y\rangle
  \end{gather*}
  \highl{Fokalgruppe} von $H$ in $G$.
\end{defini}

\begin{bemerk}
  Dann: $H'\subseteq F\coloneq\Foc_{G}(H)\subseteq H\cap G'$. Das bedeutet
  insbesondere $F\unlhd H$ und $H/F$ abelsch.Für alle $g\in G, h\in H$ mit
  $[g,h]\in H$ ist ferner $ghg^{-1}F=ghg^{-1}h^{-1}Fh=[g,h]Fh=Fh=hF$. Folglich
  $V^{G}_{H/F}(h)= h^{\abs{G\colon H}}F$ für alle $h\in H$.
\end{bemerk}

\begin{satz}\label{satz:153}
  Seien $G$ eine endliche Gruppe, $H$ eine Untergruppe von $G$, $F$ die
  Fokalgruppe, $N\coloneq \ker(V^{G}_{H})$ und $\ggt(\abs{G\colon H},
  \abs{H\colon F})=1$. Dann gilt:
  \begin{enumerate}
   \item $F=H\cap G'= H\cap N$
   \item $HN=G$ und $G/N\cong H/F$
   \item $G/G'=HG'/G'\oplus N/G'$
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}
     \item Wegen $G/N\cong\Bld(V^{G}_{H/F})\leq H/F$ ist $G/N$ abelsch, d.\,h.
      $G'\subseteq N$ und $F\subseteq H\cap G'\subseteq H\cap N$. Für $h\in
      H\cap N$ ist umgekehrt $1=V^{G}_{H/F}(h)= h^{\abs{G\colon H}}F$. Ferner
      haben wir $h^{\abs{H\colon F}}=1$ nach \textsc{Fermat}. Nach
      Voraussetzung ist also $hF=1$, d.\,h. $h\in F$.
     \item Aus Teil (i) folgt: $\abs{G/N}\geq\abs{HN/N}= \abs{H/H\cap N}=
      \abs{H/F}\geq\abs{G/N}$. Daher folgt: $HN=G$ und Verlagerung ist
      surjektiv. Folglich ist $G/N\cong H/F$.
     \item Nach Teil (ii) ist $G/G'= (HG'/G')(N/G')$. Nach Teil (i) ist $N\cap
      HG'= (N\cap H)G'$ wegen der Dedekindschen Identität. Insgesamt ergibt
      sich weiter $(N\cap H)G'=G'$, d.\,h. $(N/G')\cap(HG'/G')=1$.
    \end{enumerate}
  \end{proof}
\end{satz}


\begin{gather*}
  \xymatrix{G \ar@{-}[rr] \ar@{-}[dd] & & H \ar@{-}[dd]\\
            \ar[rr]^{\sim}_{V^{G}_{H/F}} & & \\
            N \ar@{-}[rr] & & F}
\end{gather*}

\begin{bsp}
  Die oben geforderte Teilerfremdheit ist dann erfüllt, wenn $H$ eine
  Hallgruppe von $G$ ist.
\end{bsp}

%def 15.4
\begin{defini}[Hyperfokale Gruppe]
  Sei $H$ eine  Untergruppe  einer endlichen Gruppe $G$. Setze $H_{1}\coloneq
  H, H_{n+1}\coloneq\Foc_{G}(H_{n})$ für alle natürlichen Zahlen $n$. Ist
  $H_{m}=1$ für ein $m\in \N$, so heißt die Fokalgruppe
  \highl{hyperfokal}\index{Gruppe!hyperfokale}.
\end{defini}

\begin{bemerk}
  Gegebenenfalls ist jede Untergruppe $K\leq H$ wieder hyperfokal in $G$ wegen
  $\Foc_{G}(K)\subseteq\Foc_{G}(H)$. Ferner ist $H$ auch hyperfokal in jeder
  Untergruppe $U\leq G$ mit $H\leq U$ wegen $\Foc_{U}(H)\subseteq\Foc_{G}(H)$.
  Schließlich ist $H$ nilpotent wegen $H^{n}\subseteq H_{n}$ für alle
  natürlichen $n$.
\end{bemerk}

\begin{satz}\label{satz:154}
  Jede hyperfokale Hallgruppe $H$ einer endlichen Gruppe $G$ hat ein normales
  Komplement in $G$.
  \begin{proof}
    Der Beweis wird per Induktion über die Gruppenordnung durchgeführt. \OE
    sei $H\neq1$. Dann $F\coloneq\Foc_{G}(H)<H$. Nach \autoref{satz:153} ist
    $N\coloneq\ker(V^{G}_{H/F})\unlhd G$ und $G/N\cong H/F\neq1$. Die
    Hallgruppe $H\cap N$ von $N$ ist nach obiger Bemerkung hyperfokal in $G$
    und auch in $N$. Wegen der Induktion hat $H\cap N$ ein normales Komplement
    $K$ in $N$. Als Hallgruppe von $N$ ist $K$ charakteristisch in $N$. Daher
    ist $K\unlhd G$. Ferner $HK= H(H\cap N)K= HN= G$ und $H\cap K= H\cap N\cap
    K= 1$.
  \end{proof}
\end{satz}

%satz 15.5
\begin{satz}\label{satz:155}
  Sei $H$ eine nilpotente Hallgruppe einer endlichen Gruppe $G$. Je zwei
  Elemente in $H$, die in $G$ konjugiert sind, seien auch in $H$ konjugiert.
  Dann hat $H$ ein normales Komplement in $G$.
  \begin{proof}
    Setze $H_{1}\coloneq H, H_{n+1}\coloneq \Foc_{G}(H_{n})$ für $n\in\N$.
    Nach \autoref{satz:154} genügt es zu zeigen, dass $H_{n}\subseteq H^{n}$
    für natürliche Zahlen $n$. Für $n=1$ ist das klar. Sei also
    $H_{n}\subseteq H^{n}$ für $n\in\N$. Für $g\in G$ und $h\in H_{n}$ mit
    $ghg^{-1}h^{-1}\in H_{n}$ ist $ghg^{-1}\in H_{n}$. Nach Voraussetzung
    existiert eine Element $k\in H$ mit $ghg^{-1}=khk^{-1}$. Folglich: $[g,h]=
    ghg^{-1}h^{-1}= khk^{-1}h^{-1}= [k,h]\in[H,H_{n}]\subseteq[H,H^{n}]=
    H^{n+1}$. Dies zeigt, dass das $H_{n+1}= \langle[g,h]\colon g\in G,h\in
    H_{n}, [g,h]\in H_{n}\rangle\subseteq H^{n+1}$.
  \end{proof}
\end{satz}

%satz 15.6
\begin{satz}\label{satz:156}
  Sei $H$ eine abelsche Hallgruppe einer endlichen Gruppe $G$. Dann sind je
  zwei Elemente  $x,y\in H$, die in $G$ konjugiert sind, auch in $N_{G}(H)$
  konjugiert.
  \begin{proof}
    Sei $g\in G$ mit $y=gxg^{-1}\in H\cap gHg^{-1}$. Dann sind $H$ und
    $gHg^{-1}$ Hallgruppen von $C_{G}(y)$. Nach \autoref{satz:136} (Satz von
    \textsc{Wielandt}) existiert ein $c\in C_{G}(y)$ mit $H=cgHg^{-1}c^{-1}$
    und $y= cyc^{-1}= cgxg^{-1}c^{-1}$ mit $cg\in N_{G}(H)$.
  \end{proof}
\end{satz}

%satz 15.7
\begin{satz}[Satz von \textsc{Burnside}]\label{satz:157}
  Jede Hallgruppe $H$ einer endlichen Gruppe $G$ mit $N_{G}(H)= C_{G}(H)$ hat
  ein normales Komplement in $G$.
  \begin{proof}
    Seien $x,y\in H$ mit $x\sim_{G}y$. Nach \autoref{satz:156} ist
    $x\sim_{N_{G}(H)}y$ und wegen der Voraussetzung $x\sim_{C_{G}(H)}
    y\Rightarrow x=y$, d.\,h. $x\sim_{H}y$. Nun wenden wir \autoref{satz:155}
    an.
  \end{proof}
\end{satz}

\begin{bemerk}
  Nach der Voraussetzung des obigen Satzes ist $H$ auf jeden Fall abelsch.
\end{bemerk}

%satz 15.8
\begin{satz}
  Seien $G$ eine endliche Gruppe, $p$ der kleinste Primteiler von $\abs{G}$
  und $P\in\Syl_{p}(G)$ zyklisch. Dann hat $P$ ein normales Komplement in $G$.
  \begin{proof}
    Sei $\abs{P}=p^{n}$. Dann: $\abs{\Aut(P)}= p^{n}-p^{n-1}= p^{n-1}(p-1)$.
    Da $N_{G}(P)/C_{G}(P)$ zu einer Untergruppe con $\Aut(P)$ isomorph ist,
    folgt, $\abs{N_{G}(P)/C_{G}(P)} \mid p-1$. Nach der Wahl von $p$ ist
    $N_{G}(P)/C_{G}(P)=1$, d.\,h. $N_{G}(P)= C_{G}(P)$. Nun wenden wir
    \autoref{satz:157} an.
  \end{proof}
\end{satz}

\begin{bemerk}
  Hat $G$ eine zyklische 2"~Sylow"=Gruppe $P$, so hat $P$ ein normales
  Komplement $K$ in $G$ nach dem Satz. Wegen $2\nmid\abs{K}$ ist $K$ auflösbar
  (wegen \textsc{Feit}"~\textsc{Thompson}). Somit ist auch $G$ auflösbar.
\end{bemerk}

\begin{bsp}
  Aus dem Satz folgt insbesondere, dass für ungerade $n\in\N$ jede Gruppe der
  Ordnung $2n$ einen Normalteiler der Ordnung $n$ enthält (vgl. Übung).
\end{bsp}

