set xtics ("1/3" .33333, "1/2" .5, 1)
set ytics (1, 2, 3)
set style line 2 linewidth 2
set output "1x.png"
set terminal png crop

f_til(x, a) = a*x <= 1? a:0

plot [0.2:1.1] 1/x notitle, \
     f_til(x,3) title "f_3" linestyle 2, \
     f_til(x,2) title "f_2" linestyle 2, \
     f_til(x,1) title "f_1" linestyle 2
