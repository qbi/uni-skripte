\chapter{Distributionen}

schwache Lösungen:
$\Delta u = f$ in $\Omega$, $\Omega$ Gebiet in $\R^{n}$,
$u\big|_{\partial \Omega} = 0$ auf $\partial\Omega$.

klassisch: $f\in C(\Omega)$ sucht Lösungen $u\in
C(\overline{\Omega})\cap C^{2}(\Omega)$ $\Longrightarrow$
\begin{gather*}
  \int_{\Omega} \Delta u(x) v(x)\,dx = \int_{\Omega} f(x) v(x)\,dx, v\in
  C^{1}_{0}(\Omega), \supp v\in \Omega
\end{gather*}

Gauß:
\begin{gather*}
  \int_{\Omega} div g\,dx = \int_{\partial\Omega} g \vec{u}\,dt
\end{gather*}

$g = v\operatorname{grad} u$ $\Longrightarrow$
\begin{align*}
  \operatorname{div} g
     &= \sum_{j=1}^{n} \frac{\partial}{\partial x_{j}}
     \big(v\frac{\partial u}{\partial x_{j}}\big)\\
  &= \sum_{j=1}^{n} \frac{\partial v}{\partial x_{j}}
     \frac{\partial u}{\partial x_{j}} + v\delta u
\end{align*}

$\Longrightarrow$
\begin{align*}
  \int_{\Omega} f(x) v(x)\,dx &= \int_{\Omega} \Delta u(x) v(x)\,dx\\
  &= \int_{\Omega} \operatorname{div} g\,dx - \sum_{j=1}^{n} \int_{\Omega}
     \frac{\partial v}{\partial x_{j}} \frac{\partial u}{\partial
     x_{j}}\,dx\\
  &= \int_{\partial\Omega} v \operatorname{grad} u \vec{u} \diff x
     - \sum_{j=1}^{n} \int_{\Omega} \frac{\partial v}{\partial x_{j}}
     \frac{\partial u}{\partial x_{j}} \diff x
\end{align*}

$\Delta u=f$ $\Longrightarrow$ $\int_{\Omega} f(x) v(x)\,dx =
-\sum_{j=1}^{n} \int_{\Omega} \frac{\partial v}{\partial x_{j}}
\frac{\partial u}{\partial x_{j}}\,dx, v\in C^{1}(\Omega), \supp v\subset
\Omega$

keine Punktweise Aussage mehr (für $f$) $\Longrightarrow$ $u$
\highl{schwache Lösung} (Sobolev 1936)

\begin{itemize}
 \item verallgemeinerte Ableitungen
  $f\colon \R \rightarrow \R, f'= g \Longleftrightarrow
  \int_{-\infty}^{\infty} f'(x) \phi(x) \,dx = \int_{-\infty}^{\infty}
  g(x) \phi(x), \phi\in C_{0}^{\infty}(\R), \supp \phi$ kompakt
  $\Longleftrightarrow$ $\underbrace{-\int_{-\infty}^{\infty} f(x)
  \phi'(x)\,dx}_{nicht nur punktweise sinnvoll} =
  \int_{-\infty}^{\infty} g(x)\phi(x)\,dx$

  $f(x) = \begin{cases}\infty& x\geq0\\0x<0\end{cases}$

  \begin{gather*}
    \int_{-\infty}^{\infty} g(x) \phi(x) \diff x
       = -\int_{0}^{\infty} \phi'(x) \diff x,
       \qquad \phi\in C_{0}^{\infty}(\R)
  \end{gather*}

  \highl{$\delta$"~Funktion} der Physiker:
  \begin{gather*}
    \delta(x)
       = \begin{cases}
           \infty& x=0\\
           0& x\ne0
         \end{cases}
       \qquad
       \int_{-\infty}^{\infty} \delta(x) \diff x = 1
  \end{gather*}

  \highl{$\delta$"~Distribution}: $\delta\colon
  C_{0}^{\infty}(\R)\rightarrow\C, \delta(\phi)=\phi(0)$
\end{itemize}

\section{\texorpdfstring{$D(\Omega)\times D'(\Omega)$}{D(Omega)xD'(Omega)}}
\subsection{Definitionen und Beispiele}
$\Omega\subset\R^{n}$ Gebiet, $C_{0}^{\infty}(\Omega)$ wie in
\autoref{sec:1.2.3}

\begin{defini}[Konvergenz in $C_{0}^{\infty}(\Omega)$]
  Seien $\phi_{j}, j\in\N$ und $\phi\in C_{0}^{\infty}$ Dann konvergiert
  $\phi_{j}\rightarrow \phi$ auf $D$, falls gelten:
  \begin{enumerate}
   \item $\exists K\subset\Omega, K$ kompakt, $\supp \phi_{j}\subset K,
    \supp \phi\subset K$
   \item $\forall \alpha\in \N_{0}^{n}$ $D^{\alpha}
    \phi_{j} \xrightarrow{gleichm.} D^{\alpha} \phi$ auf $K$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{itemize}
   \item $D^{\alpha}\phi =
    \frac{\partial^{\alpha}\phi}{\partial^{\alpha_{1}} x_{1}\ldots
      \partial^{\alpha_{n}}x_{n}}, \alpha =
    (\alpha_{1},\ldots,\alpha_{n}), \abs{\alpha} = \abs{\alpha_{1}} + \dotsb
    +\abs{\alpha_{n}}$
    \help{hier fehlt was}
   \item $D(\Omega) = C_{0}^{\infty}(\Omega) + \help{hier fehlt was}$
  \end{itemize}
\end{bemerk}

\begin{bemerk}
  \begin{itemize}
   \item $T\colon D(\Omega) \rightarrow \C$
   \item $T(a_{1}\phi_{1} + a_{2}\phi_{2}) = a_{1}(T\phi_{1}) +
    a_{2}T(\phi_{2}), a_{1}, a_{2}\in \C, \phi_{1}, \phi_{2}\in D(\Omega)$
   \item $\phi_{j} \xrightarrow[D]{} \phi$ $\Longrightarrow$ $T(\phi_{j})
    \rightarrow T(\phi)$
   \item $\bigl[D(\Omega)\bigr]' = D'(\Omega)$
   \item L.\,Schwartz 1950/51
  \end{itemize}
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item reguläre Distributionen: $L_{1}^{loc}$ ($g\in L_{1}^{loc}$,
    falls $\int_{K} g(x)\,dx$ existiert für alle kompakten $K\subset
    \R^{n}$)

    $L_{1}^{loc}(\Omega) \subset D'(\Omega)$ bei geeigneter Intepretation
    von "`$\subset$"'

    $f\in L_{1}^{loc}(\Omega), \phi \in D(\Omega) \Longrightarrow
    f\phi\in L_{1}(\Omega)$
    \help{hier fehlt was}

    $T_{f}(\phi) := \int_{\Omega} f(x) \phi(x)\,dx, \phi\in D(\Omega)$

    Behauptung: $T_{f}\in D'(\Omega)$, linear klar, stetig:
    $\phi_{j} \xrightarrow[D]{}\phi\colon D^{\gamma}\phi_{j} \rightarrow
    D^{\gamma}_{\phi}$ gleichmäßig auf $K$, insbesondere:
    $\phi_{j}\xrightarrow{glm.}\phi$ $\Longrightarrow$ $\exists M>0
    \forall x\in K\forall j\in \N: \abs{\phi_{j}(x)}\leq M$
    $\Longrightarrow$ $\abs{f(x)\phi_{j}(x)} \leq
    \begin{cases}M\abs{f(x)} & x\in M\\0&\text{sonst}\end{cases}\in
    L_{1}(\Omega)$

    Lebesgue $\Longrightarrow$ $T_{f}(\phi_{j}) = \int_{\Omega} f(x)
    \phi_{j}(x)\,dx \xrightarrow{j\rightarrow\infty} \int_{\Omega} f(x)
    \phi(x)\,dx = T_{f}(\phi)$
   \item
  \end{enumerate}
\end{bsp}

\subsection{Träger einer Distribution und Lokalisationsprinzip}

\autoref{satz:1.2.3/2}: $\Omega\subset\R^{n}$ offen, $K\subset
\Omega$ kompakt: $\exists\phi\in C_{0}^{\infty}(\R^{n})$
\begin{itemize}
 \item $0\leq \phi(x) \leq 1$, $x\in \Omega$
 \item $\phi(x) = 1$, $x\in K$
 \item $\supp \phi \subset \Omega$
\end{itemize}

\begin{lemma}[Zerlegung der Einheit]\label{lem:5.1.2/1}
  Sei $\Gamma\subset\R^{n}$ kompakt und $K_{j}, j=1,\ldots,m$ offene
  Mengen mit $\Gamma\subset\bigcup_{j=1}^{m} K_{j}$. Dann gibt es
  $\phi_{j}\in D(K_{j}), j=1,\ldots,m$, so dass gilt:
  \begin{gather*}
    \sum_{j=1}^{m} \phi_{j}(x) = 1, x\in\Gamma
  \end{gather*}

  \begin{proof}
    o.\,B.\,d.\,A. $K_{j} = K_{r_{j}}(jj)$ offene Kugeln, $\delta>0
    K_{j}^{\delta}= \{x\in\R^{n}\colon \abs{x-y_{j}}<\delta_{rj}\}$

    \begin{schritte}
     \item zeigen $\exists\delta_{0}, 0 < \delta_{0} < 1
      \forall\delta>\delta_{0}$,
      \begin{gather}\label{eq:48}
        \Gamma\subset\bigcup_{j=1}^{m} K_{j}^{\delta}
      \end{gather}

      Ann.: $\exists\delta_{k},
      \delta_{k}\xrightarrow{k\rightarrow\infty}1$ $\exists
      (z_{k})_{k=1}^{\infty}, z_{k}\in \Gamma\setminus\bigcup_{j=1}^{m}
      K_{j}^{\delta_{k}}$

      $\Gamma$ kompakt, $\lim_{k\rightarrow\infty} z_{k} = z\in \Gamma$
      (eventuell Teilfolge)

      $P\subset \bigcup_{j=1}^{m}K_{j} \xrightarrow{o.\,B.\,d.\,A.} z\in K_{1}
      \Longrightarrow^{K_{1} \text{~offen}} \exists k_{0} \forall k > k_{0}:
      z_{k}\in K_{1} \Longrightarrow^{\delta_{k}\rightarrow1} \exists
      l_{0} \forall l>l_{0} \exists k_{0}' \forall k> k_{0}': z_{k}\in
      K_{1}^{\delta_{l}} \Longrightarrow \exists k_{0}', l_{0} \forall k
      > k_{0}' \forall l > l_{0}: z_{k}\in \bigcup_{j=1}^{m}
      K_{j}^{\delta_{l}} \Longrightarrow \exists \nu\colon z_{\nu}\in
      \bigcup_{j=1}^{m} K_{j}^{\delta_{\nu}}$ $\Rightarrow$ Widerspruch
     \item zeigen $\exists h_{0} \forall 0 < h < h_{0} \Gamma_{h}\subset
      \bigcup_{j=1}^{m} K_{j}, \Gamma_{h}:= \{x\in \C^{n}\colon \dist(x,
      \Gamma) < h\}$

      Ann: $\exists (h_{l})_{l}, h_{l} \rightarrow 0$ $\exists
      (z_{k})_{k},  z_{k}\in \Gamma_{h_{l}}, z_{k}\subset\in
      \bigcup_{j=1}^{m} K_{j}$

      $\Longrightarrow$ $\lim_{k\rightarrow\infty} z_{k} = z\in \Gamma,
      \Gamma$ kompakt

      $\Gamma\subset\bigcup_{j=1}^{m} K_{j} \xrightarrow{o.\,B.\,d.\,A.}z\in
      K_{1}$ $\Longrightarrow^{K_{1} \text{~offen}}$ $\exists K_{1}\colon \forall k >
      k_{1}\colon z_{k}\in K_{1}\subset \bigcup_{j=1}^{m}K_{j}$
      $\Longrightarrow$ Widerspruch

      d.\,h. $\exists h_{0}>0 \exists \delta_{0}< 1 \forall h < h_{0}\forall
      \delta > \delta_{0}\colon$
      \begin{gather*}
        \gamma_{0} \subset \bigcup_{j=1}^{m} K_{j}^{\delta}
      \end{gather*}
     \item wenden \autoref{satz:1.2.3/2} an auf
      \begin{itemize}
       \item $\Omega = K_{j}, K = \overline{K_{j}^{\delta}},
        j=1,\ldots,m$ $\exists$ $\psi_{j}\in D(K_{j}), 0\leq \psi_{j}
        \leq 1, \psi_{j} \equiv 1$ auf $\overline{K_{j}^{\delta}}$
        ($j=1,\ldots,m$)
       \item $\Omega = \Gamma_{k}, K= \Gamma$ $\exists \psi \in
        D(\Gamma_{h}), 0 \leq \psi \leq 1, \psi \equiv 1$ auf $\Gamma$

        \begin{gather*}
          \phi_{j}(x) =
          \begin{cases}
            \frac{\psi(x) \psi(k)\help{fehler}}{\sum_{k=1}^{m}
              \psi_{k}(x)} & x \in \bigcup_{j=1}^{m}K_{j}^{\delta}\\
            0&\text{sonst}
          \end{cases}
        \end{gather*}
       \item $x\in \bigcup_{j=1}^{m} K_{j}^{r} \Longrightarrow \exists j\colon
        \psi_{j}(x) = 1 \Longrightarrow \sum_{l=1}^{m} \psi_{l} \geq 1$
       \item $\supp \phi_{j} \subset \supp \psi_{j}\subset K_{j}$
       \item $\phi_{j}\in C^{\infty}(\R^{n})$
       \item $x\in\Gamma\subset\bigcup_{j=1}^{m} K_{j}^{\delta}
        \Longrightarrow \exists \nu \in \{1,\ldots,n\}\colon x\in
        K_{\nu}^{\delta} \Longrightarrow \sum_{j=1}^{m}\phi_{j}(x) =
        \frac{(\sum_{j=1}^{m} \psi_{j}(x)) \psi(x)}{\sum_{j=1}^{m}
        \psi_{j}(x)} = 1$
      \end{itemize}
    \end{schritte}
  \end{proof}
\end{lemma}

\begin{defini}
  \begin{enumerate}
   \item $T\in D'(\Omega), \Omega' \subset \Omega, \Omega'$ offen,
    $T_{\Omega'}\colon T_{\Omega'}(\phi) = T(\phi) \forall \phi \in D(\Omega')$
    heißt \highl{Einschränkung} von $T$ auf $\Omega'$
   \item $T_{1},T_{2} \in D'(\Omega)$ heißen \highl{gleich} auf
    $\Omega'\subset \Omega$ $\Longleftrightarrow$ $(T_{1})_{\Omega'} =
    (T_{2})_{\Omega'}$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{itemize}
   \item $T_{1} =T_{2}$ auf $\Omega'$ $\Longleftrightarrow$ $T_{1}(\phi)
    = T_{2}(\phi) \forall \phi \in D(\Omega')$
   \item $x\in \Omega, U(x)$ heißt \highl{Umgebung} von $x$, falls  $x\in
    U(X)$, $U(x)$ offene Menge
  \end{itemize}
\end{bemerk}

\begin{satz}
  Seien $T_{1}, T_{2}\in D'(\Omega)$. Dann gilt $T_{1}= T_{2}$ auf
  $\Omega$ genau dann, wenn für alle $x\in \Omega$ eine Umgebung $U(x)$
  existiert mit $(T_{1})_{U(x)} = (T_{2})_{U(x)}$

  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] klar
     \item["`$\Leftarrow$"'] wissen, dass für alle $x\in \Omega$ ein $U(x)$
      existiert, so dass
      \begin{gather}\label{eq:49}
        (T_{1})_{U(x)} = (T_{2})_{U(x)}
      \end{gather}

      z.\,z.: $T_{1}(\phi) = T_{2}(\phi) \forall \phi \in D(\Omega)$ Sei
      $\phi\in D(\Omega)$ $\Longrightarrow$ $\supp \phi = K \subset
      \Omega, K$ kompakt $\Longrightarrow$ $\exists$ $(x_{k})_{k=1}^{m}:
      K\subset \bigcup_{j=1}^{m}U(x_{k})$

      \autoref{lem:5.1.2/1} anwenden mit $\Gamma = K$ $K_{j} = U(x_{j})$
      $\Longrightarrow$ $\exists\phi_{j}\in D(U(x_{j})), 0\leq \phi_{j}
      \leq 1, \forall x\in K\colon \sum_{j=1}^{m} \phi_{j}(x) =1$

      $T_{1}(\phi) = T_{1}( \sum_{j=1}^{m} \phi_{j}\phi) = \sum_{j=1}^{m}
      T_{1}(\underbrace{\phi_{j}\phi}_{\in D(U(x_{j}))}) = \sum_{j=1}^{m}
      (T_{1})_{U(x_{j})(\phi_{j}\phi)} =^{\text{\autoref{eq:49}}} =
      \sum_{j=1}^{m} (T_{2})_{U(x_{j})} (\phi_{j}\phi)  = T_{2}(
      \sum_{j=1}^{m} \phi_{j}\phi) = T_{2}(\phi)$
    \end{description}
  \end{proof}
\end{satz}

