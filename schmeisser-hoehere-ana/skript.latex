\begin{bemerk}\label{bem:3}
  $1\leq p \leq \infty, \norm{f}_{p}$ ist Hilbertraum, $\norm{f}_{p} = 0
  \Longleftrightarrow f= 0$ $\mu$-f.\,ü.

  $f\sim g :\Longleftrightarrow f= g$ f.\,ü.

  In diesem Sinne gilt:
  $[f] = \{ g \colon g\sim f\}$, $[0] = \{ g\colon g=0\fue\}$,
  $\norm{[f]}_{p} := \norm{f}_{p}$ falls $f\in [f]$
\end{bemerk}

\begin{satz}\label{satz:17}
  \begin{enumerate}
   \item $1\leq p \leq \infty$. $L_{p}(A, \mu)$ ist Banachraum
   \item $1\leq p \leq \infty$. Dann ist die Menge aller
    Treppenfunktionen dicht in $L_{p}(A,\mu)$ .
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item folgt aus \autoref{satz:hoelder} und \autoref{satz:3} und aus
      \autoref{bem:3}.
     \item
      \begin{schritte}
       \item $p=1, f\in L_{1}(A, \mu)$ Def $\Longrightarrow$ $\exists$
        Folge $(s_{k})_{k}$ von Treppenfunktionen mit:

        \begin{align*}
          s_{k}(x) \rightarrow f(x)\fue\\
          \int_{A}\abs{s_{k}-s_{j}}\,d\mu \xrightarrow{j,k \rightarrow \infty} 0\\
          \int_{A}f\,d\mu = \lim_{k\rightarrow\infty} \int_{A} s_{k}\,d\mu
        \end{align*}

        Es ist $\lim_{j\rightarrow\infty} \abs{s_{k}-s_{j}}= \abs{s_{k}-f}$ f.\,ü.
        $\wedge \abs{s_{k}-f}\in L_{1}(A,\mu)$

        Lemma von Faton (\autoref{satz:faton-lemma}) $\Longrightarrow$
        $\int_{A}\abs{s_{k}-f}\,d\mu = \int_{A}\lim_{k\rightarrow\infty}
        \abs{s_{k}-s_{j}}\,d\mu \leq \liminf_{k\rightarrow\infty}
        \int_{A}\abs{s_{k}-s_{j}}\,d\mu \leq \epsilon$ für $j,k \rightarrow
        j(\epsilon)$
       \item $1 < p<\infty$. Sei $f\in L_{p}(A,\mu), 0 < \epsilon <1$

        $A_{\epsilon}:= \{ x\colon \epsilon \leq \abs{f(x)} \leq \frac1{\epsilon} \}
        \in \mathcal{A}$
        $\Rightarrow$ $\int_{A}\abs{f}^{p}\,d\mu \geq
        \in_{A_{\epsilon}}\abs{f}\,d\mu \geq \epsilon^{p}\mu(A_{\epsilon})
        \Longrightarrow \mu(A_{\epsilon}) < \infty$

        Außerdem: $\norm{\chi_{A_{\epsilon}}f} \leq \frac1{\epsilon}
        \chi_{A_{\epsilon}} \in L_{1}(A,\mu)$, da $\mu(A)_{\epsilon}<\infty$

        $\Rightarrow$ $\chi_{A_{\epsilon}}f\in L_{1}(A,\mu)$
        $\Rightarrow^{\text{nach 1.\,Schritt}} \exists$ Folge
        $(g^{\epsilon}_{k})_{k}$ von Treppenfunktionen mit
        $\norm{g_{k}^{\epsilon}-\chi_{A_{\epsilon}}f}_{1}\rightarrow 0$ und
        $g_{k}^{\epsilon}\rightarrow \chi_{A_{\epsilon}}f = f^{\epsilon}$
        f.\,ü. $g_{k}^{\epsilon}=0$ auf $A\setminus A_{\epsilon}$

        setzen: $f_{k}^{\epsilon}(x) :=
        \begin{cases} g_{k}^{\epsilon}(x)& x\in A_{\epsilon}\wedge
          \abs{g_{k}^{\epsilon}(x)} \leq \frac2{\epsilon}\\
          \frac2{\epsilon}\frac{g_{k}^{\epsilon}(x)}
          {\abs{g_{k}^{\epsilon}(x)}}& x\in A \wedge \abs{g_{k}^{\epsilon}(x)} >
          \frac2{\epsilon}\\
          0& x\in A\setminus A_{\epsilon}
        \end{cases}$

        $\Rightarrow$ $f_{k}^{\epsilon}$ ist Treppenfunktion,
        $\abs{f_{k}^{\epsilon}(x)} \leq \frac{2}{\epsilon}$ auf $A$. Für
        $\abs{g_{k}^{\epsilon}(x)}\geq \frac2{\epsilon}$ und $x\in
        A_{\epsilon}$ ist:
        \begin{gather*}
          \abs{g_{k}^{\epsilon}(x)} > \frac1{\epsilon} + \abs{f(x)}
             \Longrightarrow \frac1{\epsilon} < \abs{g_{k}^{\epsilon}(x)} -
             \abs{f(x)}\label{eq:17}\\
          \intertext{und somit:}
          \abs{f_{k}^{\epsilon}(x) - f(x)} \leq \abs{f_{k}^{\epsilon}(x)}+ \abs{f(x)}
             \leq^{\text{\autoref{eq:17}}} \frac{3}{\epsilon} 3 \abs{g_{k}^{\epsilon}(x)-f(x)}\\
          \Longrightarrow \norm{f_{k}^{\epsilon}-f^{\epsilon}}_{1}
             \xrightarrow{k\rightarrow\infty} 0\\
          \begin{aligned}
            \Longrightarrow \int_{A}\abs{f-f_{k}^{\epsilon}}^{p}\, d\mu &=
               \int_{A\setminus A_{\epsilon}} \abs{f(x)}^{p}\,d\mu +
               \int_{A_{\epsilon}} \underbrace{\abs{f_{k}^{\epsilon}-
               f^{\epsilon}}^{p-1}}_{\leq (\frac3{\epsilon})^{p-1}}
               \abs{f_{k}^{\epsilon}-f}\,d\mu\\
            &\leq \underbrace{\int_{A\setminus A_{\epsilon}}
               \abs{f(x)}^{p}\,d\mu}_{\xrightarrow{\epsilon \rightarrow0}0\text{~da~}
               \mu(A\setminus A_{\epsilon})\rightarrow0} +
               (\frac3{\epsilon})^{p-1}
               \norm{f_{k}^{\epsilon}-f^{\epsilon}}_{1} \Longrightarrow
               \norm{f-f_{k}^{\epsilon}} \xrightarrow{k\rightarrow\infty,
               \epsilon\rightarrow0}
          \end{aligned}
        \end{gather*}
      \end{schritte}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item $\Omega=\N, \mathcal{A}=\mathcal{P}(\N), \mu(A) = \card(A), a\colon \N\rightarrow\K$
    $\Longrightarrow$ $\int_{\N}\abs{a}^{p}\,d\mu = \sum_{k=1}^{\infty}
    \abs{a_{k}}^{p}$ d.\,h. $L_{p}(\N,\mu) = l_{p}$
   \item jetzt: $L_{p}(\Omega), \Omega\subset \R^{n}$ offen ($\R^{n},
    \mathcal{L}, \lambda)$ Lebesgue'scher Maßraum
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:1.4.5/5}
  $\Omega\subset \R^{n}$ offen, $1 \leq p < \infty$
  \begin{enumerate}
   \item Die Menge der finiten Treppenfunktionen (kompakter Träger in
    $\Omega$) ist dicht in $L_{p}(\Omega)$.
   \item Die Menge $C_{0}^{\infty}(\Omega)$ liegt dicht in $L_{p}(\Omega)$.
   \item $L_{p}(\Omega)$ ist separabel.
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}[zu~(1)]
     \item
      $\Omega = \bigcup_{j=1}^{\infty} \Omega_{j}$ mit $\Omega_{j} = \{
      x\in \Omega\colon \abs{x}\leq j \wedge d(x,\partial\Omega) > \frac1j \}$
      $\Longrightarrow$ $\overline{\Omega_{j}}\subset \Omega_{j+1}$
      $f\in L_{p}(\Omega) \Longrightarrow \lim_{j\rightarrow\infty}
      \int_{\Omega\setminus \Omega_{j}} \abs{f(x)}^{p}\, dx =0$
      $\Longrightarrow$ $\chi_{\Omega_{j}}
      f\xrightarrow{j\rightarrow\infty} f$ in $L_{p}(\Omega)$

      Nach \autoref{satz:17} $\Longrightarrow$ $\chi_{\Omega_{j}}f$ kann
      durch Treppenfunktionen in $L_{p}(\Omega_{j})$ approximiert werden.
      haben Träger in $\overline{\Omega_{j}}$

     \item Nach (1) ausreichend: Approximation von
      Treppenfunktionen $s(x)$ mit $\supp s\subseteq\Omega$ kompakt
      \begin{gather*}
        s(x) = \sum_{j=1}^{r} c_{j}\chi_{A_{j}}\qquad A_{j}\text{ messbar},
           c_{j}\in \K, A_{j} \subset \supp s
      \end{gather*}
      endliche Summe $\Longrightarrow$ ausreichend Approximation von
      $\chi_{A}$ mit $A$ messbar, $A\subset K \subset \Omega$ kompakt.

      Nach \autoref{satz:1.3.1/3} existiert eine offene Menge
      $O_{\epsilon}$ mit $A\subset O_{\epsilon} \subset
      \overline{O_{\epsilon}} \subset \Omega$ kompakt und
      $\abs{O_{\epsilon}\setminus A} < \epsilon\ (\forall \epsilon)$. Jede
      offene Menge ist abzählbare Vereinigung offener Würfel.
      $\Longrightarrow$ ausreichend die Approximation von $\chi_{Q}$,
      wobei $Q$ offener Würfel mit $\overline{Q}\subset \Omega$.

      Nach \autoref{satz:1.2.3/2} existiert $\phi_{k}\in C_{0}^{\infty}(\R^{n})$
      mit $\phi_{k}(x) = 1$ auf $\overline{Q_{k}}, 0\leq \phi_{k}\leq 1,
      \supp \phi_{k} \subset \overline{Q_{k}} \subset \Omega, k\geq
      K_{0}$ wobei $\overline{Q_{k}} = \{ x\colon d(x, \overline{Q}) \leq
      \frac{1}{k} \} \subset \Omega$

      $\Longrightarrow$ $\int_{\Omega}\abs{\chi_{Q}- \phi_{k}}^{p}\,dx =
      \int_{\overline{Q_{k}}\setminus Q} \phi_{k}(x)^{p}\,dx \leq
      \abs{\overline{Q_{k}}\setminus Q} = c \frac1k
      \xrightarrow{k\rightarrow\infty}0$

     \item Treppenfunktionen $\sum_{i=1}^{r} c_{j} \chi_{Q_{i}},
      Q_{j}$ Quader in $\Omega$, es ist also außreichend zu zeigen: $c_{j} = \alpha_{j}+ i
      \beta_{j}\quad(\alpha_{j}, \beta_{j} \text{rational})$ Ecken von
      $Q_{j}$ rationale Koordinaten

      $\exists \infty$-viele l.\,u.\,Treppenfunktionen in $L_{p}(\Omega)$
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item Finite Funktionen nicht dicht in $L_{\infty}(\Omega)$.

    Betrachte: $f(x) = 1, \norm{1-\phi}_{\infty} \geq 1$
   \item $L_{\infty}(\Omega)$ nicht separabel.

    $\mu(Q_{j})\approx 2^{jn}$ Man kann den Beweis auf Folgenräume
    zurückführen?
  \end{enumerate}
\end{bemerk}

\subsection{Satz von Fubini, Transformationsformeln}

\begin{satz}\label{satz:fubini}
  Es sei $f\colon\R^{n+m} \rightarrow \K$ ein Lebesgue-messbare Funktion.
  $f = f(x,y)$ mit $x\in\R^{n}, y\in \R^{m}$. Es existiere eines der
  folgenden Integrale:
  \begin{align*}
    I_{1} &= \int_{\R^{n+m}} \abs{f(x,y)}\,d(x,y)\\
    I_{2} &= \int_{\R^{m}} \Bigl( \int_{\R^{n}}\abs{f(x,y)}\,dx\Bigr)\,dy\\
    I_{3} &= \int_{\R^{n}} \Bigl( \int_{\R^{m}}\abs{f(x,y)}\,dy\Bigr)\,dx
  \end{align*}

  Dann gelten folgende Aussagen:
  \begin{enumerate}
   \item $f(\cdot,y)\in L_{1}(\R^{n})$ für fast alle $y\in \R^{m}$\\
    $f(x,\cdot)\in L_{1}(\R^{m})$ für fast alle $x\in \R^{n}$ und\\
    $\int_{\R^{n}}f(x,y)\,dx\in L_{1}(\R^{m}),\quad
    \int_{\R^{m}}f(x,y)\,dy\in L_{1}(\R^{n})$
   \item $I_{1}= I_{2}= I_{3}$
   \item \begin{gather*}
           \int_{\R^{n+m}} f(x,y)\,d(x,y) = \int_{\R^{n}} \Bigl( \int_{\R^{m}}
              f(x,y)\,dy\Bigr)\,dx = \int_{\R^{m}} \Bigl( \int_{R^{n}}
              f(x,y)\,dx\Bigr)\,dy
         \end{gather*}
  \end{enumerate}
\end{satz}

\begin{satz}[Transformationsformel]
  Es sei $\Omega\subset \R^{n}$ offen, $g\colon G\rightarrow\Omega$ ein
  \highl{Diffeomorphismus} (d.\,h. $g$ bijektiv, $C^{1}$-Abbildung) und
  $\frac{\partial(g_{1},\ldots,g_{n})}{\partial(u_{1},\ldots,u_{n})}
  \ne0$ auf $G$.

  Dann:
  \begin{gather*}
    \exists \int_{\Omega} f\,dx \Longleftrightarrow \exists \int_{G}
       f\circ g(u)
       \frac{\partial(g_{1},\ldots,g_{n})}{\partial(u_{1},\ldots,u_{n})}\,du\\
    \intertext{und}
       \int_{\Omega} f\,dx = \int_{G} (f\circ g)(u)
       \abs{\frac{\partial(g_{1},\ldots,g_{n})}
       {\partial(u_{1},\ldots,u_{n})}}\,du
  \end{gather*}
\end{satz}

\begin{bsp}[Polarkoordinaten im $\R^{n}$]
  (siehe Walter, Analysis Band~2, Seite~254)
  \begin{alignat*}{2}
    x_{1} &= r&\cos \phi\sin \theta_{1}\sin \theta_{2} \ldots
       \sin\theta_{n-2}\\
    x_{2} &= r&\sin \phi\sin \theta_{1}\sin \theta_{2} \ldots
       \sin\theta_{n-2}\\
    x_{3} &= r&\cos \theta_{1}\sin \theta_{2} \ldots \sin\theta_{n-2}\\
    \vdots\\
    x_{n-1} &= r& \cos \theta_{n-3}\sin \theta_{n-2}\\
    x_{n} &=r&\cos \theta_{n-2}
  \end{alignat*}
  mit $r\geq 0, 0 \leq \phi \leq 2\pi, 0\leq \theta_{i} \leq \pi
  (i=1,\ldots,n-2)$

  $\Longrightarrow$ $x^{2}_{1}+\ldots+x_{n}^{2} = r^{2}$

  Jacobi-Determinante:
  \begin{gather*}
    \frac{\partial (x_{1},\ldots,x_{n})}
       {\partial(r,\phi,\theta_{1},\ldots,\theta_{n-2})} = r^{n-1}\sin
       \theta_{1} (\sin \theta_{2})^{2} \ldots (\sin \theta_{n-2})^{n-2}
  \end{gather*}

  Def:
  \begin{gather*}
    d\omega_{n} := \sin\theta_{1} (\sin \theta_{2})^{2} \ldots (\sin
       \theta_{n-2})^{n-2} d\phi d\theta_{1} \ldots d\theta_{n-2}\\
    \begin{split}
      \int_{\omega_{n}} f\,d\omega_{2} := \int_{0}^{2\pi}
      \underbrace{\int_{0}^{\pi}\ldots \int_{0}^{\pi}}_{(n-2)\text{-mal}}
      f\big(g(1,\phi,\theta_{1},\ldots,\theta_{n-2})\big)\\\cdot
      \sin\theta_{1} (\sin \theta_{2})^{2} \ldots (\sin
      \theta_{n-2})^{n-2} d\phi\, d\theta_{1} \ldots d\theta_{n-2}
    \end{split}\\
    \abs{\omega_{n}} := \int_{\omega_{n}} d\omega_{n}\qquad\text{Oberfläche
       der Einheitskugel im $\R^{n}$}
  \end{gather*}
\end{bsp}

\begin{folger}
  \begin{enumerate}
   \item
    \begin{gather*}
      \int_{\R^{n}} f\,dx = \int_{0}^{\infty} \int_{\omega_{n}}
         (f\circ g)r^{n-1}\,d\omega_{n}\,dr
    \end{gather*}
   \item $f= f(r) \Longrightarrow$
    \begin{gather*}
      \int_{\R^{n}} f\,dx = \Bigr(\int_{0}^{\infty} f(r) r^{n-1}\,dr\Bigl) \abs{\omega_{n}}
    \end{gather*}
  \end{enumerate}
\end{folger}

\begin{satz}[Einheitskugel im $\R^{n}$]
  $n \geq 2$. Es gilt:
  \begin{gather*}
    \abs{\omega_{n}} = \frac{2\pi^{\nicefrac{n}{2}}}{\Gamma(\frac{n}2)} =
       2\pi^{\nicefrac{n}{2}}
       \begin{cases}
         \frac{1}{(\frac{n}{2}-1)!} & n\text{ gerade}\\
         \frac{1}{\sqrt{\pi} (\frac{n}{2}-1)
           (\frac{n}{2}-2) \ldots \frac12} &
         n\text{ ungerade}
       \end{cases}\\
    \Gamma(x) = \int_{0}^{\infty} e^{-t}\: t^{x-1}\,dt\qquad x>0
  \end{gather*}

  \begin{proof}
    \begin{gather*}
      I = \int_{-\infty}^{\infty} e^{-t^{2}}\,dt = \sqrt{\pi}\\
      \Longrightarrow \pi^{\nicefrac{n}{2}} = I^{n} =^{Fubini}
         \int_{\R^{n}} e^{-(x_{1}^{2}+\ldots+x_{n}^{2})}\,dx =
         \abs{\omega_{n}}\int_{0}^{\infty} e^{-r^{2}} r^{n-1}\,dr\\
      r^{2} :=\rho, d\rho = 2\pi\,dr\\
      = \abs{\omega_{n}}\int_{0}^{\infty} e^{-\rho} \rho^{\frac{n-1}{2}}
         \frac12 \rho^{-\frac12}\,d\rho = \frac12 \abs{\omega_{n}}
         \int_{0}^{\infty} e^{-\rho} \rho^{\frac{n}2 -1}\,d\rho = \frac12
         \abs{\omega_{n}} \Gamma(\frac{n}2)\\
      \Longrightarrow \abs{\omega_{n}} = \frac{2 \pi^{\nicefrac{n}{2}}}{
         \Gamma(\frac{n}2)}
    \end{gather*}

    Es ist $\Gamma(\frac12) = \sqrt{\pi}$, da
    \begin{gather*}
      \Gamma(\frac{n}2) = \int_{0}^{\infty} e^{-\rho}
         \rho^{-\frac12}\,d\rho = 2 \int_{0}^{\infty} e^{-r^{2}}\,dr =
         \int_{-\infty}^{\infty} e^{-r^{2}}\,dr = \sqrt{\pi}
    \end{gather*}
  \end{proof}
\end{satz}

\begin{bemerk}
  \begin{gather*}
    \abs{\omega_{2}} = 2\pi,\; \abs{\omega_{3}} = 4\pi,\; \abs{\omega_{4}} =
       2\pi^{2},\;
       \abs{\omega} = \frac83\pi^{2},\; \abs{\omega_{6}}= \pi^{3},\; \abs{\omega_{7}} =
       \frac{16}{15} \pi^{3} = \max_{n\geq 2} \abs{\omega_{n}}\\
    \lim_{n\rightarrow\infty} \abs{\omega_{n}} = 0
  \end{gather*}
\end{bemerk}

\subsection{Faltung und Glättung}

\begin{satz}\label{satz:1.3.7/1}
  Gelten die Bedingungen $1 \leq p < \infty, f\in L_{p}(\R^{n}), y\in
  \R^{n}$
  \begin{gather*}
    T_{y}f(x) := f(x+y)\quad(x\in \R^{n})\quad\qquad\text{Translationsoperator}
  \end{gather*}

  so folgt daraus:
  \begin{enumerate}
   \item $\norm{T_{y}f}_{p} = \norm{f}_{p}$
   \item $\lim_{\abs{y} \rightarrow0} \norm{T_{y}f-f}_{p} = 0$ (Stetigkeit der
    Translation im $p$-ten Mittel)
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item klar nach Transformation $x+y = z$
     \item \autoref{satz:1.4.5/5} $\Longrightarrow$ $\exists g \in
      C_{0}^{\infty}(\R^{n})\quad(\supp g = K~\text{kompakt})$ mit
      $\norm{f-g}_{p} < \epsilon$ (für vorgegebenes $\epsilon >0$)

%      $\Longrightarrow$
      \begin{align*}
        \Longrightarrow \norm{T_{y}f -f}_{p} &\leq \norm{f-g}_{p} + \norm{g-T_{y}g}_{p} +
           \norm{\underbrace{T_{y} g - T_{y} f}_{=T_{y}(g-f)}}_{p}\\
        &= 2\underbrace{\norm{f-g}_{p}}_{<\epsilon} +
           \Bigl(\int_{\R^{n}}\abs{g(x+y)-g(x)}^{p}\,dx\Bigr)^{\frac1p}
           &&\text{nach (1)}\\
        \intertext{Für $\abs{y}<1$: $K^{1}=\{x\colon d(x,K) \leq 1\}$ $\Longrightarrow$ $\supp
          g(\cdot+y)\subset K^{1} \Longrightarrow$}
        &= 2\underbrace{\norm{f-g}_{p}}_{<\epsilon} +
           (\int_{\K^{1}}\abs{\underbrace{g(x+y)-g(x)}_{\leq \epsilon, falls
           \abs{y} \leq \delta(\epsilon)}}^{p}\,dx)^{\frac1p}\\
        &\leq 2\epsilon + \epsilon \abs{K^{1}}^{\frac1p}
      \end{align*}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerk}
  Für $\{T_{y}\}_{y\in \R^{n}}$ gilt:
  \begin{gather*}
    T_{y}T_{z} = T_{y+z}\\
    \lim_{\abs{y} \rightarrow 0} T_{y} f = f\qquad \text{für}~\abs{y}<1
  \end{gather*}
\end{bemerk}

\begin{defini}
  $f,g\colon \R^{n} \rightarrow \K$ Lebesgue-messbar. $\exists$ f.\,ü.
  \begin{gather*}
    (f\ast g)(x) := \int_{\R^{n}} f(x-y)g(y)\,dy
  \end{gather*}
  $f\ast g$ heißt \highl{Faltung} von $f$ und $g$.
\end{defini}

\begin{satz}\label{satz:1.3.7/2}
  $f,g,g_{1}\colon \R^{n} \rightarrow \K$ Lebesgue-messbar
  \begin{enumerate}
   \item $\exists (f\ast g)(x) \Longrightarrow (f\ast g)(x) = (g\ast f)(x)$

    $\exists (f\ast g)(x), \exists (f\ast g_{1})(x), \lambda\in\K
    \Longrightarrow \exists (f\ast (g+g_{1}))(x) = (f\ast g)(x) + (f\ast
    g_{1})(x), (f\ast \lambda g)(x) = \lambda(f\ast g)(x)$
   \item $f\in L_{p}(\R^{n}), g\in L_{p'}(\R^{n})$ mit $1 \leq p <\infty,
    \frac1p+\frac1{p'} = 1$

    $\Longrightarrow$ $\exists f\ast g\in \overline{C}(\R^{n})$
    (gleichmäßig stetig und beschränkt) und $\norm{f\ast g}_{\infty} \leq
    \norm{f}_{p} \cdot \norm{g}_{p'}$
   \item $f\in L_{p}(\R^{n}), g\in L_{1}(\R^{n})$ mit $1\leq p < \infty$

    $\Longrightarrow$ $\exists(f\ast g)(x)$ f.\,ü. und $f\ast g\in
    L_{p}(\R^{n})$ $\norm{f\ast g}_{p} \leq \norm{f}_{p}\cdot \norm{g}_{1}$
   \item Vor wie in (2) oder (3)

    $\Longrightarrow$ $\supp(f\ast g) \subset \supp f+\supp g$
    $T_{h}(f\ast g) = (T_{h} f)\ast g = (f\ast T_{h}g)$ für alle $h\in
    \R^{n}$
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}[zu (1)]
      \stepcounter{enumi}
     \item Höldersche Ungleichung \autoref{eq:hoelder-int}
      $\Longrightarrow$ $\int_{\R^{n}} \abs{f(x-y)} \abs{g(y)}\,dy \leq
      \norm{f}_{p}\cdot \norm{g}_{p'}$
      \begin{align*}
        \abs{(f\ast g)(x+h) - (f\ast g)(x)} &\leq
           \int_{\R^{n}}\abs{f(x+h-y) - f(x-y)}\cdot \abs{g(y)}\,dy\\
        &\leq\underbrace{\norm{f(x+h) -f(x)}_{p} }_{\mathclap{\leq \epsilon \text{falls
           $\abs{h}< \delta(\epsilon)$ (unabhängig von $x$)}}}\cdot \norm{g}_{p'} \leq
           \epsilon \norm{g}_{p'}
      \end{align*}
      $\Longrightarrow$ $f\ast g\in \overline{C}(\R^{n})$
     \item
      \begin{description}
       \item[$p=1$:]
        \begin{gather*}
          \exists \int_{\R^{n}}\int_{\R^{n}} \abs{f(x-y)} \abs{g(y)}\,dx\,dy =
             \norm{f}_{1}\cdot \norm{g}_{1}\\
          \text{nach \autoref{satz:fubini} } \exists \int_{\R^{n}} \underbrace{f(x-y)
             g(y)}_{ = (f\ast g)}\,dy\fue\ \wedge \norm{f\ast g}_{1} \leq \norm{f}_{1}
             \norm{g}_{1}
        \end{gather*}
        $L_{1}$ ist Faltungsalgebra
       \item[$p>1$:] ($f\in L_{p}, g\in L_{1}$) folgt aus:
        \begin{align*}
          \int_{\R^{n}} \abs{f(x-y)} \abs{g(y)}\,dy &= \int_{\R^{n}} \abs{f(x-y)}
             \abs{g(y)}^{\frac1p} \abs{g(y)}^{\frac1{p'}}\,dy\\
          &\leq (\int_{\R^{n}} \abs{f(x-y)}^{p}
             \abs{g(y)}\,dy)^{\frac1p}\norm{g}^{\frac1{p'}}_{1}&&\text{nach
             \autoref{eq:hoelder-int}}\\
          \intertext{und}
             \norm{(\int_{\R^{n}} \abs{f(x-y)}^{p}
             \abs{g(y)}\,dy)^{\frac1p}}_{p}^{p} &= \int_{\R^{n}}
             (\int_{\R^{n}} \abs{f(x-y)}^{p} \abs{g(y)}\,dy)\,dx\\
          &= \norm{f}_{p}^{p} \norm{g}_{1}&&\text{nach \autoref{satz:fubini}}
        \end{align*}
        Anmerkung: Alles rückwerts deuten!

        $\Longrightarrow$ $\exists (f\ast g)$ f.\,ü. und $\norm{f\ast
        g}^{p}_{p} \leq \norm{f}_{p}^{p}\norm{g}_{1}^{1+\frac{p}{p'}}$
      \end{description}
     \item $x\notin\overline{\supp f + \supp g}$ $\Longrightarrow$
      $x\notin \supp(f\ast g)$

      $y\in K_{\delta}(x)$
      \begin{align*}
        \Longrightarrow& y\notin \supp f + \supp g\\
        \Longrightarrow& \forall z\in \supp g\colon y-z\notin \supp f\\
        \Longrightarrow& f(y-z) = 0\quad\forall z\in \supp g\\
        \Longrightarrow& (f\ast g)(y) = \int_{\supp g} f(y-z) g(z)\,dz =
           0\qquad\forall y\in K_{\delta}(x)\\
        \Longrightarrow& x\notin\supp(f\ast g)
      \end{align*}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{satz}\label{satz:1.3.7/3}
  \begin{enumerate}
   \item $f\in L_{p}(\R^{n}), 1\leq p \leq \infty, \phi \in C^{m}(\R^{n})
    (m\in\N_{0})$ und $\supp \phi$ sei kompakt.

    Dann gilt:$f\ast \phi \in \overline{C}^{m}(\R^{n}) und \forall \alpha,
    \abs{\alpha} \leq m, gilt D^{\alpha}(f\ast \phi) = f\ast D^{\alpha}\phi$
   \item $f\in L_{p}(\R^{n}), 1 \leq p < \infty, \int_{\R^{n}}\phi(x)\,dx
    = 1$

    Es sei für $h>0$:
    \begin{gather*}
      \phi_{h}(x) := \frac1{h^{n}}\;\phi\bigl(\frac{x}{h}\bigr)
    \end{gather*}

    Dann gilt:
    \begin{gather*}
      \norm{f-f\ast \phi_{h}}_{p} \xrightarrow{h\rightarrow 0} 0
    \end{gather*}
   \item Wenn $f\in \overline{C}(\R^{n})$ und $\phi$ wie in (2), dann
    folgt daraus
    \begin{gather*}
      \norm{f-f\ast \phi_{h}}_{\infty} \xrightarrow{h\rightarrow 0} 0
    \end{gather*}
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}[zu (1)]
     \item $D^{\alpha}\phi \in L_{p'}(\R^{n}) \forall \abs{\alpha} \leq m$,
      da $\supp D^{\alpha}\phi$ kompakt

      \autoref{satz:2} $\Longrightarrow$ $f\ast D^{\alpha} \phi \in
      \overline{C}(\R^{n}) \forall \abs{\alpha} \leq m$

      zeigen: $D^{\alpha}(f\ast \phi) = f\ast (D^{\alpha}\phi)$\\
      Es gilt:
      \begin{align*}
        \lim_{t\rightarrow 0} \frac{(f\ast \phi)( x+t e_{j}) - (f\ast
           \phi)(x)}t &= \lim_{t\rightarrow 0} \int_{\R^{n}}
           \frac{\phi(x+t e_{j} - y) - \phi(x-y)}t f(y)\,dy\\
        &= \int_{\R^{n}} \frac{\partial \phi}{\partial x_{j}}(x-y) f(y)
           \,dy = \bigl(f\ast \frac{\partial \phi}{\partial x_{j}}(x)\bigr)
      \end{align*}
      dies gilt, da
      \begin{gather*}
        \int_{\R^{n}} \underbrace{\abs{\frac{\phi(x + te_{j} -y) -
           \phi(x-y)}t} \abs{f(y)}}_{\help{was stand hier?}}\,dy
           = \int_{\abs{x-y} \leq R+1}
           \abs[\Big]{\underbrace{\frac{\partial\phi}{\partial x_{j}}(x-y+\tau
           e_{j})}_{\leq \norm[\big]{\frac{\partial \phi}{\partial
           x_{j}}}_{\infty}}} \leq \norm[\Bigg]{\frac{\partial \phi}{\partial
           x_{j}}}_{\infty} \norm{f}_{p} \abs{K_{R+1}(0)}^{\nicefrac1{p'}} < \infty
      \end{gather*}
      \help{Stimmen obige Gleichungen?}

      nach \autoref{satz:lebesgue} $\Longrightarrow$ $\exists \frac{\partial}{\partial
      x_{j}} (f\ast \phi) = \phi\ast\partial x_{j}$ \help{irgendwie
      weiteriterieren, aber wie genau?}
     \item Es ist $\int_{\R^{n}} \phi_{h}(x)\,dx = 1$. Wie im Beweis von
      \autoref{satz:1.3.7/2} zeigt man
      \begin{align*}
        \norm{f-f\ast \phi_{h}}_{p}^{p} &\leq
           \norm[\bigg]{\int_{\R^{n}}\abs{f(x)-f(x-y)}\;\abs{\phi_{h}(y)}^{\frac{1}{p}}\;
           \abs{\phi_{h}(y)}^{\frac{1}{p'}}\,dy}_{p}^{p}\\
        &\leq \int_{\R^{n}} \biggl( \int_{\R^{n}} \abs{f(x)-f(x-y)}^{p}\;
           \abs{\phi_{h}(y)}\,dy \biggr)\,dx\;\norm{\phi_{h}(y)}_{1}^{\frac{p}{p'}}\\
        &= \int_{\R^{n}} \norm{f(x)-f(x-y)}^{p}_{p}\;\abs{\phi_{h}(y)}\,dy\;
           \norm{\phi}_{1}^{\frac{p}{p'}}&&\text{nach Fubi \autoref{satz:fubini}}\\
        &= \int_{\abs{y}\leq\sigma} \ldots + \int_{\abs{y}>\sigma} \ldots =
           I_{1}(\sigma) + I_{2}(\sigma)
      \end{align*}
      Es nun $\epsilon>0$ und so existiert nach \autoref{satz:1.3.7/1} ein
      $\delta$, so dass für alle $y\leq\sigma$ gilt
      $\norm{f(x)-f(x-y)}_{p}\leq\epsilon$. Daraus folgt nun:
      \begin{align*}
        I_{1}(\epsilon) &\leq \epsilon^{p}\;\norm{\phi_{h}(y)}_{1}\;
           \norm{\phi}_{1}^{\frac{p}{p'}} = \epsilon^{p}\;\norm{\phi}_{1}^{p}\\
        I_{2}(\sigma) &\leq
           \bigl(2\norm{f}_{p}\bigr)^{p}\underbrace{\int_{\abs{y}\leq\sigma}
           \abs{\phi_{h}(y)}\,dy}_{=\int_{\abs{y}>\sigma\,h^{-1}} \abs{\phi(y)}\,dy
                               \leq \epsilon^{p}\text{ für } h<h_{0}}\;
           \norm{\phi}_{1}^{\frac{p}{p'}} \leq c_{f,\phi,\sigma}\epsilon^{p}
      \end{align*}
     \item
      \begin{align*}
        \norm{f(x)-f\ast \phi_{h}(x)}_{\infty} &\leq \norm[\bigg]{\int_{\R^{n}}
           \abs{f(x)-f(x-y)}\;\abs{\phi_{h}(y)}\,dy}\\
        &\leq \int_{\R^{n}} \norm{f(x) -f(x-y)}_{\infty}\;\abs{\phi_{h}(y)}\,dy\\
        &= \int_{\abs{y}\leq\sigma} \ldots + \int_{\abs{y}>\sigma} \ldots\\
        & \leq \epsilon \norm{\phi}_{1} + 2 \norm{f}_{\infty} \epsilon
           \norm{\phi}_{1} = c\:\epsilon&&\text{da $f\in\overline{C}(\R^{n})$}
      \end{align*}
    \end{enumerate}
  \end{proof}
\end{satz}

\subsubsection{Sobolev'sches Mittelungsverfahren (Friedrichs'sches
  Glättungsverfahren)}

Man betrachte die Funktion $\phi\in C_{0}^{\infty}(\R^{n})$, die die
Bedingungen $\phi\geq0$ und $\int_{\R^{n}}\phi(x)\,dx=1$ erfülle.

\parbox{\linewidth}{
  \ifpdf\input{sobolev1.pdf_t}\else\input{sobolev1.eps_t}\fi\hfill
  \ifpdf\input{sobolev2.pdf_t}\else\input{sobolev2.eps_t}\fi
}

$(\phi_{h})_{h}$ ist eine "`\highl{Dirac-Folge}"' ($h\rightarrow0$)
\begin{gather*}
  \phi_{h}(x) \rightarrow \begin{cases}\infty& x=0\\0& x\ne0\end{cases}
\end{gather*}

O.\,B.\,d.\,A. sei der Träger $\supp \phi \subset K_{1}(0)$. Nun definiert man die
Funktion $f\in L_{p}(\R^{n})\ (1\leq p\leq \infty)$ als
\begin{gather*}
  f_{h}(x) := (f\ast \phi_{h})(x) = \int_{\R^{n}}f(x-y)\phi_{h}(y)\,dy
\end{gather*}

\begin{satz}
  $\phi, \phi_{h}$ und $f_{h}$ wie oben, $f\in L_{p}(\Omega)$ mit Null
  außerhalb von $\Omega$ fortgesetzt, $1\leq p< \infty$

  Dann gilt:
  \begin{enumerate}
   \item $f_{h}\in\overline{C}^{\infty}(\R^{n})\cap L_{p}(\R^{n})$ und
    das $\supp f_{h} \subset \overline{\overline{\Omega}+
    \underbrace{\supp \phi_{h}}_{K_{h}(0)}}$
   \item Für alle $f\in L_{p}(\Omega)$ geht die $\norm{f-f_{h}}_{p,\R^{n}}
    \rightarrow 0$
   \item Für alle $f\in \overline{C}(\R^{n})$ geht die
    $\norm{f-f_{h}}_{\infty}\rightarrow 0$
  \end{enumerate}

  \begin{proof}
    folgt aus \autoref{satz:1.3.7/2} und \autoref{satz:1.3.7/3}
  \end{proof}
\end{satz}

\begin{satz}[Präkompakte Mengen im $L_{p}(\Omega)$]\label{satz:1.3.7/5}
  $1\leq p<\infty, \Omega$ sei eine offene und beschränkte Teilmenge des
  $\R^{n}$.

  Ist $M\subset L_{p}(\Omega)$ beschränkt ($\sup_{M} \norm{f}_{p}
  <\infty$) und das $\sup_{x\in M} \norm{f(x-y)-f(x) }_{p} \xrightarrow{\abs{y}
  \rightarrow0} 0$ ($f=0$ außerhalb von $\Omega$), dann ist $M$
  präkompakt in $L_{p}(\Omega)$.

  \begin{proof}
    \begin{schritte}
      \item
      \begin{gather*}
        f_{h}(x) = \int_{\R^{n}} f(x-y)\:\phi_{h}(y)\,dy \in
        C_{0}^{\infty}(\R^{n})
      \end{gather*}
      Wir setzen $M_{h}:=\{f_{h}\colon f\in M\}$ ($h>0$ fest). $M_{h}$ ist
      beschränkt in $C(\overline{\Omega})$, da der Betrag
      $\abs{f_{h}(x)} \leq \norm{\phi_{h}}_{p', \R^{n}} \norm{f}_{p,\Omega}$
      beschränkt in $\overline{C}(\Omega)= C(\overline{\Omega})$ ist.
      Außerdem gilt:
      \begin{align*}
        \abs{\phi_{h}(x)&-f_{h}(x')} \leq \int_{\R^{n}}
           \abs{\phi_{h}(x-y)-\phi_{h}(x'-y)}\:\abs{f(y)}\,dy\\
        &\leq \norm{\phi_{h}(x-\cdot)-\phi_{h}(x'-\cdot)}_{p'}\:
           \norm{f}_{L_{p}(\Omega)}&\text{nach Hölder
           \autoref{satz:hoelder}}\\
        &= \norm{\phi_{h}(\cdot) - \phi_{h}(x'-x+\cdot)}_{p'}\:
           \norm{f}_{L_{p}(\Omega)} \leq \epsilon \norm{f}_{L_{p}(\Omega)}
           &\text{falls } \abs{x-x'}\leq \delta(\epsilon)\\
        &\leq \epsilon \sup_{M}\norm{f}_{p}
      \end{align*}

      $M_{h}$ ist also gleichmäßig stetig und $C(\overline{\Omega})$
      beschränkt, so folgt aus dem Satz von Arzela-Ascoli
      (\autoref{satz:arzela}): $C(\overline{\Omega})$ ist präkompakt.
     \item Es bleibt noch zu zeigen, dass $M\subset L_{p}(\Omega)$
      präkompakt ist.

      Wir wissen bereits, dass $M_{h}$ präkompakt in $L_{p}(\Omega)$ ist,
      da $C(\overline{\Omega})\subset L_{p}(\Omega)$ ist. Wir wählen also
      nun $\epsilon>0$ und zeigen $h\leq h_{0}(\epsilon)$ woraus dann
      folgt, dass $M_{h}$ ein $\epsilon$-Netz für $M$ ist und somit nach
      \autoref{fol:1.1.3/3} $M$ präkompakt.

      Da der Träger $\supp\phi \subset K_{1}(0)$ gilt:
      \begin{align*}
        \norm{f-f_{h}}_{p}^{p} &\leq \int_{\Omega} \abs[\bigg]{\int_{\abs{y}\leq1}
           \phi(y) \bigl(f(x)-f(x-hy)\bigr)\,dy }^{p}\,dx\\
        % &\leq \int_{\Omega} \Bigl(\int_{\abs{y}\leq1}
        %    \abs[\big]{\phi(y)\: \bigl(f(x)-f(x-hy)\bigr)}\,dy \Bigr)^{p}\,dx\\
        &\text{die Hölder'sche Unlgeichung \autoref{eq:hoelder-int} liefert}\\
        &\leq \int_{\Omega} \Biggl(
           \Bigl(\int_{\abs{y}\leq1} \abs{\phi(y)}^{p'}\,dy\Bigr)^{p\frac{1}{p'}}
           \Bigl(\int_{\abs{y}\leq1} \abs{f(x)-f(x-hy)}^{p}\,dy\Bigr)^{p\frac{1}{p}}
           \Biggr)\,dx\\
        &= \norm{\phi}_{p'}^{p} \int_{\Omega} \int_{\abs{y}\leq1}
           \abs{f(x)-f(x-hy)}^{p}\,dy\,dx\\
        &\text{nach Fubini \autoref{satz:fubini} können die Integrale
          vertauscht werden}\\
        &= \norm{\phi}_{p'}^{p} \int_{\abs{y}\leq1} \norm{f(x)-f(x-hy)}_{p}^{p}\,dy
      \end{align*}
      Für $\abs{hy}\leq h\leq h_{0}(\epsilon)$ ist $\norm{f(x)-f(x-hy)}_{p}^{p}
      \leq \sup_{x\in M} \norm{f(x)-f(x-hy)}_{p}^{p} \leq \epsilon^{p}$ nach
      Voraussetzung und wir erhalten
      \begin{gather*}
        \norm{\phi}_{p'}^{p} \int_{\abs{y}\leq1} \norm{f(x)-f(x-hy)}_{p}^{p}\,dy
        \quad\leq\quad \underbrace{\norm{\phi}_{p'}^{p}\:
        \abs{K_{1}(0)}}_{=c^{p}} \epsilon
      \end{gather*}
      Somit ist $M_{h}$ ein $c\cdot\epsilon$-Netz von $M$, falls $h\leq
      h_{0}(\epsilon)$.
    \end{schritte}
  \end{proof}
\end{satz}

\section{Beschränkte und kompakte Operatoren}
\subsection{Beschränkte Operatoren}

\begin{defini}
  Seien $(E, \norm{\cdot}_{E})$ und $(F, \norm{\cdot}_{F})$ zwei normierte
  Vektorräume. Der lineare Operator $A\colon E\rightarrow F$ heißt
  \highl{beschränkt} ($A\in \linOp(E,F)$)
  \index{L(.,.)@$\linOp(\cdot,\cdot)$} $:\Longleftrightarrow$ ein
  $c>0$ existiert, so dass für alle $x\in E$ gilt: $\norm{Ax}_{F} \leq
  c\norm{x}_{E}$
\end{defini}

\begin{satz}\label{satz:1.4.1/1}
  Folgende Aussagen sind für den linearen Operator $A\colon E\rightarrow F$
  äquvalent:
  \begin{enumerate}
   \item $A$ ist beschränkt
   \item $A$ ist Lipschitz-stetig
   \item $A$ ist gleichmäßig stetig
   \item $A$ ist stetig
   \item $A$ ist stetig in $x_{0}\in E$ (für ein $x_{0}$)
   \item $\sup_{\norm{x}_{E}\leq1} \norm{Ax}_{F} < \infty$
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[(1)$\Rightarrow$(2)] $\norm{Ax-Ax'}_{F} = \norm{A(x-x')}_{F} \leq c
      \norm{x-x'}_{E}$
     \item[(2)$\Rightarrow$(3)$\Rightarrow$(4)$\Rightarrow$(5)] klar
     \item[(5)$\Rightarrow$(6)] Da $A$ stetig in $x_{0}$, exstiert ein
      $\delta>0$, so dass für alle $x\in K_{\delta}(x_{0})$
      ($\norm{x-x_{0}}\leq\delta$) gilt: $\norm{Ax-Ax_{0}}_{F}<1$.

      Für alle $\tilde{x}$ mit $\norm{\tilde{x}}_{E}\leq 1$ ist
      $\norm{(x_{0}-\delta \tilde{x})-x_{0}} = \delta\norm{\tilde{x}}\leq\delta$
      und wir können statt $x$ $x_{0}-\delta \tilde{x}$ in die Aussage
      einsetzen und erhalten: $\norm{A(x_{0}+\delta \tilde{x})-Ax_{0}} =
      \delta \norm{A\tilde{x}}<1$ $\Rightarrow$ $\norm{A\tilde{x}}_{F} < \frac{1}{\delta}$
     \item[(6)$\Rightarrow$(1)]
      $\norm{Ax}_{F} = \norm{A(\frac{x}{\norm{x}_{E}})}_{F}\:\norm{x}_{E}  \leq
      \underbrace{\sup_{\norm{z}_{E}\leq1} \norm{Az}_{F}}_{=:c <\infty}\ \norm{x}_{E}$
    \end{description}
  \end{proof}
\end{satz}

\begin{defini}
  \label{def:Opnorm}
  $A\in\linOp(E,F)$

  \begin{gather*}
    \norm{A} = \norm{A}_{\linOp(E,F)} = \norm{A}_{E\rightarrow F} :=
    \sup_{\norm{x}_{E}\leq1} \norm{Ax}_{F}
  \end{gather*}
  heißt \highl{Operatornorm} von $A$.
\end{defini}

\begin{satz}\label{satz:1.4.1/2}
  \begin{enumerate}
   \item Wenn $A\in\linOp(E,F)$ ist, dann gilt:
    \begin{align*}
      \norm{A} &= \sup_{\norm{x}_{E}=1} \norm{Ax}_{F} = \sup_{\norm{x}\leq1}
         \norm{Ax}_{F} = \sup_{x\ne0} \frac{\norm{Ax}_{F}}{\norm{x}_{E}}\\
      &= \inf\{c \colon\ \forall x\colon \norm{Ax}_{F} \leq c \norm{x}_{E} \}
    \end{align*}
   \item Ist $F$ ein Banachraum, dann ist auch $\linOp(E,F)$ ein
    Banachraum.
   \item Ist $A\in\linOp(E,F)$ und $B\in\linOp(F,G)$, dann ist
    die Verkettung $B\circ A = BA \in\linOp(E,G)$ und es gilt:
    $\norm{BA}_{E\rightarrow G} \leq \norm{B}_{F\rightarrow G}
    \norm{A}_{E\rightarrow F}$
   \item Ist $E$ ein Banachraum, so ist $\linOp(E) :=
    \linOp(E,E)$ \index{L(.)@$\linOp(\cdot)$} eine
    \highl{Banachalgebra} ($I$ identischer Operator, Einselement)
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item
      \begin{align*}
        \sup_{x\ne0} \frac{\norm{Ax}_{F}}{\norm{x}_{E}}
           &= \sup_{x\ne0} \norm[\Big]{A\bigl(\frac{x}{\norm{x}_{E}}\bigr)}_{F}
           = \sup_{\norm{y}_{E} = 1} \norm{Ay}_{F}
           \leq \sup_{0\leq\norm{y}_{E}\leq 1} \norm{Ay}_{F}\\
        &\leq \sup_{0<\norm{y}_{E}\leq1} \frac{1}{\norm{y}_{E}}\:\norm{Ay}_{F}
           = \sup_{\norm{x}=1} \norm{Ax}_{F}
      \end{align*}

      Außerdem ist nach \autoref{def:Opnorm}
      $\norm{Ax}_{F} \leq \norm{A} \norm{x}_{E}$. Für das Infimum von $c$ folgt
      \begin{gather*}
        \inf\{c\colon \norm{Ax}\leq c\norm{x}\} \leq \frac{\norm{Ax}}{\norm{x}} \leq \norm{A}
      \end{gather*}

      Annahme: Es existiert ein $c' <\norm{A}$ mit $\norm{Ax}\leq c'\norm{x}$.
      Dann wäre $\sup_{x\ne0}\frac{\norm{Ax}}{\norm{x}} \leq c' < \norm{A}$, das im
      Widerspruch zu \help{was?} steht.
     \item Um nachzuweisen, dass $\linOp(E,F)$ ein Banachraum ist,
      müssen wir seine Vollständigkeit zeigen. Dafür müssen wir zeigen,
      dass jede Cauchyfolge $(A_{k})_{k}$ in $\linOp(E,F)$
      konvergiert und ihr Grenzwert $A$ in $\linOp(E,F)$ liegt.

      Es sei $x\in E$ und $(A_{k})_{k}$ eine Cauchyfolge in
      $\linOp(E,F)$.
      \begin{gather*}
        \norm{A_{k} x - A_{l}x}_{F} \leq \norm{A_{k}-A_{l}}_{\linOp(E,F)}
        \: \norm{x}_{E} \xrightarrow{k,l\rightarrow\infty} 0
      \end{gather*}
      Damit ist $(A_{k}x)_{k}$ eine Cauchyfolge in $F$ und da $F$
      Banachraum ist existiert ein $y\in F$ mit $A_{k}x \rightarrow y$.
      Setzen wir also $Ax := y = \lim_{k\rightarrow\infty} A_{k}x$. Aus
      \help{irgendeiner der beiden Definitionen} folgt, dass $A\colon E\rightarrow F$ linear ist, woraus wiederum
      \begin{gather*}
        \norm{(A-A_{k})x}_{F} = \lim_{l\rightarrow\infty}
        \norm{A_{l}x-A_{k}x}_{F} \leq \liminf_{l\rightarrow\infty}
        \norm{A_{l}-A_{k}}\:\norm{x}
      \end{gather*}
      folgt. $A-A_{k}$ ist also $\in\linOp(E,F)$ und
      \begin{gather*}
        \norm{A-A_{k}}\leq \liminf_{l\rightarrow\infty} \norm{A_{l}-A_{k}}
        \xrightarrow{k\rightarrow\infty} 0
      \end{gather*}
      $A_{k}$ konvergiert also gegen $A\in\linOp(E,F)$.
     \item $E \xrightarrow{A} F \xrightarrow{B} G$ linear und beschränkt
      \begin{gather*}
        \norm{BAx}_{G} \leq \norm{B}_{F\rightarrow G} \norm{Ax}_{F}
        \leq \norm{B}_{F\rightarrow G} \norm{A}_{E\rightarrow F} \norm{x}_{E}
      \end{gather*}
     \item Folgt aus (3) indem man $F=E$ setzt.
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bsp}
  Ist $E$ ein endlich dimensionaler Vektorraum, so ist jede lineare
  Abbildung $A\in\linOp(E,F)$ stetig.

  \begin{proof}
    Sei $\{e_{1},\ldots,e_{n}\}$ eine Basis von $E$. Dann lässt sich
    $x\in E$ als Linearkombination der $e_{j}$ schreiben $x=\sum_{j=1}^{n}
    \lambda_{j} e_{j}$. Als Norm in $E$ wählen wir $\norm{x}_{1} :=
    \sum_{j=1}^{n}\abs{\lambda_{j}}$.

    Die Norm $\norm{Ax}$ in $F$ ist beschränkt und somit nach
    \autoref{satz:1.4.1/1} stetig.
    \begin{gather*}
      \norm{Ax}_{F} = \norm{\sum_{j=1}^{n} \lambda_{j} A e_{j}}_{F}
      \leq \sum_{j=1}^{n}\abs{\lambda_{j}} \norm{A e_{j}}_{F}
      \leq \norm{x}_{1} \underbrace{\max_{j=1,\ldots,n}\norm{Ae_{j}}_{F}}_{=c}
    \end{gather*}
  \end{proof}
\end{bsp}

\begin{bsp}[Matrix-Operatoren]\label{bsp:1.4.1/2}
  $(a_{jk})_{j,k\in \N} \in l_{2} (\N\times \N)$
  \begin{gather*}
    x\in l_{2}, (Ax)_{j} := \sum_{k=1}^{\infty} a_{jk}
       x_{k}\qquad(j\in\N)\\
    \Longrightarrow \norm{Ax}_{2} = \Bigl(\sum_{j=1}^{\infty}
       \abs[\big]{\sum_{k=1}^{\infty} a_{jk} x_{k}}^{2}
       \Bigr)^{\nicefrac12} \leq \Bigl(\sum_{j=1}^{\infty}
       \sum_{k=1}^{\infty} \abs{a_{jk}}^{2} \cdot \sum_{k=1}^{\infty}
       \abs{x_{k}}^{2}\Bigr)^{\nicefrac12} =
       \underbrace{\Bigl(\sum_{j,k=1}^{\infty}
       \abs{a_{jk}}^{2}\Bigr)^{\nicefrac12}}_{=c} \norm{x}_{2}\\
    \Longrightarrow A\in \linOp(l_{2}), \norm{A} \leq
       \norm{a}_{l_{2}(\N\times\N)}
  \end{gather*}
\end{bsp}

\begin{bsp}[Multiplikationsoperatoren]
  \begin{enumerate}
   \item $(M f)(x) = x f(x)\qquad f\in C([0,1])$
    \begin{align*}
      &\Longrightarrow \norm{M f}_{\infty} = \sup_{x\in [0,1]} \norm{x f(x)}
         \leq \norm{f}_{\infty}\\
      & \Longrightarrow M\in\linOp(C([0,1])), \norm{M} = 1
    \end{align*}

    \begin{align*}
      f = 1 \in C([0,1]) \Longrightarrow \norm{M1}_{\infty} = \norm{x}_{\infty}
         = 1
    \end{align*}
   \item $\phi \in L_{\infty} (\Omega), f\in L_{p}(\Omega), 1 \leq p \leq \infty$
    \begin{align*}
      M_{\phi} f := \phi\cdot f \Longrightarrow & M_{\phi}\in
         \mathcal{L}\bigl(L_{p}(\Omega)\bigr)\\
      & \norm{M_{\phi} f}_{p} = \Big(\int \abs{\phi(x)}^{p}\cdot
         \abs{f(x)}^{p}\,dx \Big)^{\nicefrac1p} \leq \norm{\phi}\cdot \norm{f}
    \end{align*}
  \end{enumerate}
\end{bsp}

\begin{bsp}[Faltungsoperatoren]
  $g\in L_{1}(\R^{n}), f\in L_{p}(\R^{n}), 1 \leq p \leq \infty$
  \begin{align*}
    &(\mathcal{K}_{g} f)(x) = (g\ast f)(x) = \int_{\R^{n}} g(x-y) f(y)\,dy\\
    & \Longrightarrow \mathcal{K}_{g}\in \mathcal{L} (L_{p}(\R^{n})),
       \norm{\mathcal{K}_{g}f}_{p} = \norm{g\ast f}_{p} \leq \norm{g}_{1}
       \norm{f}_{p} \Longrightarrow \norm{\mathcal{K}_{g}} \leq \norm{g}_{1}
  \end{align*}
\end{bsp}

\begin{bsp}[Differenzialoperatoren]
  $f\in C^{1}([a,b]), Df := f'$ $\Longrightarrow$ $D\colon C^{1} \rightarrow
  C([a,b])$ ist linearer Operator mit der Norm $\norm{f}_{C^{1}} :=
  \norm{f}_{\infty} + \norm{f'}_{\infty}$ $\Longrightarrow$ $\norm{Df}_{\infty} =
  \norm{f'}_{\infty} \leq \norm{f}_{C^{1}}$

  Eine weiter Norm wäre $\norm{f}^{\ast}_{C^{1}} = \norm{f}_{\infty}
  \Longrightarrow$ Operator nicht beschränkt. Betrachten [a,b] = [0,1]
  $f_{n}(x) = x^{n} \Longrightarrow \norm{x^{n}}_{\infty} = 1$, aber
  $\norm{Dx^{n}}_{\infty} = \norm{nx^{n-1}}_{\infty} = n
  \xrightarrow{n\rightarrow\infty} \infty$ $\Longrightarrow$ $D\colon (C^{1},
  \norm{\cdot}_{\infty}) \rightarrow C([a,b])$ nicht beschränkt
\end{bsp}

\begin{bsp}[Lineare Funktionale]\label{bsp:linfunk}
  $F = \C (\R), \linOp(E, \C)$
  \begin{enumerate}
   \item
    \begin{gather*}
      E = C([0,1]) \Longrightarrow Tf := f(0) \in \C, \abs{f(0)} \leq
      \sup_{x\in[0,1]}\abs{f(x)} = \norm{f}_{\infty}
    \end{gather*}
   \item \label{en:linfunk2}
    \begin{gather*}
      E = C([0,1]), g\in L_{1}([0,1]): T_{g}(f) := \int_{0}^{1}g(x)
      f(x)\,dx \in \C\\
      T_{g} \colon C([0,1]) \rightarrow \C, \abs{T_{g}(f)} \leq \int_{0}^{1}
         \abs{g(x)} \abs{f(x)}\,dx \leq \norm{f}_{\infty} \norm{g}_{1} \Longrightarrow
         \norm{T_{g}} \leq \norm{g}_{1}\\
      \intertext{Für $\epsilon > 0$ sei}
      f_{\epsilon} := \frac{\overline{g(x)}}{\abs{g(x)}+\epsilon}
         \Longrightarrow f_{\epsilon}\in C([0,1]),
         \norm{f_{\epsilon}}_{\infty} \leq 1\\
      \Longrightarrow \norm{T_{g}} \geq \norm{T_{g}(f_{\epsilon})} =
         \abs[\Big]{\int_{0}^{1}
         \frac{g(x)\overline{g(x)}}{\abs{g(x)}+\epsilon}\,dx
         }\taglabel{eq:funkop} \geq \int_{0}^{1}
         \frac{\abs{g(x)}^{2}-\epsilon^{2}}{\abs{g(x)}+\epsilon}\,dx =
         \int_{0}^{1} \abs{g(x)}\,dx - \epsilon = \norm{g}_{1} -\epsilon\\
      \epsilon \rightarrow 0 \Longrightarrow \norm{T_{g}} \geq \norm{g}_{1}\\
      \Longrightarrow \norm{T_{g}} = \norm{g}_{1}
    \end{gather*}
  \end{enumerate}
\end{bsp}

\begin{bsp}[Integraloperatoren]\index{Integraloperatoren}
  Es sei $k(x,y) \colon [0,1]\times[0,1] \rightarrow \K$ stetig ($k\in
  C([0,1]\times[0,1])$), $f\in C([0,1])$
  \begin{align*}
    (\mathcal{K}f)(x) &:= \int_{0}^{1} k(x,y)\: f(y)\,dy &(x\in[0,1])\\
    \Longrightarrow \mathcal{K} &\colon C([0,1]) \rightarrow
       C([0,1])\text{ linear und beschränkt}\\
    \norm{\mathcal{K}} &= \sup_{x\in[0,1]} \int_{0}^{1}\abs{k(x,y)}\,dy \leq
       \norm{k(x,y)}_{\infty}
  \end{align*}

  \begin{proof}
    \begin{gather*}
      \abs{(\mathcal{K}f)(x)-(\mathcal{K}f)(x')} \leq \int_{0}^{1}
      \abs{k(x,y)-k(x',y)}\:\abs{f(y)}\,dy \leq \epsilon \norm{f}_{\infty}
    \end{gather*}
    Da $k$ gleichmäßig stetig ist, kann die Diverenz $k(x,y)-k(x',y)$
    beliebig klein ($\leq\epsilon$) werden, wählt man
    $\abs{x-x'}<\delta(\epsilon)$ entsprechend.

    \begin{align*}
      \norm{\mathcal{K}} &= \sup_{\norm{f}_{\infty}\leq1} \norm{\mathcal{K}f}_{\infty}\\
      &= \sup_{\abs{f}_{\infty}\leq1} \sup_{x\in[0,1]} \abs{\mathcal{K}f(x)}\\
      &= \sup_{x\in[0,1]} \underbrace{\sup_{\norm{f}_{\infty}\leq1}
         \abs[\Big]{\int_{0}^{1}k(x,y)\,f(y)\,dy}}
         _{=\norm{T_{k(x,\cdot)}}_{C([0,1])\rightarrow C([0,1])}}\\
      &= \sup_{x\in[0,1]} \norm{k(x,\cdot)}_{1}&&\text{nach
         \autoref{en:linfunk2} in \autoref{bsp:linfunk}}\\
      &= \sup_{x\in[0,1]} \int_{0}^{1}\abs{k(x,y)}\,dy
    \end{align*}
  \end{proof}
\end{bsp}

\begin{satz}
  Es sei $D\subset E$ ein dichter Teilraum von $E$ und es sei $F$ ein
  Banachraum.

  Dann gibt es für $A\in\linOp(D,F)$ genau eine stetige
  (beschränkte) Fortsetzung $\tilde{A}\in\linOp(E,F)$, d.\,h.
  $\tilde{A}\in\linOp(E,F) \wedge \tilde{A}\big|_{D}=A$. Zusätzlich ist
  $\norm{\tilde{A}}_{E\rightarrow F} = \norm{A}_{D\rightarrow F}$.

  \begin{proof}
    Für ein $x\in F$ existiert eine Folge $(x_{k})\subseteq D$ mit
    $\lim_{x_{k}}=x$. Da $\norm{Ax_{k}-Ax_{l}}_{F} \leq
    \norm{A}_{D\rightarrow F} \norm{x_{l}-x_{k}}_{E}$ ist, ist $(Ax_{k})$ eine
    Cauchyfolge in $F$ und es existiert ein $y\in F$ mit
    $Ax_{k}\rightarrow y$. Wir setzen $\tilde{A}x:=y$. $y$ ist unabhängig
    von der Auswahl der Folge $(x_{k})$: $x_{k}'\rightarrow x$. Es
    existiert also ein $y'$, das Grenzwert von $Ax_{k}'$ ist
    ($Ax_{k}'\rightarrow y'$).
    \begin{gather*}
      \norm{y-y'}_{F} \leq \norm{Ax_{k}-y}_{F} +
      \underbrace{\norm{Ax_{k}-Ax_{k}'}_{F}} _{\leq \norm{A}_{D\rightarrow
      F}\norm{x_{k}-x_{k}'}}+ \norm{Ax_{k}'-y'} < \epsilon\qquad\forall\epsilon>0
    \end{gather*}
    Das bedeutet, $\tilde{A}\big|_{D}=A$.
    \begin{gather*}
      \norm{\tilde{A}x}_{F} = \norm{\lim_{k\rightarrow\infty} Ax_{k}}_{F} =
         \lim_{k\rightarrow\infty} \norm{Ax_{k}}_{F} \leq \norm{A}_{D\rightarrow
         F} \norm{x}_{E}\\
      \norm{\tilde{A}_{E\rightarrow F}} \leq \norm{A}_{D\rightarrow F} =
         \sup_{\norm{x}_{D}\leq 1} \norm{Ax} \leq \sup_{\norm{x}_{E}\leq 1}
         \norm{\tilde{A}x} = \norm{\tilde{A}}_{E\rightarrow F}
    \end{gather*}
    Eindeutigkeit: $B$ ist also in $\linOp(E,F)$ und $B\big|_{D}=A$.

    $x\in E$ $\Longrightarrow$ $Bx = \lim_{k\rightarrow\infty} Bx_{k}$
    (da $B\in\linOp(E,F)$) $= \lim_{k\rightarrow\infty} Ax_{k}$ (da
    $B\big|_{D}=A$) $=\tilde{A}x$.
  \end{proof}
\end{satz}

\subsection{Lineare Funktionale}

\begin{satz}
  $1\leq p \leq \infty, \frac1p + \frac1{p'}=1$

  Dann
  \begin{gather*}
    (L_{p}(A,\mu))' \cong L_{p'}(A,\mu)
  \end{gather*}
\end{satz}

\begin{bemerk} % 3
  Übung, Blatt\,6, Aufgabe\,5

  \begin{gather}\label{eq:18}
    L_{p}(A,\mu) \hookrightarrow (L_{p'}(A,\mu))'\qquad 1\leq p\leq\infty
  \end{gather}

  \begin{align*}
    f\in L_{p}(A,\mu) &\Longrightarrow T_{f} := \int_{A}
       f(x)g(x)\,d\mu,\quad T_{f}\in (L_{p'}(A,\mu))'&&\text{nach
       \autoref{eq:hoelder-int}}\\
    \norm{f}_{p} & = \sup_{\norm{g}_{p'}}
       \abs[\Big]{\int_{A}f(x)g(x)\,d\mu}
  \end{align*}

  Gleichheit in \autoref{eq:18} gilt genau dann wenn $1< p \leq\infty$.

  \textbf{Aber:}
  \begin{gather*}
    L_{1}(A,\mu) \supsetneq (L_{\infty}(A,\mu))'\qquad\text{falls }\dim
    L_{1}(A,\mu) = \infty
  \end{gather*}
\end{bemerk}

\begin{satz}[Riesz] % 3
  (siehe Werner, S.\,62--64)

  $K$ kompakter metrischer Raum
  \begin{gather*}
    \Longrightarrow (C(K))' \cong M(K) \quad\text{(reguläre Borelmaße)}
  \end{gather*}
  wobei für $\mu\in M(K): T_{\mu}(f) := \int_{K}f\,d\mu, (f\in C(K))$
\end{satz}

\begin{bemerk}[später]
  \begin{enumerate}
   \item Satz von Hahn-Banach: $(E,\norm{\cdot}_{E})$ normierter Vektorraum,
    $U\subset E$ Reilraum von $E$ (nicht notwendig dicht) und $U'\colon U
    \rightarrow\K$ linear und stetig.

    Dann $\exists e'\colon E\rightarrow\K$ linear und stetig mit $e'\big|_{U} =
    U'$ (Forsetzung von $U'$) und $\norm{e'}_{E\rightarrow\K} =
    \norm{U'}_{U\rightarrow\K}$
   \item $p=2 (\Rightarrow p'=2)$ $\Longrightarrow$ $(l_{2})' \cong
    l_{2}, (L_{2}(A,\mu))'\cong L_{2}(A,\mu)$

    Es gilt: $H$ Hilbertraum $\Longrightarrow$ $H' \cong H$
   \item $0<p<1$ $\Longrightarrow$ $(L_{p})' = \{0\}$
  \end{enumerate}
\end{bemerk}

\subsection{Kompakte Operatoren} %1.5.3

\begin{defini}
  $E,F$ normierte Vektorräume, $A\colon E\rightarrow F$ linear

  $A$ heißt kompakt $:\Longleftrightarrow$ Das Bild jeder in $E$
  beschränkten Menge ist präkompakt in $F$.

  Bezeichnung: $(A\in K(E,F))$.
\end{defini}

\begin{bemerk}
  äquivalent: $B_{E} := \{x\in E\colon \norm{x}_{E}\leq 1\}$
  \begin{itemize}
   \item $A(B_{E})$ ist präkompakt in $F$.
   \item $\overline{A(B_{E})}$ ist kompakt in $F$.
  \end{itemize}
\end{bemerk}

\begin{satz}\label{satz:122}
  $E,F$ normierte Vektorräume.
  \begin{enumerate}
   \item $A\in K(E,F) \Longrightarrow A\in\linOp(E,F)$
   \item $A\colon E\rightarrow F$ linear und kompakt $\Longleftrightarrow$ Aus
    jeder beschränkten Folge $(x_{k})_{k}$ in $E$ kann man eine Teilfolge
    $(x_{k_{l}})_{k_{l}}$ auswählen, so daß $(A x_{k_{l}})$ konvergent in $F$
    ist.
   \item $A\in\linOp(E,F), \dim E < \infty$ oder $\dim F < \infty$
    Dann folgt: $A$ ist kompakt.
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item $A$ kompakt
      \begin{align*}
        \Longrightarrow& \{Ax \colon \norm{x}\leq 1\}\ \text{präkompakt}\\
        \Longrightarrow& \{Ax \colon \norm{x}\leq 1\}\ \text{beschränkt}\\
        \Longrightarrow& \sup_{\norm{x}\leq 1}\norm{A x}_{F} < \infty\\
        \Longrightarrow& A\ \text{beschränkt}
      \end{align*}
     \item \help{Aus irgendwelchen Gründen soll das trivial sein}
     \item $\dim F <\infty$ dann $M\subset F$ präkompakt
      $\Longleftrightarrow$ $M$ beschränkt in $F$.

      $\dim E < \infty$ dann $M\subset E$ präkompakt $\Leftrightarrow$
      $M$ beschränkt in $E$ $\Longrightarrow$ $M$ präkompakt in $E$
      $\Longrightarrow$ $A\in\linOp(E,F)$ $A(M)$ präkompakt in $F$
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{satz}\label{satz:123}
  \begin{enumerate}
   \item $A\in \linOp(E,F), B\in\linOp(F,G)$

    Wenn $A$ \emph{oder} $B$ kompakt $\Longrightarrow$ $BA\colon
    E\rightarrow G$ ist kompakt
   \item Wenn $E$ normierter Vektorraum, $F$ Banachraum $\Longrightarrow$
    $K(E,F)$ ist ein \highl{abgeschlossener Teilraum} von
    $\linOp(E,F)$.
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item beschränkte Operatoren bilden beschränkte Mengen auf
      beschränkte Mengen ab, präkompakte Operatoren bilden auf
      präkompakte Mengen ab.
      \begin{center}
        \ifpdf\input{satz2-diagramm.pdf_t}\else\input{satz2-diagramm.eps_t}\fi
        % \includegraphics{satz2-diagramm}
      \end{center}
     \item $A,B \in K(E,F) \Longrightarrow A+B\in K(E,F)$

      $(x_{k})_{k}$ beschränkt in $E$ $\Longrightarrow$ $\exists$
      Teilfolge $(y_{k})_{k}$ mit $(Ay_{k})$ konvergent $\Longrightarrow$
      $\exists$ Teilfoge $(z_{k})_{k}$ von $(y_{k})_{k}$ mit
      $(Bz_{k})_{k}$ konvergent $\Longrightarrow$
      $\bigl((A+B)z_{k}\bigr)_{k}$ konvergent

      Es sei $(A_{k})_{k}, A_{k}$ kompakt mit $\norm{A_{k}-A} \rightarrow 0$
      in $\linOp(E,F)$

      \underline{zu zeigen:} $A$ ist kompakt

      \underline{Idee:} Es sei $B_{E}$ Einheitskugel in $E$. Wir zeigen: $A(B_{E})$ ist
      präkompakt in $F$. Ausreichend: $\exists$ endliches $\epsilon$-Netz
      für $A(B_{E})$ ($\forall \epsilon>0$)

      wählen $A_{k}$ mit $\norm{A-A_{k}}\leq\frac\epsilon3$, $A_{k}$
      kompakt, $\overline{A_{k}(B_{E})}\subset \bigcup_{x\in B_{E}}
      K_{\frac\epsilon3}(A_{k}x)$. Weil $A_{k}$ kompakt existiert eine
      endliche Teilüberdeckung nach \autoref{satz:heineborel}.
      \begin{gather*}
        A_{k}(B_{E}) \subset \bigcup_{i=1}^{m}
        K_{\frac\epsilon3}(A_{k}x_{i})\qquad\{x_{1},\ldots,x_{m}\} \subset B_{E}
      \end{gather*}

      Betrachte $(K_{\epsilon}(A x_{i}))_{i=1}^{m}$ $\Longrightarrow$
      $\epsilon$-Netz für $A(B_{E})$

      \begin{align*}
        \norm{x}\leq 1 \Longrightarrow \norm{Ax -Ax_{i}}_{F} &\leq \norm{Ax -
           A_{k}x}_{F} + \underbrace{\norm{A_{k}x - A_{k}x_{i}}_{F}}_{\leq
           \exists i: \frac\epsilon3} + \norm{A_{k}x_{i}-Ax_{i}}_{F}\\
        &\leq \underbrace{\norm{A_{k}-A}}_{\leq \frac\epsilon3}
           \underbrace{\norm{x}_{E}}_{\leq 1} + \frac\epsilon3 +
           \underbrace{\norm{A_{k}-A}}_{\leq \frac\epsilon3}
           \underbrace{\norm{x_{i}}_{F}}_{\leq 1} < \epsilon
      \end{align*}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{folger}\label{fol:1}
  $E$ normierter Vektorraum, $F$ Banachraum, $A_{k}$ Folge von linearen
  Operatoren mit $\dim R(A_{k}) <\infty$ ("`finite rank operators"')
  ($\Longrightarrow$ $A_{k}$ kompakt nach \autoref{satz:122}) und
  $\norm{A_{k}-A} \xrightarrow{k\rightarrow\infty} 0$ (\help{irgendwie
  spielt \autoref{satz:123} hier eine Rolle}) $\Longrightarrow$ $A$
  ist kompakt.
\end{folger}

\begin{bsp}
  $(a_{jk})\in l_{2}(\N\times \N), x= (x_{k})_{k}\in l_{2}(\N)$
  \begin{gather*}
    (Ax)_{j} := \sum_{k=1}^{\infty} a_{jk} x_{k}\qquad(j\in\N)
  \end{gather*}
  $\Longrightarrow$ $A\colon l_{2} \rightarrow l_{2}$ beschränkt nach
  \autoref{bsp:1.4.1/2}

  $A$ ist kompakt da:
  \begin{gather*}
    (A_{n}x)_{j} = \begin{cases}(Ax)_{j}&j=1,\ldots,n\\0&j>n\end{cases}
  \end{gather*}
  $\Longrightarrow$ $\dim R(A_{n}) \leq n$ $A_{n}$ kompakt

  \begin{align*}
    \norm{A_{n}x -Ax}_{2} = (\sum_{j=n+1}^{\infty}\abs[\Big]{\sum_{k=1}^{\infty}
       a_{jk} x_{k}}^{2})^{\frac12}\\
    &\leq \underbrace{\Bigl(\sum_{j=n+1}^{\infty}\sum_{k=1}^{\infty}
       \abs{a_{jk}}^{2}\Bigr)^{\frac12}}_{\xrightarrow{n\rightarrow\infty}0}
       \norm{x}_{2}\\
    \Longrightarrow& \norm{A_{n}-A} \leq \Bigl(\sum_{j=n+1}^{\infty}\sum_{k=1}^{\infty}
       \abs{a_{jk}}^{2}\Bigr) \rightarrow 0
  \end{align*}
\end{bsp}

\begin{bsp}
  (Blatt\,4, Aufgabe\,3)

  $C^{\alpha}([0,1]), 0<\alpha\leq 1$ Banachraum

  $0 < \alpha < \beta \leq 1, \norm{f} =
  \norm{f}_{\infty}+\sup_{x,y}\frac{\abs{f(x)-f(y)}}{\abs{x-y}^{\alpha}}$

  $\Longrightarrow$ $C^{\beta}([0,1])\subset C^{\alpha}([0,1])$\\
  $id\colon C^{\beta} \rightarrow C^{\alpha}$ ist kompakt ("`kompakte
  Einbettung"')
\end{bsp}

\begin{satz}[Integraloperatoren]\label{satz:int-op}
  $\Omega\subset \R^{n}$ offen und beschränkt. Betrachten:
  $(\mathcal{K}f)(x) := \int_{\Omega} K(x,y)f(y)\,dy\quad x\in\Omega$
  \begin{enumerate}
   \item $K(x,y) \in C(\overline{\Omega}\times\overline{\Omega})
    \Longrightarrow \mathcal{K}\colon C(\overline{\Omega}) \rightarrow
    C(\overline{\Omega})$ kompakt, $\norm{\mathcal{K}} \leq \norm{K(x,y)}_
    {C(\overline{\Omega}\times\overline{\Omega})} \abs{\Omega}$
   \item $K(x,y) \in L_{2}(\Omega\times\Omega)$ $\Longrightarrow$
    $\mathcal{K}\colon L_{2}(\Omega) \rightarrow L_{2}(\Omega)$ kompakt
    $\norm{\mathcal{K}} \leq \norm{K(x,y)}_{L_{2}(\Omega\times\Omega)}$
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item $\norm{f}_{\infty} \leq 1, f\in C(\overline{\Omega})
      \Longrightarrow \norm{\mathcal{K}f}_{\infty} = \norm{\int_{\Omega}
      k(x,y)f(y)\,dy}_{\infty} \leq \norm{K}_{\infty,\Omega\times\Omega}
      \underbrace{\norm{f}_{\infty}}_{\leq 1}\abs{\Omega}$

      $\Longrightarrow$ $\{\mathcal{K}f\colon \norm{f}\leq 1\}$ beschränkt\\
      $\abs{(\mathcal{K}f)(x+h)-(\mathcal{K}f)(x)} \leq \int_{\Omega}
      \abs{\underbrace{k(x+h,y)-k(x,y)}_{<\epsilon, \text{falls
      $\abs{h}<\delta(\epsilon)$, unabhängig von $x,y$, da glm. stetig}}}
      \abs{f(y)}\,dy \leq \epsilon\abs{\Omega}$\\
      $\Longrightarrow$ $\{ \mathcal{K}f\colon \norm{f}_{\infty} \leq 1\}$
      gleichmäßig stetig $\Longrightarrow$ präkompakt nach
      \autoref{satz:arzela}

     \item $K\in L_{2}(\Omega\times\Omega), \norm{f}_{2} \leq 1$

      $\Longrightarrow$ $\norm{\mathcal{K}f}_{2} = \norm{\int_{\Omega} K(x,y)
      f(y)\,dy}_{2} \leq^{\text{\autoref{eq:hoelder-int}}}
      \underbrace{\norm[\Bigl]{\Bigl(\int_{\Omega}\abs{k(x,y)}^{2}\,dy
      \Bigr)^{\frac12}}_{2}}_{=\norm{K}_{L_{2}(\Omega\times\Omega)}}
      \underbrace{\norm{f}_{2}}_{\leq 1}$
      $\Longrightarrow$ $\{\mathcal{K}f\colon \norm{f}_{2} \leq 1\}$ beschränkt

      $\norm{\mathcal{K}f(x+h)-\mathcal{K}f(x)}_{2} \leq
      \underbrace{\norm{k(\cdot+h,\cdot)-k(\cdot,\cdot)}_{L_{2}(\Omega\times\Omega)}}
       _{<\epsilon,\text{falls $\abs{h}<\delta(\epsilon)$ nach \autoref{satz:1.3.7/1}}}
      \underbrace{\norm{f}_{2}}_{\leq 1} < \epsilon\qquad\forall
      f\colon\norm{f}_{2}\leq1$

      $\Longrightarrow$ $\{\mathcal{K}f\colon \norm{f}_{2}\leq 1\}$ präkompakt,
      nach \autoref{satz:1.3.7/5} $\Longrightarrow$ $\mathcal{K}$ kompakt
    \end{schritte}
  \end{proof}
\end{satz}

\subsection{Inverse Abbildungen, Neumann'sche Reihe} % 1.5.4
\label{sec:1.4.4}
\begin{description}
 \item[\underline{Problem:}] $A\in \linOp(E,F), Ax=y$ ($y\in F$ gegeben, $x\in E$
  gesucht)
 \item[\underline{Annahme:}] $\exists A^{-1}\in \linOp(F,E) \Longrightarrow
  x=A^{-1}y \Longrightarrow \norm{x-x'}_{E} \leq \norm{A^{-1}} \norm{y-y'}_{F}$
  (stetige Abhängigkeit der Lösung von rechter Seite)
\end{description}

\begin{bemerk}
  $A\in\linOp(E,F)$ kompakt.

  Nehmen wir einmal an, es existiere$A^{-1}\in \linOp(F,E)$. Dann
  ist die identische Abbildung $I= A^{-1}A\colon E\rightarrow E$ kompakt. Dies
  gilt nach \autoref{satz:122} genau dann wenn \help{der Satz sagt aber nur
  $\Rightarrow$ nicht $\Leftrightarrow$} $\dim E < \infty$.
  Analog: $\dim F < \infty$
\end{bemerk}

\begin{defini}\label{def:1.4.4/1}
  $A\in\linOp(E), E$ komplexer Banachraum
  \begin{align*}
    M_{A} &:= \{ \lambda\in \C\colon \exists(\lambda I-A)^{-1}\in
    \linOp(E) \}\\
    S_{A} &:= \C\setminus M_{A}
  \end{align*}
  $M_{A}$ heißt \highl{Resolventenmenge} von $A$, $S_{A}$ heißt
  \highl{Spektrum} von $A$.
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $R(\lambda I-A)$ Wertebereich von $\lambda I-A$

    $\lambda\in M_{A} \Longleftrightarrow \exists(\lambda I-A)^{-1}\in
    \linOp(E) \Longleftrightarrow$
    \begin{enumerate}
     \item $R(\lambda I-A) =E$,
     \item $\lambda I-A$ injektiv,
     \item $(\lambda I-A)^{-1}$ beschränkt
    \end{enumerate}

    später: (1)+(2) $\Longrightarrow$ (3) (Satz vom inversen Operator)
   \item Eigenwerte: $\lambda\in\C$ Eigenwert $\Longleftrightarrow$
    $\exists v\in E, v\ne0, Av=\lambda v \Longrightarrow \lambda\in S_{A}$

    $N(\lambda I-A) = \{v\colon (\lambda I-A)v=0\}, \dim N(\lambda I-A)$
    Vielfachheit vom Eigenwert $\lambda$
   \item $A\in \linOp(E)$ kompakt, $\dim E=\infty$ $\Longrightarrow$
    $0\in S_{A}$
   \item formal: $\lambda=1, A\in\linOp(E)$
    \begin{gather*}
      (I-A)^{-1} = \frac{I}{I-A} = \underbrace{\sum_{j=0}^{\infty}
      A^{j}}_{\in \linOp(E), \text{falls} \norm{A}<1}
    \end{gather*}
    Wenn $\norm{A}< 1$ $\Longrightarrow$ $\sum_{j=0}^{\infty}
    \underbrace{\norm{A}^{j}}_{\geq \norm{A^{j}}}$
  \end{enumerate}

  \begin{description}
   \item[Frage] Wann existiert $\sum_{j=0}^{\infty} A^{j}$ in
    $\linOp(E)$:
    \begin{enumerate}
     \item $\norm{A}<1$ $\Longrightarrow$ $\exists \sum_{j=0}^{\infty}
      A^{j}\in \linOp(E)$
     \item Wurzelkriterium:
      $\exists\lim_{j\rightarrow\infty}\norm{A^{j}}^{\frac1j} < 1$
      $\Longrightarrow$ $\sum_{j=0}^{\infty} \norm{A^{j}} < \infty
      \Longrightarrow \exists \sum_{j=0}^{\infty} A^{j} \in \linOp(E)$
    \end{enumerate}
  \end{description}
\end{bemerk}

\begin{lemma}
  \label{lem:1.4.4/1}
  $E$ ist ein Banachraum und es existiere $S := \sum_{j=0}^{\infty} A^{j}\in
  \linOp{L}(E)$.

  Dann $\exists (I-A)^{-1}=S\in\linOp(E)$.

  \begin{proof}
    Es gilt: $SA = \Bigl(\sum_{j=0}^{\infty} A^{j}\Bigr) A =
    \sum_{j=0}^{\infty} A^{j+1} = \sum_{j=0}^{\infty} A^{j} - A^{0} =S-I$

    $AS = A\Bigl(\sum_{j=0}^{\infty} A^{j}\Bigr) = \sum_{j=0}^{\infty}
    A^{j+1} = S-I$

    denn: $S_{n} := \sum_{j=0}^{n} A^{j}, S_{n}
    \xrightarrow{n\rightarrow\infty} S$ in $\linOp(E)$

    $\Longrightarrow$ $\norm{S_{n}A-SA} \leq \norm{S_{n}-S} \norm{A}
    \xrightarrow{n\rightarrow\infty} 0$

    $\norm{AS_{n}-AS} \leq \norm{A}\norm{S_{n}-S} \xrightarrow{n\rightarrow\infty}0$

    $\Longrightarrow$ $I = S-SA = S(I-A)$ und $I = S-AS = (I-A)S$. Aus
    ersterem folgt $I-A$ ist injektiv, aus dem zweiten folgt $I-A$ ist
    surjektiv. Daraus folgt wiederrum, dass $(I-A)^{-1}$ existiert und
    gleich $S\in\linOp(E)$ ist.

    \begin{description}
     \item[Frage] Wann konvergiert $\sum_{j=0}^{\infty} A^{j}$?
      \begin{enumerate}
       \item $\sum \norm{A^{j}} \leq \sum \norm{A}^{j} <\infty$, falls
        $\norm{A}<1$ $\Rightarrow$ $\exists\sum A^{j}$
       \item Wurzelkriterium:
        $\exists\lim_{j\rightarrow\infty} \norm{A^{j}}^{\frac{1}{j}} = \alpha
        < 1$ $\Rightarrow$ Konvergenz oder $\alpha>1$ $\Rightarrow$ Divergenz.
      \end{enumerate}
    \end{description}
  \end{proof}
\end{lemma}

\begin{lemma}
  \label{lem:1.4.4/2}
  $a_{j}\geq 0$. Es gelte:
  \begin{gather}\label{eq:19}
    a_{j+k} \leq a_{j}a_{k}\qquad \forall j,k
  \end{gather}
  $\Longrightarrow$ $\exists$ $\lim_{j\rightarrow\infty} \sqrt[j]{a_{j}} =
  \inf_{j} \sqrt[j]{a_{j}}$

  \begin{proof}
    Es sei $a := \inf_{j} \sqrt[j]{a_{j}}$ (existiert, da $\sqrt[j]{a_{j}}\geq0$).
    Dann $\exists k\in\N\colon a \leq
    \sqrt[k]{a_{k}} < a+\epsilon$ $(\epsilon>0)$ $\Longrightarrow$ $a_{k}
    < (a+\epsilon)^{k}$

    $\forall j\in\N \exists p_{j}\in\N_{0}\exists q_{j}\in
    \{0,1,\ldots,k-1\}$ mit $j=kp_{j}+q_{j}$

    $\Longrightarrow$ $\forall j\colon a_{j} = a_{kp_{j}+q_{j}}
    \leq^{\text{\autoref{eq:19}}} a_{kp_{j}} a_{q_{j}} \leq a_{k}^{p_{j}}
    \underbrace{\max\{a_{0},a_{1},\ldots,a_{k-1}\}}_{=: M} \leq M
    a_{k}^{p_{j}}$

    $\Longrightarrow$ $a \leq \sqrt[j]{a_{j}} \leq M^{\frac1j}
    a_{k}^{p_{j}} \leq M^{\frac1j} (a+\epsilon)^{\frac{kp_{j}}{j}} =
    M^{\frac1j} (a+\epsilon)^{1-\frac{q_{j}}{j}} = (a+\epsilon)
    \frac{M^{\frac1j}}{\underbrace{\bigl((a+\epsilon)^{q_{j}}\bigr)^{\frac1j}}
      _{\xrightarrow{j\rightarrow\infty} 1}} \leq a+2\epsilon$ falls $j
    \geq j_{0}(\epsilon)$

    $\epsilon\rightarrow 0$ $\Longrightarrow$ a=$\lim_{j\rightarrow\infty}\sqrt[j]{a_{j}}$
  \end{proof}
\end{lemma}

\begin{lemma}
  $A\in\linOp(E)$. Dann existiert
  \begin{gather*}
    \lim_{j\rightarrow\infty} \norm{A^{j}}^{\frac{1}{j}}
       = \inf_{j} \norm{A^{j}}^{\frac{1}{j}}
       \leq \norm{A}
  \end{gather*}

  \begin{proof}
    Verwende \autoref{lem:1.4.4/2} mit $a_{j}=\norm{A^{j}}$.
  \end{proof}
\end{lemma}

\begin{defini}
  $A\in\linOp(E)$.
  \begin{gather*}
    r(A) :=\lim_{j\rightarrow\infty} \sqrt[j]{\norm{A^{j}}}
  \end{gather*}
  heißt \highl{Spektralradius} von $A$. ($r(A)\leq\norm{A}$)
\end{defini}

\begin{satz}\label{satz:1.4.4}
  $E$ Banachraum, $A\in\linOp(E)$
  \begin{enumerate}
   \item $\abs{\lambda}>r(A)$ $\Rightarrow$ $\exists (A-\lambda
    I)^{-1}\in\linOp(E)$ und es gilt:
    \begin{gather*}
      (\lambda I-A)^{-1} = \sum_{j=0}^{\infty} \frac{A^{j}}{\lambda^{j+1}}
         \qquad \text{(\highl{Neumann'sche Reihe})}
    \end{gather*}
    Es gilt: $\norm{(\lambda I-A)^{-1}} \leq
    \frac{1}{\abs{\lambda}-\norm{A}}$, falls $\abs{\lambda}> \norm{A}$.

   \item Die Neumannsche Reihe konvergiert \gdw{in $\linOp(E)$}
    $\abs{\lambda}>r(A)$ ist.
  \end{enumerate}

  \begin{proof}
    $\lambda I-A = \lambda(I-\frac{1}{\lambda}A)$ $\Rightarrow$
    $(\lambda I -A)^{-1} = \frac{1}{\lambda}(I-\frac{1}{\lambda}A)^{-1}$

    Es ist $r(\frac{1}{\lambda}A) = \frac{1}{\abs{\lambda}}r(A) < 1$
    $\Rightarrow$ $\exists\lim_{j\rightarrow\infty}
    \sqrt[j]{\norm{(\frac{1}{\lambda}A)^{j}}} < 1$

    \autoref{lem:1.4.4/1} $\Rightarrow$ $\exists (I-\frac{1}{\lambda}A)^{-1} =
    \sum_{j=0}^{\infty} \frac{A^{j}}{\lambda^{j}}\in\linOp(E)$
    $\Longrightarrow$ (1)

    zu (2) zeigen: $\exists\sum_{j=0}^{\infty} \frac{A^{j}}{\lambda^{j+1}}$
    $\Rightarrow$ $r(A)<\abs{\lambda}$
    $\Rightarrow$ $\norm{\frac{A^{j}}{\lambda^{j}}} \rightarrow 0$
    $\Rightarrow$ $\forall j\geq j_{0}$ ist $\norm{\frac{A^{j}}{\lambda^{j}}}
    < 1$
    $\Rightarrow$ $\frac{1}{\abs{\lambda}} (\norm{A^{j}}^{\frac{1}{j}})<1$

    $\Rightarrow$ $\frac{1}{\abs{\lambda}}\inf_{j} \norm{A^{j}}^{\frac{1}{j}}<1$

    mit \autoref{lem:1.4.4/2} $\Rightarrow$ $\frac{1}{\abs{\lambda}}\lim_{j}
    \norm{A^{j}}^{\frac{1}{j}}<1$
  \end{proof}
\end{satz}

\begin{bemerk}
  möglich: $\exists (A-\lambda I)^{-1}\in\linOp$, aber Neumannsche Reihe
  divergent.

  \begin{center}
    \input{bem1-4-4-3.pdf_t}
  \end{center}
  \begin{itemize}
   \item $\exists\lambda\in S_{A}$ mit $\abs{\lambda} = r(A)$ (Beweis:
    \cite[S.~255]{Werner})
   \item $S_{A}$ kompakt, $M_{A}$ offen (Übungen)
  \end{itemize}
\end{bemerk}

\subsection{Volterrasche Integralgleichungen}

Betrachten: $\Omega=(a,b)\subset\R$ beschränkt, $K(x,y)\colon[a,b]\times[a,b]
\rightarrow \C$ Lebesgue"=messbar mit $K\in L_{\infty}([a,b]\times[a,b])$ und
$K(x,y)=0$ für $x<y$.

\begin{center}
  \input{sec1-4-5.pdf_t}
\end{center}

\begin{defini}
  $\vIntOp$ heißt \highl{Volterra'scher Integraloperator}
  \begin{gather*}
    \vIntOp f(x) := \int_{a}^{x} K(x,y)f(y)\,dy \qquad\text{für $x\in[a,b]$}
  \end{gather*}
\end{defini}

\begin{bemerk}
  $K\in L_{\infty}([a,b]\times[a,b]) \subset L_{2}([a,b]\times[a,b])$
  $\Rightarrow$ spezieller (Fredholmscher) Integraloperator. Aus Satz 1.5.3/3
  \help{oder soll das 1.\,4.3/3 sein?} \todo{link} folgt: $\vIntOp\colon
  L_{2}\rightarrow L_{2}$ kompakt. Außerdem $0\in S_{\vIntOp}$.
\end{bemerk}

\begin{satz}
  \begin{enumerate}
   \item $K\in L_{\infty}([a,b]\times[a,b]), K(x,y) =0$ für $x<y$. Dann
    \begin{itemize}
     \item $\vIntOp\colon L_{2}([a,b]) \rightarrow L_{2}([a,b])$ kompakt
     \item $S_{\vIntOp}=\{0\}$
     \item Die Integralgleichung $\lambda f-\vIntOp f=g$ besitzt für
      $\lambda\ne$0 und $g\in L_{2}([a,b])$ genau eine Lösung $f\in
      L_{2}([a,b])$. Diese lässt sich mit Hilfe der Neumannschen Reihe
      berechnen und konvergiert in $L_{2}$.
    \end{itemize}

   \item $K\colon[a,b]\times[a,b] \rightarrow C$ sei stetig auf
    $D=\{(x,y)\colon a \leq x\leq b, a\leq y\leq x\}$ und $K(x,y)=0$ für
    $x<y$. Dann:
    \begin{itemize}
     \item $\vIntOp\colon C([a,b]) \rightarrow C([a,b])$ kompakt
     \item $S_{\vIntOp}=\{0\}$
     \item $\lambda f-\vIntOp f=g$ besitzt für jedes $g\in C([a,b])$ genau
      eine Lösung $f\in C([a,b])$. Diese lässt sich mit Hilfe der Neumannschen
      Reihe (konvergent in $C([a,b])$) berechnen.
    \end{itemize}
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item $\vIntOp\colon C([a,b]) \rightarrow C([a,b])$ kompakt.
      \begin{align*}
        \abs{\vIntOp f(x') -\vIntOp f(x)}
           &\leq \int_{a}^{x} \abs{\underbrace{K(x',y) -
           K(x,y)}_{\leq\epsilon\text{~für~}\abs{x-x'}\leq\delta_{0}(\epsilon)}}
           \abs{f(y)}\,dy
           + \int_{x}^{x'} \abs{\underbrace{K(x',y)}_{\leq\norm{K}_{\infty}}}
           \abs{f(y)}\,dy\\
        & \epsilon(b-a)\norm{f}_{\infty} + \abs{x-x'} \norm{K}_{\infty}
           \norm{f}_{8}\\
        & c\epsilon\norm{f}_{\infty}\quad\text{für~} \abs{x-x'} \leq\delta(\epsilon)
      \end{align*}
      $\Rightarrow$ $\{\vIntOp f\colon\norm{f}_{C}\leq1\} \subset C([a,b])$
      präkompakt. $\norm{\vIntOp}_{C\rightarrow C} \leq
      (b-a)\norm{K(x,y)}_{\infty}$

     \item $\vIntOp\colon L_{2}\rightarrow L_{2}$ z.\,z.
      Spektralradius~$r(\vIntOp) =0$ oder $\vIntOp\colon C\rightarrow C$
      z.\,z. Spektralradius~$r(\vIntOp)=\lim_{j\rightarrow\infty}
      \sqrt[j]{\norm{\vIntOp^{j}}}$
      \begin{align*}
        (\vIntOp^{2}f)(x)
           &= \int_{a}^{x} \big(\int_{a}^{y} K(y,z) f(z)\,dz\big) K(x,y)\,dy\\
        &= \int_{a}^{x} \underbrace{\big(\int_{z}^{x} K(y,z)
           k(x,y)\,dy\big)}_{=: K_{2}(x,z)} f(z)\,dz &\text{mit Fubini}
      \end{align*}
      Iteration $\Rightarrow$
      \begin{gather*}
        (\vIntOp^{j}f)(x) = \vIntOp(\vIntOp^{j-1}f)(x)
           = \int_{a}^{x} \underbrace{\big(\int_{z}^{x} K_{j-1}(x,y)
           K(y,z)\,dy\big)}_{=: K_{j}(x,z)} f(z)\,dz
      \end{gather*}

      Es sei $m :=\norm{K(x,y)}_{\infty}$. Dann: (Induktion)
      \begin{enumerate}
       \item $\abs{K_{2}(x,z)}\leq \abs{x-z} m^{2}$
       \item Sei $\abs{K_{j}(x,z)} \leq \abs{x-z}^{j-1}
        \cdot\frac{m^{j}}{(j-1)!}$ $\Rightarrow$ $\abs{K_{j+1}(x,z)} \leq
        \abs{x-z}^{j} \frac{m^{j+1}}{j!}$
      \end{enumerate}

      \begin{proof}
        o.\,B.\,d.\,A. $z<x$
        \begin{gather*}
          \abs{K_{j+1}} (x,z)
             \leq \int_{z}^{x} \abs{K_{j}(x,y)}
             \underbrace{\abs{K(y,z)}}_{\leq m}\,dy
             \leq \frac{m^{j+1}}{(j-1)!} \int_{z}^{x} (x-y)^{j}\,dy \leq
             \frac{m^{j+1}}{j!} (x-z)^{j+1}
        \end{gather*}
      \end{proof}
      $\Rightarrow$
      \begin{align*}
        \abs{\vIntOp^{j}f(x)}
           &\leq \int_{a}^{x} \abs{K_{j}(x,y)} \abs{f(y)}\,dy &f\in C([a,b])\\
        &\leq \frac{m^{j}}{(j-1)!} \int_{a}^{x} (x-y)^{j-1}\,dy \norm{f}_{\infty}\\
        &\leq \frac{m^{j}}{j!} (x-a)^{j} \norm{f}_{\infty}
           \leq \frac{m^{j}(b-a)^{j}}{j!} \norm{f}_{\infty}
      \end{align*}
      $\Rightarrow$ $\norm{\vIntOp^{j}}_{C\rightarrow C} \leq
      \frac{m^{j}(b-a)^{j}}{j!}$ $\Rightarrow$ $r(\vIntOp)\leq m(b-a) \lim
      \frac{1}{\sqrt[j]{j}!} = 0$

      $f\in L_{2}([a,b])$ $\Rightarrow$
      \begin{align*}
        \abs{\vIntOp^{j}f(x)}^{2}
           &\leq \int_{a}^{x} \abs{K_{j}(x,y)}^{2}\,dy \norm{f}^{2}_{2}&
           \text{Hölder}\\
        &\leq \frac{m^{2j}}{((j-1)!)^{2}} \int_{a}^{x} (x-y)^{2j-2}\,dy
           \norm{f}_{2}^{2}\\
        &\leq \frac{m^{2j}}{((j-1)!)^{2}} \frac{(x-a)^{2j-1}}{2j-1}
           \norm{f}_{2}^{2}
      \end{align*}

      \begin{align*}
        \Rightarrow \norm{\vIntOp^{j}f}_{2}^{2}
           &\leq \frac{m^{2j}}{((j-1)!)^{2}} \frac{1}{2j-1} \int_{a}^{b}
           (x-a)^{2j-1}\,dx \norm{f}_{2}^{2}\\
        \Rightarrow \norm{\vIntOp^{j}f}_{2}
           &\leq \frac{m^{j}}{j!}
           \frac{(b-a)^{j}}{\sqrt{\underbrace{2j(2j-1)}_{\geq j^{2}}}}
           \norm{f}_{2}\\
        \Rightarrow \norm{\vIntOp^{j}}_{L_{2}\rightarrow L_{2}}
           &\leq \frac{m^{j}}{(j-1)!} \cdot\frac{(b-a)^{j}}{j}
      \end{align*}
      $\Rightarrow$ $r(\vIntOp) \leq m(b-a)\frac{1}{\lim \sqrt[j]{j!}} =0$
    \end{schritte}
  \end{proof}
\end{satz}

\begin{bemerk}
  $(\vIntOp f)(X) = \int_{0}^{x}f(y)\,dy$, $f\in C([0,1])$
  \begin{enumerate}
   \item Dann: $\vIntOp\colon C([0,1]) \rightarrow C([0,1])$ injektiv (HDI
    \help{Was heißt das?}) $R(\vIntOp)\subset C([0,1])$ echt, da $(\vIntOp
    f)(0)=0$

   \item $(\lambda I-\vIntOp)^{-1} g(x) = \sum_{j=0}^{\infty}
    \frac{1}{\lambda^{j+1}} \vIntOp^{j} g(x) = \sum_{j=0}^{\infty}
    \frac{1}{\lambda^{j+1}} \int_{0}^{x} K_{j}(x,y)g(y)\,dy = \dotsb$
    ($\vIntOp_{0}=I$) Summe und Integral vertauschen, falls gleichmäßig
    konvergent $\dotsb = \frac{1}{\lambda} g(x) + \int_{0}^{x}
    \big(\sum_{j=1}^{\infty} \frac{K_{j}(x,y)}{\lambda^{j+1}}\big) g(y)\,dy$
  \end{enumerate}
\end{bemerk}

\chapter{Operatoren im Hilbertraum}
\section{Hilberträume}
\subsection{Skalarprodukt und Orthogonalität}

\begin{defini}
  $H$ Vektorraum über $\K=\R,\C$

  Eine Abbildung $\sprod{\cdot,\cdot}\colon H\times H\rightarrow\K$ mit
  den Eigenschaften
  \begin{enumerate}
   \item $\forall x\in H\colon \sprod{x,x} \geq 0$
   \item $\sprod{x,x} = 0 \Leftrightarrow x=0$
   \item $\forall x,y\in H\colon \sprod{x,y} = \overline{\sprod{y,x}}$
   \item $\forall x,y\in H, \forall \lambda\in \K\colon \sprod{ \lambda x,y}
    = \lambda \sprod{x,y}$
   \item $\forall x_{1},x_{2},y \in H\colon \sprod{x_{1}+x_{2},y} =
    \sprod{x_{1},y} + \sprod{x_{2},y}$
  \end{enumerate}
  heißt \highl{Skalarprodukt}
\end{defini}

\begin{bemerk}
  Aus den Eigenschaften (3) und (4) folgt:
  \begin{gather*}
    \sprod{x,\lambda y} = \overline{\lambda} \sprod{x,y}
  \end{gather*}

  Aus den Eigenschaften (3) und (5) folgt:
  \begin{gather*}
    \sprod{x,y_{1}+y_{2}} = \sprod{x,y_{1}} + \sprod{x,y_{2}}
  \end{gather*}
\end{bemerk}

\begin{satz}
  $H$ Vektorraum, $\sprod{\cdot,\cdot}$ Skalarprodukt

  \begin{enumerate}
   \item Dann gilt die Cauchy-Schwarz'sche Ungleichung:
    \begin{gather*}
      \abs{\sprod{x,y}} \leq \sqrt{\sprod{x,x}\sprod{y,y}}
    \end{gather*}

    Gleichheit gilt genau dann wenn $x$ und $y$ linear abhänging sind.
   \item Durch $\norm{x} := \sqrt{\sprod{x,x}}$ wird eine Norm
    auf $H$ definiert.
  \end{enumerate}

  \begin{proof}
    siehe lineare Algebra oder Analysis\,2
  \end{proof}
\end{satz}

\begin{defini}
  Ein normierter Raum $H$ heißt \highl{Prä-Hilbertraum}, falls ein
  Skalarprodukt existert, mit $\norm{x} = \sqrt{\sprod{x,x}}$.
  Ein Hilbertraum ist ein vollständiger Prä-Hilbertraum.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item $\R^{n}$ mit $\sprod{x,y} = \sum_{j=1}^{n} x_{j}y_{j}$\\
    $\C^{n}$ mit $\sprod{x,y} = \sum_{j=1}^{n} x_{j}\overline{y_{j}}$
   \item $l_{2}(\N)$ mit $\sprod{x,y} := \sum_{j=1}^{\infty}
    x_{j} \overline{y_{j}}, \norm{x}_{2} =
    \Bigl(\sum_{j=1}^{\infty}\abs{x_{j}}^{2}\Bigr)^{\frac12} =
    \sqrt{\sprod{x,x}}$
   \item $L_{2}(A,\mu), (\Omega, \mathcal{A}, \mu)$ $\sigma$-endlicher
    Maßraum, $A\subset \Omega$ messbar
    \begin{gather*}
      \sprod{f,g} = \int_{A} f(x) \overline{g(x)}\,d\mu\\
      \norm{f}_{2} = \Bigl( \int_{A} \abs{f(x)}^{2}\,d\mu\Bigr)^{\frac12} =
         \sprod{f,f}^{\frac12}
    \end{gather*}

    von Interesse sind $d\mu = dx$ Lebesgue-Maß und $d\mu = w(x)dx$
    ($\int_{A}\abs{f}^{2}\,w(x)dx < \infty$)
    gewichtete $L_{2}$-Räume mit $w(x) \geq 0$ und f.\,ü. messbar
   \item $C([a,b])$ oder $R([a,b])$ sind Prä-Hilbertraum bzgl.
    \begin{gather*}
      \sprod{f,g} = \int_{a}^{b} f(x)\overline{g(x)}\,dx
    \end{gather*}
    nicht vollständig, denn
    $f_{n}(x) = \sqrt{\max\{x,\frac1n\}}, n\in \N$\\
    $\Longrightarrow$ $f_{n}(x) \rightarrow x^{-\frac13}$ bzgl.
    $\norm{\cdot}_{2}$ (s. \autoref{bsp:1.1.1/3}) Aber $x^{-\frac13}\notin
    C([0,1])$ (da nicht stetig), $\notin R([0,1])$ (da nicht beschränkt)
    \help{Irgendwas stimmt hier nicht. Thomas:
      $f_{n}(x)=(\max\{x,\frac{1}{n}\})^{-\frac{1}{3}}$}
  \end{enumerate}
\end{bsp}

\begin{satz}[Eigenschaften]
  $H, \sprod{\cdot,\cdot}$ Prä-Hilbertraum.

  Dann:
  \begin{enumerate}
   \item $x_{j} \rightarrow x\in H$, $y_{j} \rightarrow y\in H$
    $\Longrightarrow$ $\sprod{x_{j},y_{j}} \rightarrow \sprod{x,y}$
    (Stetigkeit des Skalarprodukts)
   \item
    \begin{gather*}
      \sum_{j=0}^{\infty} x_{j} = x\in H, y\in H \Longrightarrow
      \sprod{x,y} = \sum_{j=0}^{\infty} \sprod{x_{j}, y}
    \end{gather*}
   \item $U\subset H$ dichter Teilraum und $\forall u\in U\colon \sprod{x,u} =
    0$ $\Longrightarrow$ $x = 0$
   \item $\K = \R$ $\Longrightarrow$ $\sprod{x,y} = \frac14
    (\norm{x+y}^{2} - \norm{x-y}^{2})$\\
    $\K = \C$ $\Longrightarrow$ $\sprod{x,y} = \frac14
    (\norm{x+y}^{2} - \norm{x-y}^{2}) + i\frac14 (\norm{x+iy}^{2}-\norm{x-iy}^{2})$
   \item Es gilt die Parallelogrammgleichung:
    \begin{gather}\label{eq:parallelo}
      \norm{x+y}^{2} + \norm{x-y}^{2} = 2(\norm{x}^{2} + \norm{y}^{2})
    \end{gather}
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item
      \begin{align*}
        \abs{\sprod{x,y} - \sprod{x_{i},y_{i}}} &\leq
           \abs{\sprod{x-x_{j},y} + \sprod{x_{j},y} - \sprod{x_{j},y_{j}}}\\
        & \leq \abs{\sprod{x-x_{j},y}} + \abs{\sprod{x_{j},y-y_{j}}}\\
        & \leq^{Cauchy-Schw} \underbrace{\norm{x-x_{j}}}_{\leq\epsilon''}
           \norm{y_{j}} + \norm{x_{j}}
           \underbrace{\norm{y-y_{j}}}_{\leq\epsilon'} \leq \epsilon
      \end{align*}
     \item folgt aus (1)
      $s_{n} = \sum_{j=0}^{n} x_{j} \xrightarrow{n\rightarrow\infty}
      x$ $\Longrightarrow$ $\sprod{s_{n},y}
      \xrightarrow{n\rightarrow\infty} \sprod{x,y}$
     \item $x\in H \exists u_{j} \in U\colon u_{j} \rightarrow x$ nach (1)
      $\Longrightarrow$ $\sprod{x,x} = \lim_{j\rightarrow\infty}
      \sprod{x,u_{j}} = 0$ $\Longrightarrow$ $x=0$
     \item und (5): $\norm{x\pm y}^{2} = \sprod{x\pm y,x\pm y} =
      \norm{x}^{2} + \norm{y}^{2} \pm \sprod{x,y} \pm \sprod{y,x}$
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{satz}
  Ein normierter Vektorraum $H$ ist genau dann ein Prä-Hilbertraum, wenn die
  Parallelogrammgleichung~\autoref{eq:parallelo} gilt. Das Skalarprodukt
  ist dann wie in (4) definiert

  \begin{proof}
    Übung Blatt 1, Aufg. 5 oder Werner "`Funktional analysis"' S.\,199--200
  \end{proof}
\end{satz}

\begin{defini}
  $(H,\sprod{\cdot,\cdot})$ Prä-Hilbertraum

  \begin{enumerate}
   \item $x,y$ heißen \highl{orthogonal} $:\Longleftrightarrow$
    $\sprod{x,y}=0$ (schreiben $x\perp y$)
   \item $U,V\subset H$ \highl{orthogonal} $:\Longleftrightarrow$ $\forall
    u\in U, \forall v\in V \colon \sprod{u,v} =0$
   \item $U\subset H, U^{\perp} := \{ v\in H\colon \forall u\in U\colon \sprod{u,v}
    =0\}$ heißen \highl{orthogonales Komplement} von $U$.
  \end{enumerate}
\end{defini}

\begin{satz}
  $H,\sprod{\cdot,\cdot}$ Prä-Hilbertraum

  Dann
  \begin{enumerate}
   \item $x\perp y$ $\Longrightarrow$ $\norm{x}^{2} + \norm{y}^{2} =
    \norm{x+y}^{2}$ (Pythagoras)
   \item $U\subset H$ $\Longrightarrow$ $U^{\perp}$ abgeschlossener
    Teilraum von $H$
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item $\sprod{x+y,x+y} = \norm{x}^{2} + \norm{y}^{2} +
      \underbrace{\sprod{x,y}}_{=0} + \underbrace{\sprod{y,x}}_{=0}$
     \item $v_{j}\in U^{\perp}, v_{j} \rightarrow v\in H$

      $u \in U \Longrightarrow \sprod{u,v} =^{Satz1}
      \lim_{j\rightarrow\infty} \sprod{u,v_{j}} =0$

      $U^{\perp}$ linearer Raum, $v_{1}, v_{2} \in U^{\perp},
      \lambda_{1}, \lambda_{2}\in \K, u\in U$ $\Longrightarrow$ $\sprod{
      \lambda_{1}v_{1}+\lambda_{2}v_{2},u} = \lambda_{1}\sprod{v_{1},u} +
      \lambda_{2} \sprod{v_{2},u} = 0$
    \end{enumerate}
  \end{proof}
\end{satz}

\subsection{Der Riesz'sche Darstellungssatz}\label{sec:2.1.2}
\begin{description}
 \item[Vorbem.] $(l_{2})' \cong l_{2}$ $y\in l_{2} \rightarrow T_{y}(x)
  := \sum_{j=1}^{\infty} x_{j} y_{j} = \sprod{x,\overline{y}}$

  $(L_{2})' \cong L_{2}$ $g\in L_{2} \rightarrow T_{g}(f) := \int_{A}
  f(x)g(x)\,d\mu = \sprod{f,\overline{g}}$
 \item[Frage:] Ist $H' \cong H$ für $H$ Hilbertraum
 \item[Versuch:] $y\in H \rightarrow T_{y}(x) := \sprod{x,y}\in
  \K$

  Die Linearität des Skalarprodukts überträgt sich auch auf $T_{y}$.

  $\abs{T_{y}(x)} = \abs{\sprod{x,y}} \leq \norm{y} \norm{x} \Rightarrow T_{y}
  \in H', \norm{T_{y}} \leq \norm{y}$
\end{description}

\begin{defini}
  $\emptyset\ne K\subset H$, $H$ Vektorraum

  $K$ heißt konvex $:\Longleftrightarrow$ $\{\lambda_{1} x_{1}
  +\lambda_{2}x_{2}\colon x_{1},x_{2}\in K, \lambda_{1}, \lambda_{2}\geq 0,
  \lambda_{1}+\lambda_{2}=1\} \subset K$
\end{defini}

\begin{lemma}\label{lem:2.1.2/1}
  $H$ Hilbertraum, $K\subset H$ konvex und abgeschlossen
  ($K\ne\emptyset$)

  Dann existiert für jedes $x\in H$ ein eindeutig bestimmtes Element
  $y_{0}\in K$ mit
  \begin{gather*}
    \norm{x-y_{0}} = \inf_{y\in K}\norm{x-y}
  \end{gather*}

  \begin{proof}
    \begin{description}
     \item[Existenz:] Es sei $\gamma := \inf_{y\in K} \norm{x-y}$. Wählen
      $(y_{j})_{j}\subset K$ mit $\norm{x-y_{j}} \rightarrow \gamma$. Wir
      zeigen: $y_{j} \rightarrow y_{0}\in K$ ausreichend: $(y_{j})_{j}$
      ist Cauchyfolge, da $H$ vollständig und $K$ abgeschlossen.

      Wir setzen: $u=x-y_{j}, v=x-y_{k}, j\ne k$ $\Longrightarrow$ $u-v
      = y_{k}-y_{j}, u+v = 2(x-\underbrace{\frac{y_{j}+y_{k}}2}
      _{\mathclap{\in K, \text{ da $K$ konvex}}})$

      Parallelogrammgleichung \autoref{eq:parallelo} $\Longrightarrow$
      $\norm{u+v}^{2} + \norm{u-v}^{2} = 2\norm{u}^{2} + 2\norm{v}^{2}$\\
      $\Longrightarrow$
      \begin{align*}
        \norm{y_{k}-y_{j}} = 2\norm{x-y_{j}}^{2} + 2\norm{x-y_{k}}^{2}
           - 4 \norm{x-\frac{y_{j}+y_{k}}2}^{2}\\
        = 2\norm{x-y_{j}}^{2} + 2\norm{x-y_{j}}^{2} -
           (\underbrace{\norm{2x - y_{j} -y_{k}}}
            _{\geq 2\inf_{y\in K}\norm{x-y}=2\gamma})^{2}\\
        \leq 2\norm{x-y_{j}}^{2} + 2\norm{x-y_{j}}^{2} - 4\gamma^{2}
           \xrightarrow{j,k\rightarrow\infty} 0
      \end{align*}
      $\Longrightarrow$ $(y_{j})$ ist Cauchyfolge
     \item[Unität:] Annahme: $y_{0}, y_{1}$ mit $\norm{x-y_{i}} = \gamma$\\
      $\Longrightarrow$ $(y_{0}, y_{1}, y_{0}, y_{1},\ldots)$ ist eine
      Folge wie oben $\Longrightarrow$ eine Cauchyfolge $\Longrightarrow$
      $y_{0}=y_{1}$
    \end{description}
  \end{proof}
\end{lemma}

\begin{bemerk}\label{bem:2.1.2/2}
  Wenn $H$ ein Prä-Hilbertraum und $U,V$ Teilräume von $H$ sind, dann
  wollen wir schreiben: $H= U\oplus V$ $:\Longleftrightarrow$ $H=U+V
  \wedge U\perp V$ "`orthogonale Summe"'

  Eigenschaften:
  \begin{enumerate}
   \item $H = U \oplus V \Longrightarrow V = U^{\perp}$
    \begin{proof}
      Aus der Definition von $\oplus$ folgt sofort $V \subset U^{\perp}$.

      Für ein $x\in U^{\perp}$ folgt aus der Definition, dass sich $x$
      als $x = u+v (u\in U, v\in V)$ schreiben lässt. $\Longrightarrow$
      $\sprod{u,u} = \sprod{x-v,u} = \underbrace{\sprod{x,u}}_{=0,\text{
      da }x\in U^{\perp}} - \underbrace{\sprod{v,u}}_{=0, \text{ da }
      U\perp V} = 0$\\ $\Longrightarrow$ $u=0 \Longrightarrow x=v\in V$
    \end{proof}
   \item Die Darstellung von $x=u+v$ mit $u\in U, v\in V$ ist eindeutig.
    \begin{proof}
      \begin{gather*}
        \left.\begin{aligned}
                x &= u_{0} + v_{0}\\
                x &= u_{1} + v_{1}
              \end{aligned}\right\}
        \Longrightarrow u_{0}-u_{1} = v_{0}-v_{1} \Longrightarrow
        \left\{\begin{aligned}
          \sprod{u_{0}-u_{1},u_{0}-u_{1}} &= 0 \Longrightarrow
            u_{0}=u_{1}\\
          \sprod{v_{0}-v_{1},v_{0}-v_{1}} &=0
          \Longrightarrow v_{0}=v_{1}
        \end{aligned}\right.
      \end{gather*}
    \end{proof}
  \end{enumerate}
\end{bemerk}

\begin{lemma}
  $H$ Hilbertraum, $U\subset H$ \emph{abgeschlossener} Teilraum
  \begin{enumerate}
   \item Es sei $x\in H$ und $u_{0}\in U$ mit $\norm{x-u_{0}} = \inf_{u\in
    U} \norm{x-u}$ $\Longrightarrow$ $(x-u_{0}) \perp U$, d.\,h. $x-u_{0}\in
    U^{\perp}$
   \item $\forall x\in H$ existiert \emph{genau ein} $u\in U$ und $v\in
    U^{\perp}$, so daß $x= u+v$, d.\,h. $H = U\oplus U^{\perp}$
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item zeigen $x-u_{0} \perp U$. Sei $\lambda \in \C, u\in U (u \ne
      0)$ $\Longrightarrow^{Def.\,v. u_{0}}$ $\norm{x-u_{0}}^{2} \leq \norm{x-
      (u_{0}+ \lambda u)}^{2} = \sprod{x-u_{0}+ \lambda u, x-u_{0}+
      \lambda u} = \norm{x-u_{0}}^{2} - \lambda \sprod{u,x-u_{0}}
       - \overline{\lambda} \sprod{x-u_{0}, u} + \lambda
      \overline{\lambda} \norm{u}^{2}$

      Setzen: $\lambda:=\frac1{u^{2}}\sprod{x-u_{0}, u}$
      $\Longleftrightarrow$ $\sprod{x-u_{0},u} =\lambda \norm{u}^{2}$

      $\Longrightarrow$ $\norm{x-u_{0}}^{2} \leq \norm{x-u_{0}}^{2} - \lambda
      \overline{\lambda} \norm{u}^{2} - \lambda \overline{\lambda} \norm{u}^{2}
      + \lambda \overline{\lambda} \norm{u}^{2}$ $\Longrightarrow$ $0 \leq
      -\abs{\lambda}^{2} \norm{u}^{2}$ $\Longleftrightarrow$ $\lambda = 0$
      $\Longrightarrow$ $\sprod{x-u_{0}, u} =0$
     \item $x\in H$ $\Longrightarrow$ $x=u_{0}+\underbrace{x-u_{0}}_{\in
      U^{\perp}}$. Darstellung eindeutig nach \autoref{bem:2.1.2/2}.
    \end{enumerate}
  \end{proof}
\end{lemma}

\begin{defini}
  $H$ Hilbertraum, $U\subset H$ abgeschlossener Teilraum, $x\in H$

  $P_{U}x := u_{0}$ heißt \highl{orthogonale Projektion} von $x$ auf $U$
  wobei $\norm{x-u_{0}} = \inf_{u\in U} \norm{x-u}$ (oder $x=u_{0}+v_{0}$ mit
  $u_{0}\in U, v_{0}\in U^{\perp}$). $P_{U}\colon H\rightarrow H$ heißt
  \highl{orthogonaler Projektor}.
\end{defini}

\begin{bemerk}
  $P_{U} \in \linOp(H)$ und $P_{U}^{2} = P_{U}, P_{U}\big|_{U} =
  \id\big|_{U}, P_{U}\big|_{U^{\perp}} = 0$
\end{bemerk}

\begin{satz}\label{satz:2.1.2/1}
  $H$  Hilbertraum, $U\subset H$ abgeschlossener Teilraum, $V\subset H$
  Teilraum

  Dann: $H= U\oplus V$ $\Longleftrightarrow$ $V = U^{\perp}$
\end{satz}

\begin{satz}[Riesz-Fréchet]\label{satz:riesz}
  $H$ Hilbertraum. Dann gilt:
  \begin{gather*}
    T\in H' \Longleftrightarrow \exists y\in H, \forall x\in H\colon T(x) =
    \sprod{x,y}
  \end{gather*}
  $y$ ist eindeutig bestimmt und es gilt: $\norm{T}_{H'} = \norm{y}_{H}$

  \begin{proof}
    \begin{description}
     \item["`$\Leftarrow$"'] $y\in H$ $T(x) := \sprod{x,y}
      \Longrightarrow T$ linear und beschränkt, $\abs{T(x)} \leq \norm{y} \norm{x}$
      $\Longrightarrow$ $\norm{T}_{H'} \leq \norm{y}$. Für $x=y \Longrightarrow
      \abs{T(y)} = \abs{\sprod{y,y} } =\norm{y}^{2} \Longrightarrow
      \norm{T}_{H'} = \norm{y}$
     \item["`$\Rightarrow$"'] Existenz von $y$: Es sei $T\in H',
      T\ne 0$. Idee: $T(x) = 0, T(x) = \sprod{x,y}
      \Longrightarrow \sprod{x,y} =0 \Longrightarrow y\perp N(T)
      = \{ x\colon T(x) = 0\}$ $y$ ist also im orthogonalen Komplement vom
      Kern von $T$

      Nach \autoref{satz:2.1.2/1} ist $H = N(T) \oplus N(T)^{\perp}$ Es ist
      $\dim N(T)^{\perp} = 1$, da $T \colon N(T)^{\perp} \rightarrow \C$
      Isomorphismus

      $x,x'\in N(T)^{\perp}$ mit $T(x) = T(x') \Longrightarrow T(x-x') =
      0 \Longrightarrow x-x' = 0 \Longrightarrow x=x' \Longrightarrow T$
      injektiv

      $T\ne 0$ linear $\Longrightarrow$ $T(N(T)^{\perp}) = \C$ surjektiv

      $\Longrightarrow$ $N(T)^{\perp } = \Span\{y_{1}\}$ mit $\norm{y_{1}} =
      1, T(y_{1}) \ne0$

      $x\in H \Longrightarrow x=x_{0} +\lambda y_{1}$ mit $x_{0}\in N(T)$
      und eindeutig bestimmten $\lambda\in \C$ $\Longrightarrow$ $T(x) =
      \lambda T(y_{1}) = \lambda T(y_{1}) \sprod{y_{1},y_{1}} =
      \lambda \sprod{y_{1}, \underbrace{\overline{T(y_{1})} y_{1}}_{:=
      y\in N(T)^{\perp}}} = \sprod{\lambda y_{1}, y} = \sprod{x_{0} +
      \lambda y_{1}, y} = \sprod{x,y}$

      Unität: $\sprod{x,y} = \sprod{x,y^{\ast}} \forall x
      \Longrightarrow y=y^{\ast}$
    \end{description}
  \end{proof}
\end{satz}

\begin{bemerk}\label{bem:4}
  $J\colon H\rightarrow H'$ mit $J(y)(\cdot) = \sprod{\cdot,y}$ ist
  bijektiv, isometrisch und \highl{konjugiert linear} (d.\,h.
  $J(\lambda_{1} y_{1} + \lambda_{2} y_{2})(\cdot) = \sprod{ \cdot,
  \lambda_{1} y_{1} + \lambda_{2} y_{2}} = \overline{\lambda_{1}}
  \sprod{\cdot, y_{1}} + \overline{\lambda_{2}}
  \sprod{\cdot,y_{2}} = \overline{\lambda_{1}} J(y_{1})(\cdot) +
  \overline{\lambda_{2}} J(y_{2})(\cdot)$)

  $J^{\ast}\colon H\rightarrow H'$ mit $J^{\ast}(y)(\cdot) =
  \overline{\sprod{\cdot, y}}$ ist isometrisch isomorph

  $\Longrightarrow$ $H' \cong H$
\end{bemerk}

\begin{folger}
  $H$ Hilbertraum. Dann $(H')' \cong H$, d.\,h. $H$ ist \highl{reflexiv}

  \begin{proof}
    $H'$ ist Hilbertraum bzgl. $\sprod{S,T}_{H'} := \sprod{J^{-1}(S),
    J^{-1}(T)}_{H}$

    Nach \autoref{satz:riesz} ist $(H')' \cong H'$ und nach der \autoref{bem:4}
    $H' \cong H$.
  \end{proof}
\end{folger}

\subsection{Orthonormalbasis}
\begin{defini}
  $H$ Prä-Hilbertraum
  \begin{enumerate}
   \item $(x_{j})_{j=1}^{\infty}\subset H$ heißt
    \highl{Orthonormalsystem} (ONS) $:\Longleftrightarrow$
    $\sprod{x_{j}, x_{k}} = \begin{cases}0& j\ne k\\1&j=k\end{cases}$
   \item $(x_{j})_{j=1}^{\infty}$ heißt \highl{Orthonormalbasis}
    $:\Longleftrightarrow$ $(x_{j})_{j}$ ist ONS und für jedes $x\in H$
    existieren $(\alpha_{j})_{j=1}^{\infty} \subset \K$ mit
    $\sum_{j=1}^{\infty} \alpha_{j} x_{j} = x$ (Konvergent in $H$)
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item ONB ist keine Vektorbasis! d.\,h. keine endliche
    Linearkombination falls $\dim H =\infty$
   \item $(x_{j})_{j}$ ONS $\Longrightarrow$ $S=\{x_{1},\ldots,x_{n}\}$
    linear unabhängig
   \item $(x_{j})_{j}$ ONB $\Longrightarrow$ Darstellung x=
    $\sum_{j=1}^{\infty} \alpha_{j} x_{j}$ in $H$ ist eindeutig. Es gilt:
    $\sprod{x, x_{k}} = \sum_{j=1}^{\infty } \alpha_{j}\sprod{x_{j},
    x_{k}} = \alpha_{k}\quad\forall k$
   \item $(x_{j})_{j}$ ONB $\Longrightarrow$ $x=\sum_{j=1}^{\infty}
    \sprod{x, x_{j}} x_{j}$ abstrakte Fourierreihe, $\sprod{x,x_{j}}$
    Fourierkoeffizient bzgl. der ONB $(x_{j})_{j}$
   \item $\exists$ ONB in $H$ $\Longrightarrow$ $H$ ist separabel, da
    $\Span S$ dicht in $H$
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:2.1.3/1}
  $H$ Prä-Hilbertraum, $(x_{j})_{j}$ ONS.

  Dann:
  \begin{enumerate}
   \item $\norm{\alpha_{1}x_{1}+\ldots+\alpha_{n}x_{n}}^{2} =
    \abs{\alpha_{1}}^{2}+ \ldots + \abs{\alpha_{n}}^{2}\qquad\forall n\in\N$
   \item $x=\sum_{j=1}^{\infty} \alpha_{j} x_{j}$ $\Longrightarrow$
    $\exists \sum_{j=1}^{\infty} \abs{\alpha_{j}}^{2} = \norm{x}^{2}$
   \item Für $x\in H$ ist $\norm{x-\sum_{j=1}^{n}\alpha_{j}x_{j}}$
    minimal, genau dann wenn $\alpha_{j} = \sprod{x, x_{j}}
    (j=1,\ldots,n)$ Minimumeigenschaft der Fourierkoeffizienten
   \item $\forall x\in H$ gilt:
    \begin{gather*}
      \norm{x-\sum_{j=1}^{n} \sprod{x, x_{j}}
         x_{j}}^{2} = \norm{x}^{2} - \sum_{j=1}^{n} \abs{\sprod{x,x_{j}}}^{2}\\
      \sum_{j=1}^{\infty} \abs{\sprod{x,x_{j}}}^{2} \leq
         \norm{x}^{2}\taglabel{eq:bessel}
    \end{gather*}
    \autoref{eq:bessel} ist die \highl{Bessel'sche Ungleichung}.
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item $\sprod{\sum_{j=1}^{n} \alpha_{j}x_{j}, \sum_{k=1}^{n}
      \alpha_{k}x_{k}} = \sum_{j=1}^{n}\sum_{k=1}^{n} \alpha_{j}
      \overline{\alpha_{k}} \sprod{x_{j},x_{k}}
      = \sum_{j=1}^{n} \abs{\alpha_{j}}^{2}$
     \item $s_{n} = \sum_{j=1}^{n} = \sum_{j=1}^{n} \alpha_{j} x_{j}
      \rightarrow x$ in $H$ $\Longrightarrow$ $\norm{x}^{2} = \sprod{x,x} =
      \lim_{n\rightarrow\infty} \sprod{s_{n}, s_{n}}
      = \lim_{n\rightarrow\infty} \sum_{j=1}^{n} \abs{\alpha_{j}}^{2}$
     \item $\norm{x-\sum_{j=1}^{n}\alpha_{j}x_{j}}^{2} = \sprod{
      x-\sum_{j=1}^{n}\alpha_{j}x_{j}, x-\sum_{j=1}^{n}\alpha_{j}x_{j}
      } = \norm{x}^{2} + \sum_{j=1}^{n} (\alpha_{j}- \sprod{x, x_{j}})
      (\overline{\alpha_{j}} - \sprod{x_{j}, x}) =
      \norm{x}^{2} - \sum_{j=1}^{n} \alpha_{j} \sprod{x_{j}, x} -
      \sum_{j=1}^{n} \overline{\alpha_{j}} \sprod{x, x_{j}} +
      \sum_{j=1}^{n} \alpha_{j} \overline{\alpha_{j}} =
      \norm{x}^{2} + \sum_{j=1}^{n} \abs{\alpha_{j} - \sprod{x, x_{j}}}^{2} -
      \sum_{j=1}^{n} \abs{\sprod{x, x_{j}}}^{2}\label{eq:299}$
      $\Longrightarrow$ (3) und aus (3) folgt mit \autoref{eq:299} (4)
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{folger}
  $H$ Prä-Hilbertraum und $(x_{j})_{j}$ ONS

  Dann sind folgende Aussagen äquivalent:
  \begin{enumerate}
   \item $(x_{j})_{j=1}^{\infty}$ ist ONB, d.\,h. $x=\sum_{j=1}^{\infty}
    \sprod{x,x_{j}}x_{j}, \forall x\in H$
   \item
    \begin{gather}\label{eq:parseval}
      \norm{x}^{2} = \sum_{j=1}^{\infty} \abs{\sprod{x, x_{j}}}^{2}
    \end{gather}
    \highl{Parseval'sche Gleichung}
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] nach \autoref{satz:2.1.3/1} Punkt (2)
     \item[$(2)\Rightarrow(1)$] folgt aus \autoref{satz:2.1.3/1} Punkt (4)
    \end{description}
  \end{proof}
\end{folger}

\begin{satz}\label{satz:2.1.3/2}
  $H$ Hilbertraum und $(x_{j})_{j=1}^{\infty}$ ONS.

  Dann sind äquivalent:
  \begin{enumerate}
   \item $(x_{j})_{j}$ ist ONB
   \item $\forall x,y \in H$ gilt: $\sprod{x,y} =
    \sum_{j=1}^{\infty} \sprod{x, x_{j}} \sprod{x_{j}, y}$
   \item $\forall x\in H$ gilt: $\norm{x}^{2} = \sum_{j=1}^{\infty}
    \abs{\sprod{x, x_{j}}}^{2}$ Parseval \autoref{eq:parseval}
   \item $\{ x_{1}, x_{2},\ldots\}^{\perp} = \{0\}$, d.\,h. $\sprod{x,
    x_{j}} = 0\ \forall j \Longleftrightarrow x=0$
   \item $\Span\{ x_{1}, x_{2}, \ldots \}$ ist dicht in $H$.
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] durch ausrechnen:
      \begin{gather*}
        \sprod{x,y} = \sprod{\sum_{j=1}^{\infty} \sprod{x,x_{j}} x_{j},
        \sprod{\sum_{k=1}^{\infty} \sprod{y, x_{k}} x_{k}}}
          =\sum_{j=1}^{\infty} \sprod{x, x_{j}} \sprod{x_{k}, y}
      \end{gather*}
     \item[$(2)\Rightarrow(3)$] setze $x=y$ in (2)
     \item[$(3)\Rightarrow(4)$] $\sprod{x, x_{j}} = 0 \forall j
      \Longrightarrow^{\text{Parseval \autoref{eq:parseval}}} \norm{x}^{2} = 0 \Longrightarrow x=0$
     \item[$(4)\Rightarrow(5)$] $S := \{x_{1},x_{2},\dotsc\}, U := \Span
      S$ \autoref{satz:2.1.2/1} $\Longrightarrow$ $H=\overline{U}\oplus
      \overline{U}^{\perp}$, außerdem $\overline{U}^{\perp} =
      U^{\perp}\subset S^{\perp} = \{0\}$ damit ist $H=\overline{U}$, d.\,h. $U$
      dicht in $H$
     \item[$(5)\Rightarrow(4)$] $x\in H$ $\Rightarrow$
      $x=\lim_{k\rightarrow\infty} u_{k}, u_{k}\in U$

      Es sei $\sprod{x, x_{j}} = 0\forall j$ $\Rightarrow$
      $\sprod{x, u_{k}} = 0\forall k$ $\Rightarrow$ $\sprod{x,x} =
      \lim_{k\rightarrow\infty} \sprod{x, u_{k}} =0
      \Rightarrow x=0$
     \item[$(4)\Rightarrow(1)$] $x\in H, s_{n} = \sum_{j=1}^{n} \sprod{x,
      x_{j}} x_{j} \Longrightarrow (s_{n})_{n}$ ist Cauchyfolge,
      da nach Satz-1/1 gilt $n>m$ $\norm{s_{n}-s_{m}}^{2} = \sum_{j=m+1}^{n}
      \abs{\sprod{x, x_{j}}}^{2}$ und dieses konvergiert gegen 0 nach der
      Bessel'schen Ungleichung \autoref{eq:bessel} aus \autoref{satz:2.1.3/1}.

      $H$ vollständig $\Rightarrow$ $\exists, y\in H,
      y=\sum_{j=1}^{\infty} \sprod{x, x_{j}} x_{j}$

      $(x_{j})$ ONS $\Longrightarrow$ $\sprod{x, x_{j}} = \sprod{y, x_{j}} \Longrightarrow \sprod{x-y, x_{j}} =
      0\forall j \Longrightarrow^{(4)} x=y$
    \end{description}
  \end{proof}
\end{satz}

\begin{bemerk}\label{bem:5}
  $H$ Hilbertraum, $\dim H=\infty$, $\exists$ ONB $(x_{j})_{j}$
  Dann ist $H$ separabel, wenn eine abzählbare und dichte
  Teilmenge $U\subset H$ existiert. o.\,B.\,d.\,A. sei $U=\{u_{1},u_{2},\ldots\}$
  und die $u_{j}$ linear unabhängig.
\end{bemerk}

\begin{lemma}[Gram-Schmidt]\label{lem:gram}
  $U=\{u_{1},u_{2},\ldots\}$ linear unabhängig

  Dann existiert ein ONS $(x_{j})_{j=1}^{\infty}$ in $H$ mit
  \begin{enumerate}
   \item $\Span \{ u_{1},\ldots,u_{n}\} = \Span \{ x_{1},\ldots,x_{n}\}$
    für alle $n\in \N$
   \item $\Span U = \Span \{ x_{1},x_{2},\ldots\}$
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[$n=1$] $x_{1} := \frac{u_{1}}{\norm{u_{1}}}$
     \item[$n=2$] $v_{2} = u_{2} - \sprod{u_{2}, x_{1}} x_{1}$
      $\Longrightarrow$ $\sprod{v_{2}, x_{1}} = 0$\\
      $x_{2} = \frac{v_{2}}{\norm{v_{2}}}$
     \item[$n$-ter Schritt]
      \begin{align*}
        v_{n} &:= u_{k} - \sum_{j=1}^{n-1} \sprod{u_{n}, x_{j}} x_{j}\\
        x_{n} &:= \frac{v_{n}}{\norm{v_{n}}}
      \end{align*}
    \end{description}
  \end{proof}
\end{lemma}

\begin{satz}
  $H$ Hilbertraum, $\dim H=\infty$. Dann gilt:
  \begin{enumerate}
   \item $H$ ist separabel $\Longleftrightarrow$ $\exists$ ONB
    $(x_{j})_{j=1}^{\infty}$ in $H$
   \item $H$ separabel $\Longrightarrow$ $H \cong l_{2}(\N)$ isometrisch
    isomorph
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item folgt aus der \autoref{bem:5}, dem \autoref{lem:gram} und
      \autoref{satz:2.1.3/2}
     \item $J\colon H \rightarrow l_{2}(\N)$ mit $J(x) :=
      (\sprod{x,x_{j}})_{j=1}^{\infty}$ diese Abbildung ist nach
      \autoref{satz:2.1.3/2} linear und injektiv. Die Parselval'sche
      Gleichung (\autoref{eq:parseval}) sagt:
      $\norm{J(x)}_{l_{2}} = \norm{x}_{H}$ auch surjektiv: $(\alpha_{j}) \in
      l_{2} \Longrightarrow x=\sum_{j=1}^{\infty} \alpha_{j} x_{j}$
      (existiert!) $\Rightarrow$ $\alpha_{j} = \sprod{x, x_{j}}$
    \end{enumerate}
  \end{proof}
\end{satz}

\subsection{Fourierreihen}
\begin{gather*}
  H=L_{2}(\pi) = L_{2,2\pi}([-\pi,\pi])\\
  \sprod{f,g} = \int_{-\pi}^{\pi} f(x)\overline{g(x)}\,dx\\
  S = \big( \frac{1}{\sqrt{2\pi}}e^{ikx}\big)_{k\in\Z}\quad\text{ist ein
     ONS}
\end{gather*}

\begin{satz}
  \begin{enumerate}
   \item $S$ ist eine ONB in $L_{2}(\pi)$, d.\,h. $\forall f\in
    L_{2}(\pi)$ gilt
    \begin{gather*}
      f = \sum_{k=-\infty}^{\infty} c_{k} e^{ikx}\\
      \text{mit } c_{k} = \frac1{2\pi} \int_{-\pi}^{\pi} f(x)
         e^{-ikx}\,dx = \frac1{\sqrt{2\pi}}
         \sprod{f,\frac1{\sqrt{2\pi}}e^{ikx} },\quad k\in\Z
    \end{gather*}
   \item $\forall f\in L_{2}(\pi)$ gilt:
    \begin{gather*}
      \norm{f}^{2} = 2\pi\sum_{k=-\infty}^{\infty} \abs{c_{k}}^{2}
    \end{gather*}
   \item Die trigonometrischen Polynome liegen dicht in $L_{2}(\pi)$
   \item $\int_{-\pi}^{\pi} f(x)e^{ikx}\,dx = 0, \forall k\in\Z$
    $\Longleftrightarrow$ $f(x)=0$ f.\,ü.
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
      \setcounter{enumi}{2}
     \item $f\in L_{2}(\pi) \Longrightarrow \exists g\in C_{2\pi}$ mit
      $\norm{f-g}_{2} < \epsilon$ Nach dem Satz von Stone-Weierstraß
      (\autoref{satz:weierstr}) existiert ein
      trigonometrisches Polynom $t$ mit $\norm{g-t}_{\infty} < \epsilon$
      $\Longrightarrow$ $\norm{f-t}_{2} \leq \norm{f-g}_{2} + \norm{g-t}_{2} \leq
      \epsilon \sqrt{2\pi} \norm{g-t}_{\infty} < \epsilon (1+\sqrt{2\pi})$
    \end{enumerate}

    Der Rest folgt aus \autoref{satz:2.1.3/2}

    \begin{gather*}
      f = \sum_{k} \sprod{f,\phi_{k}} \phi_{k} = \sum
      \underbrace{\frac1{\sqrt{2\pi}} \sprod{f,\frac1{\sqrt{2\pi}}
        e^{ikx}}}_{=:c_{k}} e^{ikx}
    \end{gather*}
  \end{proof}
\end{satz}

\begin{bemerk}
  Ist $f$ reellwertig, benutzt man die folgenden Schreibweise:
  \begin{gather*}
    c_{k} = \frac1{2\pi} \int_{-\pi}^{\pi} f(x)\cos kx\,dx -
    i\frac1{2\pi}\int_{-\pi}^{\pi} f(x)\sin kx\,dx = \frac12(a_{k} -
    ib_{k})
  \end{gather*}

  Man sieht jetzt, dass
  \begin{align*}
    c_{0} &= a_{0}\\
    c_{-k} &= \frac12 (a_{k}+ib_{k})\\
    \norm{f}^{2} &= \pi \Big( \frac{a_{0}^{2}}2 + \sum_{k=1}^{\infty} (a_{k}^{2}
       + b_{k}^{2}) \Big)&&&\text{Parseval'sche Gleichung \autoref{eq:parseval}}\\
    f(x) &= \frac{a_{0}}2 + \sum_{k=1}^{\infty} (a_{k}\cos kx + b_{k}\sin
       kx)\\
    a_{k} &= \frac1\pi \int_{-\pi}^{\pi}f(x)\cos kx\,dx&&&k\in\N\\
    b_{k} &= \frac1\pi \int_{-\pi}^{\pi} f(x)\sin kx\,dx&&& k\in\N
  \end{align*}
\end{bemerk}

\begin{bemerk}
  $f\in L_{p}(\pi), 1\leq p\leq\infty$ $\Longrightarrow$
  $c_{k},a_{k},b_{k}$ sind sinnvoll definiert

  $\Longrightarrow$ $s_{n} f(x) :=\sum_{k=-n}^{n} c_{k}e^{ikx}$

  Frage: $s_{n}f \xrightarrow{n\rightarrow\infty} ?$
  \begin{enumerate}
   \item $1 < p<\infty$ $\Longrightarrow$ $\norm{f-s_{n}f}_{p}
    \xrightarrow{n\rightarrow\infty} 0$ und $s_{n}f(x) \rightarrow
    f(x)$ f.\,ü. Carlson-Hunt-Theorem über f.\,ü.-konvergenz für Fourierreihen
   \item $p=1$ $\Longrightarrow$ $\epsilon f\in L_{1}$ so dass
    $\big(s_{n}f(x)\big)$ divergent f.\,ü. (Kolmogorov)
   \item $p=\infty\sim f\in C_{2\pi}$ $\Longrightarrow$ $\exists f\in
    C_{2\pi}$ so dass $\big(s_{n}f(x)\big)$ divergent in
    $x_{0}\in[-\pi,\pi]$
  \end{enumerate}
\end{bemerk}

\begin{bemerk}[Legendre-Polynome]
  $U = \{ 1, x, x^{2}, x^{3}, \ldots \}$ in $L_{2}([-1,1])$ ist linear
  unabhängig und $\overline{\Span U} = L_{2}([-1,1])$. Mithilfe des
  Schmidt-Cram kann man ein ONS in $L_{2}([-1,1])$ aus
  Polynomen konstruieren.
  \begin{gather*}
    L_{n}(x) = \frac1{2^{n}n!} \frac{d^{n}}{dx^{n}} \big(
    (x^{2}-1)^{n}\big)\qquad\text{\highl{Legendre-Polynome}}
  \end{gather*}

  $\big(\sqrt{\frac{2n+1}{2}} L_{n}\big)_{n=0}^{\infty}$ ist eine ONB.
\end{bemerk}

\section{Operatoren im Hilbertraum}
\subsection{Der adjungierte Operator}\label{sec:2.2.1}

Wir betrachten Operatoren $A\in \linOp(H_{1}, H_{2}), H_{1},H_{2}$
Hilberträume

\begin{satz}\label{satz:2.2.1/1}
  Es sei $A\in \linOp(H_{1}, H_{2})$. Dann existiert genau ein
  $A^{\ast}\in \linOp(H_{2}, H_{1})$ mit $\sprod{Ax, y}_{H_{2}} =
  \sprod{x, A^{\ast} y}_{H_{1}}\ \forall x\in H_{1}\ \forall y\in H_{2}$

  \begin{proof}
    Sei $y\in H_{2}$. Betrachten $T(x) := \sprod{Ax, y}_{H_{2}}$
    für $x\in H_{1}$. Der Operator ist linear und beschränkt:
    \begin{gather*}
      \abs{T(x)} = \abs{\sprod{Ax, y}_{H_{2}}}
         \leq \norm{Ax}_{H_{2}} \norm{y}_{H_{2}}
         \leq \underbrace{\norm{A} \norm{y}_{H_{2}}}_{\text{konst.}}
         \norm{x}_{H_{1}}\quad\forall
      x\in H_{1}
    \end{gather*}
    $\Longrightarrow$ $T\in H_{1}'$ mit $\norm{T} \leq \norm{A} \norm{y}_{2}$

    \autoref{satz:riesz} $\Longrightarrow$ es existiert genau ein
    $y^{\ast}\in H_{1}$ mit $T(x) = \sprod{x, y^{\ast}}_{H_{1}}$
    und $\norm{T} = \norm{y^{\ast}}_{1}$. Wir definieren $A^{\ast}y :=
    y^{\ast}$. $\Longrightarrow$ $\sprod{Ax, y}_{H_{2}} = \sprod{x,
    A^{\ast}y}_{H_{1}}$

    $A^{\ast}\colon H_{2} \rightarrow H_{1}$ ist linear: $\forall x\in H_{1}$:
    \begin{gather*}
      \sprod{x, A^{\ast}(\lambda_{1}y_{1}+\lambda_{2}y_{2})}_{H_{1}}
      =^{Def} \sprod{Ax, \lambda_{1}y_{1}+\lambda_{2}y_{2}}_{2} =
      \overline{\lambda_{1}} \sprod{Ax,y_{1}}_{H_{2}}+
      \overline{\lambda_{2}} \sprod{Ax,y_{2}}_{H_{2}}\\
      =
      \overline{\lambda_{1}} \sprod{x, A^{\ast} y_{1}}_{H_{1}} +
      \overline{\lambda_{2}} \sprod{x, A^{\ast} y_{2}}_{H_{1}} =
      \sprod{x, \lambda_{1}A^{\ast}y_{1}+ \lambda_{2}A^{\ast}y_{2}}
    \end{gather*}

    $\Longrightarrow$ $A^{\ast}(\lambda_{1}y_{1}+\lambda_{2}y_{2}) =
    \lambda_{1} A^{\ast}y_{1} + \lambda_{2}A^{\ast}y_{2}$.

    $A^{\ast}$ beschränkt:
    \begin{gather*}
      \norm{A^{\ast}y}_{H_{1}} = \norm{y^{\ast}}_{H_{1}} = \norm{T} \leq \norm{A}
      \norm{y}_{2}\quad\forall y\in H_{2}
    \end{gather*}
    $\Longrightarrow$ $A^{\ast}\in \linOp(H_{2}, H_{1})$ und
    $\norm{A^{\ast}} \leq \norm{A}$
  \end{proof}
\end{satz}

\begin{defini}
  $A^{\ast}$ aus \autoref{satz:2.2.1/1} heißt adjungierter Operator von
  $A\in\linOp(H)$
\end{defini}

\begin{satz}[Grundlegene Eigenschaften]\label{satz:2.2.1/2}
  Seien $A,B\in \linOp(H_{1}, H_{2})$ und $C\in \linOp(H_{2},
  H_{3}), \lambda\in\K$.

  Dann
  \begin{enumerate}
   \item $(A+B)^{\ast} = A^{\ast} + B^{\ast}$
   \item $(\lambda A)^{\ast} = \overline{\lambda} A^{\ast}$
   \item $(CA)^{\ast} = A^{\ast} C^{\ast}$
   \item $(A^{\ast})^{\ast} = A$
   \item $\norm{A^{\ast}} = \norm{A}$
   \item $\norm{AA^{\ast}} = \norm{A^{\ast}A} = \norm{A}^{2}$
   \item Wenn $A$ eine Bijektion ist (d.\,h. $\exists A^{-1}\in
    \linOp(H_{2}, H_{1})$) $\Longrightarrow$ $\exists
    (A^{\ast})^{-1}=(A^{-1})^{\ast}$
   \item $R(A)^{\perp}$ = $N(A^{\ast})$, $R(A^\ast)^{\perp} = N(A)$
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[(1)--(4)] einfach, z.\,B.
      \begin{gather*}
        \sprod{CAx, y} = \sprod{Ax, C^{\ast} y} = \sprod{x, A^{\ast}
        C^{\ast} y} \qquad \forall x,y
      \end{gather*}
      Nach \autoref{satz:2.2.1/1} folgt daraus $(CA)^{\ast} = A^{\ast} C^{\ast}$

      \begin{gather*}
        \sprod{x, A,y} = \overline{\sprod{Ay, x}} =
        \overline{\sprod{y, A^{\ast} x}} = \sprod{A^{\ast}x,y} =
        \sprod{x, (A^{\ast})^{\ast} y}
      \end{gather*}
      $\Longrightarrow$ $(A^{\ast})^{\ast} = A$

     \item[(5) und (6)]
      \begin{gather*}
        \norm{x}_{H_{1}} = 1 \Longrightarrow \norm{Ax}_{H_{2}}^{2}
           = \sprod{Ax, Ax}_{H_{2}} = \sprod{A^{\ast} Ax, x}_{H_{1}}\\
           \leq \norm{A^{\ast}Ax}_{H_{1}} \norm{x}_{H_{1}}
           \leq \norm{A^{\ast}A} \leq \norm{A^{\ast}} \norm{A}
      \end{gather*}
      $\Longrightarrow$ $\norm{A}^{2} \leq \norm{A^{\ast}A} \leq \norm{A^{\ast}}
      \norm{A}$\\
      $\Longrightarrow$ $\norm{A} \leq \norm{A^{\ast}}
      \leq^{\text{\autoref{satz:2.2.1/1}}} \norm{A}$

      $\Longrightarrow$ $\norm{A} = \norm{A^{\ast}}$ und $\norm{A^{\ast}A} =
      \norm{A}^{2}$ sowie $\norm{AA^{\ast}} = \norm{A^{\ast}A}$\help{wieso?}

     \item[(7)]
      \begin{gather*}
        \left.\begin{aligned}
                A^{-1} A = I \Longrightarrow A^{\ast}(A^{-1})^{\ast} = I\\
                AA^{-1} = I \Longrightarrow (A^{-1})^{\ast} A^{\ast} = I
              \end{aligned}\right\} \Longrightarrow (A^{-1})^{\ast} = (A^{\ast})^{-1}
      \end{gather*}

     \item[(8)]
      \begin{align*}
        Ax = 0 &\Leftrightarrow \sprod{Ax, y}_{H_{2}} = 0&&\forall
           y\in H_{2}\\
        &\Leftrightarrow \sprod{x, A^{\ast}y}_{H_{1}} = 0&&\forall y\in
           H_{2}\\
        & \Leftrightarrow \sprod{x,z}_{H_{1}} = 0&&\forall
           z\in\R(A^{\ast})\\
        & \Leftrightarrow x\perp R(A^{\ast})
      \end{align*}
      $\Longrightarrow$ $R(A^{\ast})^{\perp} = N(A)$
    \end{description}
  \end{proof}
\end{satz}

\begin{folger}
  \begin{gather*}
    H = \overline{R(A)} \oplus N(A^{\ast})
       = \overline{R(A)^{\ast}} \oplus N(A)
  \end{gather*}
\end{folger}

\begin{bsp}
  \begin{enumerate}
   \item $A\in \linOp(\K^{n}) \Longrightarrow A=(a_{jk}) \Rightarrow
    A^{\ast} = (\overline{a_{kj}})$
   \item $(\mathcal{K}f)(x) = \int_{\Omega} k(x,y) f(y)\,dy, k\in
    L_{2}(\Omega\times\Omega), H=L_{2}(\Omega)$

    $\mathcal{K}\colon L_{2}(\Omega) \rightarrow L_{2}(\Omega)$ linear und
    beschränkt

    \begin{align*}
      \sprod{\mathcal{K} f,g} &= \int_{\Omega} (\mathcal{K}f)(x)
         \overline{g(x)}\,dx = \int_{\Omega} \Big(\int_{\Omega}
         k(x,y)f(y)\,dy \Big)\overline{g(x)}\,dx\\
      &=^{\text{nach satz v. Fubini \autoref{satz:fubini}}} \int_{\Omega} \Big( f(y) \int_{\Omega}
         k(x,y)\overline{g(x)}\, dx \Big)\,dy\\
      &= \int_{\Omega} f(y)
         \underbrace{\overline{\int_{\Omega} \overline{k(x,y)} g(x)\,dx}}
         _{(\mathcal{K}^{\ast}g)(y) := \int_{\Omega}
           \overline{k(x,y)}g(x)\,dx}\,dy\\
      &= \sprod{f, \mathcal{K}^{\ast}g}
    \end{align*}
  \end{enumerate}
\end{bsp}

\begin{defini}
  \begin{enumerate}
   \item $A\in \linOp(H_{1}, H_{2})$: $A$ heißt \highl{unitär}
    $:\Longleftrightarrow$ $\exists A^{-1}\in \linOp(H_{2}, H_{1})$
    und $A^{-1} = A^{\ast}$
   \item $A\in \linOp(H)$: $A$ heißt \highl{selbstadjungiert} $:\Longleftrightarrow$
    $A^{\ast} = A$
   \item $A\in \linOp(H)$: $A$ heißt \highl{normal}
    $:\Longleftrightarrow$ $A^{\ast}A = AA^{\ast}$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $A\in \linOp(H_{1}, H_{2})$ unitär $\Longleftrightarrow$
    $A$ surjektiv und $\sprod{Ax, Ay} = \sprod{x,y}
    (\forall x,y)$
   \item $A = A^{\ast} \Longleftrightarrow \sprod{Ax,y} = \sprod{x, Ay}
    (\forall x,y\in H)$
   \item $A$ normal $\Longleftrightarrow$ $\sprod{Ax, Ay} =
    \sprod{A^{\ast}x, A^{\ast}y}, (\forall x,y \in H)$
   \item Wenn $A\in \linOp(H)$ unitär oder selbstadjungiert ist,
    dann folgt daraus, dass $A$ auch normal ist.
  \end{enumerate}
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item $H, (x_{j})_{j}$ ONB und $T\colon H\rightarrow l_{2}$ mit $Tx =
    (\sprod{x,x_{j}})_{j}, (\forall x\in H)$ $\Longrightarrow$ $T\in
    \linOp(H, l_{2})$ unitär
   \item $\mathcal{K} = \mathcal{K}^{\ast} \Longleftrightarrow k(x,y) =
    \overline{k(y,x)}$ f.\,ü.
   \item Multiplikationsoperatoren: $H=L_{2}(\Omega)$ sei $\phi\in
    L_{\infty}(\Omega)$ und $M_{\phi}f(x) := \phi(x)f(x), (f\in
    L_{2}(\Omega))$ $\Longrightarrow$ $M_{\phi} \colon L_{2}(\Omega)
    \rightarrow L_{2}(\Omega)$ linear und beschränkt

    \begin{gather*}
      \norm{M_{\phi}f}_{2}^{2} = \int_{\Omega} \big(
      \underbrace{\abs{\phi(x)}}_{\leq \norm{\phi}_{\infty}} \abs{f(x)}
      \big)^{2}\,dx \leq \norm{\phi}^{2}_{\infty} \norm{f}^{2}_{2}
    \end{gather*}
    \begin{align*}
      \sprod{M_{\phi}f, g} &= \int_{\Omega} \phi(x) f(x)
         \overline{g(x)}\,dx
         = \int_{\Omega} \overline{\overline{f(x) \phi(x)}
         g(x)}\,dx\\
      &= \sprod{f, M_{\overline{\phi}}f}
    \end{align*}
    $\Rightarrow$ $M_{\phi}^{\ast} = M_{\overline{\phi}}$ $\Rightarrow$
    $M_{\phi}$ normal

    $M_{\phi}$ selbstadjungiert $\Leftrightarrow$ $\phi =
    \overline{\phi}$ f.\,ü.\\
    $M_{\phi}$ unitär $\Leftrightarrow$ $M_{\phi}M_{\overline{\phi}} =
    \id \Leftrightarrow \phi(x) \overline{\phi(x)} f(x) = f(x)\ \text{f.\,ü.,}
    (\forall f) \Leftrightarrow (\phi(x)) = 1\fue$
  \end{enumerate}
\end{bsp}

\begin{bemerk}\label{bem:2.2.1/2}
  $A\colon D(A) \rightarrow H, D(A)\subset H$ dicht

  $A$ \highl{symmetrisch} $:\Longleftrightarrow$ $\forall x,y\in D(A)\colon
  \sprod{Ax, y}= \sprod{x,Ay}$

  \emph{Satz von Hellinger-Toeplitz} (\autoref{satz:teoplitz}):\\
  $D(A) = H, A$ symmetrisch $\Longrightarrow$ $A\in \linOp(H)$ und
  $A = A^{\ast}$
\end{bemerk}

\begin{satz}\label{satz:2.2.1/3}
  Ist $A \in \linOp(H)$ selbstadjungiert, dann gilt für alle $x\in
  H$, dass das Skalarprodukt $\sprod{Ax, x}$ reellwertig ist und
  $\norm{A} = \sup_{\norm{x}=1} \abs{\sprod{Ax,x}}$

  \begin{proof}
    $A = A^{\ast} \Longrightarrow \sprod{Ax, x} = \sprod{x, Ax} =
    \overline{\sprod{Ax, x}} \Longrightarrow \sprod{Ax, x}$ reell

    setzen $\alpha := \sup_{\norm{x}=1} \abs{\sprod{Ax, x}},
    \norm{x}=1 \Rightarrow \abs{\sprod{Ax, x}} \leq \norm{Ax} \leq \norm{A}
    \Longrightarrow \alpha \leq \norm{A}$

    Es gilt:
    \begin{gather}\label{eq:234}
      \abs{\sprod{Az, z}} = \norm{z}^{2} \abs[\big]{\sprod{A\frac{z}{\norm{z}},
      \frac{z}{\norm{z}}}} \leq \alpha \norm{z}^{2}
    \end{gather}

    Außerdem gilt:
    \begin{gather*}
      \sprod{A(x+y), x+y} - \sprod{A(x-y), x-y} = 2
      \sprod{Ax, y} + 2 \sprod{Ay, x} \in \R\\
    \end{gather*}

    \begin{gather}\label{eq:235}
      \Longrightarrow \sprod{Ax, y} + \sprod{Ay + \sprod{Ay, x}}
       \leq^{\text{\autoref{eq:234}}} \frac12 \alpha \big( \norm{x+y}^{2} +
      \norm{x-y}^{2} \big) =^{\text{\autoref{eq:parallelo}}} \alpha (\norm{x}^{2} +
      \norm{y}^{2})
    \end{gather}

    Wir setzen $x=\beta u$ und $y = Au$, wobei die Norm $\norm{u}=1$ seien
    soll und $\beta = \norm{Au}$. Aus \autoref{eq:235} folgt dann:
    \begin{align*}
      \sprod{\beta Au, Au} + \sprod{A^{2} u, \beta u} &\leq
         \alpha (\beta^{2} + \beta^{2})\\
      2\beta \norm{Au}^{2} = 2\beta^{3} &\leq 2\alpha\beta^{2}\\
      \beta &\leq \alpha
    \end{align*}
    $\Longrightarrow$ $\forall u, \norm{u}=1$ gilt $\norm{Au} \leq \alpha$
    $\Rightarrow$ $\norm{A} \leq \alpha$
  \end{proof}
\end{satz}

\begin{satz}\label{satz:2.2.1/4}
  \begin{enumerate}
   \item $A\in \linOp(H)$ selbstadjungiert und $\sprod{Ax,x}
    = 0 (\forall x)$ $\Longrightarrow$ $A=0$
   \item $A \in \linOp(H)$ normal $\Longleftrightarrow$ $\norm{Ax} =
    \norm{A^{\ast}x} (\forall x\in H)$
   \item $H$ komplexer Hilbertraum ($\K =\C$) und $A\in \linOp(H)$.
    Dann gilt: $A = A^{\ast} \Longleftrightarrow \sprod{Ax, x}$ ist
    reell für alle $x\in H$.
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item $\sprod{Ax, x} = 0 (\forall x)
      \Longrightarrow^{\text{\autoref{satz:2.2.1/3}}} \norm{A} = 0 \Longrightarrow
      A=0$
     \item
      \begin{align*}
        \norm{Ax}^{2} - \norm{ A^{\ast}x}^{2} &= \sprod{Ax, Ax} -
           \sprod{A^{\ast} x, A^{\ast} x} - \sprod{A^{\ast} A x, x}
           - \sprod{AA^{\ast} x, x}\\
        &=  \sprod{(A^{\ast}A - AA^{\ast}) x, x} (\forall x\in H)
      \end{align*}
     \item
      \begin{description}
       \item["`$\Rightarrow$"'] nach \autoref{satz:2.2.1/3}
       \item["`$\Leftarrow$"']
        $\underbrace{\sprod{A(x+y), x+y}}_{\in \R} =
        \underbrace{\sprod{Ax, x}}_{\in\R} +
        \underbrace{\sprod{Ay,y}}_{\in\R} + \sprod{Ax, y} + \sprod{Ay, x}
        \Longrightarrow \sprod{Ax,y} + \sprod{Ay, x} \in \R, (\forall
        x,y\in H)$

        $\Longrightarrow$ $\Im \sprod{Ax, y} = -\Im \sprod{Ay, x}
        = - \Im \overline{\sprod{x, Ay}} = \Im \sprod{x, Ay}, (\forall
        x,y)$

        Setzen: $ix$ anstelle von $x$

        $\Longrightarrow$ $\Re \sprod{Ax, y} = \Im i\sprod{Ax,y} = \Im
        \sprod{A (ix), y} = \Im \sprod{ix, Ay} = \Im i\sprod{x, Ay} =
        \Re\sprod{x, Ay}$
      \end{description}
    \end{enumerate}
  \end{proof}
\end{satz}

\subsection{Projketionen}\label{sec:2.2.2}

$P\colon H \rightarrow H$ orthogonale Projektion $:\Longleftrightarrow$
$\exists U\subset H$ abgeschlossener Teilraum, so dass $P = P_{U}$, wobei
$P_{U}x := x_{1}$, falls $x= x_{1}+x_{2}$ mit $x_{1}\in U, x_{2}\in
U^{\perp}$ $\Longleftrightarrow$ $\norm{x-x_{1}} = \inf_{u\in U}\norm{x-u},
x_{1}\in U$

siehe \autoref{sec:2.1.2}: $P\in \linOp(H), \norm{P} = 1$

\begin{satz}
  $P\in \linOp(H)$. Dann sind äquivalent:
  \begin{enumerate}
   \item $P$ ist orthogonale Projektion
   \item $P=P^{2} = P^{\ast}$
   \item $P^{2} = P$ und für alle $x\in H$ gilt: $\sprod{Px, x}
    \geq 0$
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] Es sei $P = P_{U}$ $\Longrightarrow$
      $P_{U}^{2} x = P_{U}x, (\forall x) \Longrightarrow P_{U}^{2} = P_{U}$

      $x,y\in H \Longrightarrow \sprod{P_{U}x,y} =
      \sprod{\underbrace{P_{U}x}_{\in U}, \underbrace{P_{U}y}_{\in U}+
      \underbrace{(y- P_{U}y)}_{\in U^{\perp}}} = \sprod{P_{U} x, P_{U}y}
      = \sprod{\underbrace{(P_{U}x-x)}_{\in U^{\perp}}+x,
      \underbrace{P_{U}y}_{\in U}} = \sprod{x, P_{U}y} \Longrightarrow
      P_{U}^{\ast} = P_{U}$
     \item[$(2)\Rightarrow(3)$] $x\in H$ $\Rightarrow$ $\sprod{Px,x} =
      \sprod{P^{2}x,x} = \sprod{Px, Px} \geq0$
     \item[$(3)\Rightarrow(1)$] setzen $U:= R(P) = \{Px\colon x\in H\} = \{ z\colon
      Pz = z\}$ Es sei $x\in H \Rightarrow x = \underbrace{Px}_{\in U}+
      \underbrace{(x - Px)}_{\in N(P)}$ mit $Px\in
      U, P(x-Px) = Px-P^{2}x = 0 \Rightarrow x-Px\in N(P)$

      Es ist $U\cap N(P) = \{0\}$, denn $x= Px\in U\cap N(P)
      \Longrightarrow Px = 0 \Longrightarrow x=0$.

      noch zu zeigen: $U\perp N(P)$\\
      Es sei $x=x_{1}+x_{2}$ mit $x_{1}-Px, x_{2}\in N(P)$

      Vor.: $0 \leq \sprod{Px, x} = \sprod{x_{1}, x_{1}+x_{2}}
      = \sprod{x_{1}, x_{1}} + \sprod{x_{1},x_{2}}$
      \begin{gather}\label{eq:321}
        \Longrightarrow -\norm{x_{1}}^{2} \leq \sprod{x_{1},
        x_{2}} \quad \forall x_{1}\in U, x_{2}\in N(P)
      \end{gather}

      Annahme: $\sprod{x_{1}, x_{2}} =: \overline{\alpha} \ne 0
      (x_{1} \ne 0)$\\
      setzen: $y_{1} := x_{1}, y_{2} = -\frac{2}{\alpha} \norm{x_{1}}^{2}
      x_{2} \in N(P)$

      \autoref{eq:321} $\Longrightarrow -\norm{x_{1}}^{2} = -\norm{y_{1}}^{2}
      \leq \sprod{y_{1}, y_{2}} = \sprod{x_{1}, -\frac{2}{\alpha}
      \norm{x_{1}}^{2} x_{2}} = -\frac{2}{\alpha} \norm{ x_{1}}^{2}
      \underbrace{\sprod{x_{1},x_{2}}}_{=\overline{\alpha}} = -2
      \norm{x_{1}}^{2} \Longrightarrow -\norm{x_{1}}^{2} = -2 \norm{x_{1}}^{2}
      \Longrightarrow \norm{x_{1}} =0$ Widerspruch zu $(x_{1}\ne0)$
      \end{description}
  \end{proof}
\end{satz}

\subsection{Kompakte Operatoren}\label{sec:2.2.3}
$A\in \linOp(H)$ Ziel: $A$ kompakt $\Longleftrightarrow$ $A^{\ast}$
kompakt

\begin{lemma}\label{lem:2}
  $A\in\linOp(H), \dim R(A) < \infty (=n)$ ("`finite rank"'
  operator oder finit oder ausgeartet)

  Daraus folgt:
  \begin{gather*}
    Ax = \sum_{j=1}^{n} \sprod{x, e_{j}^{\ast}} e_{j}
  \end{gather*}
  mit $(e_{j})_{j=1}^{n}$ ONS in $H$, $e_{j}^{\ast}\in H$

  $\dim R(A^{\ast}) = \dim R(A)$ und $A^{\ast}y = \sum_{j=1}^{n}
  \sprod{y, e_{j}} e_{j}^{\ast}$

  \begin{proof}
    $\dim R(A)=n$ $\Longrightarrow$ $\exists$ ONS $(e_{j})_{j=1}^{n}$ mit
    $Ax = \sum_{j=1}^{n} \sprod{Ax, e_{j}} e_{j} = \sum_{j=1}^{n}
    \sprod{x, \underbrace{A^{*}e_{j}}_{=: e_{j}^{\ast}}} e_{j}$

    $x,y\in H \Longrightarrow \sprod{Ax, y} = \sprod{
    \sum_{j=1}^{n} \sprod{x, e_{j}^{\ast}} e_{j}, y} =
    \sum_{j=1}^{n} \sprod{x, e_{j}^{\ast}} \sprod{e_{j}, y}
    = \sprod{x, \sum_{j=1}^{n} \sprod{y, e_{j}}, e_{j}^{\ast}}$

    $\Longrightarrow$ $A^{\ast} y = \sum_{j=1}^{n} \sprod{y, e_{j}}
    e_{j}^{\ast}\quad\forall y\in H$ $\Longrightarrow$ $\dim R(A^{\ast})
    \leq n = \dim R(A)$

    Andererseits: $\dim R(A) = \dim R\big((A^{\ast})^{\ast}\big) \leq
    \dim R(A^{\ast})$
  \end{proof}
\end{lemma}

\begin{satz}\label{satz:2.2.3}
  Ein Operator $A\in \linOp(H)$ ist genau dann
  \highl[Operator!kompakt]{kompakt}, wenn eine der beiden Aussagen gilt:
  \begin{enumerate}
   \item Es existiert eine Folge $(A_{n})_{n}$ finiter Operatoren ($\dim
    R(A_{n})\leq\infty$), die gegen $A$ konvergiert.
   \item $A^{\ast}$ ist kompakt.
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item
      \begin{description}
       \item["`$\Leftarrow$"'] klar nach \autoref{fol:1}
       \item["`$\Rightarrow$"'] Es sei $A$ kompakt, $A\in \linOp(H)$
        $\Longrightarrow$ $M= \{ Ax \colon \norm{x} \leq 1\}$ präkompakt in $H$

        Es sei $\epsilon>0$. Nach \autoref{satz:1.1.3/2} existiert für $M$ ein
        endliches $\epsilon$-Netz $\{x_{1},\ldots,x_{N}\}$, d.\,h.
        \begin{gather*}
          M\subset \bigcup_{j=1}^{N} K_{\epsilon}(x_{j})
        \end{gather*}
        Es sei $U= \Span\{x_{1},\ldots,x_{N}\} \Longrightarrow \dim U
        \leq N < \infty$. Daraus folgt $U$ ist auch ein abgeschlossener
        Teilraum. Es sei $P=P_{U}$ die zugehörige orthogonale Projektion.
        $\Longrightarrow$ $B:= PA$ ist finiter Operator, da $\dim R(B)
        \leq N$

        Es sei $\norm{x} \leq 1 \Rightarrow \norm{Ax - PAx} = \inf_{u\in U}
        \norm{Ax-u} \leq \min_{j=1,\ldots,N} \norm{Ax -x_{j}} \leq \epsilon$

        Weiterhin gilt für alle $x$ mit $\norm{x}=1$: $\norm{A-PA}\leq \epsilon$.
      \end{description}
     \item $A$ sei also nun kompakt. Aus (1) folgt, dass eine Folge
      $(A_{n})_{n}$ finiter Operatoren existiert, die gegen $A$
      konvergiert. \autoref{satz:2.2.1/2} liefert
      \begin{gather*}
        \norm{A_{n}^{\ast}- A^{\ast}} = \norm{A_{n}-A}
        \xrightarrow{n\rightarrow\infty} 0
      \end{gather*}

      Aus \autoref{lem:2} folgt $\dim R(A^{\ast}_{n})= \dim R(A_{n}) <
      \infty$ und (1) $A^{\ast}$ kompakt.

      $A^{\ast}$ kompakt $\Longrightarrow$ $(A^{\ast})^{\ast} = A$ kompakt
    \end{enumerate}
  \end{proof}
\end{satz}

\section{Spektraltheorie kompakter Operatoren}
\subsection{Die Fredholm'sche Alternative}\label{sec:2.3.1}

Ziel der Spektraltheorie ist das Lösen von Gleichungen der Form $Ax=y$
mit $A\in\nolinebreak\linOp(H)$ und $\dim H=\infty$

Treffen wir eine kleine Annahme: $\exists A^{-1}\in \linOp(H)$ und
$A$ sei kompakt. Dann wäre auch $I= A^{-1}A$ kompakt und damit $\dim H <
\infty$. Wir können also nicht erwarten, dass sich die Gleichung $Ax=y$ mit einer
stetigen inversen Abbildung lösen lässt.

Die Fredholm'sche Alternative betrachtet stattdessen Gleichungen der Form
$(A-\lambda I)x =y$ ($\lambda \ne0$). Setzen wir o.\,B.\,d.\,A. $\lambda=1$
\help{Warum gilt o.\,B.\,d.\,A.? Warum machen wir das?} bleibt die Gleichung
\begin{gather*}
  Ax - x =y
\end{gather*}

\begin{bemerk}
  Wir wissen aus \autoref{sec:2.2.1}: $H= \overline{R(A-I)}\oplus N(A^{\ast}-I) =
  \overline{R(A^{\ast}-I)} \oplus N(A-I)$, d.\,h. $N(A-I)^{\perp} =
  \overline{R(A^{\ast}-I)}$
\end{bemerk}

\begin{lemma}\label{lem:3}
  Wenn $A\subset \linOp(H)$ kompakt, dann existiert ein $c>0$ mit
  \begin{align*}
    \norm{z} &\leq c \norm{(A-I)z}&&\forall z\in N(A-I)^{\perp}
  \end{align*}

  \begin{proof}
    Annahme: $\forall j\in \N\ \exists z_{j}\in N(A-I)^{\perp}$ mit
    \begin{gather}\label{eq:333}
      \norm{(A-I) z_{j}}\leq \frac1j
    \end{gather} und
     $\norm{z_{j}}=1$

    $A$ kompakt $\Longrightarrow$ $\{ Az_{j}\colon j\in \N \}$ ist präkompakt
    $\Longrightarrow$ $\exists$ konvergente Teilfolge $(Az_{jk})_{k=1}^{\infty}$

    $Az_{jk} \xrightarrow{k\rightarrow\infty} y\in H$ $\Longrightarrow$
    $z_{jk} = Az_{jk} - (\underbrace{A z_{jk} - z_{jk}}_{\rightarrow 0
    \text{nach \autoref{eq:333}}}) \rightarrow y$

    $\Longrightarrow$ $y \in N(A-I)^{\perp}$ andererseits gilt
    $(A-I)z_{jk} \rightarrow (A-I)y$ und nach \autoref{eq:333}
    $(A-I)z_{jk} \rightarrow 0$  $\Longrightarrow$ $y\in N(A-I)$

    $\Longrightarrow$ $y=0$ Widerspruch zu $\norm{y}=1$
  \end{proof}
\end{lemma}

\begin{satz}[Fredholmsche Alternative]\label{satz:2.3.1}
  $A\in\linOp(H)$ kompakt. Dann gilt:
  \begin{enumerate}
   \item\label{enu:1} $\dim N(A-I) <\infty$
   \item\label{enu:2} $R(A-I)$ ist abgeschlossen
   \item\label{enu:3} $R(A-I) = N(A^{*}-I)^{\perp}$ ($H=R(A-I)\oplus N(A^{*}-I)$)
   \item\label{enu:4} $R(A-I) = H$ \gdw{} $N(A-I)=\{0\}$
   \item\label{enu:5} $\dim N(A-I) = \dim N(A^{*}-I)$
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item (Beweis von \autoref{enu:1}, indirekt) Annahme $\dim N(A-I)=\infty$
      $\Rightarrow$ $\exists$ ONS $(u_{k})_{k=1}^{\infty} \subset N(A-I)$ nach
      \autoref{lem:gram} (Gram"=Schmidt) (für $H=N(A-I)$) $\Rightarrow$ $(A-I)
      u_{k} =0 \sim Au_{k}=u_{k}$ $\Rightarrow$ $\norm{Au_{k}-Au_{l}}^{2} =
      \norm{u_{k}-u_{l}}^{2} = \norm{u_{k}}^{2} + \norm{u_{l}}^{2} = 2$
      $\Rightarrow$ $\{Au_{k}\}$ nicht präkompakt, aber $\{u_{k}\}$ beschränkt
      $\Rightarrow$ Widerspruch zu $A$ kompakt

     \item (Beweis von \autoref{enu:2}) $v_{k}\in R(A-I), v_{k}\rightarrow
      v\in H$ $\Rightarrow$ $v_{k}=(A-I)u_{k}, u_{k}=x_{k}+z_{k}$ mit
      $x_{k}\in N(A-I)$ und $z_{k}\in N(A-I)^{\perp}$ $\Rightarrow$
      $v_{k}=(A-I)z_{k}$

      \autoref{lem:3} $\Rightarrow$ $\norm{z_{k}-z_{l}} \leq c
      \norm{v_{k}-v_{l}}$ $\Rightarrow$ $z_{k}\rightarrow z\in H$ und
      $v_{k}=(A-I)z_{k} \rightarrow (A-I)z = v$ $\Rightarrow$ $v\in R(A-I)$.

     \item (Beweis von \autoref{enu:3}) folgt aus $H=R(\overline{A-I}) \oplus
      N(A^{*}-I)$ (Vorbemerkung) und \autoref{enu:2} ($R(\overline{A-I}) =
      R(A-I)$)

     \item $N(A-I) = \{0\}$ $\Rightarrow$ $R(A-I)=H$, indirekt: Annahme
      $R(A-I)\subseteq H$ (echter Teilraum). Aus \autoref{enu:3} folgt mit
      \autoref{sec:2.2.3} $N(A^{*}-I)\ne\{0\}$. Also existiert eine $y_{1}\in
      N(A^{*}-I)$ mit $\norm{y_{1}}=1$, d.\,h. $(A^{*}-I)y_{1}=0$.
      $N(A-I)=\{0\}$ mit \autoref{enu:3} $\Rightarrow$ $H=R(A^{*}-I)$
      $\Rightarrow$ $\exists y_{2}\in H$ mit $(A^{*}-I)y_{2}=y_{1}, y_{2}\ne0$
      $\Rightarrow$ $(A^{*}-I)^{2}y_{2} = (A^{*}-I)y_{1} =0$, d.\,h. $y_{2}\in
      N\big((A^{*}-I)^{2}\big)$

      Iteratrion: $\exists y_{3}\ne0$ mit $(A^{*}-I)y_{3}=y_{2}$ \ldots{}
      $y_{n}\ne0$ mit $(A^{*}-I)y_{n}=y_{n-1}$.

      Eigenschaften von $(y_{n})_{n}$ ($n\geq2$)
      \begin{align*}
        (A^{*}-I)^{n}y_{n} &= (A^{*}-I)^{n-1} y_{n-1} = \dotsb= (A^{*}-I)y_{1}
           = 0\\
        (A^{*}-I)^{n}y_{n} &= (A^{*}-I)^{n-1} y_{n-1} = \dotsb= (A^{*}-I)y_{2}
           = y_{1}\ne0
      \end{align*}

      $\Rightarrow$
      \begin{gather*}
        \underbrace{N\big((A^{*}-I)^{n-1}\big)}_{H_{n-1}}
           \subsetneq \underbrace{N\big((A^{*}-I)^{n}\big)}_{H_{n}}
      \end{gather*}
      echte Teilräume $n=2,3,\dotsc$ $H_{n}$ Hilberträume $\Rightarrow$
      $H_{n}=H_{n-1}\oplus H_{n-1}^{\perp}$ mit $H_{n-1}^{\perp}\ne\{0\}$ für
      $n=2,3,\dotsc$

      wählen $z_{n}\in H_{n-1}^{\perp}$ mit $\norm{z_{n}}=1$ $A^{*}$ kompakt
      nach \autoref{satz:2.2.3} $\Rightarrow$ $\{A^{*}z_{n}\}$ präkompakt.

      Andererseits gilt: für $n>m$
      \begin{gather*}
        \norm{A^{*}z_{n}-A^{*}z_{m}} = \norm{z_{n}+ (A^{*}-I) z_{n} - z_{m} -
           (A^{*}-I)z_{m}}
      \end{gather*}
      Es gilt: $z_{n}\in N\big((A^{*}-I)^{n}\big)$ $\Rightarrow$
      $(A^{*}-I)z_{n}\in N\big((A^{*}-I)^{n}\big) = H_{n-1}$ und $z_{m}\in
      N\big((A^{*}-I)^{m}\big)$ $\Rightarrow$ $(A^{*}-I)z_{m}\in
      N\big((A^{*}-I)^{m-1} \subset N\big((A^{*}-I)^{n-1}\big)=H_{n-1})$, da
      $m<n$

      $\Rightarrow$ $\norm{A^{*}z_{n}-A^{*}z_{m}} = \norm{z_{n}+z}$ mit $z\in
      H_{n-1}$. Nach Konstruktion ist $z_{n}\perp z$

      $\Rightarrow$ $\norm{A^{*}z_{n}-A^{*}z_{m}}^{2} = \norm{z_{n}}^{2} +
      \norm{z} \geq \norm{z_{n}} \geq 1$
      \help{sollte das nicht auch $\norm{z}^{2}$ sein?}
      Widerspruch zu $\{A^{*}z_{n}\}$ präkompakt

     \item $R(A-I)=H$ $\Rightarrow$ $N(A-I)=0$. Aus \autoref{enu:3} folgt
      $N(A^{*}-I)^{\perp}=H$ $\Rightarrow$ $N(A^{*}-I)=\{0\}$ mit 4.~Schritt
      $\Rightarrow$ $R(A^{*}-I)=H$, da $A^{**}=A$ $\Rightarrow$ $N(A-I)=\{0\}$

     \item (Beweis von \autoref{enu:5}) Annahme: $\dim N(A-I) = m<n = \dim
      N(A^{*}-I) = \dim R(A-I)^{\perp}$ $\Rightarrow$ $\exists$ linear
      beschränkte Abbildung $\tilde{L}\colon N(A-I)\rightarrow R(A-I)^{\perp}$
      injektiv und \emph{nicht} surjektiv.

      Wir setzen
      \begin{gather*}
        Lx =
           \begin{cases}
             \tilde{L}x& x\in N(A-I)\\
             0& x\in N(A-I)^{\perp}\\
             \tilde{L}x_{1}& x=x_{1}+x_{2}, x_{1}\in N(A-I), x_{2}\in
             N(A-I)^{\perp}
           \end{cases}
      \end{gather*}
      $\Rightarrow$ $L\colon H\rightarrow R(A-I)^{\perp}$ linear und kompakt,
      da $\dim R(L)=m$ $\Rightarrow$ $K=A+L$ kompakt.

      Außerdem: $N(K-I)=\{0\}$, denn $0=(K-I)u = Au+Lu-u$ $\Rightarrow$ $Lu =
      u-Au$ $\Rightarrow$ $(A-I)u \in R(A-I)^{\perp} \cap R(A-i)$
      $\Rightarrow$ $(A-I)u=0$ $\Rightarrow$ $Lu=0$ $\Rightarrow$ $u=0$, da
      $L$ injektiv auf $N(A-I)$.

      Es sei nun $v\in R(A-I)^{\perp}$ und $v\notin R(L)$ (da $L$ injektiv,
      aber nicht surjektiv)

      Aus \autoref{enu:4} folgt: $R(K-I)=H$. $\Rightarrow$ $\exists u\wedge
      (K-I)u=v$ $\Rightarrow$ $(A-I)u = v-Lu\in R(A-I)^{\perp}\cap R(A-I)$
      $\Rightarrow$ $(A-I)u=0$ $\Rightarrow$ $v=Lu\in R(L)$ Widerspruch

      $\Rightarrow$ $\dim N(A-I) \geq \dim N(A^{*}-I)$.

      Vertauschen von $A$ und $A^{*}$ und ausnutzen von $A=A^{**}$ liefert
      $\dim N(A^{*}-I)\geq N(A-I)$.
    \end{schritte}
  \end{proof}
\end{satz}

\begin{folger}[Fredholmsche Alternative]
  \begin{enumerate}
   \item Entweder ist $u-Au=v$ eindeutig lösbar für alle $v\in H$ oder
    $u-Au=0$ ist nichttrivial lösbar.

   \item Der Raum aller Lösungen der homogenen Gleichung $u-Au=0$ ist
    endlichdimensional. Besitzt die Gleichung $u-Au=0$ eine nichttriviale
    Lösung, dann ist die Gleichung $u-Au=v$ lösbar, genau dann wenn $v\in
    N(A^{*}-I)^{\perp}$.

   \item Wenn $u-Au=v$ eindeutig lösbar für alle $v\in H$ ist, dann existiert
    $(A-I)^{-1}\in\linOp(H)$.

    \begin{proof}
      $N(A-I) = \{0\}$ $\Rightarrow$ $N(A-I)^{\perp} = H$, nach
      \autoref{lem:3} $\Rightarrow$ $\norm{z}\leq c \norm{(A-I)z}$ $\forall
      z\in H$

      $\forall y\in H\colon \norm{(A-I)^{-1}y}\leq c\norm{y}$ beschränkt
    \end{proof}
  \end{enumerate}
\end{folger}

\subsection{Das Spektrum kompakter Operatoren}

$M_{A}=\{\lambda\in\C\colon \exists(A-\lambda I)^{-1}\in\linOp(H)\}$
\highl{Resolventenmenge}, $S_{A}=\C\setminus M_{A}$ Spektrum, $A\colon
H\rightarrow H$ linear und beschränkt, $\lambda$ heißt \gdw{\highl{Eigenwert}
mit Vielfachheit~$n$} $\dim N(A-\lambda I)=n$, $n$ Eigenvektor zu $\lambda$
\gdw{} $u\ne0\quad Au=\lambda u$, $\lambda$ Eigenwert $\Rightarrow$
$\lambda\in S_{A}$

\begin{satz}\label{satz:2.3.2}
  Ist $A\in\linOp(H)$ kompakt und $\dim H=\infty$, dann gelten folgenden
  Aussagen:
  \begin{enumerate}
   \item\label{enu:6} Das Spektur~$S_{A}$ besteht aus $0$ und höchstens abzählbar vielen
    Eigenwerten endlicher Vielfachheit verschieden von $0$.
   \item\label{enu:7} Wenn abzählbar unendlich viele Eigenwerte existieren, so häufen sich
    diese in $0$.
   \item\label{enu:8} Es gilt $S_{A^{*}}=\{\overline{\lambda}\colon\lambda\in S_{A}\}$
  \end{enumerate}

  \begin{proof}
    zu \autoref{enu:6} $0\in S_{A}$ folgt aus \autoref{sec:1.4.4} (sonst
    $I\colon H\rightarrow H$ kompakt $\Rightarrow$ $\dim H<\infty$)

    $\lambda\ne0$, $\lambda\in S_{A}$, $\lambda$ kein Eigenwert $\Rightarrow$
    $N(A-\lambda I)=\{0\}$ $\Rightarrow$ $N(\frac{1}{\lambda}A-I)=\{0\}$ mit
    \autoref{sec:2.3.1} $\Rightarrow$ $R(\frac{1}{\lambda}A-I)=H$ und
    Folgerung $\exists (\frac{1}{\lambda}A-I)^{-1}\in\linOp(H)$ $\Rightarrow$
    $\lambda\in M_{A}$

    $\dim N(A-\lambda I)<\infty$ ebenfalls nach \autoref{satz:2.3.1}
    ($\lambda\ne0$)

    zu \autoref{enu:7} Es sei $R>0$. Wir zeigen: $\exists$ höchstens endlich
    viele Eigenwerte $\lambda$ mit $\abs{\lambda}\geq R$.

    Annahme: $\exists (\lambda_{j})_{j=1}^{\infty}, \lambda_{j}\ne\lambda_{k}$
    Folge von Eigenwerten $\abs{\lambda_{j}}\geq R$. Es sei $(x_{j})_{j}$ die
    Folge zugehöriger Eigenvektoren: $Ax_{j}=\lambda_{j}x_{j}, \norm{x_{j}}=1$
    $\Rightarrow$ $(x_{j})_{j=1}^{\infty}$ linear unabhängig (durch Induktion)
    $\{x_{1},\dotsc, x_{n}\}$ linear unabhängig $\Rightarrow$ sei
    $\alpha_{1}x_{1}+\dotsb+ \alpha_{n} x_{n} + \alpha_{n+1}x_{n+1}=0$

    Multiplikation mit $\lambda_{n+1}$ $\Rightarrow$
    $\alpha_{1}\lambda_{n+1}x_{1} + \dotsb+ \alpha_{n}\lambda_{n+1}x_{n} +
    \alpha_{n+1}\lambda_{n+1}x_{n+1}=0$

    Multiplikation mit $A$ $\Rightarrow$ $\alpha_{1}\lambda_{1}x_{1} + \dotsb+
    \alpha_{n}\lambda_{n}x_{n} + \alpha_{n+1}\lambda_{n+1}x_{n+1}=0$

    $\Rightarrow$ $\alpha_{1}(\lambda_{n+1}-\lambda_{1})x_{1} + \dotsb+
    \alpha_{n}(\lambda_{n+1}-\lambda_{n})x_{n} = 0$

    linear unabhängig und $\lambda_{j}\ne\lambda_{k}$ $\Rightarrow$
    $\alpha_{1}=\dotsb=\alpha_{n}=0$ $\Rightarrow$ $\alpha_{n+1}=0$, da
    $\lambda_{n+1}\geq R$

    Es sei $H_{n}:=\Span\{x_{1},\dotsc, x_{n}\}$. Es ist $H_{n}=H_{n-1}\oplus
    H_{n-1}^{\perp}$ (orthogonales Komplement bzgl. $H_{n}$).

    Wir wählen $u_{n}\in H_{n}\cap H_{n-1}^{\perp}$ mit $\norm{u_{n}}=1$. Für
    $m<n$ gilt $H_{m-1} \subsetneq H_{m} \subseteq H_{n-1} \subsetneq H_{n}$.
    Außerdem $(A-\lambda_{n}I)(H_{n}) \subset H_{n-1}$, da
    $(A-\lambda_{n}I)(\sum_{j=1}^{n} \alpha_{j}x_{j}) = \sum_{j=1}^{n-1}
    \alpha_{j}(\lambda_{j}-\lambda_{n})x_{j}$

    $\Rightarrow$
    \begin{align*}
      \norm{Au_{n}-Au_{m}}
         &= \norm{\underbrace{(A-\lambda_{n}I)u_{n}}_{\in H_{n-1}}
         + \lambda_{n}u_{n}-\lambda_{m}u_{m} +
         \underbrace{(\lambda_{m}I-A)u_{m}}_{\in H_{m-1}}}\\
      &= \norm{\lambda_{n}u_{n}-u}& \text{mit~}u\in H_{n-1}, u_{n}\in
         H_{n-1}^{\perp}\\
      &= (\abs{\lambda_{n}}^{2} \norm{u_{n}}^{2} + \norm{u}^{2})^{\frac{1}{2}}
         \geq \abs{\lambda_{n}} \norm{u_{n}} \geq R
    \end{align*}
    $\Rightarrow$ $(u_{n})_{n}$ beschränkt. $(Au_{n})_{n}$ nicht präkompakt
    $\Rightarrow$ Widerspruch zu $A$ kompakt.

    zu \autoref{enu:8} $\overline{\lambda}\in M_{A}$ $\Rightarrow$
    $\exists(A-\overline{\lambda}I)^{-1}\in\linOp(H)$. Nach
    \autoref{satz:2.2.1/2} $\Rightarrow$ $\exists\big(
    (A-\overline{\lambda}I)^{*}\big)^{-1} = \big(
    (A-\overline{\lambda}I)^{-1}\big)^{*}$ $\Rightarrow$ $\exists
    (A^{*}-\lambda I)^{-1}\in\linOp(H)$ $\Rightarrow$ $\lambda\in M_{A^{*}}$

    gleiches Argument mit $A^{*}$: $\lambda\in M_{A}$ $\Rightarrow$
    $\overline{\lambda}\in M_{(A^{*})^{*}} = M_{A}$ $\Rightarrow$ $\lambda\in
    M_{A^{*}}$ $\Leftrightarrow$ $\overline{\lambda}\in M_{A}$
  \end{proof}
\end{satz}

\subsection{Spektraldarstellung kompakter selbstadjungierter Operatoren}
\label{sec:2.3.3}

Betrachen $A\in\linOp(H)$ kompakt, $A^{*}=A$.

wissen: \autoref{satz:2.3.2} $\Rightarrow$ $S_{A} = \{0\}\cup \{
\text{Eigenwerte endlicher Vielfachheit}\}$ höchstens abzählbar unendlich
viele, dann 0 einziger Häufungspunkt.

weiterhin: \autoref{satz:1.4.4} $A\in\linOp(H)$ $\Rightarrow$ $S_{A}\subset
\{\lambda\in\C\colon \abs{\lambda}\leq r(A)\}$ Spektralradium
$r(A)=\lim_{j\rightarrow\infty} \sqrt[j]{\norm{A}^{j}} \leq\norm{A}$

\autoref{satz:2.2.1/3} $A\in\linOp(H), A=A^{*}$ $\Rightarrow$
\begin{gather*}
  \norm{A} = \sup_{\norm{x}=1} \abs{\sprod{Ax,x}}
\end{gather*}

\begin{lemma}\label{lem:4}
  $H$ Hilbertraum
  \begin{enumerate}
   \item $A\in\linOp(H), A=A^{*}$ $\Rightarrow$ Eigenwerte sind reell,
    Eigenvektoren zu verschiedenen Eigenwerten stehen senkrecht aufeinander

   \item $A\in\linOp(H)$ kompakt, $A=A^{*}$ $\Rightarrow$ es existiert ein
    Eigenwert~$\lambda$ mit $\abs{\lambda}=\norm{A}=\sup_{\norm{x}=1}
    \abs{\sprod{Ax,x}}$
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item $\lambda\sprod{x,x}=\sprod{Ax,x} =^{A=A^{*}} \sprod{x,Ax} =
      \overline{\lambda}\sprod{x,x}$ $\Rightarrow$ $\lambda=\overline{\lambda}$

      $Ax= \lambda x$ und $Ay=\mu y, \mu\ne\lambda$ $\Rightarrow$
      $\lambda\sprod{x,y}=\sprod{Ax,y}=\sprod{x,Ay}=\mu\sprod{x,y}$
      $\Rightarrow$ $\sprod{x,y}=0$

     \item $\alpha:=\sup_{\norm{x}=1} \abs{\sprod{Ax,x}}$ $\Rightarrow$
      $\exists (x_{n})_{n}, \norm{x_{n}}=1$ mit $\abs{\sprod{Ax_{n},x_{n}}}
      \rightarrow \alpha$

      $A$ kompakt und $\sprod{Ax_{n},x_{n}}$ beschränkt $\Rightarrow$
      o.\,B.\,d.\,A. $Ax_{n}\rightarrow\tilde{x}\in H$ (sonst Teilfolge),
      $\sprod{Ax_{n},x_{n}}\rightarrow\lambda$ $\abs{\lambda}=\alpha$

      $\Rightarrow$
      \begin{gather*}
        0\leq\norm{Ax_{n}-\lambda x_{n}}^{2}
           = \underbrace{\norm{Ax_{n}}^{2}}_{\leq(\norm{A}\norm{x_{n})^{2}}
           \leq\alpha^{2} =\lambda^{2}}
           - 2 \lambda \sprod{Ax_{n},x_{n}} + \lambda^{2}
           \leq 2\lambda^{2} - 2\lambda\sprod{Ax_{n},x_{n}}
           \xrightarrow{n\rightarrow\infty} 0
      \end{gather*}

      Außerdem: $\lambda x_{n} = Ax_{n} - (\underbrace{A-\lambda
      I}_{\rightarrow0})x_{n} \rightarrow \tilde{x}$

      $\Rightarrow$ $x_{n}\rightarrow\frac{\tilde{x}}{\lambda} = x$

      $A\in\linOp(H)$ $\Rightarrow$ $Ax_{n} \rightarrow Ax
      =^{\tilde{x}=\lambda x} \lambda x$
    \end{enumerate}
  \end{proof}
\end{lemma}

\begin{satz}\label{satz:2.3.3/1}
  $H$ Hilbertraum $\dim=\infty$, $A\in\linOp(H)$ kompakt und $A^{*}=A$. Dann
  gilt:
  \begin{enumerate}
   \item Das Spektrum besteht aus 0 und abzählbar unendlich vielen reellen
    Eigenwerten endlicher Vielfachheit, die sich in 0 häufen.

   \item Es sei $(\lambda_{j})_{j}$ eine Folge aller Eigenwerte, geordnet nach
    Vielfachheit und der Größe der Beträge:
    \begin{gather*}
      \abs{\lambda_{1}} \geq \abs{\lambda_{2}} \geq \dotsb\geq
         \abs{\lambda_{j}} \geq \dotsb \rightarrow 0
    \end{gather*}
    Dann existiert ein ONS von Eigenvektoren $(x_{n})_{n=1}^{\infty}$ mit
    \begin{enumerate}
     \item $Ax_{j} = \lambda_{j}x_{j}$ und
     \item ist $H_{n}:= \{x\in H\colon\sprod{x,x_{j}}=0 \forall x_{j},
      j=1,\dotsc,n-1\}$ so gilt:
      \begin{gather*}
        \abs{\lambda_{n}}
           = \sup_{\substack{x\in H_{n}\\\norm{x}=1}} \abs{\sprod{Ax,x}}
           = \norm{A}_{\linOp(H_{n})}
      \end{gather*}
    \end{enumerate}

   \item Für alle $x\in H$ gilt (Spektraldarstellung):
    \begin{gather*}
      Ax = \sum_{n=1}^{\infty} \lambda_{n}\sprod{x,x_{n}} x_{n}
    \end{gather*}

   \item Ist $\lambda=0$ kein Eigenwert, so ist $(x_{n})_{n}$ eine ONB in $H$.
    Insbesondere ist dann $H$ separabel.
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item es existieren abzählbar unendlich viele Eigenwerte, \autoref{lem:4}
      $\Rightarrow$ es existiert eine Eigenwert $\lambda_{1}$ mit
      $\abs{\lambda_{1}} = \sup_{\substack{x\in H\\\norm{x}=1}}
      \abs{\sprod{Ax,x}}$ wählen zugehörigen Eigenvektor~$v_{1}\in
      N(A-\lambda_{1}I)$ mit $\norm{v_{1}}=1$

      setzen $H_{2}:=\{x\in H\colon\sprod{x,v_{1}}=0\}$ und betrachten
      $A\beschr{H_{2}}\colon H_{2}\rightarrow H_{2}$, denn $\sprod{Ax,
      v_{1}} = \sprod{x,Av_{1}} = \sprod{x,\lambda_{1}x_{1}}$ $\Rightarrow$
      $\forall x\in H_{2}\colon Ax\in H_{2}$

      $A\beschr{H_{2}}$ ist kompakt und selbstadjungiert.

      \autoref{lem:4} $\Rightarrow$ $\exists$ Eigenwert $\lambda_{2}$ mit
      $\abs{\lambda_{2}} = \sup_{\substack{x\in H_{2}\\\norm{x}=1}}
      \abs{\sprod{Ax,x}} \leq\abs{\lambda_{1}}$

      wählen $v_{2}\in N(A-\lambda_{2}I)\cap H_{2}$ $\Rightarrow$, setzen
      $H_{3}:=\{x\in H_{2}\colon \sprod{x,v_{2}}=0\}$ $\sprod{v_{2},v_{1}}=0$

      \ldots{} Iterieren \ldots{}

      $H_{n}:=\{x\in H_{n-1}\colon \sprod{x,v_{n-1}=0}\} = \{x\in H\colon
      \sprod{x,v_{k}}=0, k=1,\dotsc,n-1\}$, $\lambda_{k}$ Eigenwert zu
      Eigenvektor $v_{k}\in H_{k}$, $\abs{\lambda_{k}} = \sup_{\substack{x\in
      H_{k}\\\norm{x}=1}} \abs{\sprod{Ax,x}}$, $\norm{v_{k}}=1$

      $\Rightarrow$ ONS $(v_{k})_{k}$, $H=H_{1}\supset H_{2} \supset H_{3}
      \supset \dotsb \supset H_{k} \supset \dotsb$

      Anm.: $H_{n}=\{0\}$ $\Rightarrow$ $\forall x\colon \sprod{x,v_{k}}=0$
      für $k=1,\dotsc,n$ $\Rightarrow$ $x=0$ $\Rightarrow$ $(v_{k})_{k=1}^{n}$
      ONB in $H$ $\Rightarrow$ $\dim H<\infty$ Widerspruch

      Aus \autoref{enu:6} und \autoref{enu:7} in \autoref{satz:2.3.2} folgt:
      $\abs{\lambda_{n}} \xrightarrow{n\rightarrow\infty} 0$

     \item Spektraldarstellung $(\lambda_{k})_{k}$ aus 1.~Schritt, auch
      $(v_{k})_{k}$
      \begin{align*}
        \norm{Ax - \sum_{k=1}^{n} \lambda_{k}\sprod{x,v_{k}}v_{k}}^{2}
           &= \norm{A - \underbrace{\big(x-\sum_{k=1}^{n}\sprod{x,v_{k}}
           v_{k}\big)}_{\in H_{n+1}, \text{~da~}\perp H_{n}}}^{2}\\
        & \leq \underbrace{\norm{A}^{2}_{\linOp(H_{n+1})}}
           _{=\abs{\lambda_{n+1}\text{~nach \autoref{satz:2.2.1/3}}}}
           \norm{x- \sum_{k=1}^{n}\sprod{x,v_{k}} v_{k}}^{2}\\
        &\leq \abs{\lambda_{n+1}} \big(\norm{x}^{2} - \sum_{k=1}^{n}
           \abs{\sprod{x,v_{k}}v_{k}}^{2}\big) \leq \abs{\lambda_{n+1}}
           \norm{x}^{2} \xrightarrow{n\rightarrow\infty} 0\quad\text{nach
           1.~Schritt}
      \end{align*}

     \item Annahme: Es existiert der Eigenwert $\lambda\ne0,
      \lambda\ne\lambda_{k} \forall k$ mit $Av=\lambda v, v\ne0$

      $\Rightarrow$ $\lambda v = Av =
      \sum_{k=1}^{\infty}\lambda_{k}\sprod{v,v_{k}} v_{k}$ $\Rightarrow$
      $\lambda\sprod{v,v_{k}} = \lambda_{j}\sprod{v,v_{j}}$ $\Rightarrow$
      $\sprod{v,v_{j}}=0 \forall j$ $\Rightarrow$ $\lambda v=Av=0$
      $\Rightarrow$ Widerspruch zu $v\ne0$

     \item $\lambda=0$ kein Eigenwert, $x\in H$, $(v_{k})_{k=1}^{\infty}$ ONS
      aus 1.~Schritt
      \begin{align*}
        &\Rightarrow \sum_{k=1}^{\infty} \abs{\sprod{x,v_{k}}}^{2} < \infty
           &\text{nach Besselscher Ungleichung \autoref{satz:2.1.3/1}}\\
        &\Rightarrow s_{n} := \sum_{k=1}^{n} \sprod{x,v_{k}} v_{k}
           &\text{ist Cauchyfolge}\\
        &\Rightarrow \exists y\in H, \sum_{k=1}^{\infty} \sprod{x,v_{k}} v_{k}
           =y\\
        &\Rightarrow Ay = \sum_{k=1}^{\infty} \lambda_{k} \sprod{x,v_{k}}
           v_{k} =Ax &\text{nach 2.~Schritt}\\
        &\Rightarrow A(x-y) = 0 \Rightarrow x=y &\text{da 0 kein Eigenwert}\\
        &\Rightarrow x=\sum_{k=1}^{\infty} \sprod{x,v_{k}} v_{k} \Rightarrow
           (v_{k})_{k} \text{~ist ONB in $H$}
      \end{align*}
    \end{schritte}
  \end{proof}
\end{satz}

Anwendung: $A\in\linOp(H)$ kompakt und selbstadjungiert, $y\in H$,
$\lambda\ne0$, gesucht $x$ mit $(A-\lambda I) x=y$
\begin{align*}
  &\Leftrightarrow \sum \lambda_{j} \sprod{x,x_{j}} -\lambda x = y\\
  &\Leftrightarrow x = -\frac{1}{\lambda} y + \frac{1}{\lambda}
     \sum_{j=1}^{\infty} \lambda_{j} \sprod{x,x_{j}} x_{j}\\
  &\Rightarrow \sprod{x,x_{k}} = -\frac{1}{\lambda} \sprod{y,x_{k}} +
     \frac{1}{\lambda} \lambda_{k} \sprod{x,x_{k}}\\
  &\Rightarrow \sprod{x,x_{k}} (1-\frac{\lambda_{k}}{\lambda}) =
     -\frac{1}{\lambda} \sprod{y,x_{k}}\\
  &\Rightarrow \sprod{x,x_{k}} = -\frac{1}{\lambda-\lambda_{k}} \sprod{y,x_{k}},
     \text{~falls $\lambda\ne\lambda_{k}$}
\end{align*}
$\lambda=\lambda_{k}$ $\Rightarrow$ $\sprod{y,x_{k}}=0$

\begin{satz}[Hilbertsche Methode]\label{satz:2.3.3/2}
  $H$ Hilbertraum, $A\in\linOp(H)$ kompakt und selbstadjungiert,
  $\lambda\ne0$. Es seien $(\lambda_{j})_{j=1}^{\infty}$ Eigenwerte und
  $(x_{j})_{j}$ ONS von Eigenvektoren wie in \autoref{satz:2.3.3/1}.
  $\Rightarrow$ Die Gleichung $(A-\lambda I) x=y$ ist genau dann lösbar, wenn
  $y\perp N(A-\lambda I)$. Durch
  \begin{gather}\label{eq:444}
    x= -\frac{1}{\lambda} y - \frac{1}{\lambda} \sum_{\lambda_{j}\ne\lambda}
       \frac{\lambda_{j}}{\lambda-\lambda_{j}} \sprod{y,x_{j}} x_{j}
  \end{gather}
  wird eine Lösung beschrieben. Sämtliche Lösungen sind durch $x+N(A-\lambda
  I)$ gegeben.

  \begin{proof}
    \autoref{eq:444} sinnvoll, da Reihe konvergent:
    \begin{gather*}
      s_{n} = \sum_{\substack{j=1\\\lambda_{j}\ne\lambda}}^{n}
         \frac{\lambda_{j}}{\lambda-\lambda_{j}} \sprod{y,x_{j}} x_{j},
         \lambda_{j} \rightarrow 0\\
      \Rightarrow \norm{s_{n}-s_{m}}^{2} = \sum_{j=m+1}^{n}
         \abs{\frac{\lambda_{j}}{\lambda-\lambda_{j}}}^{2}
         \abs{\sprod{y,x_{j}}}^{2}
         \leq c \sum_{j=m+1}^{n} \abs{\sprod{y,x_{j}}}^{2} \rightarrow 0
         \text{nach Besselscher Ungleichung}
    \end{gather*}
    $n>m\geq n_{0}$

    $H$ vollständig $\Rightarrow$ $s_{n} \rightarrow s\in H$. Sei $x$ wie in
    \autoref{eq:444} $\Rightarrow$
    \begin{align*}
      (A-\lambda I) x &=
         -\frac{1}{\lambda}(A-\lambda I)y - \frac{1}{\lambda}
         \sum_{\lambda_{j}\ne\lambda} \frac{\lambda_{j}}{\lambda-\lambda_{j}}
         \sprod{y,x_{j}} \underbrace{(A-\lambda
         I)x_{j}}_{=(\lambda_{j}-\lambda)x_{j}}\\
      &= y-\frac{1}{\lambda} Ay + \frac{1}{\lambda}
         \underbrace{\sum_{\lambda_{j}\ne\lambda} \lambda_{j}\sprod{y,x_{j}}
           x_{j}}_{=Ay\text{~nach \autoref{satz:2.3.3/1}, falls $\lambda$ kein
         Eigenwert}}
    \end{align*}
    $\Rightarrow$ $(A-\lambda I)x=y$, falls $\lambda$ kein Eigenwert, dann
    eindeutig lösbar.

    $\lambda$ Eigenwert, $\lambda\perp N(A-\lambda I)$ (notwendig nach
    \autoref{satz:2.3.1}, Fredholmsche Alternative) $\Rightarrow$
    \begin{gather*}
      Ay = \sum_{j=1}^{\infty} \lambda_{j}\sprod{y,x_{j}} x_{j}
         = \sum_{\lambda_{j}\ne\lambda} \lambda_{j}\sprod{y,x_{j}} x_{j}
    \end{gather*}
    da $y\perp x_{j}$ für $x_{j}\in N(A-\lambda I)$
  \end{proof}
\end{satz}

\begin{bemerk}
  Integraloperatoren mit symmetrischen Kern $k(x,y)= \overline{k(y,x)}, k\in
  L_{2}(\Omega\times \Omega)$ $\Rightarrow$
  \begin{gather*}
    \vIntOp f(x) = \int_{\Omega} k(x,y) f(y)\,dy \qquad (x\in\Omega)
  \end{gather*}
  ($x\in\Omega\subset \R^{n}$ beschränkt und offen)
  $\vIntOp\in\linOp(H)$ kompakt und selbstadjungiert. Eigenwerte und
  Eigenvektoren $\Rightarrow$ Lösung nach \autoref{satz:2.3.3/2}
\end{bemerk}

\begin{bemerk}
  $A\in \linOp(H)$ nicht kompakt, aber selbstadjungiert
  ($A=A^{\ast}$) $\Rightarrow$ $S_{A}\subset [-\norm{A}, \norm{A}]$
\end{bemerk}

\begin{bsp}
  $Af(x) := xf(x)$ mit $x\in [0,1], f\in L_{2}([0,1]) = H$

  $A\in \linOp(H), \norm{A} \leq 1, A=A^{\ast}, S_{A}\subset[-1,1]$
  \begin{itemize}
   \item $\lambda \notin
    [0,1] \Longrightarrow \exists (A-\lambda I)^{-1}\in \linOp(H)
    \Rightarrow \lambda \in M_{A}$
    \begin{gather*}
      (A-\lambda I)^{-1} g = \frac{1}{t-\lambda} g(t)\qquad t\in[0,1]
    \end{gather*}
   \item Da $\lambda\in [0,1]$, ist $\lambda \in S_{A}$ und
    kein Eigenwert von $A$. Denn aus $Af = \lambda f$ folgt $xf(x) = \lambda
    f(x)\fue$ und somit ist
    \begin{gather*}
      xf(x) - \lambda f(x) = Af(x) - \lambda f(x) = (\lambda -\lambda)
      f(x) = 0\fue
    \end{gather*}

    $S_{A} = [0,1]$

   \item $\lambda\in[0,1]$ $\Rightarrow$ $\lambda\in S_{A}$, o.\,B.\,d.\,A.
    $\epsilon>0$, so dass $[\lambda,\lambda+\epsilon] \subset[0,1]$ (sonst
    $[\lambda-\epsilon,\lambda]$)
    \begin{gather*}
      f_{\epsilon} (x) :=
         \begin{cases}
           \frac{1}{\sqrt{\epsilon}}& t\in[\lambda,\lambda+\epsilon]\\
           0& t\notin[\lambda,\lambda+\epsilon]
         \end{cases}
    \end{gather*}
    $\Rightarrow$ $\norm{f_{\epsilon}(x)}_{2}=1$

    $(A-\lambda E) f_{\epsilon} = (t-\lambda) f_{\epsilon}$, $(A-\lambda I)$
    injektiv, da $\lambda$ kein Eigenwert $R(A-\lambda I) \subsetneq H$

    \begin{gather*}
      \norm{(A-\lambda E)f_{\epsilon}}^{2}
         = \frac{1}{\epsilon} \int_{\lambda}^{\lambda+\epsilon}
         (t-\lambda)^{2}\,dt
         = \frac{\epsilon^{2}}{3} \xrightarrow{\epsilon\rightarrow0} 0
    \end{gather*}
    $\Rightarrow$ $(A-\lambda I)$ nicht beschränkt
  \end{itemize}

  später: $[0,1]=C_{A}$ stetiges Spektrum, $(f_{\frac{1}{j}})_{j}$ Weylsche
  Folge für $\lambda$
\end{bsp}

\subsection{Normaldarstellung kompakter Operatoren}

\begin{description}
 \item[Ziel:] $A\in\linOp(H)$ kompakt $\Longrightarrow$ $Ax =
  \sum_{k=1}^{\infty} s_{k} \sprod{x, e_{k}} f_{k}$ wobei
  $(s_{k})_{k}$ eine Nullfolge ist und $(e_{k})_{k}$ und $(f_{k})_{k}$
  sind eine ONS.
\end{description}

\begin{defini}
  Sei $A\in\linOp(H)$ selbstadjungiert. $A$ heißt genau dann
  \highl{positiv}, wenn für alle $x\in H$ gilt: $\sprod{A x,x} \geq 0$
\end{defini}

\begin{bemerk}
  Nach \autoref{satz:2.2.1/4} gilt: Ist $H$ ein komplexer Hilbertraum, dann
  ist $A=A^{\ast} \Longleftrightarrow \sprod{Ax,x}$ reell für alle $x\in H$
\end{bemerk}

\begin{satz}
  $A\in \linOp(H)$ selbstadjungiert, kompakt und positiv. Dann
  existiert genau ein selbstadjungierter, kompakter und positiver
  Operator $B$ mit der Eigenschaft: $B^{2} = A$ ("`$B=\sqrt{A}$"').

  \begin{proof}
    $A$ positiv $\Longrightarrow$ Eigenwerte sind nicht negativ:
    $\lambda$ EW $\Rightarrow$ $\lambda x =Ax$ (mit $\norm{x}=1$)
    $\Rightarrow$ $\lambda = \lambda\sprod{x,x} = \sprod{Ax,x} \geq 0$

    \autoref{satz:2.3.3/1} $\Rightarrow$ $Ax = \sum_{k=1}^{\infty} \lambda_{k}
    \sprod{x, x_{k}} x_{k}$ mit $\lambda_{k}\rightarrow0,
    \lambda_{k+1}\leq\lambda_{k}$

    Definieren: $Bx := \sum_{k=1}^{\infty} \sqrt{\lambda_{k}}
    \sprod{x,x_{k}} x_{k}$ ($\exists$ für jedes $x\in H$, da
    $\sqrt{\lambda_{k}} \leq c$)

    $\Rightarrow$ $B$ ist selbstadjungiert und positiv (einsetzten),
    $B^{2}=A$

    $B$ kompakt:
    \begin{gather*}
      \norm{Bx - \underbrace{\sum_{k=1}^{n} \sqrt{\lambda_{k}}
      \sprod{x, x_{k}} x_{k}}_{=: B_{n}x}}^{2} =
      \norm{\sum_{k=n+1}^{\infty}\sqrt{\lambda} \sprod{x, x_{k}}
      x_{k}}^{2} = \sum_{k=n+1}^{\infty} \lambda_{k}
      \abs{\sprod{x,x_{k}}}^{2} \leq \lambda_{n} \norm{x}^{2}
    \end{gather*}
    $\Longrightarrow$ $\norm{B-B_{n}}_{L(H)} \leq \sqrt{\lambda_{n}}
    \xrightarrow{n\rightarrow\infty} 0$

    Annahme: $C^{2} = A$, $C$ positiv und kompakt, $C=C^{\ast}$
    \begin{gather*}
      \Rightarrow Cx = \sum_{k=1}^{\infty} \nu_{k} \sprod{x,y_{k}} y_{k}
    \end{gather*}
    mit $\nu_{k}\geq 0, \nu_{k+1}\leq \nu_{k}, \nu_{k}\rightarrow0$, ONS
    $(\nu_{k})$, $Cy_{k} = \nu_{k} y_{k}\quad\forall k$

    o.\,B.\,d.\,A. $\nu_{k+1} <\nu <\nu_{1}$

    $\Rightarrow$ $C^{2}x = C\big(C(x)\big) = \sum_{k=1}^{\infty}\nu_{k}
    \sprod{Cx, y_{k}} y_{k} =^{C=C^{\ast}} \sum_{k=1}^{\infty}
    \nu_{k}^{2} \sprod{x, y_{k}} y_{k}$

    $C^{2}=A \Rightarrow$ $Ax = \sum_{k=1}^{\infty} \nu_{k}^{2} \sprod{x,
    y_{k}} y_{k}$\\
    $\Rightarrow$ $Ay_{l} = \nu_{l}^{2} y_{l} \Rightarrow \nu_{l}^{2} =
    \lambda_{l}$ und $\lambda_{l} = x_{l}$\\
    $\Rightarrow$ $Cx = Bx\quad\forall x \Rightarrow C=B$
  \end{proof}
\end{satz}

\begin{defini}
  $T\in\linOp(H_{1}, H_{2})$ kompakt $\Rightarrow$
  $T^{\ast}T\in\linOp(H_{1})$ komapt, selbstadjungiert und positiv.

  Setzen: $\abs{T} = (T^{\ast}T)^{\frac12}$

  (Vergleiche: $z\in\C \Rightarrow \abs{z} = (z\overline{z})^{\frac12}$)
\end{defini}

\begin{satz}[Polarzerlegung]\label{satz:4}
  $T\in \linOp(H_{1}, H_{2})$ komapkt. Dann existiert ein
  $U\in\linOp(H_{1},H_{2})$ mit $T= U \abs{T}$ und
  \begin{gather*}
    \sprod{Ux,Uy} = \begin{cases}
                      \sprod{x,y}&\colon x,y\in N(T)^{\perp}\\
                      0&\colon x,y\in N(T)
                    \end{cases}
  \end{gather*}

  \begin{proof}
    $\abs{T}$ ist selbstadjungiert und kompakt. Dann bedeutet das:
    \begin{gather}\label{eq:441}
      \sprod{\abs{T}x, \abs{T}y} = \sprod{x, T^{\ast}T y} = \sprod{Tx, Ty}
    \end{gather}
    Dann sind die Nullräume von $T$ und $\abs{T}$ gleich: $N(\abs{T}) = N(T)$.

    Wir können also den Raum $H_{1}$ zerlegen in $H_{1} =
    \underline{\overline{R(\abs{T})}}_{=N(T)^{\perp}}\oplus N(T)$

    Wir definieren $U$ zunächst auf $R(\abs{T})$ durch: $U(\abs{T}x) := Tx$.
    Daraus folgt dann: $U$ ist ein unitärer Operator und bildet ab von
    $R(\abs{T}) \rightarrow R(T)$. eindeutige Fortsetzung auf
    $\overline{R(\abs{T})} = N(T)^{\perp}$ ist unitär
    $\overline{R(\abs{T})}\rightarrow \overline{R(T)}$

    setzen $U=0$ auf $N(T)$, $x= x_{1}+x_{2}, x_{1}\in N(T)^{\perp},
    x_{2}\in N(T), Ux := Ux_{1}$ $\Longrightarrow$
    $U\in\linOp(H_{1},H_{2})$ mit $Tx = T(x_{1}+x_{2}) = Tx_{1} =
    U\abs{T}x_{1} + U\abs{T}x_{2} = U\abs{T}x$
  \end{proof}
\end{satz}

\begin{satz}\label{satz:5}
  $T\in\linOp(H_{1}, H_{2})$ kompakt. Dann existieren ONS
  $(e_{k})_{k}\subset H_{1}$ und $(f_{k})_{k}\subset H_{2}$, sowie Zahlen
  $(s_{k})_{k}$ mit $s_{1}\geq s_{2} \geq \ldots \geq s_{k} \geq
  s_{k+1}\geq \ldots \xrightarrow{k\rightarrow\infty} 0$, so daß $\forall
  x\in H_{1}\colon Tx = \sum_{k=1}^{\infty} s_{k}\sprod{x, e_{k}} f_{k}$

  Die Zahlen $s^{2}_{k}$ sind die entsprechend der Größe und Vielfachheit
  angeordneten Eigenwerte von $T^{\ast}T$. Die Zahlen heißen
  \highl{singuläre Zahlen} (\highl{Singulärwert}) von $T$.

  \begin{proof}
    Nach \autoref{satz:4} ist $T=U\abs{T}$.

    Aus den Betrachtungen in \autoref{sec:2.2.3} folgt
    \begin{gather*}
      \abs{T}x = \sum_{k=1}^{\infty} s_{k} \sprod{x, e_{k}} e_{k}
    \end{gather*}
    mit $\abs{T}e_{k} = s_{k} e_{k}$ ($\Rightarrow$ $e_{k}\in R(\abs{T})$)
    $s_{k}\geq s_{k+1} \rightarrow 0$

    $\Rightarrow$ $T=U\abs{T} = \sum_{k=1}^{\infty} s_{k}\sprod{x,e_{k}}
    \underbrace{Ue_{k}}_{=: f_{k}\in H_{2}}$ mit $\sprod{f_{k}, f_{l}} =
    \sprod{Ue_{k}, Ue_{l}} = \sprod{e_{k},e_{l}}$

    $\Rightarrow$ $(f_{k})_{k}$ ONS in $H_{2}$
  \end{proof}
\end{satz}

\begin{bemerk}
  Es gilt: $s_{1} = \norm{T}$, denn: $s_{1}^{2}=\norm{T^{\ast}T}$ (nach
  \autoref{sec:2.3.3}) $=\norm{T}^{2}$ (nach \autoref{sec:2.2.2})
\end{bemerk}

\begin{folger}
  $T\in \linOp(H_{1}, H_{2})$ kompakt. Dann:
  \begin{gather*}
    \begin{CD}
      H_{1} @>T>> H_{2}\\
      @VUVV @AAVA\\
      l_{2} @>S>> l_{2}
    \end{CD}
  \end{gather*}

  $T = VSU$ mit $U\in \linOp(H_{1}, l_{2})$ mit $\norm{U}=1$,
  $V\in\linOp(l_{2}, H_{2})$ mit $\norm{V}=1$
  \begin{gather*}
    S = \begin{pmatrix}s_{1}&&0\\&s_{2}\\0&&\ddots\end{pmatrix}\in
    \linOp(l_{2})
  \end{gather*}
  mit $\norm{S}=\norm{T}$, $S(a_{k}) := (s_{k}a_{k})_{k}$
  "`\highl{Diagonaloperator}"'

  \begin{proof} nach \autoref{satz:5}
    $Tx = \sum s_{k}\sprod{x,e_{k}} f_{k}$
    Ux = $(\sprod{x, e_{k}})_{k}$ $\norm{Ux}\leq \norm{x}$ (Bessel)

    $V(b_{k}) := \sum_{k=1}^{\infty} b_{k}f_{k}$

    $\norm{V(b_{k})} = \norm{b_{k}}_{l_{2}}$

    $\Rightarrow$ $T=VSU$, $\norm{T} \leq \norm{VSU} \leq \norm{V} \norm{S} \norm{U} =
    \norm{S}$ andererseits: $\norm{S} \leq \sup_{k} s_{k} = s_{1} = \norm{T}$
  \end{proof}
\end{folger}

