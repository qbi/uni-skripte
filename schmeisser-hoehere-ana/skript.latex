\chapter{Hauptsätze der Funktionalanalysis}
\section{Vorbetrachtungen}
\subsection{Zorn'sches Lemma und Auswahlaxiom}

\begin{defini}[Ordnungsrelation]
  $X\ne\emptyset$ Menge, $R\subset X\times X$ heißt
  \highl{Ordnungsrelation} ("`$\leq$"') $:\Longleftrightarrow$
  \begin{enumerate}
   \item $R$ ist reflexiv, d.\,h. für alle $x\in X$ ist $x\leq x$ (
    $(x,x)\in R$ )
   \item $R$ ist antisymmetrisch, d.\,h. $x\leq y \wedge y \leq x
    \Rightarrow x=y$
   \item $R$ ist transitiv, d.\,h. $x\leq y \wedge y\leq z \Rightarrow
    x\leq z$
  \end{enumerate}

  $(X, \leq)$ heißt dann \highl{gordnete Menge}.

  Für eine Teilmenge $X_{1}\subset X$ bezeichnet man $(X_{1}, \leq)$ als
  \highl{Kette} oder lineare geordnete Menge $:\Longleftrightarrow$ für
  alle $x,y\in X_{1}$ gilt entweder $x\leq y$ oder $y\leq x$.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item $X =\{a,b,c\}, a \leq b \leq c$
   \item $(\N, \leq)$ ist eine Kette
   \item $A_{1} \subset A_{2} \subset A_{3} \subset \ldots \subset A_{j}
    \subset \ldots$ für $j\in\N$ ist eine Kette
  \end{enumerate}
\end{bsp}

\begin{defini}
  $(X, \leq)$ geordnete Menge, $X_{1}\subset X$

  $x\in X_{1}$ heißt \highl{maximales Element} $:\Longleftrightarrow$ für
  alle $y\in X_{1}$ mit $x\leq y$ $\Longrightarrow$ $y=x$
\end{defini}

\begin{bsp}
  $ a\leq b \leq c,d,e$ hat drei maximale Elemente
\end{bsp}

\begin{defini}\label{def:3.1.1/1}
  $(X, \leq)$ heißt \highl{induktiv geordnet} $:\Longleftrightarrow$ für jede
  Kette $(X_{0},\leq)\subset (X,\leq)$ existiert ein $x\in X$ mit $y\leq
  x$ für alle $y\in X_{0}$. (="`$\exists\sup$"')
\end{defini}

\begin{bsp}
  $(\mathcal{P}(M), \subset)$ Potenzmenge ist induktiv geordnet.\help{was
  war nochmal der Grund?}

  $(\N, \leq)$ ist nicht induktiv geordnet.
\end{bsp}

\begin{axiom}[Zorn'sches Lemma]\label{axi:zorn}
  Ist $(X, \leq)$ eine induktiv geordnete Menge, so existiert in $X$
  mindestens ein maximales Element.
\end{axiom}

\begin{defini}
  $\mathfrak{M}$ Menge von Mengen (\highl{Mengensystem}). $f$ heißt
  Auswahlfunktion $:\Longleftrightarrow$
  \begin{enumerate}
   \item $f\colon\mathfrak{M} \rightarrow \bigcup_{M\in \mathfrak{M}}$
   \item $\forall M (M\in\mathfrak{M} \Rightarrow f(M)\in M)$
  \end{enumerate}
\end{defini}

\begin{bsp}
  $\mathfrak{M} = \{A_{j}\colon j\in\N\}, A_{j}\ne\emptyset \forall j,
  a_{j}\in A_{j}\colon f(A_{j})= a_{j}$
\end{bsp}

\begin{axiom}[Auswahlaxiom]\label{axi:auswahl}
  Zu jedem Mengensystem $\mathfrak{M}$ mit $\emptyset\notin\mathfrak{M}$
  exsistiert eine Auswahlfunktion.
\end{axiom}

\begin{bemerk}
  Zorn'sches Lemma und Auswahlaxiom sind gleichwertig (\autoref{axi:zorn}
  $\Longleftrightarrow$ \autoref{axi:auswahl})
\end{bemerk}

\begin{bemerk}
  Auswahlaxiom wird oft unbewußt benutzt, z.\,B. $I$ Indexmenge,
  $A_{j}\ne\emptyset$
  \begin{gather*}
    \prod_{j\in I} A_{j}:= \{(x_{j})_{j\in I}\colon \forall j (j\in I
    \Rightarrow x_{j}\in A_{j})\}
  \end{gather*}
\end{bemerk}

\begin{satz}\label{satz:3.1.1/1}
  Jeder Vektorraum $E\ne\{0\}$ besitzt eine Basis. Diese nennt man
  "`Hamelbasis"', benannt nach Alfred Hamel 1877--1954.

  \begin{proof}
    \textup{Wiederholung:}
    $B\subset E$ heißt Basis $\Leftrightarrow$
    \begin{enumerate}
     \item $B$ linear unabhängig, d.\,h. je endlich viele Elemente aus
      $B$ sind linear unabhägig.
     \item $\Span B =E$
    \end{enumerate}

    Es sei $X := \{ M \colon M\subset E \text{ linear unabhängig}\}$.
    Dann ist $(X,\subset)$ ist eine geordnete Menge und sogar induktiv
    geordnet, denn für eine Kette $X_{0}$ in $X$ ist $\bigcup_{U \subset
    X_{0}} U =: M$ linear unabhängig ($M\in X, U\subset M, \forall U\in
    X_{0}$)

    Jetzt können wir \autoref{axi:zorn} anwenden und erhalten so, dass ein
    maximales Element $B\in X$ existiert. Dieses $B$ ist eine Basis in
    $E$, denn: sei $a\in E, a\notin B$, betrachte $M = B\cup \{ a\}$.
    $M$ ist linear abhängig, da $B$ maximales Element. $a$ ist also
    bereits im $\Span B$.
  \end{proof}
\end{satz}

\begin{satz}\label{satz:3.1.1/2}
  $E\ne\{0\}$ Vektorraum. Zu jeder linear unabhängigen Menge $M\subset
  E$ gibt es eine Basis in $E$, die $M$ enthält.

  \begin{proof}
    $X:= \{\tilde{M}\colon \tilde{M}\subset E \text{ linear unabhängig},
    M\subset \tilde{M}\}$ $\Longrightarrow$ $(X,\subseteq)$ induktiv
    geordnete Menge. Weiter wie im Beweis von \autoref{satz:3.1.1/1}.
  \end{proof}
\end{satz}

\subsection{Komplementärräume}\label{sec:3.1.2}

$E$ Vektorraum (reell oder komplex), $x_{0}\in E, \lambda\in\K,
M,N\subset E$
\begin{align*}
  x_{0}\pm M &:=\{ x_{0}\pm x\colon x\in M\}\\
  M\pm M &:= \{x\pm y\colon x\in M, y\in N\}\\
  \lambda M &:= \{\lambda x\colon x\in M\}
\end{align*}

\begin{defini}
  $G,F \subset E$ Teilräume, $G$ heißt \highl{algebraisches Komplement}
  von $F$ $:\Longleftrightarrow$
  \begin{enumerate}
   \item $E = F+G$
   \item $F\cap G = \{0\}$
  \end{enumerate}

  \textup{Schreibweise:} $E= F\oplus G$
\end{defini}

\begin{bemerk}\label{bem:3.1.2/1}
  \begin{enumerate}
   \item Gilt $E= F\oplus G$, dann existiert für jedes $z\in E$ eine
    eindeutige Zerlegung $z =x+y$ mit $x\in F$ und $y\in G$.

    \begin{proof}
      Nehmen wir an, ein Element aus $E$ liese sich auf zwei
      unterschiedliche Arten durch Elemente aus $F$ und $G$ darstellen:
      $x_{1}+y_{1} = x_{2}+y_{2}$. Durch Umstellen erhält man
      $x_{1}-x_{2} = y_{2}-y_{1}$. Beide Differenzen sind ein Elemente des
      jeweiligen Raums. Die Gleichheit kann aber nur das
      Nullelement erfüllen, da dieses Element beider Teilräume ist. Wenn
      aber die Differenz Null ist, so müssen $x_{1}$ und $x_{2}$ bzw.
      $y_{1}$ und $y_{2}$ gleich sein, was ein Widerspruch zur Annahme
      ist.
    \end{proof}
   \item In einem Hilbertraum $H$ ist für einen abgeschlossenen Teilraum
    $F$, der Teilraum $F^{\perp}$ mit $H = F\oplus F^{\perp}$ eindeutig
    bestimmt.
  \end{enumerate}
\end{bemerk}

\begin{satz}
  $E\ne\{0\}$ sie Vektorraum.

  Zu jedem Teilraum von $E$ gibt es ein algebraisches Komplement.

  \begin{proof}
    $F\subset E$ Teilraum. Wir wählen $M$ als eine Basis in $F$ (möglich
    nach \autoref{satz:3.1.1/1}). Dann existiert eine Basis $N$ von $E$,
    die $M$ enthält (möglich nach \autoref{satz:3.1.1/2}). Setze $G :=
    \Span N\setminus M$ $\Longrightarrow$ $E= F\oplus G$.
  \end{proof}
\end{satz}

\begin{satz}
  Zwei Komplementärräume eines Teilraums in $E$ sind entweder beide
  unendlich dimensional oder beide haben die gleiche (endliche) Dimension.

  \begin{proof}
    $E = F\oplus G_{2}$ und $E=F \oplus G_{2}$, $\dim G_{2}<\infty$.

    Wir konstruieren einen Isomorphismus zwischen $G_{1}$ und $G_{2}$ um
    zu zeigen, dass $\dim G_{1} = \dim G_{2}$.

    Dazu definieren wir eine Abbildung $Tu := v$, falls $u= y+v$ mit
    $y\in F$ und $v\in G$. \help{$G_{1}$ oder $G_{2}$?} Daraus folgt $T\colon G_{1} \rightarrow G_{2}$ ist
    ein Isomorphismus (d.\,h. linear, injektiv und surjektiv -- selber
    überprüfen!).
  \end{proof}
\end{satz}

\begin{defini}
  $E$ ist eine Vektorraum, $F \subset E$ Teilraum, $G$ algebraisches
  Komplement von $F$.\highl{Kodimension}
  \begin{gather*}
    \codim F := \dim G
  \end{gather*}
\end{defini}

\subsection{Quotientenräume}
$(E,\norm{\cdot}_{E})$ normierter Vektorraum, $F\subset E$ abgeschlossener
Teilraum, Äquivalenzrelation: $x\sim y :\Leftrightarrow x-y \in F$

\begin{defini}
  $\nicefrac{E}{F}$\ldots Menge aller Äquivalenzklassen bzgl. $\sim$
  heißt \highl{Quotentenraum} von $E$ nach $F$.

  \textup{Schreibweise:} $x\in E, \overline{x}:= x+F$, die durch $x$
  repräsentierte Äquivalenzklasse.

  $\overline{O}=F$, $\overline{x} = \overline{y} :\Longleftrightarrow
  \overline{x}- \overline{y} = \overline{0}$
\end{defini}

\begin{defini}
  \begin{enumerate}
   \item Die Abbildung $x \rightarrow \overline{x}$ von $E$ nach
    $\nicefrac{E}{F}$ heißt \highl{kanonische Abbildung}.
   \item $\overline{x}+\overline{y} := x+y+F = \overline{x+y}$\\
    $\lambda\overline{x} := \lambda x+F = \overline{\lambda x}$

    \textup{Schreibweise:} $x\in E, \overline{x} := x+F$, die durch $x$
    repräsentierte Äquivalenzklasse
   \item $\norm{\overline{x}} := \inf_{y\in F} \norm{x+y}_{E}$
  \end{enumerate}
\end{defini}

\begin{satz}
  \begin{enumerate}
   \item Die Abbildung $\overline{x}\rightarrow\norm{\overline{x}}$ ist eine
    Norm auf $\nicefrac{E}{F}$. $\nicefrac{E}{F}$ ist normierter
    Vektorraum.
   \item Die kannonische Abbildung von $E$ nach $\nicefrac{E}{F}$ ist
    linear, beschränkt und hat Norm $\leq1$.
   \item $E$ Banachraum $\Longrightarrow$ $\nicefrac{E}{F}$ ist Banachraum.
  \end{enumerate}

  \begin{proof}
    Normeigenschaft: $0 = \norm{\overline{x}} = \inf_{y\in F}\norm{x+y}_{E}$
    $\Longrightarrow$ $\exists$ Folge $(y_{j})\subset F$ mit
    $\norm{x+y_{j}}_{E} \rightarrow 0$ $\Longrightarrow$ $y_{j}\rightarrow
    -x$. Da $F$ abgeschlossen ist, folgt daraus $x\in F$ und somit
    $\overline{x} =0$

    $E$ vollständig $\Longrightarrow$ $\nicefrac{E}{F}$ vollständig.
    Verwenden \autoref{satz:1.1.1/1}. Es sei $\sum_{n=1}^{\infty}
    \norm{\overline{x_{n}}}< \infty$. wähle $y_{n}\in F$ mit
    $\norm{\underbrace{x_{n}+y_{n}}_{=: z_{n}}}_{E} \leq
    \norm{\overline{x_{n}}} + \frac{1}{2^{n}}$ $\Longrightarrow$ $\sum
    \norm{z_{n}}_{E} \leq \sum_{n=1}^{\infty} \norm{\overline{x_{n}}} +
    \sum_{n=1}^{\infty} \frac{1}{2^{n}} < \infty$.

    $E$ vollständig $\Longrightarrow$ $\exists z\in E$ mit
    $\sum_{n=1}^{N} z_{n} \xrightarrow{N\rightarrow\infty} z$ in $E$.

    Es gilt: $\overline{x_{n}} = \overline{z_{n}} \Rightarrow
    \norm{\sum_{n=1}^{N} \overline{x_{n}} - \overline{z}} = \norm{
    \overline{\sum_{n=1}^{N} z_{n}-z}} \leq \norm{\sum_{n=1}^{N}
    z_{n}-z}_{E} \rightarrow 0$ $\Longrightarrow$ $\exists$
    $\sum_{n=1}^{\infty} \overline{x_{n}} = \overline{z}$
  \end{proof}
\end{satz}

\section{Der Satz von Baire und Folgerungen}
\subsection{Der Satz von Baire für vollständige metrische Räume}

\textsl{Wiederholung von Begriffen:}
$(X,d)$ metrischer Raum, $K_{\epsilon}(x_{0}) = \{x\in X\colon d(x,x_{0}) <
\epsilon\}$ offene Kugel um $x_{0}$, $A\subset X$ offen, $A$ abgeschlossen,
$\overline{A}$ Abschluss, $A^{0}$ alle inneren Punkte, $x_{0}$ innerer
Punkt, $A$ dicht in $X$ $:\Longleftrightarrow$ $\overline{A}=X$,
Konvergenz

\begin{lemma}[Stetigkeit der Metrik]\label{lem:3.2.2/1}
  $(X,d)$ metrischer Raum, $x_{j} \rightarrow x$ in $X$ $\Longrightarrow$
  $\forall y\in X\colon d(x_{j},y) \rightarrow d(x,y)$.

  \begin{proof}
    $\abs{d(x_{j}, y) - d(x,y)} \leq d(x_{j}, x) \rightarrow 0$
  \end{proof}
\end{lemma}

\begin{lemma}["`Intervallschachtelung"']\label{lem:3.2.2/2}
  $(X,d)$ vollständiger metrischer Raum

  $E_{j} = \{x\in X \colon d(x,x_{j}) \leq \epsilon_{j} \} (j\in\N)$ Folge von
  abgeschlossenen Kugeln mit $\lim_{j\rightarrow\infty} \epsilon_{j} =0$
  und $E_{j+1}\subset E_{j}$ für alle $j\in\N$. $\Longrightarrow$
  $\exists$ genau eine $x\in X$ mit $\bigcap_{j=1}^{\infty}  E_{j} =
  \{x\}$.

  \begin{proof}
    \begin{gather}\label{eq:442}
      k > j \Longrightarrow E_{k}\subset E_{j} \Longrightarrow d(x_{k},
         x_{j}) \leq \epsilon_{j}
    \end{gather}
    $\Longrightarrow (x_{j})_{j}$ ist Cauchyfolge $\Longrightarrow$
    $x_{j}\rightarrow x\in X$, da der metrische Raum vollständig ist. Aus
    \autoref{eq:442} folgt mit \autoref{lem:3.2.2/1} für
    $k\rightarrow\infty$, dass $d(x, x_{j}) \leq \epsilon_{j}$ für alle
    $j$. Daraus folgt wiederrum, dass $x\in \bigcap_{j=1}^{\infty} E_{j}$.

    Ann.: $y\in \bigcap_{j=1}^{\infty} E_{j}$

    $\Longrightarrow$ $d(x,y) \leq d(x,x_{j}) + d(x_{j},y) \leq 2
    \epsilon_{j} \rightarrow 0$ $\Longrightarrow$ $x=y$
  \end{proof}
\end{lemma}

\begin{satz}[Baire]\label{satz:baire}
  $(X,d)$ vollständiger metrischer Raum. $(A_{j})^{\infty}_{j=1}$ Folge
  abgeschlossener Mengen aus $X$ und $\bigcup_{j=1}^{\infty}A_{j}$
  enthalte eine offene Kugel.

  Dann existiert ein $j\in\N$, so dass $A_{j}$ eine offene Kugel enthält.

  \begin{proof}
    Es sei $K_{\epsilon_{0}}(x_{0}) \subset \bigcup_{j=1}^{\infty}
    A_{j}$.

    indirekter Beweis:
    \begin{gather}\label{eq:443}
      \forall j\in \N\ \forall\epsilon>0\ \forall x\in X\colon K_{\epsilon}(x)
      \cap (X\setminus A_{j}) \ne \emptyset
    \end{gather}

    \autoref{eq:443} $\Longrightarrow$ $\exists y_{1}\subset
    K_{\epsilon_{0}}(x_{0})\cap (X\setminus A_{1})$ offen
    $\Longrightarrow$ $\exists\epsilon_{1}< 1$ $\exists$ abgeschlossene
    Kugel $E_{1} = \{ z \colon d(z,y_{1}) \leq \epsilon_{1} \} \subset
    K_{\epsilon_{0}} \cap (X\setminus A_{1})$

    \autoref{eq:443} $\Longrightarrow$ $\exists y_{2}\in
    K_{\epsilon_{1}}(y_{1}) \cap (X\setminus A_{2})$ offen
    $\Longrightarrow$ $\exists\epsilon_{2}<\frac{1}{2}$ $\epsilon$
    abgeschlossene Kugel $E_{2}= \{ z\colon d(z,y_{2})\leq \epsilon_{2}\}
    \subset K_{\epsilon_{1}}(y_{1})\cap (X\setminus A_{2})$ und
    $E_{2}\subset K_{\epsilon_{1}} \subset E_{1}$

    Iteration $\Rightarrow$ $\forall j\in\N$ $\exists y_{j}\in
    K_{\epsilon_{j-1}}(y_{j-1}) \cap (X\setminus A_{j})$ offen
    $\Longrightarrow$ $\exists$ $\epsilon_{j}< \frac{1}{j}$ $\exists$
    abgeschlossene Kugel $E_{j}= \{z\colon d(z,y_{j}) \leq
    \epsilon_{j}\}\subset K_{\epsilon_{j-1}}(x_{j-1})\cap (X\setminus
    A_{j})$ und $E_{j}\subset E_{j-1}$

    \autoref{lem:3.2.2/2} $\Longrightarrow$ $\exists y\in X$ mit
    $\bigcap_{j=1}^{\infty} E_{j}= \{y\}$. $y\in
    \bigcap_{j=1}^{\infty}(X\setminus A_{j}) = X\setminus
    \bigcup_{j=1}^{\infty} A_{j} \Longrightarrow y\in
    \bigcup_{j=1}^{\infty} A_{j}$. Andererseits hatten wir angenommen,
    dass $y\in E_{1} \subset K_{\epsilon_{0}}(x_{0}) \subset
    \bigcup_{j=1}^{\infty}A_{j}$. Wir haben also einen Widerspruch,
    weshalb unsere Annahme nicht korrekt seien kann.
  \end{proof}
\end{satz}

\begin{defini}
  \begin{enumerate}
   \item $A\subset X$ heißt \highl{nirgends dicht} $:\Longleftrightarrow$
    $\overline{A}^{0} = \emptyset$ (d.\,h. keine inneren Punkte in
    $\overline{A}$)
   \item $M$ heißt \highl{mager} (oder
    \highl[Menge!1.Kategorie@1.\,Kategorie|see{mager}]{Menge 1.\,Kategorie})
    $:\Longleftrightarrow$ $M$ ist abzählbare Vereinigung nirgends
    dichter Teilmengen. Andererseits heißt $M$
    \highl[Menge!2.Kategorie@2.\,Kategorie]{Menge 2.\,Kategorie}.
  \end{enumerate}
\end{defini}

\begin{satz}\label{satz:3.2.1/2}
  Für einen vollständigen metrischen Raum $(X,d)$ ist jede nichtleere
  offene Kugel von 2.~Kategorie.

  \begin{proof}
    $\Omega\subset X$ offen, $\Omega\ne\emptyset$

    indirekter Beweis: Ann.: $\Omega$ Menge 1.\,Kategorie. dann wäre
    $\Omega$ eine abzählbare Vereinigung dichter Teilmengen $B_{j}$ von
    $X$. Daraus folgt
    $\Omega\subset\bigcup_{j=1}^{\infty}\overline{B_{j}}$ und mithilfe
    von \autoref{satz:baire} folgt $\exists\overline{B_{j}}$, dass offene
    Kugel enthält $\Longrightarrow$ $\overline{B_{j}}^{0}\ne\emptyset$.
    Widerspruch, da $B_{j}$ nirgends dicht.
  \end{proof}
\end{satz}

\begin{folger}
  $(X,d)$ vollständiger metrischer Raum $\Longrightarrow$ $X$ ist Menge
  2.\,Kategorie
\end{folger}

\begin{folger}
  $(X,d)$ vollständiger metrischer Raum. Ist $M$ mager (1.\,Kategorie),
  so ist $X\setminus M$ dicht in $X$.

  \begin{proof}
    (indirekt) Ann.: $x\in X$ und $\exists \epsilon > 0, K_{\epsilon}(x)$
    enthält keinen Punkt aus $X\setminus M$ $\Longrightarrow$
    $K_{\epsilon}(x)\subset M \Longrightarrow K_{\epsilon}(x)$ ist mager
    (1.\,Kategorie) $\Longrightarrow$ Widerspruch zu \autoref{satz:3.2.1/2}
  \end{proof}
\end{folger}

\begin{folger}
  Es gibt keinen Banachraum mit einer abzählbar unendlichen algebraischen
  Basis (Hamelbasis), d.\,h. die Basis ist entweder endlich oder
  überabzählbar.

  \begin{proof}
    (indirekt) Annahme: $B=\{e_{1},e_{2},\ldots\}$ abzählbar unendliche
    Basis is einem Banachraum $X$.

    $U_{n} := \Span\{e_{1},\ldots,e_{n}\}$ $\Longrightarrow$ $U_{n}$ ist
    abgeschlossen und $U_{n}^{0}=\emptyset$ (keine inneren Punkte).

    $\Longrightarrow$ $U_{n}$ nirgends dicht, $\bigcup_{n=1}^{\infty}
    U_{n}=X$ $\Rightarrow$ Widerspruch zu Satz von Baire
    (\autoref{satz:baire}).
  \end{proof}
\end{folger}

\subsection{Das Prinzip der gleichmäßigen Beschränktheit}

\begin{defini}
  $(X,d)$ metrischer Raum, $F \subset \{f\big| f\colon X\rightarrow\R
  \text{ Funktion}\}$

  $F$ heißt punktweise gleichmäßig beschränkt $:\Longleftrightarrow$
  \begin{gather*}
    \forall x\in X\ \exists M_{x} > 0 \forall f\in F\colon f(x) \leq M_{x}
  \end{gather*}
\end{defini}

\begin{satz}[Osgood]\label{satz:3.2.2/1}
  $(X,d)$ vollständiger metrischer Raum und $F\subset C(X,\R)$ (stetige
  Funktionen auf $X$) sei punktweise gleichmäßig beschränkt.

  Dann gibt es eine offene Kugel $K\subset X$ und eine Konstante $M$, so
  dass $\forall x\in K\ \forall f\in F\colon f(x)\leq M$.

  \begin{proof}
    Setzen $A_{j}:=\{x\in X\colon \forall f\in F\colon f(x)\leq j\}$. Diese Mengen
    $A_{j}$ sind abgeschlossen, denn wenn ich jetzt eine Folge $x_{k}$ aus
    $A_{j}$ und $x_{k}\rightarrow x$, dann heißt das insbesondere, dass
    für alle $x_{k}$ gilt $f(x_{k})\leq j$ und $f$ stetig ist, ist $f(x)
    \leq j$ $\Rightarrow$ $x\in A_{j}$

    Weiterhin ist $\bigcup_{j=1}^{\infty} A_{j}=X$, da $F$ punktweise
    gleichmäßig beschränkt.

    Nach \autoref{satz:baire} folgt daraus, dass ein $j_{0}$ und eine
    offene Kugel $K$ mit der Eigenschaft $K\subset A_{j_{0}}$ existiert.
    Damit ist für alle $F$ und alle $x\in K$ $f(x)\leq j_{0}$
  \end{proof}
\end{satz}

\subsubsection{Anwendung auf Banachräume}
\begin{satz}[Prinzip der gleichmäßigen Beschränktheit; Hahn-Banach]\label{satz:3.2.2/2}
  Es sei $X$ ein Banachraum und $Y$ ein normierter Vektorraum.
  $\mathcal{T}\subset \linOp(X,Y)$ sei eine Teilmenge der linear und
  stetigen Abbildungen von $X$ nach $Y$. Für alle $x\in X$ existiere eine
  Konstante $M_{x}>0$, so dass für alle $T\in\mathcal{T}\colon \norm{Tx}_{Y} \leq
  M_{x}$.

  Dann existiert ein $M>0$, so dass für alle $T\in\mathcal{T}$ die
  Operatornorm $\norm{T}\leq M$ ist. ($\norm{T} = \sup_{x\ne0}
  \frac{\norm{Tx}}{\norm{x}}$)

  \begin{proof}
    $F:=\{ f\big| f(x) = \norm{Tx}_{Y}, x\in X\} \subset C(X,\R)$ ist nach
    Vorraussetzung punktweise gleichmäßig beschränkt. Aus
    \autoref{satz:3.2.2/1} folgt, dass es eine offen Kugel
    $K_{d}(x_{0})\subset X$ gibt und ein $M>0$ existiert mit:
    \begin{gather}\label{eq:414}
      \norm{Tx}\leq M, \forall x\in K_{d}(x_{0})\forall T\in\mathcal{T}
    \end{gather}

    $\Longrightarrow$
    \begin{align*}
      \norm{Tx} &= \norm{x} \norm[\big]{\frac{Tx}{\norm{x}}}\\
      &= \frac{2}{d} \norm{x} \norm[\big]{T\bigl(\frac{d\cdot x}{2\norm{x}}\bigr)}\\
      &\leq \frac{2}{d} \norm{x}
         \norm[\big]{T\bigl(\frac{d\cdot x}{2\norm{x}}+x_{0}\bigr)} +
         \frac{2}{d} \norm{x} \norm{T(x_{0})}\\
      &\leq \frac{2}{d} \norm{x} M + \frac{2}{d} \norm{x} \norm{Tx} &&\text{nach
         \autoref{eq:414}}\\
      &\leq \tilde{M} \norm{x}
    \end{align*}
  \end{proof}
\end{satz}

\begin{description}
 \item[\underline{Problem:}] $(T_{j})_{j}\subset \linOp(X,Y)$ Folge
  von linear beschränkten Operatoren. Für alle $x\in X$
  $\exists\lim_{j\rightarrow\infty} T_{j}x =: Tx$ $\Rightarrow$ $T$ ist
  linear (da der Limes linear ist). Es bleibt noch die Frage, ob $T$
  beschränkt ist.
\end{description}

\begin{satz}\label{satz:3.2.2/3}
  $X$ Banachraum und $Y$ ein normierter Raum und $(T_{j})_{j}$ eine Folge
  in $\linOp(X,Y)$. Es existiere punktweise der Limes $Tx :=
  \lim_{j\rightarrow\infty} T_{j}x$ für alle $x\in X$.

  Dann ist $T\in \linOp(X,Y)$ und es existiert ein $M>0$ und für
  alle $j$ gilt $\norm{T_{j}}\leq M$.

  \begin{proof}
    $\forall x\in X \exists M_{x} > 0 \forall j\colon \norm{T_{j}x}\leq M_{x}$,
    da $\exists\lim_{j\rightarrow\infty} \norm{T_{j}x}$.

    \autoref{satz:3.2.2/2} $\Longrightarrow$ $\exists M>0$ $\forall j\colon \norm{T_{j}}\leq M$.
    $\Longrightarrow \norm{T_{j}x} \leq M\norm{x}\quad\forall j$

    Für $j\rightarrow\infty$ $\Longrightarrow$ $\norm{Tx}\leq M\norm{x}
    \Longrightarrow T\in \linOp(X,Y)$
  \end{proof}
\end{satz}

\begin{satz}
  $X,Y$ Banachräume und $(T_{j})_{j}\subset \linOp(X,Y)$. Es sei
  \begin{enumerate}
   \item $\sup_{j}\norm{T_{j}} \leq\infty$
   \item $\exists\lim_{j\rightarrow\infty}T_{j}y$ für alle $y$ aus einer
    in $X$ dichten Teilmenge $X_{0}$
  \end{enumerate}

  Dann existiert der $\lim_{j\rightarrow\infty} T_{j}x =: Tx$ für
  \emph{alle} $x\in X$ und $T\in\linOp(X,Y)$.

  Anmerkung: Es gilt auch die Äquivalenz.

  \begin{proof}
    Nach \autoref{satz:3.2.2/3} ist es ausreichend \help{was? warum? Mist
    wieder nicht aufgepasst}

    $x\in X,\epsilon>0$, wählen $y\in X_{0}$ mit $\norm{x-y}<\epsilon$. Dann
    erhalten wir:
    \begin{align*}
      \norm{T_{j}x -T_{k}x} &\leq \norm{T_{j}x - T_{j}y} + \norm{T_{j}y - T_{k} y}
         + \norm{T_{k}y - T_{k}x}\\
      &\leq \underbrace{\norm{T_{j}}}_{\leq M}
         \underbrace{\norm{x-y}}_{<\epsilon} + \underbrace{\norm{T_{j}y -
         T_{k}y}}_{<\epsilon \text{für} j,k\geq j_{0}(\epsilon)} +
         \underbrace{\norm{T_{k}}}_{\leq M} \underbrace{\norm{x-y}}_{< \epsilon}\\
      &\leq (2M+1)\epsilon&&\forall j,k\geq j_{0}(\epsilon)
    \end{align*}
    Das heißt also, $(T_{j}x)_{j}$ ist eine Cauchyfolge in $Y$. Da $Y$
    vollständig ist, existiert auch der $\lim_{j\rightarrow\infty}T_{j}x$
  \end{proof}
\end{satz}

\subsubsection{Bemerkungen zu Anwendungen}
\begin{bsp}[Konvergenz von Quadraturformeln (Numerik)]
  \index{Numerik}\index{Quadraturformeln}
  Für eine stetige Funktion $f\in C([a,b])$ ist das Riemann-Integral
  $\int_{a}^{b}f(x)\,dx$ gesucht. Dabei seien $Q_{n}$ die Quadraturformel
  \begin{gather*}
    Q_{n}(f) := \sum_{k=0}^{n} \alpha_{k}^{(n)} f\bigl(x_{k}^{(n)}\bigr)
  \end{gather*}

  Dann gilt:
  \begin{align*}
    Q_{n}(f) &\xrightarrow{n\rightarrow\infty} \int_{a}^{b}f(x)\,dx
    \Longleftrightarrow\\
    &\sum_{k=0}^{n} \abs{\alpha_{k}^{(n)}} \leq M&&\forall n\\
    &Q_{n}(p) \rightarrow \int_{a}^{b} p\,dx&&\forall \text{Polynome }p
  \end{align*}

  siehe Heuser\cite[S.\,264--271]{Heuser}
\end{bsp}

\begin{enumerate}
 \item $\exists f\in C_{2\pi}(\R)$, so dass $(s_{n}f(0))_{n}$
  divergent, wobei
  \begin{gather}\label{eq:445}
    s_{n}f(x) = \sum_{k=-n}^{n} c_{k}f(x) e^{ikx}, c_{k}(f) =
    \frac{1}{2\pi}\int_{-\pi}^{\pi} f(x)e^{-ikx}\,dx
  \end{gather}

  $f\sim \sum_{k=-\infty}^{\infty} c_{k}e^{ikx}$

  Siehe hierzu ebenfalls das Buch von Heuse\cite{Heuser}.
 \item $T= I$ (identischer Operator) von Interesse
  \begin{folger}
    $X$ sein ein Banachraum, $(T_{j})_{j}\subset\linOp(X,X)$. Dann
    sind äquivalent:
    \begin{enumerate}
     \item $T_{j}x \rightarrow x$ für alle $x\in X$
     \item $T_{j}y \rightarrow y$ für alle $y\in X_{0}$, wobei
      $X_{0}\subset X$ dicht und $\norm{T_{j}} \leq M$ für alle $j\in \N$
    \end{enumerate}
  \end{folger}

  \begin{bsp}
    $n$-te Partialsumme $s_{n}f$ wie in \autoref{eq:445}. $\norm{s_{n}f -
    f}_{X} \xrightarrow{n\rightarrow\infty} 0$

    Ist $X= L_{1}$ oder $X= C_{2\pi}$, dann ist $\norm{s_{n}}_{X\rightarrow
    X} = \frac{4}{\pi^{2}} \log n + O(n)$

    Ist $X=L_{p} (1< p<\infty)$, dann $\norm{s_{n}}_{X\rightarrow X} \leq
    M_{p}$ gleichmäßig beschränkt.
  \end{bsp}
\end{enumerate}

\subsection[Das Prinzip der offenen Abbildungen]{Das Prinzip der offenen
  Abbildungen (Open Mapping Principle)}

\begin{defini}
  $X,Y$ normierter Vektorraum, Eine Abbildung $T\in\linOp(X,Y)$
  heißt \highl[Abbildung!offen]{offen} $:\Longleftrightarrow$ für alle
  offenen Teilmengen $M\subset X$ das Bild $T(M)\subset Y$ wiederum offen
  ist. Oder mit anderen Worten: $T$ bildet offene Urbilder auf offene
  Bilder ab.
\end{defini}

\begin{satz}\label{satz:3.2.3/1}
  $X,Y$ Banachräume, Ist $T\in\linOp(X,Y)$ eine surjektive
  Abbildung, dann folgt daraus, $T$ ist offen.

  \begin{proof}
    \begin{schritte}
     \item $U:= K_{\epsilon}(0)$ offene Kugel um $0$ in $X$, $\exists>0$
      beliebig

      Wir zeigen: $\overline{T(U)}$ enthält eine offene Kugel um $0$ in
      $Y$.

      Setzen: $U_{0} = K_{\frac{\epsilon}{2}}(0)$ $\Longrightarrow$ $X=
      \bigcup_{j=1}^{\infty} j U_{0}$

      $T$ surjektiv $\Longrightarrow$
      \begin{gather*}
        Y=T(X) = T(\bigcup_{j=1}^{\infty} jU_{0}) =
        \bigcup_{j=1}^{\infty} jT(U_{0}) = \bigcup_{j=1}^{\infty}
        \overline{jT(U_{0})}
      \end{gather*}

      \autoref{satz:baire} $\Longrightarrow$ $\exists j\in\N$ und
      $\overline{jT(U_{0})}$ enthält eine offene Kugel $\tilde{V}$
      $\Longrightarrow$ $\frac{1}{j}\tilde{V}=:V$ ist offene Kugel,
      enthalten in $\overline{T(U_{0})}$

      Außerdem gilt:
      \begin{gather*}
        V-V \subset \overline{T(U_{0})} - \overline{T(U_{0})} \subset
        \overline{T(U_{0})-T(U_{0})} \subset \overline{T(U)}
      \end{gather*}
      (Die letzte Teilmengenbeziehung gilt, da $U_{0}-U_{0}\subset U$.)
      $V-V$ ist also offen und $0\in V-V$. Daraus folgt $\overline{T(M)}$
      enthält eine offene Kugel um 0.

     \item $U=K_{\epsilon}(0)$. Zeigen $T(U)$ enthält eine offene Kugel
      um 0. Dafür setzen wir $U_{0} := K_{\frac{\epsilon}{2}}(0),
      \epsilon_{0}:= \frac{\epsilon}{2}$. Wir wählen
      $(\epsilon_{j})_{j=1}^{\infty}$ mit $\epsilon_{j}>0$ und
      $\sum_{j=1}^{\infty}\epsilon_{j}<\epsilon_{0}$ und setzen $U_{j}:=
      K_{\epsilon_{j}}(0)$ ($j=1,2,\ldots$)

      Aus dem 1.\,Schritt folgt $\exists V_{j} = K_{\eta_{j}}(0)\subset
      Y$ mit $V_{j}\subset \overline{T(U_{j})}$ ($j=0,1,\ldots$). $T$
      beschränkt $\Rightarrow$ $\eta_{j}\xrightarrow{j\rightarrow\infty} 0$

      Es sei $y\in V_{0}$. Wir zeigen $y\in T(U)$, d.\,h. $x\in U$ und $Tx=y$.

      \underline{Konstruktion von $x$:}
      \begin{alignat*}{2}
        y&\in V_{0} \Longrightarrow y\in \overline{T(U_{0})}
        &&\Longrightarrow \exists x_{0}\in U_{0}\text{ mit }
        \norm{\underbrace{y-Tx_{0}}_{\in V_{1}}}<\eta_{1}\\
        y-Tx_{0}&\in V_{1}\subset \overline{T(U_{1})} &&\Longrightarrow
        \exists x_{1}\in U_{1}\text{ mit }\norm{y-(Tx_{0} + Tx_{1})}<\eta_{2}\\
        \vdots\\
        y-\sum_{j=0}^{n}Tx_{j}&\in V_{n+1}\subset \overline{T(U_{n+1})}
        &&\Longrightarrow \exists x_{n+1}\in U_{n+1}\text{ mit }
        \norm{y-\sum_{j=0}^{n+1}Tx_{j}}<\eta_{n+2}\taglabel{eq:446}
      \end{alignat*}

      Wir bekommen eine Folge $(x_{j})_{j=1}^{\infty}$ \help{wieso j=1?
      Wir haben doch auch $\epsilon_{1}$} mit folgenden
      Eigenschaften:
      \begin{gather*}
        x_{j}\in U_{j} \Longrightarrow \sum_{j=0}^{\infty}
        \norm{x_{j}}<\sum_{j=0}^{\infty} \epsilon_{j}<\infty \Longrightarrow
        \exists x\in X \text{ mit } \sum_{j=0}^{\infty} x_{j} = x \text{,
        da $X$ vollständig}
      \end{gather*}
      $\Longrightarrow$ $\norm{x} \leq \sum_{j=0}^{\infty} \norm{x_{j}} =
      \epsilon_{0}+\epsilon_{0} = \epsilon \Longrightarrow x\in
      U-K_{\epsilon}(0)$

      Aus \autoref{eq:446} folgt für $\eta_{n}\rightarrow0$,
      \begin{gather*}
        y = \sum_{j=0}^{\infty} Tx_{j} = T(\sum_{j=0}^{\infty} x_{j}) =
        Tx \Longrightarrow y\in T(U)
      \end{gather*}
     \item $M\subset X$ offen. Zeigen $T(M)$ offen in $Y$. Wenn $x\in M$,
      dann existiert ein $\epsilon>0$ und eine Kugel
      $K_{\epsilon}(x)\subset M$. Dazu äquivalent ist
      $x+\underbrace{K_{\epsilon}(0)}_{\subset U}\subset M$. Wegen der
      Linearität von $T$ ist $Tx + T(U) \subset T(M)$.

      Aus Schritt\,2 folgt, dass eine offene Kugel $V$ um 0 in $Y$ mit
      $V\subset T(M)$ existiert. $\Longrightarrow$ $Tx+V \subset T(M)$.
      $Tx\in Tx+V, Tx+V$ offen. $\Longrightarrow$ $T(M)$ offen.
    \end{schritte}
  \end{proof}
\end{satz}

\begin{satz}[Satz vom inversen Operator]\label{satz:3.2.3/2}
  Wenn $X$ und $Y$ Banachräume sind und $T\in\linOp(X,Y)$ eine
  bijektive Abbildung, dann ist der inverse Operator
  $T^{-1}\in\linOp(Y,X)$ beschränkt.

  \begin{proof}
    $S\in \linOp(Y,X)$ stetig $\Longleftrightarrow$ $\forall
    U\subset X$ offen $\exists V\subset Y$ offen, so dass $S(V)\subset U$.

    Es sei $U\subset X$ offen. \autoref{satz:3.2.3/1} $\Longrightarrow$
    $T(U) =: V$ offen $\Longrightarrow$ $T^{-1}(V) = U$ $\Longrightarrow$
    $T^{-1}$ stetig.
  \end{proof}
\end{satz}

\begin{bemerk}
  $A\in\linOp(X)$ Wiederholung: Resolventenmenge aus
  \autoref{def:1.4.4/1}
  \begin{gather*}
    M_{A}:=\{\lambda\in\C\colon \exists (A-\lambda I)^{-1}\in\linOp(X)\}
  \end{gather*}
  $S_{A}:= \C\setminus M_{A}$

  \begin{gather*}
    \lambda\in S_{A}
    \begin{cases}
      \dim N(A-\lambda I)>0 &\rightarrow\text{nicht injektiv}\\
      R(A-\lambda I)\subsetneq Y &\rightarrow\text{nicht surjektiv}
    \end{cases}
  \end{gather*}
\end{bemerk}

\begin{bemerk}
  $A\in \linOp(X,Y)$ kompakt, $X,Y$ Banachräume

  Ist $A$ zusätzlich bijektiv, dann liefert \autoref{satz:3.2.3/2}
  $A^{-1}$ ist beschränkt. Die beiden Identitäten $I_{X}=A^{-1}A$ und
  $I_{Y}=AA^{-1}$ sind kompakte Abbildungen und es folgt $X$ und $Y$ sind
  endlich dimensional.

  Kompakte Operatoren zwischen unendlichdimensionalen Banachräumen
  können also nicht bijektiv sein.
\end{bemerk}

\subsection{Der Satz vom abgeschlossenen Graphen}

\begin{description}
 \item[\underline{Vorbemerkung:}] $X,Y$ normierte Vektorräume über $\K$
  \begin{gather*}
    X\times Y = \{(x,y)\colon x\in X\wedge y\in Y\}
  \end{gather*}
  Wir haben in diesem Raum eine gliedweise Addition und eine Multiplikation mit
  $\lambda\in\K$. Der Raum $X\times Y$ ist also ein Vektorraum über $\K$.

  Definiert man die Norm als $\norm{(x,y)} := \norm{x}+ \norm{y}$ als die Norm auf
  $X\times Y$, so folgt $(x_{j}, y_{j})_{X\times Y} \rightarrow (x,y) \Leftrightarrow
  x_{j}\rightarrow x\in X\wedge y_{j}\rightarrow y\in Y$. Sind $X,Y$
  Banachräume, so ist auch $X\times Y$ ein Banachraum
\end{description}

\begin{defini}\label{def:3.2.4/1}
  $X,Y$ normierte Vektorräume, $D\subset X$ Teilraum und $A\colon D\rightarrow
  Y$ linear ($D(A) := D$)
  \begin{enumerate}
   \item $G_{A} := \{ (x,Ax)\colon x\in D(A)\}$ heißt \highl{Graph} von $A$
   \item $A$ heißt \highl[Abbildung!abgeschlossen]{abgeschlossen}
    $:\Longleftrightarrow$ $G_{A}$ ist abgeschlossen in $X\times Y$.
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item
    Eine äquivalente Formulierung zur \autoref{def:3.2.4/1} ist die folgende:

    $G_{A}$ ist abgeschlossen $\Longleftrightarrow$ Aus $(x_{j},
    Ax_{j})_{X\times Y} \rightarrow (x,y)$ folgt $(x,y)\in G_{A}$
    $\Longleftrightarrow$ $D(A) \ni x_{j} \rightarrow x$ und $Ax_{j}
    \rightarrow y$ $\Longrightarrow$ $x\in D(A)$ und $y= Ax$

   \item klar: $A\in\linOp(X,Y)$ $\Longrightarrow$ $A$ abgeschlossen
  \end{enumerate}
\end{bemerk}

\begin{satz}[Closed Graph Theorem]\label{satz:3.2.4/1}
  $X,Y$ Banachräume, Ist $A$ abgeschlossener Operator mit $D(A) = X$,
  dann ist $A\in\linOp(X,Y)$.

  \begin{proof}
    $G_{A}= \{(x, AX)\colon x\in X\}$ abgeschlossener Teilraum in $X\times Y$
    $\Longrightarrow$ $G_{A}$ ist ein Banachraum. Betrachten die
    Projektion $P\colon (x,Ax)\rightarrow x$. $\Longrightarrow$ $P\colon
    G_{A}\rightarrow X$ linear, beschränkt und bijektiv. Nach Satz vom
    inversen Operator (\autoref{satz:3.2.3/2}) $\Rightarrow$ $P^{-1}
    \colon X\rightarrow G_{A}$ beschränkt (und stetig), d.\,h. $x_{j}\rightarrow
    x$ $\Longrightarrow^{P^{-1}}$ $(x_{j}, Ax_{j})_{X\times Y}
    \rightarrow (x,Ax)$ $\Longrightarrow$ $Ax_{j}\rightarrow Ax$
    $\Longrightarrow$ $A$ stetig, $A\in\linOp(X,Y)$.
  \end{proof}
\end{satz}

\begin{satz}[Hellinger-Toeplitz]\label{satz:teoplitz}
  $H$ Hilbertraum, $A\colon H\rightarrow H$ linear und symmetrisch
  (Widerholung \autoref{bem:2.2.1/2}: symmetrisch $\forall x,y\in H\colon
  \sprod{Ax, y} = \sprod{x,Ay}$)

  $\Longrightarrow$ $A\in\linOp(H)$ und $A= A^{\ast}$
  (selbstadjungiert)

  \begin{proof}
    zeigen: $A$ ist abgeschlossen und wir können den Satz vom
    abgeschlossenen Graphen (\autoref{satz:3.2.4/1}) anwenden.

    $x_{j} \rightarrow x$ und $Ax_{j} \rightarrow y$ $\Longrightarrow$
    $\forall z\in H\colon \sprod{Ax_{j}, z} = \sprod{x_{j}, Az}$ $\Longrightarrow$ für
    $j\rightarrow\infty$ $\sprod{y,z} = \sprod{x, Az} = \sprod{Ax, z}$
    $\Longrightarrow$ $y= Ax$
  \end{proof}
\end{satz}

