\begin{satz}[Eigenschaften integrierbarer Funktionen]
  Sei $(\Omega, \mathcal{A}, \mu)$ ein vollständiger $\sigma$-endlicher
  Maßraum, $A\in\mathcal{A}$ und das Maß von $A$ $\mu(A)>0$

  \begin{enumerate}
   \item $f,g\colon A\rightarrow\K$ integrierbar, $\alpha,\beta\in\K$
    \begin{gather*}
      \Longrightarrow \exists \int_{A}(\alpha f +\beta g)\,d\mu =
      \alpha\int_{A} f\,d\mu + \beta\int_{A}g\,d\mu
    \end{gather*}
   \item $f,g\colon A\rightarrow\K$, $f(x) = g(x)$ f.\,ü., $f$ integrierbar,
    dann ist auch $g$ integrierbar und die Integrale sind gleich.
    (vgl.~\autoref{bem:2})
   \item $f,g\colon A\rightarrow\R$ integrierbar und $f\leq g$ f.\,ü.
    \begin{gather*}
      \Longrightarrow \int_{A}f\,d\mu \leq \int_{A}g\,d\mu
    \end{gather*}
   \item $f\colon A\rightarrow\K$ integrierbar $\Longleftrightarrow$
    $\abs{f}\colon A\rightarrow\R$ integrierbar und
    \begin{gather*}
      \abs[\Big]{\int_{A}f\,d\mu} \leq \int_{A} \abs[\big]{f}\,d\mu
    \end{gather*}\help{ist das eine Forderung oder eine zweite Aussage}
   \item Ist $f\colon A\rightarrow\K$ integrierbar, dann gilt:
    \begin{gather*}
      \int_{A} \abs{f(x)}\,d\mu = 0 \Longleftrightarrow f=0\fue
    \end{gather*}
   \item $f,g\colon A\rightarrow\K$, wobei $f$ messbar und $g$ integrierbar
    ist und $\abs{f(x)} \leq \abs{g(x)}$ f.\,ü. So gilt:
    \begin{gather*}
      \exists \int_{A} \abs{f(x)}\,d\mu \leq \int_{A} \abs{g(x)}\,d\mu
    \end{gather*}
  \end{enumerate}
\end{satz}

\begin{bemerk}
  Es gibt kein Analogon zu (4) für Riemann'integrierbare Funktionen.
  Wählt man z.\,B.
  \begin{gather*}
    f(x) = \begin{cases}
             1& x\in[0,1]\cap\Q\\
             0& x\in[0,1]\setminus\Q
           \end{cases}
  \end{gather*}
  dann ist der Betrag $\abs{f(x)}=1$ und somit Riemann'integrierbar, aber
  $f(x)$ selbst ist nicht Riemann'integrierbar. Oder betrachtet man
  $f(x) = \frac{\sin x}{x}$ auf $(0,\infty)$, so existiert zwar das
  (uneigentliche) Riemann-Integral $\int_{0}^{\infty}\frac{\sin
  x}{x}\,dx$, aber das Integral $\int_{0}^{\infty}\abs[\big]{\frac{\sin
  x}{x}}\,dx=\infty$. Damit ist die Funktion aber auch nicht
  Lebesgue'integrierbar.
\end{bemerk}

\begin{satz}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum
  \begin{enumerate}
   \item $f\colon\Omega \rightarrow\K$ integrierbar, $E\in \mathcal{A}$.
    \help{ist das hier wirklich ein $\in$ oder soll es ein $\subset$ sein?} Dann:
    \begin{gather*}
      \int_{\Omega} f\,d\mu = \int_{E} f\,d\mu + \int_{\Omega\setminus E}
      f\,d\mu
    \end{gather*}
   \item Ist $f\colon\Omega\rightarrow\R$ integrierbar und $E\in\mathcal{A}$
    \help{dito}, $f>0$ auf $E$, so folgt aus $\int_{E}f\,d\mu$, dass das
    Maß $\mu(E)=0$
   \item $f\colon\Omega \rightarrow \K$ integrierbar,
    $\Omega=\bigcup_{k=1}^{\infty} E_{k}$ mit $E_{k}\in\mathcal{A}$ und
    die $E_{k}$ sind paarweise disjunkt ($\forall j\ne k\colon E_{k}\cap
    E_{j}=\emptyset$):
    \begin{gather*}
      \int_{\Omega} f\,d\mu = \sum_{k=1}^{\infty}\int_{E_{k}} f\,d\mu
      \qquad\text{abs.konv. Reihe}
    \end{gather*}
  \end{enumerate}
\end{satz}

\begin{folger}
  \begin{enumerate}
   \item $E_{1} \subset E_{2} \subset \ldots$ mit
    $\Omega=\bigcup_{k=1}^{\infty} E_{k}$, $E_{k}\in\mathcal{A}$ $\Longrightarrow$
    \begin{gather*}
      \lim_{k\rightarrow\infty}\int_{E_{k}} f\,d\mu = \int_{\Omega} f\,d\mu
    \end{gather*}
   \item Für eine nichtnegative ($w(x) \geq 0$), integrierbare Funktion
    $w\colon\Omega \rightarrow \R$ und $A\in\mathcal{A}$ ist $\nu$ als das
    \highl{endliche Maß} auf $A$ definiert als:
    \begin{gather*}
      \nu(A) := \int_{A}w(x)\,d\mu
    \end{gather*}
  \end{enumerate}
\end{folger}

\subsection{Konvergenzsätze}

\begin{satz}[Lebesgue]\label{satz:lebesgue}
  Ist $(\Omega, \mathcal{A}, \mu)$ ein vollständiger $\sigma$-endlicher
  Maßraum, $A\in \mathcal{A}, f_{k}\colon A\rightarrow \K$ messbar und es
  existiert $\lim_{k\rightarrow\infty} f_{k}(x) = f(x)$ f.\,ü. Existiert
  weiterhin eine integrierbare Funktion $g\colon A\rightarrow\K$ und
  $\abs{f_{k}(x)} \leq g(x)$ f.\,ü. ($k\in\N$),

  dann existiert auch das Integral auf $f$ und Limes und Integral sind
  vertauschbar:
  \begin{gather*}
    \int_{A} f\,d\mu = \int_{A} \bigl(\lim_{k\rightarrow\infty}
       f_{k}(x)\bigr) \,d\mu = \lim_{k\rightarrow\infty} \int_{A} f_{k}\,d\mu
  \end{gather*}
\end{satz}

% <news:d49.421a464d.9a65a@invisibletruth.local>
\begin{bsp}\hspace*{0ex}\\
  \mbox{}\hfill\input{1x.tex}\hfill\mbox{}

  Für die Funktionenfolge
  \begin{gather*}
    f_{k} = \begin{cases}
              k&\colon x\in [0,\frac1k]\\
              0&\colon\text{sonst}
            \end{cases}
  \end{gather*}
  existiert der $\lim_{k\rightarrow\infty} f_{k}(x) = 0 = g(x)$ f.\,ü.,
  \help{erfüllt das wirklich die Bedingung von \autoref{satz:lebesgue}?}
  der Betrag $\abs{f_{k}(x)}\leq g(x) = \nicefrac{1}{x}$ und
  das Integral $\int_{A} f_{k}(x)\,dx = 1$ für alle $k$ -- die $f_{k}$
  sind also messbar. Aber dennoch gilt nicht
  \begin{gather*}
    1 = \lim_{k\rightarrow\infty}\int f_{k}\,d\mu
       \ne\int \lim_{k\rightarrow\infty} f_{k}\,d\mu = 0
  \end{gather*}
  da die Funktion $g(x)$ nicht integrierbar ist.
\end{bsp}

\begin{satz}[B.~Levi, monotone + beschränkte Folgen]\label{satz:blevi}
  $A\subset \mathcal{A}$. Es sei $(f_{k})_{k}$ eine monotone Folge
  integrierbarer, reeller Funktionen $f_{k}\colon A \rightarrow \R$ und es sei die
  Folge der Integrale $(\int_{A} f_{k}\,d\mu)_{k}$ beschränkt.

  Dann existiert f.\,ü. auf $A$ der $\lim_{k\rightarrow\infty} f_{k} =:
  f(x)$, $f$ ist integrierbar und es gilt
  \begin{gather*}
    \lim_{k\rightarrow\infty}\int_{A} f_{k}\,d\mu =
       \int_{A}\lim_{k\rightarrow\infty} f_{k}\,d\mu = \int_{A} f\,d\mu
  \end{gather*}
\end{satz}

\begin{bemerk}
  $f_{k}$ sei $\geq 0$ und integrierbar und die
  $\sum_{k=1}^{\infty}\int_{A} f_{k}\,d\mu < \infty$.

  Dann existiert $f(x) := \sum_{k=1}^{\infty}f_{k}(x)$ f.\,ü. und das
  \begin{gather*}
    \int_{A}\sum_{k=1}^{\infty} f_{k}(x)\,d\mu =
       \sum_{k=1}^{\infty}\int_{A} f_{k}(x)\,d\mu
  \end{gather*}
\end{bemerk}

\begin{satz}[Lemma von Faton]\label{satz:faton-lemma}
  $A\subset \mathcal{A}, f_{k}\colon A\rightarrow\R$ integrierbar, $f_{k} \geq
  0$ auf $A$. Es sei weiterhin der $\liminf\limits_{k\rightarrow\infty}
  \int_{A} f_{k}\,d\mu < \infty$.

  Dann ist $\liminf_{k\rightarrow\infty} f_{k}(x)$ f.\,ü. endlich
  und integrierbar und
  \begin{gather*}
    \int_{A}\liminf_{k\rightarrow\infty} f_{k}(x)\,d\mu
    \leq \liminf_{k\rightarrow\infty} \int_{A} f_{k}\,d\mu
  \end{gather*}
\end{satz}

\begin{bemerk}
  Sind die Vorraussetungen die gleichen wie im Lemma von Faton
  (\autoref{satz:faton-lemma}) und existiert f.\,ü. der
  $\lim_{k\rightarrow\infty} f_{k}(x) =f(x)$, dann ist $f$ integrierbar und
  \begin{gather*}
    \int_{A} f\,d\mu \leq \liminf_{k\rightarrow\infty} \int_{A} f_{k}\,d\mu
  \end{gather*}
\end{bemerk}

\subsection{Die Räume \texorpdfstring{$L_{p}(A,\mu)$}{Lp(A,\textmu)}}

\begin{defini}\index{Lp@$L_{p}$}\index{Lunend@$L_{\infty}$}
  \index{Norm!p@$p$}\index{Norm!unend@$\infty$}
  \label{def:Lp-Raeume}

  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum,
  $0<p<\infty, A\in \mathcal{A}$
  % \begin{enumerate}
  %  \item
    \begin{align*}
      L_{p}(A, \mu) &:= \{ f\colon A\rightarrow\K \colon f\ \text{messbar},
         \exists\int_{A} \abs{f}^{p}\,d\mu < \infty\}\\
      \norm{f}_{p} &:= \Bigl(\int_{A} \abs{f}^{p}\,d\mu\Bigr)^{\frac1p}
         \\
    % \end{align*}
   % \item
    % \begin{align*}
      L_{\infty}(A,\mu) &:= \{ f\colon A\rightarrow\K \colon f\ \text{messbar},
         \exists c>0, \exists N, \mu(N) = 0 \ \text{mit}\ \abs{f(x)}\leq c
         \ \forall x\in A\setminus N\}\\
      \norm{f}_{\infty} &:= \inf \{ c\colon \mu(\{x\colon f(x) > c\}) = 0\}
    \end{align*}
  % \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item Als "`\highl{wesentliches Supremum}"' $\esssup$ bezeichnet man
    \index{esssup@$\esssup$}
    \begin{gather*}
      \esssup_{x\in A}\abs{f(x)} := \norm{f}_{\infty} = \inf \bigl\{
      \sup_{x\in A\setminus N} \abs{f(x)}: \mu(N) =0\bigr\}
    \end{gather*}
   \item
      $f\in L_{\infty}(A,\mu) \Longrightarrow N, \mu(N) =0$, so dass
    \begin{align*}
      \norm{f}_{\infty} &= \sup_{x\in A\setminus N}\abs{f(x)}\\
      \norm{f}_{\infty} = 0 &\Longleftrightarrow \forall x\in A\setminus N\colon
         \abs{f(x)}=0 \Longleftrightarrow f(x) = 0\fue
    \end{align*}
   \item $p<\infty$ $\Longrightarrow$
    \begin{gather*}
      \norm{f}_{p} = \Bigl(\int_{A} \abs{f(x)}^{p}\,d\mu\Bigr)^{\frac1p} = 0
         \Longleftrightarrow f(x) = 0\fue
    \end{gather*}

    Übergang zu Äquivalenzklassen: $f\sim g \Longleftrightarrow f=g$ f.\,ü.
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:hoelder}
  \begin{enumerate}
   \item Gilt für $p$: $1\leq p \leq \infty$ und $\frac1p + \frac1{p'} =
    1$ ($p' = \infty$, falls $p=1$). Und sei weiterhin $f\in L_{p}(A,\mu)$
    und $g\in L_{p'}(A,\mu)$. Dann ist $f\cdot g\in L_{1}(A, \mu)$ und
    $\norm{f\cdot g}_{1} \leq \norm{f}_{p}\: \norm{g}_{p'}$

    Es gilt dann die \highl{Hölder'sche Ungleichung} für Integrale:
    \begin{gather}\label{eq:hoelder-int}
      \int_{A}\abs{f(x) g(x)}\,d\mu \leq
         \Bigl(\int_{A}\abs{f(x)}^{p}\,d\mu\Bigr)^{\nicefrac1p}
         \Bigl(\int_{A}\abs{g(x)}^{p'}\,d\mu\Bigr)^{\nicefrac1{p'}}
    \end{gather}

    Vergleiche auch: \autoref{lem:hoelder-sum} mit der Ungleichung für Summen
    \autoref{eq:hoelder-sum}
   \item $0<p \leq \infty$. Es seien $f,g\in L_{p}(A,\mu)$.

    Dann ist $f+g\in L_{p}(A,\mu)$ und
    \begin{gather}
      \norm{f+g}_{p} \leq \norm{f}_{p} + \norm{g}_{p}\ \text{falls } 1\leq p \leq
         \infty
         \qquad \text{Minkowski'sche Ungleichung}\label{eq:minkowski}\\
      \norm{f+g}_{p}^{p} \leq \norm{f}_{p}^{p} + \norm{g}_{p}^{p} \leq
         2^{1-p}\big(\norm{f}_{p} + \norm{g}_{p}\big)^{p}\ \text{falls}\ 0<p <1
         \notag
    \end{gather}
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}[zu~(1)]
     \item  $1<p<\infty$. Aus \autoref{lem:young-ungl} folgt:
      \begin{gather*}
        \abs{f(x) g(x)} \leq \frac{\abs{f(x)}^{p}}p + \frac{\abs{g(x)}^{p'}}p' \in L_{1}(A,\mu)
      \end{gather*}
      $f\cdot g$ messbar $\Longrightarrow$ $f\cdot g\in L_{1}$

      o.\,B.\,d.\,A.\help{wieso gilt obda?}: $\norm{f}_{p} = \norm{g}_{p'} =1$
      \begin{gather*}
        \Longrightarrow \int_{A} \abs{f(x) g(x)}\,d\mu \leq \frac1p
           \norm{f}_{p}^{p} + \frac1{p'} \norm{g}_{p'}^{p'} =
           \frac1p + \frac1{p'} = 1 = \norm{f}_{p} + \norm{g}_{p'}
      \end{gather*}
     \item $1\leq p \leq \infty$, \autoref{lem:1.2.1/4} $\Longrightarrow$
      $\norm{f(x) + g(x)}^{p} \leq 2^{p-1}( \abs{f(x)}^{p} + \abs{g(x)}^{p})\in
      L_{1}(A,\mu)$. Da $f(x) + g(x)$ messbar $\Longrightarrow$
      $\exists\int_{A} \abs{f(x) +g(x)}^{p}\,dx<\infty$

      $0<p < 1$ $\Longrightarrow$ $\abs{f(x) +g(x)}$ messbar $\Longrightarrow$
      $f+g\in L_{p}(A,\mu)$ Beweis der Ungleichung analog zum Beweis für
      $l_{p}$ (\autoref{lem:minkowski})
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{satz}
  $\mu(A) < \infty, 0 < p < q \leq \infty$.

  Dann ist $L_{q}(A, \mu) \subset L_{p}(A,\mu)$ und $\norm{f}_{p} \leq
  \mu(\Omega)^{\frac1p-\frac1q}\norm{f}_{q}$

  \begin{proof}
    $u = \frac{q}p>1, \frac1u+\frac1v =1, \frac1v = 1-\frac{p}q$
    \begin{align*}
      \int_{A} \abs{f(x)}^{p}\,d\mu &= \int_{A} \abs{f(x)}^{p} \chi_{A}\,d\mu\\
      &\leq \left(\int_{A}\abs{f(x)}^{q}\,d\mu\right)^{\frac{p}q}\cdot
         \bigl(\mu(A)\bigr)^{1-\frac{p}q}
    \end{align*}
    $\Longrightarrow$ $\norm{f}_{p} \leq \mu(A)^{\frac1p-\frac1q}\norm{f}_{p}$
  \end{proof}
\end{satz}

\begin{satz}\label{satz:3}
  $(\Omega, \mathcal{A}, \mu)$ wie in \autoref{def:Lp-Raeume},
  $A\in\mathcal{A}, 1 \leq p \leq \infty$. Es sei $(f_{k})_{k}$ eine
  Cauchyfolge in $L_{p}(A,\mu)$.

  Dann existiert $f\in L_{p}(A,\mu)$ mit $\norm{f_{k}-f}_{p}
  \xrightarrow{k\rightarrow\infty}0$

  \begin{proof}
    $1\leq p <\infty$: \autoref{satz:1.1.1/1} anwendbar. Es sei
    \begin{gather}
      \sum_{k=1}^{\infty}\norm{g_{k}(x)}_{p} < \infty\label{eq:15}
    \end{gather}

    \underline{zu zeigen:} $\exists g\in L_{p}(A,\mu)$ mit $g = \sum_{k=1}^{\infty}
    g_{k}(x)$ in $L_{p}(A,\mu)$, d.\,h. $\norm{\sum_{k=1}^{n} g_{k}-g}_{p}
    \xrightarrow{n\rightarrow\infty}0$

    Es ist $A = \bigcup_{j=1}^{\infty} A_{j}$ mit $\mu(A_{j})<
    \infty$, da $\mu$ $\sigma$-endlich

    $\forall j \Longrightarrow \int_{A_{j}}
    \sum_{k=1}^{n}\abs{g_{k}(x)}\,d\mu \leq \norm{\sum_{k=1}^{n}
    \abs{g_{k}(x)}}_{p}\ \mu(A_{j})^{1-\frac1p}\leq c
    \sum_{k=1}^{n}\norm{g_{k}(x)}_{p} <^{\text{\autoref{eq:15}}} \infty$

    Aus dem Satz von B.\,Levi (\autoref{satz:blevi}) folgt, dass die Summe
    $\sum_{k=1}^{\infty} \abs{g_{k}(x)}$ f.\,ü. auf $A_{j} (\forall j)$
    existiert.

    $\Longrightarrow$ $\exists h(x) := \sum_{k=1}^{\infty}
    \abs{g_{k}(x)}$ f.\,ü. auf A, $\abs{g(x)} \leq h(x)$

    Es gilt:
    \begin{gather*}
      \int_{A} \biggl(\sum_{k}^{n}\abs{g_{k}(x)}_{p}\biggr)^{p}\,d\mu =
         \Biggl(\norm[\bigg]{\sum_{k=1}^{n}\abs{g_{k}(x)}}_{p}\Biggr) \leq
         \biggl(\sum_{k=1}^{n}\norm{g_{k}}_{p}\biggr)^{p} <^{\text{\autoref{eq:15}}} \infty\\
      \intertext{Nach dem Satz von B.\,Levi (\autoref{satz:blevi})}
      \Longrightarrow \exists
         \int_{A}\biggl(\sum_{k=1}^{\infty}\abs{g_{k}(x)}\biggr)^{p}\,d\mu =
         \int_{A} h(x)^{p}\,d\mu \Longrightarrow h\in L_{p}(A,\mu)\\
      \Longrightarrow g\in L_{p}(A,\mu)
    \end{gather*}
  \end{proof}
\end{satz}

