\subsection{Operationen in \texorpdfstring{$D'(\Omega)$}{D'(Omega)}}

$D'(\Omega), \Omega\subset \R^{n}$ Gebiet

\begin{defini}
  \begin{enumerate}
   \item seien $\lambda_{1}, \lambda_{2}\in \C, T_{1}, T_{2}\in D'(\Omega)$. Dann
    ist:
    \begin{gather*}
       (\lambda_{1}T_{1} + \lambda_{2} T_{2}) (\phi) =
      \lambda_{1}T_{1}(\phi) + \lambda_{2} T_{2}(\phi), \phi\in D(\Omega)
    \end{gather*}
   \item seien $\alpha\in\N_{0}^{n}, T\in D'(\Omega)$. Dann ist
    \begin{gather*}
      (D^{\alpha}T)(\phi) := (-1)^{\abs{\alpha}} T(D^{\alpha}\phi),
      \phi\in D(\Omega)
    \end{gather*}
   \item sei $a \in C^{\infty}(\Omega), T\in D'(\Omega)$
    \begin{gather*}
      (a(x) T) (\phi) := T(a\phi), \delta \in D(\Omega)
    \end{gather*}
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{itemize}
   \item $\lambda_{1}T_{1}+ \lambda_{2}T_{2}\in D'(\Omega)$
    \begin{description}
     \item[Linearität] klar
     \item[{$\phi_{j} \xrightarrow[D]{} \phi$}] $\Longrightarrow$
      $T_{1}(\phi_{j}) \rightarrow T_{1}(\phi), T_{2}(\phi_{j})
      \rightarrow T_{2}(\phi)$ $\Longrightarrow$ $(\lambda_{1}T_{1} +
      \lambda_{2} T_{2})(\phi_{j}) \rightarrow (\lambda_{1}T_{1} +
      \lambda_{2} T_{2})(\phi)$
    \end{description}
   \item $D^{\alpha}T\in D'(\Omega), \alpha\in \N_{0}^{n}$
    \begin{description}
     \item[Linearität] klar
     \item[stetig] $\phi_{j}\xrightarrow[D]{} \phi$ $\Longrightarrow$
      $D^{\alpha}\phi_{j} \xrightarrow[D]{} D^{\alpha}\phi,
      \alpha\in\N_{0}^{n}$ $\Longrightarrow^{T\in D'(\Omega)}$
      $T(D^{\alpha}\phi_{j}) \rightarrow T(D^{\alpha} \phi)$
      $\Longrightarrow$ $(D^{\alpha} T) (\phi_{j}) \rightarrow
      (D^{\alpha}T) (\phi)$
    \end{description}
   \item $a(x) T\in D'(\Omega), a\in C^{\infty}, T\in D'(\Omega)$
    \begin{description}
     \item[$\phi\in D(\Omega) \rightarrow a\phi\in D(\Omega)$]
      $\Longrightarrow$ $T(a\phi)$ def.
     \item[$aT$ linear] $(a(x) T) (\lambda_{1}\phi_{1} +\lambda_{2}T_{2})
      = T(a(x) (\lambda_{1} \phi_{1} + \lambda_{2}T_{2})) = \lambda_{1}
      T(a(x)\phi_{1}) + \lambda_{2} T(a(x) \phi_{2}) =
      \lambda_{1}(aT)(\phi_{1}) + \lambda_{2}(aT)(\phi_{2})$
     \item[stetig:] $\phi_{1} \xrightarrow[D]{} \phi$\\
      $D^{\beta}(a(x) \phi_{j}(x)) = \sum c_{\alpha\gamma} D^{\alpha}
      a(x) D^{\gamma}\phi_{j}(x) \xrightarrow{glm.} \sum c_{\alpha\gamma}
      \underbrace{D^{\alpha} a D^{\gamma}\phi}_{D^{\beta}(a\phi)}$
      $\Longrightarrow$ $a\phi_{j} \xrightarrow[D]{} a\phi$
      $\Longrightarrow$ $T(a\phi_{j}) \rightarrow T(a\phi)
      \Longrightarrow (aT)(\phi_{j}) \rightarrow (aT)(\phi)$
    \end{description}
  \end{itemize}
\end{bemerk}

