\subsection{Satz von Fubini, Transformationsformeln}

\begin{satz}\label{satz:fubini}
  Es sei $f:\R^{n+m} \rightarrow \K$ ein Lebesgue-messbare Funktion.
  $f = f(x,y)$ mit $x\in\R^{n}, y\in \R^{m}$. Es existiere eines der
  folgenden Integrale:
  \begin{align*}
    I_{1} &= \int_{\R^{n+m}} |f(x,y)|\,d(x,y)\\
    I_{2} &= \int_{\R^{m}} \Bigl( \int_{\R^{n}}|f(x,y)|\,dx\Bigr)\,dy\\
    I_{3} &= \int_{\R^{n}} \Bigl( \int_{\R^{m}}|f(x,y)|\,dy\Bigr)\,dx
  \end{align*}
  
  Dann gelten folgende Aussagen:
  \begin{enumerate}
   \item $f(\cdot,y)\in L_{1}(\R^{n})$ für fast alle $y\in \R^{m}$\\
    $f(x,\cdot)\in L_{1}(\R^{m})$ für fast alle $x\in \R^{n}$ und\\
    $\int_{\R^{n}}f(x,y)\,dx\in L_{1}(\R^{m}),\quad
    \int_{\R^{m}}f(x,y)\,dy\in L_{1}(\R^{n})$
   \item $I_{1}= I_{2}= I_{3}$
   \item \begin{equation*}
	   \int_{\R^{n+m}} f(x,y)\,d(x,y) = \int_{\R^{n}} \Bigl( \int_{\R^{m}}
	      f(x,y)\,dy\Bigr)\,dx = \int_{\R^{m}} \Bigl( \int_{R^{n}}
	      f(x,y)\,dx\Bigr)\,dy
	 \end{equation*}
  \end{enumerate}
\end{satz}

\begin{satz}[Transformationsformel]
  Es sei $\Omega\subset \R^{n}$ offen, $g: G\rightarrow\Omega$ ein
  Diffeomorphismus (d.\,h. $g$ bijektiv, $C^{1}$-Abbildung) und
  $\frac{\partial(g_{1},\ldots,g_{n})}{\partial(u_{1},\ldots,u_{n})}
  \not=0$ auf $G$.
  
  Dann:
  \begin{gather*}
    \exists \int_{\Omega} f\,dx \Longleftrightarrow \exists \int_{G}
       f\circ g(u)
       \frac{\partial(g_{1},\ldots,g_{n})}{\partial(u_{1},\ldots,u_{n})}\,du\\
    \intertext{und}
       \int_{\Omega} f\,dx = \int_{G} (f\circ g)(u)
       \left|\frac{\partial(g_{1},\ldots,g_{n})}
       {\partial(u_{1},\ldots,u_{n})}\right|\,du
  \end{gather*}
\end{satz}

