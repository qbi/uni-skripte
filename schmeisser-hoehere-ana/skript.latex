\begin{bemerk}\label{bem:3}
  $1\leq p \leq \infty, \|f\|_{p}$ ist Hilbertraum, $\|f\|_{p} = 0
  \Longleftrightarrow f= 0$ $\mu$-\fue
  
  $f\sim g :\Longleftrightarrow f= g$ \fue
  
  In diesem Sinne gilt:
  $[f] = \{ g : g\sim f\}, [0] = \{ g: g=0\ \fue,
  \|[0]\|_{p} := \|f\|_{p}$ falls $f\in [f]$
\end{bemerk}

\begin{satz}\label{satz:17}
  \begin{enumerate}
   \item $1\leq p \leq \infty$. $L_{p}(A, \mu)$ ist Banachraum
   \item $1\leq p \leq \infty$. Dann ist die Menge aller
    Treppenfunktionen dicht in $L_{p}(A,\mu)$ .
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item folgt aus \thref{satz:hoelder} und \thref{satz:3} und aus
      \thref{bem:3}.
     \item 
      \begin{schritte}
       \item $p=1, f\in L_{1}(A, \mu)$ Def $\Longrightarrow$ $\exists$
	Folge $(s_{k})_{k}$ von Treppenfunktionen mit:
	
	\begin{align*}
	  s_{k}(x) \rightarrow f(x)\ \fue\\
	  \int_{A}|s_{k}-s_{j}|\,d\mu \xrightarrow{j,k \rightarrow \infty} 0\\
	  \int_{A}f\,d\mu = \lim_{k\rightarrow\infty} \int_{A} s_{k}\,d\mu
	\end{align*}

	Es ist $\lim_{j\rightarrow\infty} |s_{k}-s_{j}|= |s_{k}-f|$ \fue
	$\wedge |s_{k}-f|\in L_{1}(A,\mu)$
	
	Lemma von Faton (\thref{satz:faton-lemma}) $\Longrightarrow$
	$\int_{A}|s_{k}-f|\,d\mu = \int_{A}\lim_{k\rightarrow\infty}
	|s_{k}-s_{j}|\,d\mu \leq \liminf_{k\rightarrow\infty}
	\int_{A}|s_{k}-s_{j}|\,d\mu \leq \epsilon$ für $j,k \rightarrow
	j(\epsilon)$ 
       \item $1 < p<\infty$. Sei $f\in L_{p}(A,\mu), 0 < \epsilon <1$

	$A_{\epsilon}:= \{ x: \epsilon \leq |f(x)| \leq \frac1{\epsilon} \}
	\in \mathcal{A}$
	$\Rightarrow$ $\int_{A}|f|^{p}\,d\mu \geq
	\in_{A_{\epsilon}}|f|\,d\mu \geq \epsilon^{p}\mu(A_{\epsilon})
	\Longrightarrow \mu(A_{\epsilon}) < \infty$
	
	Außerdem: $\|\chi_{A_{\epsilon}}f\| \leq \frac1{\epsilon}
	\chi_{A_{\epsilon}} \in L_{1}(A,\mu)$, da $\mu(A)_{\epsilon}<\infty$
	
	$\Rightarrow$ $\chi_{A_{\epsilon}}f\in L_{1}(A,\mu)$
	$\Rightarrow^{\text{nach 1.\,Schritt}} \exists$ Folge
	$(g^{\epsilon}_{k})_{k}$ von Treppenfunktionen mit
	$\|g_{k}^{\epsilon}-\chi_{A_{\epsilon}}f\|_{1}\rightarrow 0$ und
	$g_{k}^{\epsilon}\rightarrow \chi_{A_{\epsilon}}f = f^{\epsilon}$
	\fue. $g_{k}^{\epsilon}=0$ auf $A\setminus A_{\epsilon}$

	setzen: $f_{k}^{\epsilon}(x) := 
	\begin{cases} g_{k}^{\epsilon}(x)& x\in A_{\epsilon}\wedge
	  |g_{k}^{\epsilon}(x)| \leq \frac2{\epsilon}\\
	  \frac2{\epsilon}\frac{g_{k}^{\epsilon}(x)}
	  {|g_{k}^{\epsilon}(x)|}& x\in A \wedge |g_{k}^{\epsilon}(x)| >
	  \frac2{\epsilon}\\
	  0& x\in A\setminus A_{\epsilon}
	\end{cases}$
	
	$\Rightarrow$ $f_{k}^{\epsilon}$ ist Treppenfunktion,
	$|f_{k}^{\epsilon}(x)| \leq \frac{2}{\epsilon}$ auf $A$. Für
	$|g_{k}^{\epsilon}(x)|\geq \frac2{\epsilon}$ und $x\in
	A_{\epsilon}$ ist:
	\begin{align*}
	  |g_{k}^{\epsilon}(x)| > \frac1{\epsilon} + |f(x)|
	     \Longrightarrow \frac1{\epsilon} < |g_{k}^{\epsilon}(x)| -
	     |f(x)|\label{eq:17}\\
	  \intertext{und somit:}\\
	  |f_{k}^{\epsilon}(x) - f(x)| \leq |f_{k}^{\epsilon}(x)|+ |f(x)|
	     \leq^{\ref{eq:17}} \frac{3}{\epsilon} 3 |g_{k}^{\epsilon}(x)-f(x)|\\
	  \Longrightarrow \|f_{k}^{\epsilon}-f^{\epsilon}\|_{1}
	     \xrightarrow{k\rightarrow\infty} 0\\
	  \Longrightarrow \int_{A}|f-f_{k}^{\epsilon}|^{p}\, d\mu &=
	     \int_{A\setminus A_{\epsilon}} |f(x)|^{p}\,d\mu +
	     \int_{A_{\epsilon}} \underbrace{|f_{k}^{\epsilon}-
	     f^{\epsilon}|^{p-1}}_{\leq (\frac3{\epsilon})^{p-1}}
	     |f_{k}^{\epsilon}-f|\,d\mu\\
	  &\leq \underbrace{\int_{A\setminus A_{\epsilon}}
	     |f(x)|^{p}\,d\mu}_{\xrightarrow{\epsilon \rightarrow0}0 da
	     \mu(A\setminus A_{\epsilon})\rightarrow0} +
	     (\frac3{\epsilon})^{p-1}
	     \|f_{k}^{\epsilon}-f^{\epsilon}\|_{1} \Longrightarrow
	     \|f-f_{k}^{\epsilon}\| \xrightarrow{k\rightarrow\infty,
	     \epsilon\rightarrow0}
	\end{align*}
      \end{schritte}
    \end{enumerate}
  \end{proof}
\end{satz}
		  
