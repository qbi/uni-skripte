\subsection{Räume stetiger Funktionen}

\begin{defini}
  $X$ metrischer Raum, $E$ Banachraum
  \begin{gather*}
    C(X,E) := \{ f: X \rightarrow E: f \text{ stetig auf } X\}\\
    B(X,E) := \{ f: X \rightarrow E: f \text{ beschränkt auf } X\}\\
    \|f\|_{\infty} = \|f\|_{B(X,E)} := \sup_{x\in X}\|f(x)\|_{E}
  \end{gather*}
\end{defini}

\begin{satz}
  $(C(X,E) \cap B(X,E), \|\cdot\|_{\infty})$ ist abgeschlossener Teilraum
  von $B(X, E)$
  
  \begin{proof}
    wissen $(B(X,E), \|\cdot\|_{\infty})$ ist Banachraum
    
    $C(X,E) \cap B(X,E)$ ist Teilraum, normiert. Es sei $(f_{j})_{j}
    \subset C(X,E) \cap B(X,E)$ Cauchyfolge

    $\Longrightarrow$ $\exists f\in B(X,E):
    \|f_{j}-f\|_{\infty}\xrightarrow{j\rightarrow\infty} 0$
    
    Damit bleibt also zu zeigen: $f\in C(X,E)$
    \begin{gather*}
      \epsilon > 0,\ x_{0}\in X,\\
      \|f(x_{0})-f(x)\|_{E} \leq
	 \underbrace{\|f(x_{0})-f_{j}(x_{0})\|_{E}}_{<\epsilon}
	 + \underbrace{\|f_{j}(x_{0}) - f_{j}(x)\|_{E}}_{<\epsilon}+
	  \underbrace{\|f_{j}(x) - f(x)\|_{E}}_{<\epsilon}
    \end{gather*}
  \end{proof}
\end{satz}

$(X,d)$ metr. Raum, $(E, \|\cdot\|)$ Banachraum, $(B(X,E),
\|\cdot\|_{\infty})$ Banachraum, $(C(X,E)\cap B(X,E),
\|\cdot\|_{\infty})$ Banachraum

\begin{defini}
  $U\subset B(X,E) := \{ f:X\rightarrow E: f\text{ gleichmäßig stetig
  und beschränkt}\}$
\end{defini}

\begin{satz}
  $(U\subset B(X,E), \|\cdot\|_{\infty})$ abgeschlossener Teilraum von
  $B(X,E)$
  
  \begin{proof}
    wie bei Satz 1
  \end{proof}
\end{satz}

\begin{bemerk}
  $(X,d)$ kompakter metr. Raum $\Rightarrow$ $U \subset B(X,E) =
  C(X,E) \cap B(X,E) = C(X,E)$ von Interesse $E=\R,\C,
  X=\Omega\subset\R^{n}$ mit $\Omega$ offen und beschränkt
    
  $\Longrightarrow$ $\overline{\Omega}$ kompakt $\Longrightarrow$
  $C(\overline{\Omega}, E) = U\subset B(\overline{\Omega}, E)$
  
  \begin{align*}
    f\in U\subset B(\overline{\Omega}, E) &\Longrightarrow f|_{\Omega}\in
       U\subset B(\Omega, E)\\
    g \in U\subset B(\Omega, E) &\Longrightarrow \exists! f\in U\subset
       B(\overline{\Omega}): f|_{\Omega}=g
  \end{align*}
  $C$ stetige Fortsetzung auf $\overline{\Omega}$ (Ü.A.)

  In diesem Sinne: $ U\in B(\Omega, F) = U\in B(\overline{\Omega}, E)$
\end{bemerk}

\begin{defini}
  \begin{enumerate}
   \item $\Omega \in\R^{n}$ offen, beschränkt, $E=\R,\C$
    \begin{gather*}
      C(\Omega) := C(\Omega, E)\\
      C(\overline{\Omega}) := U\subset B(\overline{\Omega}, E)
    \end{gather*}
   \item $\Omega \subset \R^{n}$ offen (nicht notwendig beschränkt)
    \begin{equation*}
      \overline{C}(\Omega) := U\subset B(\Omega, E)
    \end{equation*}
    
  \end{enumerate}
  Also: $(\overline{C}(\Omega), \|\cdot\|_{\infty})$ Banachräume
\end{defini}

\begin{defini}
  $(X,d)$ kompakt, metr. Raum $E=\R,\C$
  
  $A\subset C(X,E)$ heißt \highl{gleichgradig} (gleichmäßig) stetig
  $:\Longleftrightarrow$
  \begin{equation*}
    \forall \epsilon>0 \exists\delta>0 \forall x,x'\in X:
       d(x,x')<\delta : \sup_{f\in A}|f(x)-f(x')|<\epsilon
  \end{equation*}
\end{defini}

\begin{satz}[Arzela-Ascoli]\label{satz:arzela}
  $(X,d)$ kompakter metrischer Raum, $E=\R,\C$.
  
  Dann gilt: $A\subset C(X,E)$ präkompakt $\Longleftrightarrow$ $A$ ist
  beschränkt und gleichgradig stetig.
  
  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] $A$ präkompakt $\Longrightarrow$ $A$ ist
      beschränkt und $\forall \epsilon >0$ existiert ein endliches
      $\epsilon$-Netz (nach~\thref{satz:1.1.3/2})
      
      Es sei $\epsilon>0$ und es sei $\{f_{1},\ldots,f_{m}\}\subset
      C(X,E)$ endliches $\epsilon$-Netz für $A$. Weiterhin sei $f\in A$.
      
      Dann existiert ein $j\in \{1,\ldots,m\}$ mit
      \begin{equation*}
	\|f_{j}-f\|_{\infty}=\sup_{x\in X}|f_{j}(x) - f(x)|<\epsilon
      \end{equation*}
      und $f_{1},\ldots,f_{m}$ sind gleichm. stetig $\Longrightarrow$
      $\exists\delta\ \forall x,x'\in X$ mit $d(x, x')<\delta:
      \sup_{j}|f_{j}(x)- f(x')|<\epsilon$
      
      $\Longrightarrow$ Für $f\in A$ und $d(x,x')<\delta$ gilt:
      \begin{align*}
	|f(x)-f(x')| &\leq |f(x)
	   -f_{j}(x)|+|f_{j}(x)-f_{j}(x')|+|f_{j}(x')-f(x')|\\
	&\leq 2 \underbrace{\|f_{j}-f\|_{\infty}}_{<\epsilon} +
	   \underbrace{\|f_{j}(x)-f_{j}(x')\|_{\infty}}_{<\epsilon} < 3\epsilon
      \end{align*}
      $\Longrightarrow$ $\sup_{f\in A} |f(x)-f(x')|\leq 3\epsilon$
      $\Longrightarrow$ A gleichm. stetig
     \item["`$\Leftarrow$"'] $(X,d)$ kompakt nach \thref{fol:1.1.3/1}. Es
      existiert in $X$ eine abzählbare und dichte Teilmenge $M
      =\{x_{1},x_{2},x_{3},\ldots\}\subset X$.
      
      Es sei $(f_{k})_{k}\in A$ Folge. $A$ beschränkt $\Longrightarrow$
      Jeder folge $(f_{k}(x_{j}))_{k}$ ist beschränkt in $E$.
      
      \begin{enumerate}
       \item Wählen Teilfolge $(f_{k}^{1})_{k}\subset (f_{k})_{k}$ mit
	$f_{k}^{1}(x_{1}) \rightarrow y_{1}\in E$
       \item Wähle Teilfolge $(f_{k}^{2})_{k}\in (f_{k}^{1})_{k}$ mit
	$f_{k}^{2}(x_{2}) \rightarrow y_{2}\in E$
	
	\ldots
       \item Wählen Teilfolge $(f_{k}^{l})_{k}\subset (f_{k}^{l-1})_{k}$
	mit $f_{k}^{l}(x_{l}) \rightarrow y_{l}\in E$
      \end{enumerate}
      
      Betrachten die Diagonalfolge
      \begin{equation*}
	(g_{k})_{k} := (f_{k}^{k})_{k}\in A
      \end{equation*}
      
      Nach Konstruktion ist für jedes $x_{l}\in M$ die Folge
      $(g_{k}(x_{l}))_{k}$ konvergent.
      
      \underline{Wir zeigen:} $(g_{k})$ ist konvergent in $C(X,E)$.
      
      Es sei $\epsilon>0$. Aus der gleichgradigen Stetigkeit von $A$ folgt:
      \begin{equation}\label{gl7}
	\exists\delta>0\forall x,x': d(x,x') < \delta \forall j\in \N:
	   |g_{j}(x)-g_{j}(x')|<\epsilon
      \end{equation}
      $M$ dicht in $X$ $\Longrightarrow$ $X=\bigcup_{l=1}^{\infty}
      K_{\delta}(x_{l})$\\
      $X$ kompakt $\Longrightarrow$ $\exists n$ und
      $X=\bigcup_{l=1}^{n}K_{\delta}(x_{l})$.\\
      Aus der Konvergenz $g_{j}(x^{l})\rightarrow y_{l}$ folgt:
      \begin{equation}\label{gl8}
	\exists j(\epsilon)\forall j,k\geq j(\epsilon) \forall
	   l=1,\ldots,n: |g_{j}(x^{l})-g_{n}(x^{l})|<\epsilon
      \end{equation}
      
      Es sei nun $x\in X$. $\exists l\in \{1,\ldots,n\} \wedge x\in
      K_{\delta}(x_{l})$. Dann gilt für $j,k \geq j(\epsilon):$
      \begin{equation*}
	|g_{j}(x)-g_{k}(x)|\leq
	   \underbrace{|g_{j}(x) - g_{j}(x_{l})|}_{<\epsilon
	     \text{nach (\ref{gl7})}}
	   +\underbrace{|g_{j}(x_{l})-g_{k}(x_{l})|}_{<\epsilon
	   \text{nach (\ref{gl8})}}
	   +\underbrace{|g_{k}(x_{l})- g_{k}(x)|}_{<\epsilon
	   \text{nach (\ref{gl7})}} < 3\epsilon
      \end{equation*}
      
      $\Longrightarrow$ $\|g_{j}-g_{k}\|_{\infty}\leq 3\epsilon$
      $\Longrightarrow$ $(g_{k})$ Cauchyfolge in $C(X,E)$
      $\Longrightarrow$ konvergent, da Banachraum
    \end{description}
  \end{proof}
\end{satz}

\begin{defini}
  $(X,d)$ kompakter metrischer Raum.
  \begin{enumerate}
   \item $A\subset C(X,\K)$ heißt \highl{Unteralgebra}
    $:\Longleftrightarrow$ $A$ ist Teilraum und $\forall f,g\in A$ ist
    $f\cdot g \in A$
   \item $A\subset C(X,\K)$ heißt \highl{punktetrennend}
    $:\Longleftrightarrow$\\ \makebox[1cm]{} $\forall x,y\in X$ mit $x\not= y\ \exists f\in
    A: f(x) \not= f(y)$
  \end{enumerate}
\end{defini}

\begin{lemma}\label{lemma:1}
  Es existieren Polynome $(p_{n}(t))_{n}$, so dass
  \begin{equation*}
    \sup_{|t|\leq1} |p_{n}(t)-|t|| \xrightarrow{n\rightarrow\infty} 0
  \end{equation*}
  
  \begin{proof}
    setzen $p_{0}(t) := 0, p_{n+1}(t) = p_{n}(t) + \frac12
    (t^{2}-(p_{n}(t))^{2}), n\in \N_{0}$
    
    Es gilt:
    \begin{gather*}
      |t| - p_{n+1}(t) = (|t|-p_{n}(t)) ( 1- \frac{|t|+p_{n}(t)}{2}
	 )\label{eq:11}\\
      \intertext{Außerdem:} 0\leq p_{n}(t) \leq |t| \text{(durch
	 Induktion)}\label{eq:12}
    \end{gather*}
    \begin{align*}
      p_{n}(t) \leq |t| &\Longrightarrow |t|+p_{n}(t) \leq 2|t|\\
      &\Longrightarrow 1-\frac{|t|+p_{n}(t)}{2} \geq 1-\frac{2|t|}{2} =
	 1-|t| \geq 0\\
      \text{nach (\ref{eq:11})}&\Longrightarrow |t|-p_{n+1}(t) \geq 0
    \end{align*}
    
    und:
    \begin{align*}
      (2+(n+1)|t|)(1-\frac{|t|+p_{n}(t)}{2}) &=
	 2+n|t|+|t|-(|t|-\underbrace{p_{n}(t)}_{\geq0})-
	 \frac{(n+1)|t|}{2}(|t|+p_{n}(t))\\
      &\leq 2 + n|t|\\
      \Longrightarrow& (1-\frac{|t|+p_{n}(t)}{2}) \leq
	 \frac{2+n|t|}{2+(n+1)|t|}\label{eq:13}
    \end{align*}
    
    Aus \eqref{eq:11} folgt mit \eqref{eq:13} durch Induktion:
    \begin{align}\label{eq:14}
     \begin{split}
	|t|-p_{n+1}(t) &\leq (|t|-p_{n}(t)) \frac{2+n|t|}{2+(n+1)|t|}\\
	& \leq (|t|-p_{n-1}(t))\frac{2+(n+1)|t|}{2+n|t|}
	\frac{2+n|t|}{2+(n+1)|t|}\\
	&\vdots\\
	&\leq (|t|-p_{0}(t)) \frac{2}{2+(n+1)|t|} = \frac{2|t|}{2+(n+1)|t|}
     \end{split}      
    \end{align}

    \begin{gather*}
      |t| \leq 1 \text{ und \eqref{eq:14}}  \Longrightarrow 0 \leq |t|-p_{n}(t)
	 \leq \frac{2|t|}{2+n|t|} \leq \frac{2|t|}{2|t|+n|t|} =
	 \frac{2}{2+n} \xrightarrow{n\rightarrow\infty} 0
    \end{gather*}
  \end{proof}
\end{lemma}

\begin{lemma}\label{lemma:2}
  Sei $\mathcal{A}$ Unteralgebra von $C(X,\R)$.
  
  Dann folt aus $f\in A$ $\Longrightarrow$ $|f|\in \overline{A}$
  (\highl{Abschluss} von $A$).
  
  \begin{proof}
    setzen $t:=\frac{f(x)}{\|f\|_{\infty}} (x\in X) \Longrightarrow
    |t|\leq 1$
    
    Lemma \ref{lemma:1} $\Longrightarrow$ $\exists p_{n}(t)$ mit
    $|p_{n}(t)-|t||\leq \frac{\epsilon}{\|f\|_{\infty}} (\forall t)$
    
    $\Longrightarrow \forall x:
    |\frac{f(x)}{\|f\|_{\infty}}-p_{n}(\frac{f(x)}{\|f\|_{\infty}})|\leq
    \frac{\epsilon}{\|f\|_{\infty}}$
    
    $\Longrightarrow |f(x)-\underbrace{\|f\|_{\infty}
    p_{n}(\frac{f(x)}{\|f\|_{\infty}})}_{\in A}|\leq \epsilon$
  \end{proof}
\end{lemma}

\begin{satz}[Stone-Weierstraß]\label{satz:weierstr}
  $(X,d)$ kompakter metr. Raum. Es sei $A\subset C(X,\R)$ eine
  punktetrennende Unteralgebra, die die konstanten Funktionen enthält.
  
  Dann ist $A$ dicht in $C(X,\R)$
  
  \begin{proof}
    \begin{schritte}
     \item $f,g\in A$. Aus \thref{lemma:2} folgt:
      \begin{align*}
	\max(f(x), g(x)) &= \frac12(f(x)+g(x) + |f(x)-g(x)|)\in \overline{A}\\
	\min(f(x), g(x)) &= \frac12(f(x)+g(x) - |f(x)-g(x)|)\in \overline{A}
      \end{align*}
     \item $x,y\in Y, y\not=x, \alpha,\beta\in \R \Longrightarrow f\in A:
      f(x)=\alpha\wedge f(y) =\beta$
      
      $A$ punktetrennend $\Longrightarrow$ $\exists g\in A: g(x)\not=g(y)$
      
      setzen $f(t) := \alpha +
      \frac{\beta-\alpha}{g(y)-g(x)}(g(t)-g(x))\in A$, da $1\in A$
      $\Longrightarrow$ $f(x)=\alpha\wedge f(y)=\beta$
     \item (lokale Approximation) $\epsilon>0, f\in C(X,\R), x\in X
      \Longrightarrow \exists g_{x}(t)\in \overline{A}: g_{x}(x)=f(x)
      \wedge g_{x}(t) \leq f(t)+\epsilon \alpha t$
      
      Es sei $z\in X$ (beliebig) $\Longrightarrow$ $\exists h_{z}\in A:
      h_{z}(x) =f(x) \wedge h_{z}(z) = f(z)$ Nach 2. Schritt
      
      $h_{z}, f$ stetig $\Longrightarrow$ $|h_{z}(y) -f(y)|\leq |h_{z}(y)
      - \underbrace{f(z)}_{=h_{z}(z)}| + |f(z) - f(y)| \leq \epsilon$
      für alle $y$ mit $d(y,z) \leq \delta_{z}$
      
      $\Longrightarrow$ $h_{z}(y) \leq f(y) + \epsilon\qquad \forall y:
      d(y,z) \leq \delta_{z}$
      
      Da $X$ kompakt ist, folgt aus dem Satz von Heine-Borel
      (\thref{satz:heineborel})
      \begin{equation*}
	X = \bigcup_{i=1}^{n} K_{\delta_{z_{i}}}(z_{i})
      \end{equation*}
      
      Wir definieren jetzt die Funktion $g_{x}(t)$ als
      \begin{equation*}
	g_{x}(t) := \min\{h_{z_{1}}(t),\ldots,h_{z_{n}}(t)\}
      \end{equation*}. Nach Schritt 1 gilt dann: $g_{x}(t) \in \overline{A}$
      
      $\Longrightarrow$ $g_{x}(t) = \min_{i}\{h_{z_{i}}(x) \} f(x)$ (nach
      Konstruktion) $t\in X$ $\Rightarrow$ $\exists z_{i} \wedge
      d(t,z_{i})<\delta_{z_{i}}$
      
      $\Longrightarrow$ $g_{x}(t) \leq h_{z_{i}}(t) \leq f(t) + \epsilon$
     \item $\epsilon > 0, f\in C(X,\R)$
      
      $\Longrightarrow$ $\exists \psi\in A: f(t)-\epsilon \leq \psi(t)
      \leq f(t)+\epsilon\quad \forall t\in X$
      
      Wählen $g_{x}(t)$ aus Schritt 3
      
      $\Longrightarrow$ $|g_{x}(t)-f(t)|\leq |g_{x}(t) - g_{x}(x)| +
      |\underbrace{g_{x}(x)}_{=f(x)} -f(t)| \leq \epsilon\quad \forall t:
      d(t, x) \leq \gamma_{x}$ (wegen der Stetigkeit von $f$ und $h_{x}$)
      
      $\Rightarrow$ $g_{x}(t) \geq f(t)-\epsilon$ für $d(t,x) \leq
      \gamma_{x}$
      
      $X$ kompakt, Heine Borel $\Longrightarrow$ $X = \bigcup_{i=1}^{m}
      K_{\gamma_{x_{i}}}(x_{i})$ setzen $\phi(t) :=
      \max\{g_{x_{1}}(t),\ldots,g_{x_{m}}(t)\}\in \overline{A}$ nach
      Schritt 1
      
      $t\in X$ $\Longrightarrow$ $\exists x_{i}: d(t,x_{i})
      <\gamma_{x_{i}}$ und $f(t)-\epsilon \leq g_{x_{i}}(t) \leq \phi(t)
      \leq f(t) + \epsilon\quad \forall t\in X$  $\Rightarrow$
      $\|f-\phi\|_{\infty} \leq \epsilon$
      
      $\phi\in \overline{A} \Longrightarrow \exists \psi\in A:
      \|\phi-\psi\|_{\infty} \leq \epsilon \Longrightarrow
      \|f-\psi\|_{\infty} \leq 2\epsilon$
    \end{schritte}
  \end{proof}
\end{satz}

\begin{folger}
  $\Omega\in \R^{n}$ offen und beschränkt.
  
  Dann folgt daraus: Die Menge aller Polynome $\mathcal{P}=\{
  p(x)=\sum_{\alpha=(\alpha_{1},\ldots,\alpha_{n})\text{endl.}}
  a_{\alpha}x^{\alpha} : a_{\alpha}\in \R\}$ ist dicht in $C(\overline{\Omega})$.
  
  Weiterhin gilt: $C(\overline{\Omega})$ ist separabel.
  
  \begin{proof}
    Man sieht leicht, dass $\mathcal{P}$ eine Unteralgebra ist. $1\in
    \mathcal{P}$ (für $\alpha=0$). $x^{0},y^{0}\in \overline{\Omega}$,
    $x^{0}\not= y^{0}$ $\Longrightarrow$ $\exists j \wedge x^{0}_{j}
    \not= y_{j}^{0}$ $\Longrightarrow$ Polynom $p(x) = x_{j}$ trennt
    $x^{0}$ und $y^{0}$.
  \end{proof}
\end{folger}

\begin{folger}
  Die Menge aller trigonometrischen Polynome $\mathcal{T} := \{ t(x) :=
  a_{0}+\sum_{k=1}^{n} a_{k}\cos x+b_{k}\sin x : a_{k},b_{k}\in\R,
  n\in\N\}$ ist dicht in $C_{\text{per}}([-\pi,\pi]) = \{ f: [-\pi, \pi]
  \rightarrow \R : f \text{stetig}, f(-\pi) = f(\pi)\} \cong C_{per}(\R)
  = \{ f \text{stetig}, \forall x\in \R: f(x) = f(x+2\pi) \} \cong
  C(\Pi), \Pi=\frac{\R}{2\pi\Z} = \{\overline{x}: \overline{x} = x+2\pi\Z\}$
  
  $d(\overline{x}, \overline{y}) = \inf\{|x-y+2k\pi|: k\in\Z\}$
  $\Longrightarrow$ $(\Pi, d)$ kompakter metrischer Raum
  
  $f(x) = \overline{f}(\overline{x})$
  \begin{proof}
    Man sieht leicht, dass $\mathcal{T}$ eine Unteralgebra ist und die
    konstantan Funktionen enthält.
    
    $x_{0}\in (0,\pi), y_{0} \in(-\pi,0)$ $\Longrightarrow$ $\sin x_{0}
    \not= \sin y_{0}$\\
    $x_{0},y_{0}\in [0,\pi] \vee [-\pi,0], x_{0}\not=y_{0}$
    $\Longrightarrow$ $\cos x_{0} \not= \cos y_{0}$
  \end{proof}
\end{folger}

\begin{satz}[komplexwertige Funktionen]
  $(X,d)$ komakter metrischer Raum, $A\subset C(X,\C)$ mit:
  \begin{enumerate}
   \item $A$ Unteralgebra
   \item $1\in A$
   \item $f\in A \Longrightarrow \overline{f}\in A$
   \item $A$ punktetrennend
  \end{enumerate}
  
  Dann liegt $A$ dicht in $C(X,\C)$.
  
  \begin{proof}
    $h\in A \Longrightarrow \overline{h}\in A \Longrightarrow \Re h, \Im
    h\in A$
    
    Setze $A^{\ast} := \{ \Re h: h\in A\} \cup \{ \Im h: h\in A\} \subset
    C(X,\R)$ $\Longrightarrow$ $A^{\ast}$ Unteralgebra, punktetrennend,
    $1\in A^{\ast}$
    
    $f\in C(X,\C) \Longrightarrow^{Satz 4}$
    \begin{align*}
      \exists (\phi_{j})\subset A^{\ast}: \phi_{i}\rightarrow \Re f\\
      \exists (\psi_{j})\subset A^{\ast}: \psi_{i}\rightarrow \Im f
    \end{align*}
    $\Longrightarrow \underbrace{\phi_{i} +i\psi_{j}}_{\in A (wegen satz (3))}
    \rightarrow f$ in $C(X,\C)$
  \end{proof}
\end{satz}

\begin{bsp}
  $S:= \{ z\in \C : |z|=1\}, C(S,X), A:= \{ \sum_{k=-n}^{n} c_{k}z^{k}:
  c_{k}\in \C, n\in \N_{0}\}\subset C(S,\C)$
  
  $A$ ist eine Unteralgebra, $1\in A$, $z\in S \Longrightarrow \frac1z\in
  S$, punktetrennend
  
  $\Longrightarrow$ $A$ dicht in $C(S,\C)$
  
  Folgerung; für $e^{it}$ $\{ \sum_{k=-n}^{n} c_{k}e^{ikt}: c_{k}\in \C,
