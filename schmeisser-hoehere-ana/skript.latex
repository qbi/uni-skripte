Nulldistribution: $T\in D'(\Omega)$ mit $T(\phi) = 0$, $\phi\in D(\Omega)$

\begin{defini}[Träger einer Distribution]
  Für $T\in D'(\Omega)$ ist
  \begin{gather*}
    \supp T = \{x\in \overline{\Omega}\colon \text{Für jedes $\delta>0$ ist
    $(T)_{\Omega\cap\{y\in\R^{n}\colon \abs{y-x}<\delta\}}$ ungleich der
    Nulldistribution}\}
  \end{gather*}
\end{defini}

\begin{bemerk}
  \begin{itemize}
   \item $A=\Omega\setminus(\Omega\cap\supp T)$ größte offene Menge mit
    $(T)_{h} = 0$
   \item $f\in L_{1}^{loc}(\Omega) \Longrightarrow T_{f}\in D'(\Omega),
    T_{f}(\phi) := \int_{\Omega}f(x) \phi(x)\,dx, \phi\in D(\Omega)$

    $\supp f = \overline{\{ x\in R^{k}\colon f(x) \ne0\}}$\\ $\supp
    T_{f}$\help{warum steht das hier?}
   \item $\supp f = \supp T_{j}$ irreal: $f(x) \equiv 0, g(x) =
    \begin{cases}1& x \text{rationnal}, x\in \Omega\\ 0&\text{sonst}\end{cases}$

    $\Longrightarrow$ $\supp f = \supp T_{f} = \supp T_{g} = \emptyset,
    \supp g = \overline{\Omega}$, aber $\supp T_{f}\subset \supp f$:

    $x\in \supp T_{f} \Longrightarrow \forall \delta>0\colon
    (T_{f})_{K_{\delta}\cap\Omega}$  ungleich Nulldistribution
    $\Longrightarrow$ $\forall\delta >0\colon f\ne0$ f.\,ü. auf
    $K_{\delta}\cap \Omega$ $\Longrightarrow$ $\forall\delta > 0 \exists
    z_{\delta}\in K_{\delta}(x)\colon f(z_{\delta})\ne0$
    $\Longrightarrow^{\delta\rightarrow0}$ $\in\supp f$
  \end{itemize}
\end{bemerk}

\begin{bsp}
  \begin{itemize}
   \item $f\in L_{1}^{loc}(\Omega), f$ stetig $\Longrightarrow$ $\supp f
    = \supp T_{f}$

    gzz.: $\supp f\subset \supp T_{f}$

    $x\in\supp f \Longrightarrow f(x) \ne0 \Longrightarrow^{stetig}
    \exists \delta>0 \forall y\in K_{\delta}(x): f(y) \ne0$

    Ann.: $x\notin\supp T_{f} \Rightarrow \exists \delta' > 0:
    (T_{f})_{K\delta'(x)\cap\Omega}$ Nulldistribution $\Longrightarrow$
    $(T_{f})_{K_{\delta'}(x)\cap\Omega}(\phi) =
    \int_{K_{\delta'}(x)\cap\Omega} f(x) \phi(x)\,dx = 0 \forall \phi \in
    D(K_{\delta'}(x)\cap\Omega)$ $\Longrightarrow^{Lemma 1 ind
    5.1.1.\todo{link}} f\equiv 0$ in $K_{\delta'}(x)\cap\Omega$
    $\Longrightarrow$ Widerspruch
   \item $\delta_{a}\in D'(\Omega), a\in \Omega$

    $\supp \delta_{a} = \{a\}$, z.\,z. $\forall \epsilon > 0 \forall \phi\in
    D(\Omega\cap K_{\epsilon}(a)): \delta_{a}(\phi) = \phi(a)$ ungleich
    der Nulldistribution

    $x\ne a$ $\Longrightarrow$ wählen $\epsilon$ so, dass
    $K_{\epsilon}(x)\not\ni a \Longrightarrow \forall \phi \in
    D(\Omega\cap K_{\epsilon}(a)): \delta_{a}(\phi) = 0$
    $\Longrightarrow$ $\delta_{a}$ Nulldistribution $\Longrightarrow$
    $x\notin \supp \delta_{a}$

    $x=a$ $\Longrightarrow$ für alle $\epsilon>0$ $\exists \phi\in
    D(\Omega\cap K_{\epsilon}(a)): \phi(a) \ne0$ $\Longrightarrow$
    $\delta_{a}$ nicht Nulldistribution $\Longrightarrow$ $a\in\supp
    \delta_{a}$
  \end{itemize}
\end{bsp}

\subsection{Operationen in \texorpdfstring{$D'(\Omega)$}{D'(Omega)}}

$D'(\Omega), \Omega\subset \R^{n}$ Gebiet

\begin{defini}
  \begin{enumerate}
   \item seien $\lambda_{1}, \lambda_{2}\in \C, T_{1}, T_{2}\in D'(\Omega)$. Dann
    ist:
    \begin{gather*}
       (\lambda_{1}T_{1} + \lambda_{2} T_{2}) (\phi) =
      \lambda_{1}T_{1}(\phi) + \lambda_{2} T_{2}(\phi), \phi\in D(\Omega)
    \end{gather*}
   \item seien $\alpha\in\N_{0}^{n}, T\in D'(\Omega)$. Dann ist
    \begin{gather*}
      (D^{\alpha}T)(\phi) := (-1)^{\abs{\alpha}} T(D^{\alpha}\phi),
      \phi\in D(\Omega)
    \end{gather*}
   \item sei $a \in C^{\infty}(\Omega), T\in D'(\Omega)$
    \begin{gather*}
      (a(x) T) (\phi) := T(a\phi), \delta \in D(\Omega)
    \end{gather*}
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{itemize}
   \item $\lambda_{1}T_{1}+ \lambda_{2}T_{2}\in D'(\Omega)$
    \begin{description}
     \item[Linearität] klar
     \item[{$\phi_{j} \xrightarrow[D]{} \phi$}] $\Longrightarrow$
      $T_{1}(\phi_{j}) \rightarrow T_{1}(\phi), T_{2}(\phi_{j})
      \rightarrow T_{2}(\phi)$ $\Longrightarrow$ $(\lambda_{1}T_{1} +
      \lambda_{2} T_{2})(\phi_{j}) \rightarrow (\lambda_{1}T_{1} +
      \lambda_{2} T_{2})(\phi)$
    \end{description}
   \item $D^{\alpha}T\in D'(\Omega), \alpha\in \N_{0}^{n}$
    \begin{description}
     \item[Linearität] klar
     \item[stetig] $\phi_{j}\xrightarrow[D]{} \phi$ $\Longrightarrow$
      $D^{\alpha}\phi_{j} \xrightarrow[D]{} D^{\alpha}\phi,
      \alpha\in\N_{0}^{n}$ $\Longrightarrow^{T\in D'(\Omega)}$
      $T(D^{\alpha}\phi_{j}) \rightarrow T(D^{\alpha} \phi)$
      $\Longrightarrow$ $(D^{\alpha} T) (\phi_{j}) \rightarrow
      (D^{\alpha}T) (\phi)$
    \end{description}
   \item $a(x) T\in D'(\Omega), a\in C^{\infty}, T\in D'(\Omega)$
    \begin{description}
     \item[$\phi\in D(\Omega) \rightarrow a\phi\in D(\Omega)$]
      $\Longrightarrow$ $T(a\phi)$ def.
     \item[$aT$ linear] $(a(x) T) (\lambda_{1}\phi_{1} +\lambda_{2}T_{2})
      = T(a(x) (\lambda_{1} \phi_{1} + \lambda_{2}T_{2})) = \lambda_{1}
      T(a(x)\phi_{1}) + \lambda_{2} T(a(x) \phi_{2}) =
      \lambda_{1}(aT)(\phi_{1}) + \lambda_{2}(aT)(\phi_{2})$
     \item[stetig:] $\phi_{1} \xrightarrow[D]{} \phi$\\
      $D^{\beta}(a(x) \phi_{j}(x)) = \sum c_{\alpha\gamma} D^{\alpha}
      a(x) D^{\gamma}\phi_{j}(x) \xrightarrow{glm.} \sum c_{\alpha\gamma}
      \underbrace{D^{\alpha} a D^{\gamma}\phi}_{D^{\beta}(a\phi)}$
      $\Longrightarrow$ $a\phi_{j} \xrightarrow[D]{} a\phi$
      $\Longrightarrow$ $T(a\phi_{j}) \rightarrow T(a\phi)
      \Longrightarrow (aT)(\phi_{j}) \rightarrow (aT)(\phi)$
    \end{description}
  \end{itemize}
\end{bemerk}

