\begin{satz}[Integraloperatoren]
  $\Omega\subset \R^{n}$ offen und beschränkt. Betrachten:
  $(\mathcal{K}f)(x) := \int_{\Omega} K(x,y)f(y)\,dy\quad x\in\Omega$
  \begin{enumerate}
   \item $K(x,y) \in C(\overline{\Omega}\times\overline{\Omega})
    \Longrightarrow \mathcal{K}: C(\overline{\Omega}) \rightarrow
    C(\overline{\Omega})$ kompakt, $\|\mathcal{K}\| \leq \|K(x,y)\|_
    {C(\overline{\Omega}\times\overline{\Omega})} |\Omega|$
   \item $K(x,y) \in L_{2}(\Omega\times\Omega)$ $\Longrightarrow$
    $\mathcal{K}: L_{2}(\Omega) \rightarrow L_{2}(\Omega)$ kompakt
    $\|\mathcal{K}\| \leq \|K(x,y)\|_{L_{2}(\Omega\times\Omega)}$
  \end{enumerate}
  
  \begin{proof}
    \begin{schritte}
     \item $\|f\|_{\infty} \leq 1, f\in C(\overline{\Omega})
      \Longrightarrow \|\mathcal{K}f\|_{\infty} = \|\int_{\Omega}
      k(x,y)f(y)\,dy\|_{\infty} \leq \|K\|_{\infty,\Omega\times\Omega}
      \underbrace{\|f\|_{\infty}}_{\leq 1}|\Omega|$
      
      $\Longrightarrow$ $\{\mathcal{K}f: \|f\|\leq 1\}$ beschränkt\\
      $|(\mathcal{K}f)(x+h)-(\mathcal{K}f)(x)| \leq \int_{\Omega}
      |\underbrace{k(x+h,y)-k(x,y)}_{<\epsilon, \text{falls
      $|h|<\delta(\epsilon)$, unabhängig von $x,y$, da glm. stetig}}|
      |f(y)|\,dy \leq \epsilon|\Omega|$\\
      $\Longrightarrow$ $\{ \mathcal{K}f: \|f\|_{\infty} \leq 1\}$
      gleichmäßig stetig $\Longrightarrow$ präkompakt nach
      \thref{satz:arzela}

     \item $K\in L_{2}(\Omega\times\Omega), \|f\|_{2} \leq 1$
      
      $\Longrightarrow$ $\|\mathcal{K}f\|_{2} = \|\int_{\Omega} K(x,y)
      f(y)\,dy\|_{2} \leq^{\eqref{eq:hoelder-int}}
      \underbrace{\|\Bigl(\int_{\Omega}|k(x,y)|^{2}\,dy
      \Bigr)^{\frac12}\|_{2}}_{=\|K\|_{L_{2}(\Omega\times\Omega)}}
      \underbrace{\|f\|_{2}}_{\leq 1}$
      $\Longrightarrow$ $\{\mathcal{K}f: \|f\|_{2} \leq 1\}$ beschränkt
      
      $\|\mathcal{K}f(x+h)-\mathcal{K}f(x)\|_{2} \leq
      \underbrace{\|k(\cdot+h,\cdot)-k(\cdot,\cdot)\|_{L_{2}(\Omega\times\Omega)}}
       _{<\epsilon,\text{falls $|h|<\delta(\epsilon)$ nach \thref{satz:1.3.7/1}}} 
      \underbrace{\|f\|_{2}}_{\leq 1} < \epsilon\qquad\forall
      f:\|f\|_{2}\leq1$
      
      $\Longrightarrow$ $\{\mathcal{K}f: \|f\|_{2}\leq 1\}$ präkompakt,
      nach \thref{satz:1.3.7/5} $\Longrightarrow$ $\mathcal{K}$ kompakt
    \end{schritte}
  \end{proof}
\end{satz}

\subsection{Inverse Abbildungen, Neumann'sche Reihe} % 1.5.4
\begin{description}
 \item[\underline{Problem:}] $A\in \mathcal{L}(E,F), Ax=y$ ($y\in F$ gegeben, $x\in E$
  gesucht)
 \item[\underline{Annahme:}] $\exists A^{-1}\in \mathcal{L}(F,E) \Longrightarrow
  x=A^{-1}y \Longrightarrow \|x-x'\|_{E} \leq \|A^{-1}\| \|y-y'\|_{F}$
  (stetige Abhängigkeit der Lösung von rechter Seite)
\end{description}

\begin{bemerk}
  $A\in\mathcal{L}(E,F)$ kompakt.
  
  Nehmen wir einmal an, es existiere$A^{-1}\in \mathcal{L}(F,E)$. Dann
  ist die identische Abbildung $I= A^{-1}A: E\rightarrow E$ kompakt. Dies
  gilt nach \thref{satz:122} genau dann wenn \help{der Satz sagt aber nur
  $\Rightarrow$ nicht $\Leftrightarrow$} $\dim E < \infty$.
  Analog: $\dim F < \infty$
\end{bemerk}

\begin{defini}
  $A\in\mathcal{L}(E), E$ komplexer Banachraum
  
  \begin{align*}
    M_{A} &:= \{ \lambda\in \C: \exists(\lambda I-A)^{-1}\in
    \mathcal{L}(E) \}\\
    S_{A} &:= \C\setminus M_{A}
  \end{align*}
  $M_{A}$ heißt Resolrentenmenge von $A$, $S_{A}$ heißt Spektrum von $A$.
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $R(\lambda I-A)$ Wertebereich von $\lambda I-A$
    
    $\lambda\in M_{A} \Longleftrightarrow \exists(\lambda I-A)^{-1}\in
    \mathcal{L}(E) \Longleftrightarrow$
    \begin{enumerate}
     \item $R(\lambda I-A) =E$,
     \item $\lambda I-A$ injektiv,
     \item $(\lambda I-A)^{-1}$ beschränkt
    \end{enumerate}
    
    später: (1)+(2) $\Longrightarrow$ (3) (Satz vom inversen Operator)
   \item Eigenwerte: $\lambda\in\C$ Eigenwert $\Longleftrightarrow$
    $\exists v\in E, v\not=0, Av=\lambda v \Longrightarrow \lambda\in S_{A}$
    
    $N(\lambda I-A) = \{v: (\lambda I-A)v=0\}, \dim N(\lambda I-A)$
    Vielfachheit vom Eigenwert $\lambda$
   \item $A\in \mathcal{L}(E)$ kompakt, $\dim E=\infty$ $\Longrightarrow$
    $0\in S_{A}$
   \item formal: $\lambda=1, A\in\mathcal{L}(E)$
    \begin{equation*}
      (I-A)^{-1} = \frac{I}{I-A} = \underbrace{\sum_{j=0}^{\infty}
      A^{j}}_{\in \mathcal{L}(E), \text{falls} \|A\|<1}
    \end{equation*}
    Wenn $\|A\|< 1$ $\Longrightarrow$ $\sum_{j=0}^{\infty}
    \underbrace{\|A\|^{j}}_{\geq \|A^{j}\|}$
  \end{enumerate}
  
  \begin{description}
   \item[Frage] Wann existiert $\sum_{j=0}^{\infty} A^{j}$ in
    $\mathcal{L}(E)$:
    \begin{enumerate}
     \item $\|A\|<1$ $\Longrightarrow$ $\exists \sum_{j=0}^{\infty}
      A^{j}\in \mathcal{L}(E)$
     \item Wurzelkriterium:
      $\exists\lim_{j\rightarrow\infty}\|A^{j}\|^{\frac1j} < 1$
      $\Longrightarrow$ $\sum_{j=0}^{\infty} \|A^{j}\| < \infty
      \Longrightarrow \exists \sum_{j=0}^{\infty} A^{j} \in \mathcal{L}(E)$
    \end{enumerate}
  \end{description}
\end{bemerk}

\begin{lemma}
  $E$ ist ein Banachraum und es existiere $S := \sum_{j=0}^{\infty} A^{j}\in
  \mathcal{L(E)}$.
  
  Dann $\exists (I-A)^{-1}=S$.
  
  \begin{proof}
    Es gilt: $SA = \Bigl(\sum_{j=0}^{\infty} A^{j}\Bigr) A =
    \sum_{j=0}^{\infty} A^{j+1} = \sum_{j=0}^{\infty} A^{j} - A^{0} =S-I$
    $AS = A\Bigl(\sum_{j=0}^{\infty} A^{j}\Bigr) = \sum_{j=0}^{\infty}
    A^{j+1} = S-I$
    
    denn: $S_{n} := \sum_{j=0}^{n} A^{j}, S_{n}
    \xrightarrow{n\rightarrow\infty} S$ in $\mathcal{L}(E)$
    
    $\Longrightarrow$ $\|S_{n}A-SA\| \leq \|S_{n}-S\| \|A\|
    \xrightarrow{n\rightarrow\infty} 0$
    
    $\|AS_{n}-AS\| \leq \|A\|\|S_{n}-S\| \xrightarrow{n\rightarrow\infty}0$
    
    $\Longrightarrow$ $I = S-SA = S(I-A)$ und $I = S-AS = (I-A)S$. Aus
    ersterem folgt $I-A$ ist injektiv, aus dem zweiten folgt $I-A$ ist
    surjektiv. Daraus folt wiederrum, dass $(I-A)^{-1}$ existiert und
    gleich $S\in\mathcal{L}(E)$ ist.
  \end{proof}
\end{lemma}

\begin{lemma}
  $a_{j}\geq 0$. Es gelte: 
  \begin{equation}\label{eq:124}
    a_{j+k} \leq a_{j}a_{k}\qquad \forall j,k
  \end{equation}
  $\Longrightarrow$ $\exists$ $\lim_{j\rightarrow0} \sqrt[j]{a_{j}} =
  \inf_{j} \sqrt[j]{a_{j}}$
  
  \begin{proof}
    Es sei $a := \inf_{j} \sqrt[j]{a_{j}}$. Dann $\exists k\in\N: a \leq
    \sqrt[k]{a_{k}} < a+\epsilon$ $(\epsilon>0)$ $\Longrightarrow$ $a_{k}
    < (a+\epsilon)^{k}$
    
    $\forall j\in\N \exists p_{j}\in\N_{0}\exists q_{j}\in
    \{0,1,\ldots,k-1\}$ mit $j=kp_{j}+q_{j}$
    
    $\Longrightarrow$ $\forall j: a_{j} = a_{kp_{j}+q_{j}}
    \leq^{\eqref{eq:124}} a_{kp_{j}} a_{q_{j}} \leq a_{k}^{p_{j}}
    \underbrace{\max\{a_{0},a_{1},\ldots,a_{k-1}\}}_{=: M} \leq M
    a_{k}^{p_{j}}$
    
    $\Longrightarrow$ $a \leq \sqrt[j]{a_{j}} \leq M^{\frac1j}
    a_{k}^{p_{j}} \leq M^{\frac1j} (a+\epsilon)^{\frac{kp_{j}}{j}} =
    M^{\frac1j} (a+\epsilon)^{1-\frac{q_{j}}{j}} = (a+\epsilon)
    \frac{M^{\frac1j}{\underbrace{\bigl((a+\epsilon)^{q_{j}}\bigr)^{\frac1j}}
      _{\xrightarrow{j\rightarrow\infty} 1}}} \leq a+2\epsilon$ falls $j
    \geq j_{0}(\epsilon)$

    $\epsilon\rightarrow 0$ $\Longrightarrow$ a=$\lim_{j\rightarrow\infty}\sqrt[j]{a_{j}}$
  \end{proof}
\end{lemma}

\chapter{Operatoren im Hilbertraum}
\section{Hilberträume}
\subsection{Skalarprodukt und Orthogonalität}

\begin{defini}
  $H$ Vektorraum über $\K=\R,\C$
  
  Eine Abbildung $\sprod{\cdot,\cdot}:H\times H\rightarrow\K$ mit
  den Eigenschaften
  \begin{enumerate}
   \item $\forall x\in H: \sprod{x,x} \geq 0$
   \item $\sprod{x,x} = 0 \Leftrightarrow x=0$
   \item $\forall x,y\in H: \sprod{x,y} = \overline{\sprod{y,x}}$
   \item $\forall x,y\in H, \forall \lambda\in \K: \sprod{ \lambda x,y}
    = \lambda \sprod{x,y}$
   \item $\forall x_{1},x_{2},y \in H: \sprod{x_{1}+x_{2},y} =
    \sprod{x_{1},y} + \sprod{x_{2},y}$
  \end{enumerate}
  heißt \highl{Skalarprodukt}
\end{defini}

\begin{bemerk}
  Aus den Eigenschaften (3) und (4) folgt:
  \begin{equation*}
    \sprod{x,\lambda y} = \overline{\lambda} \sprod{x,y}
  \end{equation*}
  
  Aus den Eigenschaften (3) und (5) folgt:
  \begin{equation*}
    \sprod{x,y_{1}+y_{2}} = \sprod{x,y_{1}} + \sprod{x,y_{2}}
  \end{equation*}
\end{bemerk}

\begin{satz}
  $H$ Vektorraum, $\sprod{\cdot,\cdot}$ Skalarprodukt
  
  \begin{enumerate}
   \item Dann gilt die Cauchy-Schwarz'sche Ungleichung:
    \begin{equation*}
      |\sprod{x,y}| \leq \sqrt{\sprod{x,x}\sprod{y,y}}
    \end{equation*}
  
    Gleichheit gilt genau dann wenn $x$ und $y$ linear abhänging sind.
   \item Durch $\|x\| := \sqrt{\sprod{x,x}}$ wird eine Norm
    auf $H$ definiert.
  \end{enumerate}
  
  \begin{proof}
    siehe lineare Algebra oder Analysis\,2
  \end{proof}
\end{satz}

\begin{defini}
  Ein normierter Raum $H$ heißt \highl{Prä-Hilbertraum}, falls ein
  Skalarprodukt existert, mit $\|x\| = \sqrt{\sprod{x,x}}$.
  Ein Hilbertraum ist ein vollständiger Prä-Hilbertraum.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item $\R^{n}$ mit $\sprod{x,y} = \sum_{j=1}^{n} x_{j}y_{j}$\\
    $\C^{n}$ mit $\sprod{x,y} = \sum_{j=1}^{n} x_{j}\overline{y_{j}}$
   \item $l_{2}(\N)$ mit $\sprod{x,y} := \sum_{j=1}^{\infty}
    x_{j} \overline{y_{j}}, \|x\|_{2} =
    \Bigl(\sum_{j=1}^{\infty}|x_{j}|^{2}\Bigr)^{\frac12} =
    \sqrt{\sprod{x,x}}$
   \item $L_{2}(A,\mu), (\Omega, \mathcal{A}, \mu)$ $\sigma$-endlicher
    Maßraum, $A\subset \Omega$ messbar
    \begin{gather*}
      \sprod{f,g} = \int_{A} f(x) \overline{g(x)}\,d\mu\\
      \|f\|_{2} = \Bigl( \int_{A} |f(x)|^{2}\,d\mu\Bigr)^{\frac12} =
	 \sprod{f,f}^{\frac12}
    \end{gather*}
    
    von Interesse sind $d\mu = dx$ Lebesgue-Maß und $d\mu = w(x)dx$
    ($\int_{A}|f|^{2}\,w(x)dx < \infty$)
    gewichtete $L_{2}$-Räume mit $w(x) \geq 0$ und \fue messbar
   \item $C([a,b])$ oder $R([a,b])$ sind Prä-Hilbertraum bzgl.
    \begin{equation*}
      \sprod{f,g} = \int_{a}^{b} f(x)\overline{g(x)}\,dx
    \end{equation*}
    nicht vollständig, denn
    $f_{n}(x) = \sqrt{\max\{x,\frac1n\}}, n\in \N$\\
    $\Longrightarrow$ $f_{n}(x) \rightarrow x^{-\frac13}$ bzgl.
    $\|\cdot\|_{2}$ (s. \thref{bsp:1.1.1/3}) Aber $x^{-\frac13}\not\in
    C([0,1])$ (da nicht stetig), $\not\in R([0,1])$ (da nicht beschränkt)
  \end{enumerate}
\end{bsp}

\begin{satz}[Eigenschaften]
  $H, \sprod{\cdot,\cdot}$ Prä-Hilbertraum.
  
  Dann:
  \begin{enumerate}
   \item $x_{j} \rightarrow x\in H$, $y_{j} \rightarrow y\in H$
    $\Longrightarrow$ $\sprod{x_{j},y_{j}} \rightarrow \sprod{x,y}$
    (Stetigkeit des Skalarprodukts)
   \item 
    \begin{equation*}
      \sum_{j=0}^{\infty} x_{j} = x\in H, y\in H \Longrightarrow
      \sprod{x,y} = \sum_{j=0}^{\infty} \sprod{x_{j}, y}
    \end{equation*}
   \item $U\subset H$ dichter Teilraum und $\forall u\in U: \sprod{x,u} =
    0$ $\Longrightarrow$ $x = 0$
   \item $\K = \R$ $\Longrightarrow$ $\sprod{x,y} = \frac14
    (\|x+y\|^{2} - \|x-y\|^{2})$\\
    $\K = \R$ $\Longrightarrow$ $\sprod{x,y} = \frac14
    (\|x+y\|^{2} - \|x-y\|^{2}) + i\frac14 (\|x+iy\|^{2}-\|x-iy\|^{2})$
   \item Es gilt die Parallelogrammgleichung: 
    \begin{equation}\label{eq:parallelo}
      \|x+y\|^{2} + \|x-y\|^{2} = 2(\|x\|^{2} + \|y\|^{2})
    \end{equation}
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item
      \begin{align*}
        |\sprod{x,y} - \sprod{x_{i},y_{i}}| &\leq
	   |\sprod{x-x_{j},y} + \sprod{x_{j},y} - \sprod{x_{j},y_{j}}|\\
	& \leq |\sprod{x-x_{j},y}| + |\sprod{x,y-y_{j}}|\\
	& \leq^{Cauchy-Schw} \|x-x_{j}\| \|y_{j}\| + \|x\| \help{hier
	   fehlt was}
      \end{align*}
     \item folgt aus (1)
      $s_{n} = \sum_{j=0}^{\infty} x_{j} \xrightarrow{n\rightarrow\infty}
      x$ $\Longrightarrow$ $\sprod{s_{n},y}
      \xrightarrow{n\rightarrow\infty} \sprod{x,y}$
     \item $x\in H \exists u_{j} \in U: u_{j} \rightarrow x$ nach (1)
      $\Longrightarrow$ $\sprod{x,x} = \lim_{j\rightarrow\infty}
      \sprod{x,u_{j}} = 0$ $\Longrightarrow$ $x=0$
     \item und (5): $\|x\pm y\|^{2} = \sprod{x\pm y,x\pm y} =
      \|x\|^{2} + \|y\|^{2} \pm \sprod{x,y} \pm \sprod{y,x}$
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{satz}
  Ein normierter Vektorraum $H$ ist genau dann ein Prä-Hilbertraum, wenn die
  Parallelogrammgleichung~\eqref{eq:parallelo} gilt. Das Skalarprodukt
  ist dann wie in (4) definiert
  
  \begin{proof}
    Übung Blatt 1, Aufg. 5 oder Werner "`Funktional analysis"' S.\,199--200
  \end{proof}
\end{satz}

\begin{defini}
  $H,\sprod{\cdot,\cdot}$ Prä-Hilbertraum
  
  \begin{enumerate}
   \item $x,y$ heißen \highl{orthogonal} $:\Longleftrightarrow$
    $\sprod{x,y}=0$ (schreiben $x\perp y$)
   \item $U,V\subset H$ \highl{orthogonal} $:\Longleftrightarrow$ $\forall
    u\in U, \forall v\in V : \sprod{u,v} =0$
   \item $U\subset H, U^{\perp} := \{ v\in H: \forall x\in U: \sprod{u,v}
    =0\}$ heißen \highl{orthogonales Komplement} von $U$.
  \end{enumerate}
\end{defini}

\begin{satz}
  $H,\sprod{\cdot,\cdot}$ Prä-Hilbertraum
  
  Dann
  \begin{enumerate}
   \item $x\perp y$ $\Longrightarrow$ $\|x\|^{2} + \|y\|^{2} =
    \|x+y\|^{2}$ (Pythagoras)
   \item $U\subset H$ $\Longrightarrow$ $U^{\perp}$ abgeschlossener
    Teilraum von $H$
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item $\sprod{x+y,x+y} = \|x\|^{2} + \|y\|^{2} +
      \underbrace{\sprod{x,y}}_{=0} + \underbrace{\sprod{y,x}}_{=0}$
     \item $v_{j}\in U^{\perp}, v_{j} \rightarrow v\in H$
      
      $u \in U \Longrightarrow \sprod{u,v} =^{Satz1}
      \lim_{j\rightarrow\infty} \sprod{u,v_{j}} =0$
      
      $U^{\perp}$ linearer Raum, $v_{1}, v_{2} \in U^{\perp},
      \lambda_{1}, \lambda_{2}\in \K, u\in U$ $\Longrightarrow$ $\sprod{
      \lambda_{1}v_{1}+\lambda_{2}v_{2},u} = \lambda_{1}\sprod{v_{1},u} +
      \lambda_{2} \sprod{v_{2},u} = 0$
    \end{enumerate}
  \end{proof}
\end{satz}

\subsection{Der Riesz'sche Darstellungssatz}\label{sec:2.1.2}
\begin{description}
 \item[Vorbem.] $(l_{2})' \cong l_{2}$ $y\in l_{2} \rightarrow T_{y}(x)
  := \sum_{j=1}^{\infty} x_{j} y_{j} = \sprod{x,\overline{y}}$
  
  $(L_{2})' \cong L_{2}$ $g\in L_{2} \rightarrow T_{g}(f) := \int_{A}
  f(x)g(x)\,d\mu = \sprod{f,\overline{g}}$
 \item[Frage:] Ist $H' \cong H$ für $H$ Hilbertraum
 \item[Versuch:] $y\in H \rightarrow T_{y}(x) := \sprod{x,y}\in
  \K$
  
  Die Linearität des Skalarprodukts überträgt sich auch auf $T_{y}$.
  
  $|T_{y}(x)| = |\sprod{x,y}| \leq \|y\| \|x\| \Rightarrow T_{y}
  \in H', \|T_{y}\| \leq \|y\|$
\end{description}

\begin{defini}
  $\emptyset\not=K\subset H$, $H$ Vektorraum
  
  $K$ heißt konvex $:\Longleftrightarrow$ $\{\lambda_{1} x_{1}
  +\lambda_{2}x_{2}: x_{1},x_{2}\in K, \lambda_{1}, \lambda_{2}\geq 0,
  \lambda_{1}+\lambda_{2}=1\} \subset K$
\end{defini}

\begin{lemma}
  $H$ Hilbertraum, $K\subset H$ konvex und abgeschlossen
  ($K\not=\emptyset$)
  
  Dann existiert für jedes $x\in H$ ein eindeutig bestimmtes Element
  $y_{0}\in K$ mit
  \begin{equation*}
    \|x-y_{0}\| = \inf_{y\in K}\|x-y\|
  \end{equation*}
  
  \begin{proof}
    \begin{description}
     \item[Existenz:] Es sei $\gamma := \inf_{y\in K} \|x-y\|$. Wählen
      $(y_{j})_{j}\subset K$ mit $\|x-y_{j}\| \rightarrow \gamma$. Wir
      zeigen: $y_{j} \rightarrow y_{0}\in K$ ausreichend: $(y_{j})_{j}$
      ist Cauchyfolge, da $H$ vollständig und $K$ abgeschlossen.
      
      Wir setzen: $u=x-y_{j}, v=x-y_{k}, j\not= k$ $\Longrightarrow$ $u-v
      = y_{k}-y_{j}, u+v = 2(x-\underbrace{\frac{y_{j}+y_{k}}2}_{\in K,
      \text{ da $K$ konvex}}$
      
      Parallelogrammgleichung \eqref{eq:parallelo} $\Longrightarrow$
      $\|u+v\|^{2} + \|u-v\|^{2} = 2\|u\|^{2} + 2\|v\|^{2}$\\
      $\Longrightarrow$ $\|y_{k}-y_{j}\| = 2\|x-y_{j}\|^{2} +
      2\|x-y_{k}\|^{2} - 4 \|x-\frac{y_{j}+y_{k}}2\|^{2} \leq
      2\|x-y_{j}\|^{2} + 2 2\|x-y_{j}\|^{2} - 4\gamma
      \xrightarrow{j,k\rightarrow\infty} 0$\\
      $\Longrightarrow$ $(y_{j})$ ist Cauchyfolge
     \item[Unität:] Annahme: $y_{0}, y_{1}$ mit $\|x-y_{i}\| = \gamma$\\
      $\Longrightarrow$ $(y_{0}, y_{1}, y_{0}, y_{1},\ldots)$ ist eine
      Folge wie oben $\Longrightarrow$ eine Cauchyfolge $\Longrightarrow$
      $y_{0}=y_{1}$
    \end{description}
  \end{proof}
\end{lemma}

\begin{bemerk}\label{bem:2.1.2/2}
  Wenn $H$ ein Prä-Hilbertraum und $U,V$ Teilräume von $H$ sind, dann
  wollen wir schreiben: $H= U\oplus V$ $:\Longleftrightarrow$ $H=U+V
  \wedge U\perp V$ "`orthogonale Summe"'
  
  Eigenschaften:
  \begin{enumerate}
   \item $H = U \oplus V \Longrightarrow V = U^{\perp}$
    \begin{proof}
      Aus der Definition von $\oplus$ folgt sofort $V \subset U^{\perp}$.
      
      Für ein $x\in U^{\perp}$ folgt aus der Definition, dass sich $x$
      als $x = u+v (u\in U, v\in V)$ schreiben lässt. $\Longrightarrow$
      $\sprod{u,u} = \sprod{x-v,u} = \underbrace{\sprod{x,u}}_{=0,\text{
      da }x\in U^{\perp}} - \underbrace{\sprod{v,u}}_{=0, \text{ da }
      U\perp V} = 0$\\ $\Longrightarrow$ $u=0 \Longrightarrow x=v\in V$
    \end{proof}
   \item Die Darstellung von $x=u+v$ mit $u\in U, v\in V$ ist eindeutig.
    \begin{proof}
      \begin{equation*}
	\left.\begin{aligned}
		x &= u_{0} + v_{0}\\
		x &= u_{1} + v_{1}
	      \end{aligned}\right\}
	\Longrightarrow u_{0}-u_{1} = v_{0}-v_{1} \Longrightarrow
	\begin{aligned}
	  \sprod{u_{0}-u_{1},u_{0}-u_{1}} &= 0 \Longrightarrow
	    u_{0}=u_{1}\\
	  \sprod{v_{0}-v_{1},v_{0}-v_{1}}=0
	  \Longrightarrow v_{0}=v_{1}
	\end{aligned}
      \end{equation*}
    \end{proof}
  \end{enumerate}
\end{bemerk}

\begin{lemma}
  $H$ Hilbertraum, $U\subset H$ \emph{abgeschlossener} Teilraum
  \begin{enumerate}
   \item Es sei $x\in H$ und $u_{0}\in U$ mit $\|x-u_{0}\| = \inf_{u\in
    U} \|x-u\|$ $\Longrightarrow$ $(x-u_{0}) \perp U$, d.\,h. $x-u_{0}\in
    U^{\perp}$
   \item $\forall x\in H$ existiert \emph{genau ein} $u\in U$ und $v\in
    U^{\perp}$, so daß $x= u+v$, d.\,h. $H = U\oplus U^{\perp}$
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item zeigen $x-u_{0} \perp U$. Sei $\lambda \in \C, u\in U (u \not=
      0)$ $\Longrightarrow^{Def. v. u_{0}}$ $\|x-u_{0}\|^{2} \leq \|x-
      (u_{0}+ \lambda u)\|^{2} = \sprod{x-u_{0}+ \lambda u, x-u_{0}+
      \lambda u} = \|x-u_{0}\|^{2} - \lambda \sprod{u,x-u_{0}}
       - \overline{\lambda} \sprod{x-u_{0}, u} + \lambda
      \overline{\lambda} \|u\|^{2}$
      
      Setzen: $\lambda:=\frac1{u^{2}}\sprod{x-u_{0}, u}$
      $\Longleftrightarrow$ $\sprod{x-u_{0},u} =\lambda \|u\|^{2}$
      
      $\Longrightarrow$ $\|x-u_{0}\|^{2} \leq \|x-u_{0}\|^{2} - \lambda
      \overline{\lambda} \|u\|^{2} - \lambda \overline{\lambda} \|u\|^{2}
      + \lambda \overline{\lambda} \|u\|^{2}$ $\Longrightarrow$ $0 \leq
      -|\lambda|^{2} \|u\|^{2}$ $\Longleftrightarrow$ $\lambda = 0$
      $\Longrightarrow$ $\sprod{x-u_{0}, u} =0$
     \item $x\in H$ $\Longrightarrow$ $x=u_{0}+\underbrace{x-u_{0}}_{\in
      U^{\perp}}$. Darstellung eindeutig nach \thref{bem:2.1.2/2}.
    \end{enumerate}
  \end{proof}
\end{lemma}

\begin{defini}
  $H$ Hilbertraum, $U\subset H$ abgeschlossener Teilraum, $x\in H$
  
  $P_{U}x := u_{0}$ heißt \highl{orthogonale Projektion} von $x$ auf $U$
  wobei $\|x-u_{0}\| = \inf_{u\in U} \|x-u\|$ (oder $x=u_{0}+v_{0}$ mit
  $u_{0}\in U, v\in U^{\perp}$). $P_{U}: H\rightarrow H$ heißt
  \highl{orthogonaler Projektor}.
\end{defini}

\begin{bemerk}
  $P_{U} \in \mathcal{L}(H)$ und $P_{U}^{2} = P_{U}, P_{U}\big|_{U} =
  \id\big|_{U}, P_{U}\big|_{U^{\perp}} = 0$
\end{bemerk}

\begin{satz}\label{satz:176}
  $H$  Hilbertraum, $U\subset H$ abgeschlossener Teilraum, $V\subset H$
  Teilraum
  
  Dann: $H= U\oplus V$ $\Longleftrightarrow$ $V = U^{\perp}$
\end{satz}

\begin{satz}[Riesz--Frecht]\label{satz:riesz}
  $H$ Hilbertraum. Dann gilt:
  
  \begin{equation*}
    T\in H' \Longleftrightarrow \exists y\in H, \forall x\in H: T(x) =
    \sprod{x,y}
  \end{equation*}
  $y$ ist eindeutig bestimmt und es gilt: $\|T\|_{H'} = \|y\|_{H}$
  
  \begin{proof}
    \begin{description}
     \item["`$\Leftarrow$"'] $y\in H$ $T(x) := \sprod{x,y}
      \Longrightarrow T$ linear und beschränkt, $|T(x)| \leq \|y\| \|x\|$
      $\Longrightarrow$ $\|T\|_{H'} \leq \|y\|$. Für $x=y \Longrightarrow
      |T(y)| = |\sprod{y,y} | =\|y\|^{2} \Longrightarrow
      \|T\|_{H'} = \|y\|$
     \item["`$\Rightarrow$"'] Existenz von $y$: Es sei $T\in H',
      T\not= 0$. Idee: $T(x) = 0, T(x) = \sprod{x,y}
      \Longrightarrow \sprod{x,y} =0 \Longrightarrow y\perp N(T)
      = \{ x: T(x) = 0\}$ $y$ ist also im orthogonalen Komplement vom
      Kern von $T$
      
      Nach \thref{satz:176} ist $H = N(T) \oplus N(T)^{\perp}$ Es ist
      $\dim N(T)^{\perp} = 1$, da $T : N(T)^{\perp} \rightarrow \C$
      Isomorphismus
      
      $x,x'\in N(T)^{\perp}$ mit $T(x) = T(x') \Longrightarrow T(x-x') =
      0 \Longrightarrow x-x' = 0 \Longrightarrow x=x' \Longrightarrow T$
      injektiv
      
      $T\not= 0$ linear $\Longrightarrow$ $T(N(T)^{\perp}) = \C$ surjektiv
      
      $\Longrightarrow$ $N(T)^{\perp } = \Span\{y_{1}\}$ mit $\|y_{1}\| =
      1, T(y_{1}) \not=0$
      
      $x\in H \Longrightarrow x=x_{0} +\lambda y_{1}$ mit $x_{0}\in N(T)$
      und eindeutig bestimmten $\lambda\in \C$ $\Longrightarrow$ $T(x) =
      \lambda T(y_{1}) = \lambda T(y_{1}) \sprod{y_{1},y_{1}} =
      \lambda \sprod{y_{1}, \underbrace{\overline{T(y_{1})} y_{1}}_{:=
      y\in N(T)^{\perp}}} = \sprod{\lambda y_{1}, y} = \sprod{x_{0} +
      \lambda y_{1}, y} = \sprod{x,y}$
      
      Unität: $\sprod{x,y} = \sprod{x,y^{\ast}} \forall x
      \Longrightarrow y=y^{\ast}$
    \end{description}
  \end{proof}
\end{satz}

\begin{bemerk}\label{bem:4}
  $J: H\rightarrow H'$ mit $J(y)(\cdot) = \sprod{\cdot,y}$ ist
  bijektiv, isometrisch und \highl{konjugiert linear} (d.\,h.
  $J(\lambda_{1} y_{1} + \lambda_{2} y_{2})(\cdot) = \sprod{ \cdot,
  \lambda_{1} y_{1} + \lambda_{2} y_{2}} = \overline{\lambda_{1}}
  \sprod{\cdot, y_{1}} + \overline{\lambda_{2}}
  \sprod{\cdot,y_{2}} = \overline{\lambda_{1}} J(y_{1})(\cdot) +
  \overline{\lambda_{2}} J(y_{2})(\cdot)$)
  
  $J^{\ast}: H\rightarrow H'$ mit $J^{\ast}(y)(\cdot) =
  \overline{\sprod{\cdot, y}}$ ist isometrisch isomorph
  
  $\Longrightarrow$ $H' \cong H$
\end{bemerk}

\begin{folger}
  $H$ Hilbertraum. Dann $(H')' \cong H$, d.\,h. $H$ ist \highl{reflexiv}
  
  \begin{proof}
    $H'$ ist Hilbertraum bzgl. $\sprod{S,T}_{H'} := \sprod{J^{-1}(S),
    J^{-1}(T)}_{H}$
    
    Nach \thref{satz:riesz} ist $(H')' \cong H'$ und nach der \thref{bem:4}
    $H' \cong H$.
  \end{proof}
\end{folger}

\subsection{Orthonormalbasis}
\begin{defini}
  $H$ Prä-Hilbertraum
  \begin{enumerate}
   \item $(x_{j})_{j=1}^{\infty}\subset H$ heißt
    \highl{Orthonormalsystem} (ONS) $:\Longleftrightarrow$
    $\sprod{x_{j}, x_{k}} = \begin{cases}0& j\not= k\\1&j=k\end{cases}$
   \item $(x_{j})_{j=1}^{\infty}$ heißt Orthonormalbasis
    $:\Longleftrightarrow$ $(x_{j})_{j}$ ist ONS und für jedes $x\in H$
    existieren $(\alpha_{j})_{j=1}^{\infty} \subset H$ mit
    $\sum_{j=1}^{\infty} \alpha_{j} x_{j} = x$ (Konvergent in $H$)
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item ONB ist keine Vektorbasis! d.\,h. keine endliche
    Linearkombination falls $\dim H =\infty$
   \item $(x_{j})_{j}$ ONS $\Longrightarrow$ $S=\{x_{1},\ldots,x_{n}\}$
    linear unabhängig
   \item $(x_{j})_{j}$ ONB $\Longrightarrow$ Darstellung x=
    $\sum_{j=1}^{\infty} \alpha_{j} x_{j}$ in $H$ ist eindeutig. Es gilt:
    $\sprod{x, x_{k}} = \sum_{j=1}^{\infty } \alpha_{j}\sprod{x_{k},
    x_{k}} = \alpha_{k}\quad\forall k$
   \item $(x_{j})_{j}$ ONB $\Longrightarrow$ $x=\sum_{j=1}^{\infty}
    \sprod{x, x_{j}} x_{j}$ abstrakte Fourierreihe, $\sprod{x,x_{j}}$
    Fourierkoeffizient bzgl. der ONB $(x_{j})_{j}$
   \item $\exists$ ONB in $H$ $\Longrightarrow$ $H$ ist separabel, da
    $\Span S$ dicht in $H$
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:2.1.3/1}
  $H$ Prä-Hilbertraum, $(x_{j})_{j}$ ONS.
  
  Dann:
  \begin{enumerate}
   \item $\|\alpha_{1}x_{1}+\ldots+\alpha_{n}x_{n}\|^{2} =
    |\alpha_{1}|^{2}+ \ldots + |\alpha_{n}|^{2}\qquad\forall n\in\N$
   \item $x=\sum_{j=1}^{\infty} \alpha_{j} x_{j}$ $\Longrightarrow$
    $\exists \sum_{j=1}^{\infty} |\alpha_{j}|^{2} = \|x\|^{2}$
   \item Für $x\in H$ ist $\|x-\sum_{j=1}^{n}\alpha_{j}x_{j}\|$
    minimal, genau dann wenn $\alpha_{j} = \sprod{x, x_{j}}
    (j=1,\ldots,n)$ Minimumeigenschaft der Fourierkoeffizienten
   \item $\forall x\in H$ gilt:
    \begin{gather*}
      \|x-\sum_{j=1}^{n} \sprod{x, x_{j}}
	 x_{j}\|^{2} = \|x\|^{2} - \sum_{j=1}^{n} |\sprod{x,x_{j}}|^{2}\\
      \sum_{j=1}^{\infty} |\sprod{x,x_{j}}|^{2} \leq
	 \|x\|^{2}\taglabel{eq:bessel}
    \end{gather*}
    Gleichung \eqref{eq:bessel} ist die \highl{Bessel'sche Ungleichung}.
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item $\sprod{\sum_{j=1}^{n} \alpha_{j}x_{j}, \sum_{k=1}^{n}
      \alpha_{k}x_{k}} = \sum_{j=1}^{n}\sum_{k=1}^{n} \alpha_{j}
      \overline{\alpha_{k}} x_{j} = \sum_{j=1}^{n} |\alpha_{j}|^{2}$
     \item $s_{n} = \sum_{j=1}^{n} = \sum_{j=1}^{n} \alpha_{j} x_{j}
      \rightarrow x$ in $H$ $\Longrightarrow$ $\|x\|^{2} = \sprod{x,x} =
      \lim_{n\rightarrow\infty} \sprod{s_{n}, s_{n}}
      = \lim_{n\rightarrow\infty} \sum_{j=1}^{n} |\alpha_{j}|^{2}$
     \item $\|x-\sum_{j=1}^{n}\alpha_{j}x_{j}\|^{2} = \sprod{
      x-\sum_{j=1}^{n}\alpha_{j}x_{j}, x-\sum_{j=1}^{n}\alpha_{j}x_{j}
      } = \|x\|^{2} + \sum_{j=1}^{n} (\alpha_{j}- \sprod{x, x_{j}})
      (\overline{\alpha_{j}} - \sprod{x_{j}, x}) =
      \|x\|^{2} - \sum_{j=1}^{n} \alpha_{j} \sprod{x_{j}, x} -
      \sum_{j=1}^{n} \overline{\alpha_{j}} \sprod{x, x_{j}} +
      \sum_{j=1}^{n} \alpha_{j} \overline{\alpha_{j}} =
      \|x\|^{2} + \sum_{j=1}^{n} |\alpha_{j} - \sprod{x, x_{j}}|^{2} -
      \sum_{j=1}^{n} |\sprod{x, x_{j}}|^{2}\label{eq:299}$
      $\Longrightarrow$ (3) und aus (3) folgt mit \eqref{eq:299} (4)
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{folger}
  $H$ Prä-Hilbertraum und $(x_{j})_{j}$ ONS
  
  Dann sind folgende Aussagen äquivalent:
  \begin{enumerate}
   \item $(x_{j})_{j=1}^{\infty}$ ist ONB, d.\,h. $x=\sum_{j=1}^{\infty}
    \sprod{x,x_{j}}, \forall x\in H$
   \item 
    \begin{equation}\label{eq:parseval}
      \|x\|^{2} = \sum_{j=1}^{\infty} |\sprod{x, x_{j}}|^{2}
    \end{equation}
    \highl{Parseval'sche Gleichung}
  \end{enumerate}
  
  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] nach \thref{satz:2.1.3/1} Punkt (2)
     \item[$(2)\Rightarrow(1)$] folgt aus \thref{satz:2.1.3/1} Punkt (4)
    \end{description}
  \end{proof}
\end{folger}

\begin{satz}\label{satz:2.1.3/2}
  $H$ Hilbertraum und $(x_{j})_{j=1}^{\infty}$ ONS.
  
  Dann sind äquivalent:
  \begin{enumerate}
   \item $(x_{j})_{j}$ ist ONB
   \item $\forall x,y \in H$ gilt: $\sprod{x,y} =
    \sum_{j=1}^{\infty} \sprod{x, x_{j}} \sprod{x_{j}, y}$
   \item $\forall x\in H$ gilt: $\|x\|^{2} = \sum_{j=1}^{\infty}
    |\sprod{x, x_{j}}|^{2}$ Parseval \eqref{eq:parseval}
   \item $\{ x_{1}, x_{2},\ldots\}^{\perp} = \{0\}$, d.\,h. $\sprod{x,
    x_{j}} = 0\ \forall j \Longleftrightarrow x=0$
   \item $\Span\{ x_{1}, x_{2}, \ldots \}$ ist dicht in $H$.
  \end{enumerate}
  
  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] durch ausrechnen:
      \begin{equation*}
	\sprod{x,y} = \sprod{\sum_{j=1}^{\infty} \sprod{x,x_{j}} x_{j},
	\sprod{\sum_{k=1}^{\infty} \sprod{y, x_{k}} x_{k}}}
	  =\sum_{j=1}^{\infty} \sprod{x, x_{j}} \sprod{x_{k}, y}
      \end{equation*}
     \item[$(2)\Rightarrow(3)$] setze $x=y$ in (2)
     \item[$(3)\Rightarrow(4)$] $\sprod{x, x_{j}} = 0 \forall j
      \Longrightarrow^{Parseval \eqref{eq:parseval}} \|x\|^{2} = 0 \Longrightarrow x=0$
     \item[$(4)\Rightarrow(5)$] $S := \{x_{1},\ldots,x_{n}\}, U := \Span
      S$ \thref{satz:2.1.2/1} $\Longrightarrow$ $H=\overline{U}\oplus
      \overline{U}^{\perp}$, außerdem $\overline{U}^{\perp} =
      U^{\perp}\subset S = \{0\}$ damit ist $H=\overline{U}$, d.\,h. $U$
      dicht in $H$
     \item[$(5)\Rightarrow(4)$] $x\in H$ $\Rightarrow$
      $x=\lim_{k\rightarrow\infty} u_{k}, u_{k}\in U$
      
      Es sei $\sprod{x, x_{j}} = 0\forall j$ $\Rightarrow$
      $\sprod{x, u_{k}} = 0\forall k$ $\Rightarrow$ $\sprod{x,x} =
      \lim_{k\rightarrow\infty} \sprod{x, u_{k}} =0
      \Rightarrow x=0$
     \item[$(4)\Rightarrow(1)$] $x\in H, s_{n} = \sum_{j=1}^{n} \sprod{x,
      x_{j}} x_{j} \Longrightarrow (s_{n})_{n}$ ist Cauchyfolge,
      da nach Satz-1/1 gilt $n>m$ $\|s_{n}-s_{m}\|^{2} = \sum_{j=m+1}^{n}
      |\sprod{x, x_{j}}|^{2}$ und dieses konvergiert gegen 0 nach der
      Bessel'schen Ungleichung \eqref{eq:bessel} aus \thref{satz:2.1.3/1}.
      
      $H$ vollständig $\Rightarrow$ $\exists, y\in H,
      y=\sum_{j=1}^{\infty} \sprod{x, x_{j}} x_{j}$
      
      $(x_{j})$ ONS $\Longrightarrow$ $\sprod{x, x_{j}} = \sprod{y, x_{j}} \Longrightarrow \sprod{x-y, x_{j}} =
      0\forall j \Longrightarrow^{(4)} x=y$
    \end{description}
  \end{proof}
\end{satz}

\begin{bemerk}\label{bem:5}
  $H$ Hilbertraum, $\dim H=\infty$, $\exists$ ONB $(x_{j})_{j}$
  Dann ist $H$ separabel, wenn eine abzählbare und dichte
  Teilmenge $U\subset H$ existiert. \obda sei $U=\{u_{1},u_{2},\ldots\}$
  und die $u_{j}$ linear unabhängig.
\end{bemerk}

\begin{lemma}[Gram-Schmidt]\label{lem:gram}
  $U=\{u_{1},u_{2},\ldots\}$ linear unabhängig
  
  Dann existiert ein ONS $(x_{j})_{j=1}^{\infty}$ in $H$ mit
  \begin{enumerate}
   \item $\Span \{ u_{1},\ldots,u_{n}\} = \Span \{ x_{1},\ldots,x_{n}\}$
    für alle $n\in \N$
   \item $\Span U = \Span \{ x_{1},x_{2},\ldots\}$
  \end{enumerate}
  
  \begin{proof}
    \begin{description}
     \item[$n=1$] $x_{1} := \frac{u_{1}}{\|u_{1}\|}$
     \item[$n=2$] $v_{2} = u_{2} - \sprod{u_{2}, x_{1}} x_{1}$
      $\Longrightarrow$ $\sprod{v_{2}, x_{2}} = 0$\\
      $x_{2} = \frac{v_{2}}{\|v_{2}\|}$
     \item[$n$-ter Schritt]
      \begin{align*}
	v_{n} &:= u_{k} - \sum_{j=1}^{n-1} \sprod{u_{n}, x_{j}} x_{j}\\
	x_{n} &:= \frac{v_{n}}{\|v_{n}\|}
      \end{align*}
    \end{description}
  \end{proof}
\end{lemma}

\begin{satz}
  $H$ Hilbertraum, $\dim H=\infty$. Dann gilt:
  \begin{enumerate}
   \item $H$ ist separabel $\Longleftrightarrow$ $\exists$ ONB
    $(x_{j})_{j=1}^{\infty}$ in $H$
   \item $H$ separabel $\Longrightarrow$ $H \cong l_{2}(\N)$ isometrisch
    isomorph
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item folgt aus der \thref{bem:5}, dem \thref{lem:gram} und
      \thref{satz:2.1.3/2}
     \item $J: H \rightarrow l_{2}(\N)$ mit $J(x) :=
      (\sprod{x,x_{j}})_{j=1}^{\infty}$ diese Abbildung ist nach
      \thref{satz:2.1.3/2} linear und injektiv. Die Parselval'sche
      Gleichung \eqref{eq:parseval} sagt:
      $\|J(x)\|_{l_{2}} = \|x\|_{H}$ auch surjektiv: $(\alpha_{j}) \in
      l_{2} \Longrightarrow x=\sum_{j=1}^{\infty} \alpha_{j} x_{j}$
      (existiert!) $\Rightarrow$ $\alpha_{j} = \sprod{x, x_{j}}$
    \end{enumerate}
  \end{proof}
\end{satz}

\subsection{Fourierreihen}
\begin{gather*}
  H=L_{2}(T) = L_{2,2\pi}([-\pi,\pi])\\
  \sprod{f,g} = \int_{-\pi}^{\pi} f(x)\overline{g(x)}\,dx\\
  S = \big( \frac{1}{\sqrt{2\pi}}e^{ikx}\big)_{k\in\Z}\quad\text{ist ein
     ONS}
\end{gather*}

\begin{satz}
  \begin{enumerate}
   \item $S$ ist eine ONB in $L_{2}(\pi)$, d.\,h. $\forall f\in
    L_{2}(\pi)$ gilt
    \begin{gather*}
      f = \sum_{k=-\infty}^{\infty} c_{k} e^{ikx}\\
      \text{mit } c_{k} = \frac1{1\pi} \int_{-\pi}^{\pi} f(x)
	 e^{-ikx}\,dx = \frac1{\sqrt{2\pi}}
	 \sprod{f,\frac1{\sqrt{2\pi}}e^{ikx} },\quad k\in\Z
    \end{gather*}
   \item $\forall f\in L_{2}(\pi)$ gilt:
    \begin{equation*}
      \|f\|^{2} = 2\pi\sum_{k=-\infty}^{\infty} |c_{k}|^{2}
    \end{equation*}
   \item Die trigonometrischen Polynome liegen dicht in $L_{2}(\pi)$
   \item $\int_{-\pi}^{\pi} f(x)e^{ikx}\,dx = 0, \forall k\in\Z$
    $\Longleftrightarrow$ $f(x)=0$ \fue
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
      \setcounter{enumi}{2}
     \item $f\in L_{2}(\pi) \Longrightarrow \exists g\in C_{2\pi}$ mit
      $\|f-g\|_{2} < \epsilon$ Nach dem Satz von Weierstraß
      (\thref{satz:weierstr}) existiert ein
      trigonometrisches Polynom $t$ mit $\|g-t\|_{\infty} < \epsilon$
      $\Longrightarrow$ $\|f-t\|_{2} \leq \|f-g\|_{2} + \|g-t\|_{2} \leq
      \epsilon \sqrt{2\pi} \|g-t\|_{\infty} < \epsilon (1+\sqrt{2\pi})$
    \end{enumerate}
    
    Der Rest folgt aus \thref{satz:2.1.3/2}
    
    \begin{equation*}
      f = \sum_{k} \sprod{f,\phi_{k}} \phi_{k} = \sum
      \underbrace{\frac1{\sqrt{2\pi}} \sprod{f,\frac1{\sqrt{2\pi}}
	e^{ikx}}}_{=:c_{k}} e^{ikx}
    \end{equation*}
  \end{proof}
\end{satz}

\begin{bemerk}
  Ist $f$ reellwertig, benutzt man die folgenden Schreibweise:
  \begin{equation*}
    c_{k} = \frac1{2\pi} \int_{-\pi}^{\pi} f(x)\cos kx\,dx -
    i\frac1{2\pi}\int_{-\pi}^{\pi} f(x)\sin kx\,dx = \frac12(a_{k} -
    ib_{k})
  \end{equation*}
  
  Man sieht jetzt, dass
  \begin{align*}
    c_{0} &= a_{0}\\
    c_{-k} &= \frac12 (a_{k}+ib_{k})\\
    \|f\|^{2} &= \pi \Big( \frac{a_{0}^{2}}2 + \sum_{k=1}^{\infty} (a_{k}^{2}
       + b_{k}^{2}) \Big)&&&\text{Parseval'sche Gleichung \eqref{eq:parseval}}\\
    f(x) &= \frac{a_{0}}2 + \sum_{k=1}^{\infty} (a_{k}\cos kx + b_{k}\sin
       kx)\\
    a_{k} &= \frac1\pi \int_{-\pi}^{\pi}f(x)\cos kx\,dx&&&k\in\N\\
    b_{k} &= \frac1\pi \int_{-\pi}^{\pi} f(x)\sin kx\,dx&&& k\in\N
  \end{align*}
\end{bemerk}

\begin{bemerk}
  $f\in L_{p}(\pi), 1\leq p\leq\infty$ $\Longrightarrow$
  $c_{k},a_{k},b_{k}$ sind sinnvoll definiert
  
  $\Longrightarrow$ $s_{n} f(x) :=\sum_{k=-n}^{n} c_{k}e^{ikx}$
  
  Frage: $s_{n}f \xrightarrow{n\rightarrow\infty} ?$
  \begin{enumerate}
   \item $1 < p<\infty$ $\Longrightarrow$ $\|f-s_{n}f\|_{p}
    \xrightarrow{n\rightarrow\infty} 0$ und $s_{n}f(x) \rightarrow
    f(x)$ \fue Carlson-Hunt-Theorem über \fue-konvergenz für Fourierreihen
   \item $p=1$ $\Longrightarrow$ $\epsilon f\in L_{1}$ so dass
    $\big(s_{n}f(x)\big)$ divergent \fue (Kolmogorov)
   \item $p=\infty\sim f\in C_{2\pi}$ $\Longrightarrow$ $\exists f\in
    C_{2\pi}$ so dass $\big(s_{n}f(x)\big)$ divergent in
    $x_{0}\in[-\pi,\pi]$
  \end{enumerate}
\end{bemerk}

\begin{bemerk}[Legendre-Polynome]
  $U = \{ 1, x, x^{2}, x^{3}, \ldots \}$ in $L_{2}([-1,1])$ ist linear
  unabhängig und $\overline{\Span U} = L_{2}([-1,1])$. Mithilfe des
  Schmidt-Cram kann man ein ONS\help{oder ONB?} in $L_{2}([-1,1])$ aus
  Polynomen konstruieren.
  \begin{equation*}
    L_{n}(x) = \frac1{2^{n}n!} \frac{d^{n}}{dx^{n}} \big(
    (x^{2}-1)^{n}\big)\qquad\text{\highl{Legendre-Polynome}}
  \end{equation*}
  
  $\big(\sqrt{\frac{2n+1}{2}} L_{n}\big)_{n=0}^{\infty}$ ist eine ONB.
\end{bemerk}

\section{Operatoren im Hilbertraum}
\subsection{Der adjungierte Operator}\label{sec:2.2.1}

Wir betrachten Operatoren $A\in \mathcal{L}(H_{1}, H_{2}), H_{1},H_{2}$
Hilberträume

\begin{satz}\label{satz:2.2.1/1}
  Es sei $A\in \mathcal{L}(H_{1}, H_{2})$. Dann existiert genau ein
  $A^{\ast}\in \mathcal{L}(H_{2}, H_{1})$ mit $\sprod{Ax, y}_{H_{2}} =
  \sprod{x, A^{\ast} y}_{H_{1}}\ \forall x\in H_{1}\ \forall y\in H_{2}$
  
  \begin{proof}
    Sei $y\in H_{2}$. Betrachten $T(x) := \sprod{Ax, y}_{H_{2}}$
    für $x\in H_{1}$. Der Operator ist linear und beschränkt:
    \begin{equation*}
      |T(x)| = |\sprod{Ax, y}_{H_{2}}| \leq \|Ax\|_{2} \|y\|_{2} \leq
      \underbrace{\|A\| \|y\|_{2}}_{\text{konst.}} \|x\|_{1}\quad\forall
      x\in H_{1}
    \end{equation*}
    $\Longrightarrow$ $T\in H_{1}'$ mit $\|T\| \leq \|A\| \|y\|_{2}$
    
    \thref{satz:2.1.2/2} $\Longrightarrow$ es existiert genau ein
    $y^{\ast}\in H_{1}$ mit $T(x) = \sprod{x, y^{\ast}}_{H_{1}}$
    und $\|T\| = \|y^{\ast}\|_{1}$. Wir definieren $A^{\ast}y :=
    y^{\ast}$. $\Longrightarrow$ $\sprod{Ax, y}_{H_{2}} = \sprod{x,
    A^{\ast}y}_{H_{1}}$
    
    $A^{\ast}: H_{2} \rightarrow H_{1}$ ist linear: $\forall x\in H_{1}:$
    \begin{equation*}
      \sprod{x, A^{\ast}(\lambda_{1}y_{1}+\lambda_{2}y_{2})}_{H_{1}}
      =^{Def} \sprod{Ax, \lambda_{1}y_{1}+\lambda_{2}y_{2}}_{2} =
      \overline{\lambda_{1}} \sprod{Ax,y_{1}}_{H_{2}}+
      \overline{\lambda_{2}} \sprod{Ax,y_{2}}_{H_{2}} =
      \overline{\lambda_{1}} \sprod{x, A^{\ast} y_{1}}_{H_{1}} =
      \overline{\lambda_{2}} \sprod{x, A^{\ast} y_{2}}_{H_{1}} =
      \sprod{x, \lambda_{1}A^{\ast}y_{1}+ \lambda_{2}A^{\ast}y_{2}}
    \end{equation*}
    
    $\Longrightarrow$ $A^{\ast}(\lambda_{1}y_{1}+\lambda_{2}y_{2} =
    \lambda_{1} A^{\ast}y_{1} + \lambda_{2}A^{\ast}y_{2}$.
    
    $A^{\ast}$ beschränkt:
    \begin{equation*}
      \|A^{\ast}y\|_{H_{1}} = \|y^{\ast}\|_{H_{1}} = \|T\| \leq \|A\|
      \|y\|_{2}\quad\forall y\in H_{2}
    \end{equation*}
    $\Longrightarrow$ $A^{\ast}\in \mathcal{L}(H_{2}, H_{1})$ und
    $\|A^{\ast}\| \leq \|A\|$
  \end{proof}
\end{satz}

\begin{defini}
  $A^{\ast}$ aus \thref{satz:2.2.1/1} heißt adjungierter Operator von
  $A\in\mathcal{L}(H)$
\end{defini}

\begin{satz}[Grundlegene Eigenschaften]\label{satz:2.2.1/2}
  Seien $A,B\in \mathcal{L}(H_{1}, H_{2})$ und $C\in \mathcal{L}(H_{2},
  H_{3}), \lambda\in\K$.
  
  Dann
  \begin{enumerate}
   \item $(A+B)^{\ast} = A^{\ast} + B^{\ast}$
   \item $(\lambda A)^{\ast} = \overline{\lambda} A^{\ast}$
   \item $(CA)^{\ast} = A^{\ast} C^{\ast}$
   \item $(A^{\ast})^{\ast} = A$
   \item $\|A^{\ast}\| = \|A\|$
   \item $\|AA^{\ast}\| = \|A^{\ast}A\| = \|A\|^{2}$
   \item Wenn $A$ eine Bijektion ist (d.\,h. $\exists A^{-1}\in
    \mathcal{L}(H_{2}, H_{1})$) $\Longrightarrow$ $\exists
    (A^{\ast})^{-1}=(A^{-1})^{\ast}$
   \item $R(A)^{\perp}$ = $N(A^{\ast})$, $R(A^\ast)^{\perp} = N(A)$
  \end{enumerate}
  
  \begin{proof}
    \begin{description}
     \item[(1)--(4)] einfach, z.\,B.
      \begin{equation*}
	\sprod{CAx, y} = \sprod{Ax, C^{\ast} y} = \sprod{x, A^{\ast}
	C^{\ast} y} \qquad \forall x,y
      \end{equation*}
      Nach \thref{satz:2.2.1/1} folgt daraus $(CA)^{\ast} = A^{\ast} C^{\ast}$
    
      \begin{equation*}
	\sprod{x, A,y} = \overline{\sprod{Ay, x}} =
	\overline{\sprod{y, A^{\ast} x}} = \sprod{A^{\ast}x,y} =
	\sprod{x, (A^{\ast})^{\ast} y}
      \end{equation*}
      $\Longrightarrow$ $(A^{\ast})^{\ast} = A$
    
     \item[(5) und (6)]
      \begin{equation*}
	\|x\|_{1} = 1 \Longrightarrow \|Ax\|_{2}^{2} = \sprod{Ax,
	Ax}_{H_{2}} = \sprod{A^{\ast} Ax, x}_{H_{1}} \leq
	\|A^{\ast}Ax\|_{1} \|x\|_{1} \leq \|A^{\ast}A\| \leq \|A^{\ast}\|
	\|A\|
      \end{equation*}
      $\Longrightarrow$ $\|A\|^{2} \leq \|A^{\ast}A\| \leq \|A^{\ast}\|
      \|A\|$\\
      $\Longrightarrow$ $\|A\| \leq \|A^{\ast}\|
      \leq^{\text{\thref{satz:2.2.1/1}}} \|A\|$
      
      $\Longrightarrow$ $\|A\| = \|A^{\ast}\|$ und $\|A^{\ast}A\| =
      \|A\|^{2}$ sowie $\|AA^{\ast}\| = \|A^{\ast}A\|$\help{wieso?}
    
     \item[(7)]
      \begin{equation*}
	\left.\begin{aligned}
		A^{-1} A = I \Longrightarrow A^{\ast}(A^{-1})^{\ast} = I\\
		AA^{-1} = I \Longrightarrow (A^{-1})^{\ast} A^{\ast} = I
	      \end{aligned}\right\} \Longrightarrow (A^{-1})^{\ast} = (A^{\ast})^{-1}
      \end{equation*}
    
     \item[(8)]
      \begin{align*}
	Ax = 0 &\Leftrightarrow \sprod{Ax, y}_{H_{2}} = 0&&\forall
	   y\in H_{2}\\
	&\Leftrightarrow \sprod{x, A^{\ast}y}_{H_{1}} = 0&&\forall y\in
	   H_{2}\\
	& \Leftrightarrow \sprod{x,z}_{H_{1}} = 0&&\forall
	   z\in\R(A^{\ast})\\
	& \Leftrightarrow x\perp R(A^{\ast})
      \end{align*}
      $\Longrightarrow$ $R(A^{\ast})^{\perp} = N(A)$
    \end{description}
  \end{proof}
\end{satz}

\begin{folger}
  \begin{align*}
    H &= \overline{R(A)} \oplus N(A^{\ast})
       &= \overline{R(A)^{\ast}} \oplus N(A)
  \end{align*}
\end{folger}

\begin{bsp}
  \begin{enumerate}
   \item $A\in \mathcal{L}(\K^{n}) \Longrightarrow A=(a_{jk}) \Rightarrow
    A^{\ast} = (\overline{a_{kj}})$
   \item $(\mathcal{K}f)(x) = \int_{\Omega} k(x,y) f(y)\,dy, k\in
    L_{2}(\Omega\times\Omega), H=L_{2}(\Omega)$
    
    $\mathcal{K}: L_{2}(\Omega) \rightarrow L_{2}(\Omega)$ linear und
    beschränkt
    
    \begin{align*}
      \sprod{\mathcal{K} f,g} &= \int_{\Omega} (\mathcal{K}f)(x)
	 \overline{g(x)}\,dx = \int_{\Omega} \Big(\int_{\Omega}
	 k(x,y)f(y)\,dy \Big)\overline{g(x)}\,dx\\
      &=^{\text{nach satz v. Fubini \thref{satz:fubini}}} \int_{\Omega} \Big( f(y) \int_{\Omega}
	 k(x,y)\overline{g(x)}\, dx \Big)\,dy = \int_{\Omega} f(y)
	 \underbrace{\overline{\int_{\Omega} \overline{k(x,y)} g(x)\,dx}}
	 _{(\mathcal{K}^{\ast}g)(y) := \int_{\Omega}
	   \overline{k(x,y)}g(x)\,dx}\,dy\\
      &= \sprod{f, \mathcal{K}^{\ast}g}
    \end{align*}
  \end{enumerate}
\end{bsp}

\begin{defini}
  \begin{enumerate}
   \item $A\in \mathcal{L}(H_{1}, H_{2}):$ $A$ heißt \highl{unitär}
    $:\Longleftrightarrow$ $\exists A^{-1}\in \mathcal{L}(H_{2}, H_{1})$
    und $A^{-1} = A^{\ast}$
   \item $A\in \mathcal{L}(H):$ $A$ heißt \highl{selbstadjungiert} $:\Longleftrightarrow$
    $A^{\ast} = A$
   \item $A\in \mathcal{L}(H):$ $A$ heißt \highl{normal}
    $:\Longleftrightarrow$ $A^{\ast}A = AA^{\ast}$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $A\in \mathcal{L}(H_{1}, H_{2})$ unitär $\Longleftrightarrow$
    $A$ surjektiv und $\sprod{Ax, Ay} = \sprod{x,y}
    (\forall x,y)$
   \item $A = A^{\ast} \Longleftrightarrow \sprod{Ax,y} = \sprod{x, Ay}
    (\forall x,y\in H)$
   \item $A$ normal $\Longleftrightarrow$ $\sprod{Ax, Ay} =
    \sprod{A^{\ast}x, A^{\ast}y}, (\forall x,y \in H)$
   \item Wenn $A\in \mathcal{L}(H)$ unitär oder selbstadjungiert ist,
    dann folgt daraus, dass $A$ auch normal ist.
  \end{enumerate}
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item $H, (x_{j})_{j}$ ONB und $T: H\rightarrow l_{2}$ mit $Tx =
    \sprod{x,x_{j}}, (\forall x\in H)$ $\Longrightarrow$ $T\in
    \mathcal{L}(H, l_{2})$ unitär
   \item $\mathcal{K} = \mathcal{K}^{\ast} \Longleftrightarrow k(x,y) =
    \overline{k(y,x)}$ \fue
   \item Multiplikationsoperatoren: $H=L_{2}(\Omega)$ sei $\phi\in
    L_{\infty}(\Omega)$ und $M_{\phi}f(x) := \phi(x)f(x), (f\in
    L_{2}(\Omega))$ $\Longrightarrow$ $M_{\phi} : L_{2}(\Omega)
    \rightarrow L_{2}(\Omega)$ linear und beschränkt
    
    \begin{equation*}
      \|M_{\phi}f\|_{2}^{2} = \int_{\Omega} \big(
      \underbrace{|\phi(x)|}_{\leq \|\phi\|_{\infty}} f(x)
      \big)^{2}\,dx \leq \|\phi\|_{\infty} \|f\|^{2}_{2}
    \end{equation*}
    \begin{align*}
      \sprod{M_{\phi}f, g} &= \int_{\Omega} \phi(x) f(x)
	 \overline{g(x)}\,dx = \int_{\Omega} \overline{\overline{\phi(x)}
	 g(x)}\,dx\\
      &= \sprod{f, M_{\overline{\phi}}f}
    \end{align*}
    $\Rightarrow$ $M_{\phi}^{\ast} = M_{\overline{\phi}}$ $\Rightarrow$
    $M_{\phi}$ normal
    
    $M_{\phi}$ selbstadjungiert $\Leftrightarrow$ $\phi =
    \overline{\phi}$ \fue\\
    $M_{\phi}$ unitär $\Leftrightarrow$ $M_{\phi}M_{\overline{\phi}} =
    \id \Leftrightarrow \phi(x) \overline{\phi(x)} f(x) = f(x)\ \fue,
    (\forall f) \Leftrightarrow (\phi(x)) = 1\ \fue$
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  $A: D(A) \rightarrow H, D(A)\subset H$ dicht
  
  $A$ \highl{symmetrisch} $:\Longleftrightarrow$ $\forall x,y\in D(A):
  \sprod{Ax, y}= \sprod{x,Ay}$
  
  \underline{Satz von Hellinger-Toeplitz}: (Höhere Analysis II)\\
  $D(A) = H, A$ symmetrisch $\Longrightarrow$ $A\in \mathcal{L}(H)$ und
  $A = A^{\ast}$
\end{bemerk}

\begin{satz}\label{satz:2.2.1/3}
  Ist $A \in \mathcal{L}(H)$ selbstadjungiert, dann gilt für alle $x\in
  H$, dass das Skalarprodukt $\sprod{Ax, x}$ reellwertig ist und
  $\|A\| = \sup_{\|x\|=1} |\sprod{Ax,x}|$
  
  \begin{proof}
    $A = A^{\ast} \Longrightarrow \sprod{Ax, x} = \sprod{x, Ax} =
    \overline{\sprod{Ax, x}} \Longrightarrow \sprod{Ax, x}$ reell
    
    setzen $\alpha := \sup_{\|x\|=1} |\sprod{Ax, x}|,
    \|x\|=1 \Rightarrow |\sprod{Ax, x}| \leq \|Ax\| \leq \|A\|
    \Longrightarrow \alpha \leq \|A\|$
    
    Es gilt:
    \begin{equation}\label{eq:234}
      |\sprod{Az, z}| = \|z\|^{2} \big|\sprod{A\frac{z}{\|z\|},
      \frac{z}{\|z\|}} \big| \leq \alpha \|z\|^{2}
    \end{equation}
    
    Außerdem gilt:
    \begin{equation*}
      \sprod{A(x+y), x+y} - \sprod{A(x-y), x-y} = 2
      \sprod{Ax, y} + 2 \sprod{Ay, x} \in \R\\
    \end{equation*}
    
    \begin{equation*}\label{eq:235}
      \Longrightarrow \sprod{Ax, y} + \sprod{Ay + \sprod{Ay, x}}
       \leq^{\eqref{eq:234}} \frac12 \alpha \big( \|x+y\|^{2} +
      \|x-y\|^{2} \big) =^{\eqref{eq:parallelo}} \alpha (\|x\|^{2} +
      \|y\|^{2})
    \end{equation*}
    
    Wir setzen $x=\beta u$ und $y = Au$, wobei die Norm $\|u\|=1$ seien
    soll und $\beta = \|Au\|$. Aus \eqref{eq:235} folgt dann:
    \begin{align*}
      \sprod{\beta Au, Au} + \sprod{A^{2} u, \beta u} &\leq
	 \alpha (\beta^{2} + \beta^{2})\\
      2\beta \|Au\|^{2} = 2\beta^{3} &\leq 2\alpha\beta^{2}\\
      \beta &\leq \alpha
    \end{align*}
    $\Longrightarrow$ $\forall u, \|u\|=1$ gilt $\|Au\| \leq \alpha$
    $\Rightarrow$ $\|A\| \leq \alpha$
  \end{proof}
\end{satz}

\begin{satz}\label{satz:2.2.1/4}
  \begin{enumerate}
   \item $A\in \mathcal{L}(H)$ selbstadjungiert und $\sprod{Ax,x}
    = 0 (\forall x)$ $\Longrightarrow$ $A=0$
   \item $A \in \mathcal{L}(H)$ normal $\Longleftrightarrow$ $\|Ax\| =
    \|A^{\ast}x\| (\forall x\in H)$
   \item $H$ komplexer Hilbertraum ($\K =\C$) und $A\in \mathcal{L}(H)$.
    Dann gilt: $A = A^{\ast} \Longleftrightarrow \sprod{Ax, x}$ ist
    reell für alle $x\in H$.
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item $\sprod{Ax, x} = 0 (\forall x)
      \Longrightarrow^{\text{\thref{satz:2.2.1/3}}} \|A\| = 0 \Longrightarrow
      A=0$
     \item
      \begin{align*}
        \|Ax\|^{2} - \| A^{\ast}x\|^{2} &= \sprod{Ax, Ax} -
	   \sprod{A^{\ast} x, A^{\ast} x} - \sprod{A^{\ast} A x, x}
	   - \sprod{AA^{\ast} x, x}\\
	&=  \sprod{(A^{\ast}A - AA^{\ast}) x, x} (\forall x\in H)
      \end{align*}
     \item
      \begin{description}
       \item["`$\Rightarrow$"'] nach \thref{satz:2.2.1/3}
       \item["`$\Leftarrow$"']
	$\underbrace{\sprod{A(x+y), x+y}}_{\in \R} =
	\underbrace{\sprod{Ax, x}}_{\in\R} +
	\underbrace{\sprod{Ay,y}}_{\in\R} + \sprod{Ax, y} + \sprod{Ay, x}
	\Longrightarrow \sprod{Ax,y} + \sprod{Ay, x} \in \R, (\forall
	x,y\in H)$
	
	$\Longrightarrow$ $\Im \sprod{Ax, y} = -\Im \sprod{Ay, x}
	= - \Im \overline{\sprod{x, Ay}} = \Im \sprod{x, Ay}, (\forall
	x,y)$
	
	Setzen: $ix$ anstelle von $x$
	
	$\Longrightarrow$ $\Re \sprod{Ax, y} = \Im i\sprod{Ax,y} = \Im
	\sprod{A (ix), y} = \Im \sprod{ix, Ay} = \Im i\sprod{x, Ay} =
	\Re\sprod{x, Ay}$
      \end{description}
    \end{enumerate}
  \end{proof}
\end{satz}

\subsection{Projketionen}\label{sec:2.2.2}

$P:H \rightarrow H$ orthogonale Projektion $:\Longleftrightarrow$
$\exists U\subset H$ abgeschlossener Teilraum, so dass $P = P_{U}$, wobei
$P_{U}x := x_{1}$, falls $x= x_{1}+x_{2}$ mit $x_{1}\in U, x_{2}\in
U^{\perp}$ $\Longleftrightarrow$ $\|x-x_{1}\| = \inf_{u\in U}\|x-u\|,
x_{1}\in U$

siehe Abschnitt\,\ref{sec:2.1.2}: $P\in \mathcal{L}(H), \|P\| = 1$

\begin{satz}
  $P\in \mathcal{L}(H)$. Dann sind äquivalent:
  \begin{enumerate}
   \item $P$ ist orthogonale Projektion
   \item $P=P^{2} = P^{\ast}$
   \item $P^{2} = P$ und für alle $x\in H$ gilt: $\sprod{Px, x}
    \geq 0$
  \end{enumerate}
  
  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] Es sei $P = P_{U}$ $\Longrightarrow$
      $P_{U}^{2} x = P_{U}x, (\forall x) \Longrightarrow P_{U}^{2} = P_{U}$
      
      $x,y\in H \Longrightarrow \sprod{P_{U}x,y} =
      \sprod{\underbrace{P_{U}x}_{\in U}, \underbrace{P_{U}y}_{\in U}+
      \underbrace{(y- P_{U}y)}_{\in U^{\perp}}} = \sprod{P_{U} x, P_{U}y}
      = \sprod{\underbrace{(P_{U}x-x)}_{\in U^{\perp}}+x,
      \underbrace{P_{U}y}_{\in U}} = \sprod{x, P_{U}y} \Longrightarrow
      P_{U}^{\ast} = P_{U}$
     \item[$(2)\Rightarrow(3)$] $x\in H$ $\Rightarrow$ $\sprod{Px,x} =
      \sprod{P^{2}x,x} = \sprod{Px, Px} \geq0$
     \item[$(3)\Rightarrow(1)$] setzen $U:= R(P) = \{Px: x\in H\} = \{ z:
      Pz = z\}$ Es sei $x\in H \Rightarrow x = \underbrace{Px}_{\in U}+
      \underbrace{(x - Px)}_{\in N(P)}$ mit $Px\in
      U, P(x-Px) = Px-P^{2}x = 0 \Rightarrow x-Px\in N(P)$ 
      
      Es ist $U\cap N(P) = \{0\}$, denn $x= Px\in U\cap N(P)
      \Longrightarrow Px = 0 \Longrightarrow x=0$.
      
      noch zu zeigen: $U\perp N(P)$\\
      Es sei $x=x_{1}+x_{2}$ mit $x_{1}-Px, x_{2}\in N(P)$
      
      Vor.: $0 \leq \sprod{Px, x} = \sprod{x_{1}, x_{1}+x_{2}}
      = \sprod{x_{1}, x_{1}} + \sprod{x_{1},x_{2}}$
      \begin{equation}\label{eq:321}
	\Longrightarrow -\|x_{1}\|^{2} \leq \sprod{x_{1},
	x_{2}} \quad \forall x_{1}\in U, x_{2}\in N(P)
      \end{equation}
      
      Annahme: $\sprod{x_{1}, x_{2}} =: \overline{\alpha} \not= 0
      (x_{1} \not= 0)$\\
      setzen: $y_{1} := x_{1}, y_{2} = -\frac{2}{\alpha} \|x_{1}\|^{2}
      x_{2} \in N(P)$
      
      \eqref{eq:321} $\Longrightarrow -\|x_{1}\|^{2} = -\|y_{1}\|^{2}
      \leq \sprod{y_{1}, y_{2}} = \sprod{x_{1}, -\frac{2}{\alpha}
      \|x_{1}\|^{2} x_{2}} = -\frac{2}{\alpha} \| x_{1}\|^{2}
      \underbrace{\sprod{x_{1},x_{2}}}_{=\overline{\alpha}} = -2
      \|x_{1}\|^{2} \Longrightarrow -\|x_{1}\|^{2} = -2 \|x_{1}\|^{2}
      \Longrightarrow \|x_{1}\| =0$ Widerspruch zu $(x_{1}\not=0)$
      \end{description}
  \end{proof}
\end{satz}

\subsection{Kompakte Operatoren}\label{sec:2.2.3}
$A\in \mathcal{L}(H)$ Ziel: $A$ kompakt $\Longleftrightarrow$ $A^{\ast}$
kompakt

\begin{lemma}\label{lem:2}
  $A\in\mathcal{L}(H), \dim R(A) < \infty (=n)$ ("`finite rank"'
  operator oder finit oder ausgeartet)
  
  Daraus folgt:
  \begin{equation*}
    \sum_{j=1}^{n} \sprod{x, e_{j}^{\ast}} e_{j}
  \end{equation*}
  mit $(e_{j})_{j=1}^{n}$ ONS in $H$, $e_{j}^{\ast}\in H$
  
  $\dim R(A^{\ast}) = \dim R(A)$ und $A^{\ast}y = \sum_{j=1}^{n}
  \sprod{y, e_{j}} e_{j}^{\ast}$
  
  \begin{proof}
    $\dim R(A)=n$ $\Longrightarrow$ $\exists$ ONS $(e_{j})_{j=1}^{n}$ mit
    $Ax = \sum_{j=1}^{n} \sprod{Ax, e_{j}} e_{j} = \sum_{j=1}^{n}
    \sprod{x, \underbrace{A^{*}e_{j}}_{=: e_{j}^{\ast}}} e_{j}$
    
    $x,y\in H \Longrightarrow \sprod{Ax, y} = \sprod{
    \sum_{j=1}^{n} \sprod{x, e_{j}^{\ast}} e_{j}, y} =
    \sum_{j=1}^{n} \sprod{x, e_{j}^{\ast}} \sprod{e_{j}, y}
    = \sprod{x, \sum_{j=1}^{n} \sprod{y, e_{j}}, e_{j}^{\ast}}$
    
    $\Longrightarrow$ $A^{\ast} y = \sum_{j=1}^{n} \sprod{y, e_{j}}
    e_{j}^{\ast}\quad\forall y\in H$ $\Longrightarrow$ $\dim R(A^{\ast})
    \leq n = \dim R(A)$
    
    Andererseits: $\dim R(A) = \dim R\big((A^{\ast})^{\ast}\big) \leq
    \dim R(A^{\ast})$
  \end{proof}
\end{lemma}

\begin{satz}
  \begin{enumerate}
   \item $A\in \mathcal{L}(H)$ kompakt $\Longleftrightarrow$ es existiert
    ein Folge $(A_{n})_{n}$ finiter Operatoren ($\dim R(A_{n} \leq
    \infty$), die gegen $A$ konvergiert 
   \item $A\in \mathcal{L}(H)$. Dann $A$ kompakt $\Longleftrightarrow$
    $A^{\ast}$ kompakt
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item
      \begin{description}
       \item["`$\Leftarrow$"'] klar nach \thref{fol:1}
       \item["`$\Rightarrow$"'] Es sei $A$ kompakt, $A\in \mathcal{L}(H)$
	$\Longrightarrow$ $M= \{ Ax : \|x\| \leq 1\}$ präkompakt in $H$
	
	Es sei $\epsilon>0$. Nach \thref{satz:1.1.3/2} existiert für $M$ ein
	endliches $\epsilon$-Netz $\{x_{1},\ldots,x_{N}\}$, d.\,h.
	\begin{equation*}
	  M\subset \bigcup_{j=1}^{N} K_{\epsilon}(x_{j})
	\end{equation*}
	Es sei $U= \Span\{x_{1},\ldots,x_{N}\} \Longrightarrow \dim U
	\leq N < \infty$. Daraus folgt $U$ ist auch ein abgeschlossener
	Teilraum. Es sei $P=P_{U}$ die zugehörige orthogonale Projektion.
	$\Longrightarrow$ $B:= PA$ ist finiter Operator, da $\dim R(B)
	\leq N$
	
	Es sei $\|x\| \leq 1 \Rightarrow \|Ax - PAx\| = \inf_{u\in U}
	\|Ax-u\| \leq \min_{j=1,\ldots,N} \|Ax -x_{j}\| \leq \epsilon$
	
	Weiterhin gilt für alle $x$ mit $\|x\|=1$: $\|A-PA\|\leq \epsilon$.
      \end{description}
     \item $A$ sei also nun kompakt. Aus (1) folgt, dass eine Folge
      $(A_{n})_{n}$ finiter Operatoren existiert, die gegen $A$
      konvergiert. \thref{satz:2.2.1/2} liefert
      \begin{equation*}
	\|A_{n}^{\ast}- A^{\ast}\| = \|A_{n}-A\|
	\xrightarrow{n\rightarrow\infty} 0
      \end{equation*}
      
      Aus \thref{lem:2} folgt $\dim R(A^{\ast}_{n})= \dim R(A_{n}) <
      \infty$ und (1) $A^{\ast}$ kompakt.
      
      $A^{\ast}$ kompakt $\Longrightarrow$ $(A^{\ast})^{\ast} = A$ kompakt
    \end{enumerate}
  \end{proof}
\end{satz}

\section{Spektraltheorie kompakter Operatoren}
\subsection{Die Fredholm'sche Alternative}

Ziel der Spektraltheorie ist das Lösen von Gleichungen der Form $Ax=y$
mit $A\in\nolinebreak\mathcal{L}(H)$ und $\dim H=\infty$
  
Treffen wir eine kleine Annahme: $\exists A^{-1}\in \mathcal{L}(H)$ und
$A$ sei kompakt. Dann wäre auch $I= A^{-1}A$ kompakt und damit $\dim H <
\infty$. Wir können also nicht erwarten, dass sich die Gleichung $Ax=y$ mit einer
stetigen inversen Abbildung lösen lässt.
  
Die Fredholm'sche Alternative betrachtet stattdessen Gleichungen der Form
$(A-\lambda I)x =y$ ($\lambda \not=0$). Setzen wir \obda $\lambda=1$
\help{Warum gilt \obda? Warum machen wir das?} bleibt die Gleichung
\begin{equation*}
  Ax - x =y
\end{equation*}

\begin{bemerk}
  Wir wissen aus Abschnitt~\ref{sec:2.2.1}: $H= \overline{R(A-I)}\oplus N(A^{\ast}-I) =
  \overline{R(A^{\ast}-I)} \oplus N(A-I)$, d.\,h. $N(A-I)^{\perp} =
  \overline{R(A^{\ast}-I)}$
\end{bemerk}

\begin{lemma}
  Wenn $A\subset \mathcal{L}(H)$ kompakt, dann existiert ein $c>0$ mit
  \begin{align*}
    \|z\| &\leq c \|(A-I)z\|&&\forall z\in N(A-I)^{\perp}
  \end{align*}
  
  \begin{proof}
    Annahme: $\forall j\in \N\ \exists z_{j}\in N(A-I)^{\perp}$ mit
    \begin{equation}\label{eq:333}
      \|(A-I) z_{j}\|\leq \frac1j
    \end{equation} und
     $\|z_{j}\|=1$
    
    $A$ kompakt $\Longrightarrow$ $\{ Az_{j}: j\in \N \}$ ist präkompakt
    $\Longrightarrow$ $\exists$ konvergente Teilfolge $(Az_{jk})_{k=1}^{\infty}$
    
    $Az_{jk} \xrightarrow{k\rightarrow\infty} y\in H$ $\Longrightarrow$
    $z_{jk} = Az_{jk} - (\underbrace{A z_{jk} - z_{jk}}_{\rightarrow 0
    \text{nach \eqref{eq:333}}}) \rightarrow y$
    
    $\Longrightarrow$ $y \in N(A-I)^{\perp}$ andererseits gilt
    $(A-I)z_{jk} \rightarrow (A-I)y$ und nach \eqref{eq:333}
    $(A-I)z_{jk} \rightarrow 0$  $\Longrightarrow$ $y\in N(A-I)$
    
    $\Longrightarrow$ $y=0$ Widerspruch zu $\|y\|=1$
  \end{proof}
\end{lemma}

\help{Hier fehlen zwei Vorlesungen}

$A\in \mathcal{L}$ kompakt, $A = A^{\ast}, \dim H = \infty, Ax =
\sum_{j=1}^{\infty} \lambda_{j}\sprod{x, x_{j}} x_{j}$

\begin{schritte}
 \item $\exists \infty$-viele Eigenwerte
  
  -Lemma- $\Rightarrow$ $\exists$ Eigenwert $\lambda_{1}\in \R$ mit
  $|\lambda_{1}| = \sup_{\|x\|=1} |\sprod{Ax, x}|$ wählen
  Eigenvektor $x_{1}\in N(A-\lambda_{1}I), \|x_{1}\|=1$
  
  Setzen: $H_{2} := \{x\in H: \sprod{x, x_{1}} = 0\}$ und
  betrachten $A|_{H_{2}} : H_{2} \rightarrow H_{2}$, denn $\sprod{Ax,
  x_{1}} =^{A=A^{\ast}} \sprod{x, Ax_{1}} = \lambda_{1} \sprod{x, x_{1}} = 0$ ist kompakt und selbstadjungiert
  
  Lemm $\Rightarrow$ $\exists$ Eigenwert $\lambda_{2}$ mit $|\lambda_{2}|
  = \sup_{\|x\|=1, x\in H_{2}} |\sprod{Ax, x} | =
  \|A\|_{\mathcal{L}(H_{2})} = |\lambda_{1}|$
  
  wählen Eigenvektor $x_{2} (\|x_{2}\|=1) \in N(A-\lambda_{2}I)\cap H_{2}
  \Rightarrow \sprod{x_{2}, x_{1}} = 0$
  
  setzen: $H_{3} := \{x\in H_{2}: \sprod{x, x_{2}} = 0\} = \{ x\in
  H: \sprod{x,x_{1}} = \sprod{x, x_{2}} = 0 \}$
  \ldots Iterieren \ldots
  $H_{n} := \{ x\in H_{n-1} : \sprod{x, x_{n-1}} = 0\} = \{ x\in
  H: \sprod{x,x_{j}} = 0 (j=1,\ldots,n-1)\}$
  
  $\lambda_{k}$ Eigenwerte von $A$, $A x_{k} = \lambda_{k} x_{k}$ wobei
  $x_{k}\in H_{k} (k=1,\ldots,n)$
  
  $|\lambda_{k}| = \sup_{\|x\|=1, x\in H_{k}} |\sprod{Ax, x}| =
  \|A\|_{\mathcal{L}(H_{k})} (k=1,\ldots,n)$ $\Longrightarrow$
  $(x_{j})_{j}$ ist ONS, $H=H_{1} \supset H_{2} \supset H_{3} \supset
  \ldots \supset H_{n} \supset \ldots$
  
  Annahme: $H_{n} = \{0\} \Longrightarrow \forall x\in H \Rightarrow
  \sprod{x, x_{j}} = 0 (j=1,\ldots,n-1) \Longleftrightarrow x=0$
  $\Longrightarrow$ $\{ x_{1},\ldots,x_{n-1} \}$ ONB in $H$
  $\Longrightarrow$ $\dim H < \infty$
  
  $\Longrightarrow$ $\exists \infty$-viele Eigenwerte: $|\lambda_{1}|
  \geq |\lambda_{2}| \geq \ldots \geq |\lambda_{n}| \geq |\lambda_{n-1}|
  \geq \ldots \xrightarrow{n\rightarrow\infty} 0$ nach
  \thref{satz:2.3.2/2} und \thref{satz:2.3.2/1}
  
 \item $x \in H$ $\Rightarrow$ $Ax = \sum_{j=1}^{\infty} \lambda_{j}
  \sprod{x, x_{j}} x_{j}$ wobei $(\lambda_{j}), (x_{j})$ wie aus
  Schritt~1.
  
  Es gilt: $\|Ax- \sum_{j=1}^{n} \lambda_{j}\sprod{x, x_{j}}
  x_{j}\|^{2} = \|A \underbrace{(x-\sum_{j=1}^{n} \sprod{x, x_{j}}
  x_{j})}_{\in H_{n+1}, \text{da senkrecht auf $H_{n}: \sprod{x -
  \sum_{j=1}^{n} \sprod{x,x_j}, x_{k}}=0 $}}\| \leq
  \|A\|^{2}_{\mathcal{L}(H_{n+1})} \|x-\sum_{j=1}^{n} \sprod{x, x_{j}}
  x_{j}\|^{2}$
  
  $=^{Lemma} |\lambda_{n+1}|^{2} ( \|x\|^{2} - \sum_{j=1}^{n}
  |\sprod{x,x_{j}}|^{2}) \leq |\lambda_{n+1}| \|x\|^{2}
  \xrightarrow{n\rightarrow0} 0$ nach Schritt~1
 \item Ann.: $\exists$ Eigenwert $\lambda\not=0, \lambda \not= \lambda
  (\forall j\in \N)$. Sei $x$ Eigenvektor: $Ax= \lambda x, x\not=0$
  $\Longrightarrow$ $x= \frac1{\lambda} Ax =^{2.Schritt}
  \sum_{j=1}^{\infty} \frac{\lambda_{j}}{\lambda} \sprod{x, x_{j}}
  x_{j} \Rightarrow \sprod{x, x_{k}} = \frac{\lambda_{j}}{\lambda}
  \sprod{x, x_{k}} \Rightarrow \sprod{x, x_{k}} =0
  (\forall k) $
								    
  $\Rightarrow$ $x=0$ Widerspruch zu der Annahme x sei Eigenwert

 \item 0 keine Eigenwert $\Rightarrow$  $(x_{j})_{j}$ ONB
  Es sei $x\in H$ $\Rightarrow ^{(x_{j}) ONB}$ $\sum_{j=1}^{\infty}
  |\sprod{x, x_{j}}|^{2} < \infty$ (Beselsche Ungleichung
  \eqref{eq:bessel})
  
  $\Rightarrow$ $s_{n} = \sum_{j=1}^{n} \sprod{x, x_{j}} x_{j}$
  bilden Cauchyfolge in $H$
  
  $H$ vollständig $\Rightarrow$ $\exists$ $y\in H$ mit $y=
  \sum_{j=1}^{\infty} \sprod{x, x_{j}} x_{j}$
  $\Rightarrow$ $Ay = \sum_{j=1}^{\infty} \lambda_{j} \sprod{x,x_{j}}
  x_{j} \Longrightarrow^{2.Schritt} Ax = Ay \Rightarrow
  A(x-y) = 0 \xrightarrow{0 kein EW} x=y$
\end{schritte}

\begin{bemerk}
  $(A-\lambda I)x=y, A\in \mathcal{L}(H)$ kompakt, $A=A^{\ast}, y\in H,
  \lambda \not= 0$
  
  Annahme: $(\lambda_{j})$ EW, $(x_{j})_{j}$ EV, ONS $\Rightarrow$ $Ax =
  \sum_{j=1}^{\infty} \lambda_{j} \sprod{x, x_{j}} x_{j}$
  $\Leftrightarrow$ $\sum_{j=1}^{\infty} \lambda_{j} \sprod{x, x_{j}}
  x_{j} - \lambda x = y$ $\Leftrightarrow$ $ x = -\frac1{\lambda}
  y + \frac{1}{\lambda} \sum_{j=1}^{\infty} \lambda_{j} \sprod{x, x_{j}}
  x_{j}$
  $\Rightarrow$ $\sprod{x, x_{k}} = -\frac{1}{\lambda} \sprod{y, y_{k}} +
  \frac1{\lambda} \lambda_{k} \sprod{x, x_{k}} (k\in\N)$
  
  $\lambda \not= \lambda_{k} \Rightarrow \sprod{x, x_{k}}
  (1-\frac{\lambda_{k}}{\lambda}) = -1 \frac1{\lambda} \sprod{y,y_{k}}
  \Rightarrow \sprod{x, x_{k}}$
\end{bemerk}

\begin{satz}[Hilbert'sche Methode]
  $H$ Hilbertraum, $A\mathcal{L}(H)$ kompakt und selbstadjungiert,
  $\lambda \not= 0$. Es seien $(\lambda_{j})_{j}$ Eigenwerte und
  $(x_{j})_{j}$ das ONS von Eigenvektoren aus -Satz-1-
  
  Dann gilt: Die Gleichung $(A-\lambda I) x=y$ ist genau dann lösbar,
  wenn $\lambda \perp N(A-\lambda I)$. Durch
  \begin{equation}\label{eq:444}
    x = -\frac1{\lambda} y - \frac1{\lambda}
    \sum_{\lambda_{j}\not=\lambda}
    \frac{\lambda_{j}}{\lambda-\lambda_{j}} \sprod{y, x_{j}}
    x_{j}
  \end{equation}
  ist eine Lösung von $(A-\lambda I)x=y$ gegeben.
  
  Sämtliche Lösungen sind durch $x+N(A-\lambda I)$ gegeben.
  
  \begin{proof}
    Reihe in \eqref{eq:444} ist konvergent:
    \begin{equation*}
      s_{n} = \sum_{j=1, \lambda_{j}\not=\lambda}^{n}
      \frac{\lambda_{j}}{\lambda-\lambda_{j}} \sprod{y, x_{j}}
	 x_{j}
    \end{equation*}

    $\Rightarrow$ 
    \begin{align*}
      \|s_{n}-s_{m}\|^{2} &=
	 \|\sum_{j=m+1}^{n}\frac{\lambda_{j}}{\lambda-\lambda_{j}}
	 \sprod{y, x_{j}} x_{j}\|^{2}\\
      n > m > m_{0}\qquad &= \sum_{j=m+1}^{n}
	 \Big|\frac{\lambda_{j}}{\lambda-\lambda_{j}}\Big|^{2} |\sprod{y,
	 x_{j}}|^{2} \leq c \sum_{j=m+1}^{n} |\sprod{y,x_{j}}|^{2}
	 \xrightarrow{m,n\rightarrow\infty} 0 
	 (Beselsche Ungleichung)
    \end{align*}
    $\Rightarrow$ $s_{n} \rightarrow s\in H$
    
    Es sei $x$ wie in \eqref{eq:444} $\Rightarrow$ $(A-\lambda I)x =
    -\frac1{\lambda} (A-\lambda I) y - \frac1{\lambda}
    \sum_{\lambda_{j}\not=\lambda} \sprod{y, x_{j}}
    (\lambda_{j}-\lambda) x_{j} (da Ax_{j} = \lambda_{j} x_{j}) =
    -\frac1{\lambda} Ay + y + \frac1{\lambda} \sum_{\lambda_{j} \not=
    \lambda} \lambda_{j} \sprod{y, x_{j}} x_{j}$
    
    \begin{faelle}
     \item $\lambda$ kein Eigenwert $\Rightarrow$ $\sum_{\lambda_{j} \not=
      \lambda} \lambda_{j} \sprod{y, x_{j}} x_{j} = Ay$ nach
      -Satz-1- $\Rightarrow$ $(A-\lambda I)x =y$
     \item $\lambda$ Eigenwer, $y\perp N(A-\lambda I)$ (notwendig nach
      Fredholscher Alternative)
      
      -Satz-1- $\Rightarrow$ $Ay = \sum_{j=1}^{\infty} \lambda_{j}
      \sprod{y, x_{j}} x_{j} = \sum_{\lambda_{j} \not= \lambda}
      \lambda_{j} \sprod{y, x_{j}} x_{j}$, da $y \perp x_{j}$ für
      $x_{j}\in N(A-\lambda I)$
      
      $\Longrightarrow$ $(A-\lambda I) x =y$
    \end{faelle}
  \end{proof}
\end{satz}

\begin{bemerk}
  Integraloperatoren: $\mathcal{K}f(x) := \int_{\Omega} k(x,y) f(y)\,dy$
  ($x\in\Omega\subset\R^{n}$ beschränkt und offen)
  
  $k\in L_{2}(\Omega\times \Omega)=H, k(x,y) = \overline{k(x,y)}$ \fue
  $\Rightarrow$ $\mathcal{K} H\rightarrow H$ kompakt,
  $\mathcal{K}=\mathcal{K}^{\ast}$
\end{bemerk}

\begin{bemerk}
  $A\in \mathcal{L}(H)$ nicht kompakt, aber selbstadjungiert
  ($A=A^{\ast}$) $\Rightarrow$ $S_{A}\subset [-\|A\|, \|A\|]$
\end{bemerk}

\begin{bsp}
  $Af(x) := xf(x)$ mit $x\in [0,1], f\in L_{2}([0,1]) = H$
  \begin{itemize}
   \item $A\in \mathcal{L}(H), \|A\| \leq 1, A=A^{\ast}, \lambda \not\in
    [0,1] \Longrightarrow \exists (A-\lambda I)^{-1}\in \mathcal{L}(H)
    \Rightarrow \lambda \in M_{A}$
   \item Da $\lambda\in [0,1]$, ist $\lambda \in S_{A}$ und
    kein Eigenwert von $A$. Denn aus $Af = \lambda f$ folgt $xf(x) = \lambda
    f(x)\ \fue$ und somit ist
    \begin{equation*}
      xf(x) - \lambda f(x) = Af(x) - \lambda f(x) = (\lambda -\lambda)
      f(x) = 0\ \fue
    \end{equation*}
    
    
    $S_{A} = [0,1]$
  \end{itemize}
\end{bsp}

\subsection{Normaldarstellung kompakter Operatoren}

\begin{description}
 \item[Ziel:] $A\in\mathcal{L}(H)$ kompakt $\Longrightarrow$ $Ax =
  \sum_{k=1}^{\infty} s_{k} \sprod{x, e_{k}} f_{k}$ wobei
  $(s_{k})_{k}$ eine Nullfolge ist und $(e_{k})_{k}$ und $(f_{k})_{k}$
  sind eine ONS.
\end{description}

\begin{defini}
  Sei $A\in\mathcal{L}(H)$ selbstadjungiert. $A$ heißt genau dann
  \highl{positiv}, wenn für alle $x\in H$ gilt: $\sprod{A x,x} \geq 0$
\end{defini}

\begin{bemerk}
  Nach \thref{satz:2.2.1/4} gilt: Ist $H$ ein komplexer Hilbertraum, dann
  ist $A=A^{\ast} \Longleftrightarrow \sprod{Ax,x}$ reell für alle $x\in H$
\end{bemerk}

\begin{satz}
  $A\in \mathcal{L}(H)$ selbstadjungiert, kompakt und positiv. Dann
  existiert genau ein selbstadjungierter, kompakter und positiver
  Operator $B$ mit der Eigenschaft: $B^{2} = A$ ("`$B=\sqrt{A}$"').
  
  \begin{proof}
    $A$ positiv $\Longrightarrow$ Eigenwerte sind nicht negativ:
    $\lambda$ EW $\Rightarrow$ $\lambda x =Ax$ (mit $\|x\|=1$)
    $\Rightarrow$ $\lambda = \lambda\sprod{x,x} = \sprod{Ax,x} \geq 0$
    
    -Satz-2.3.3/1- $\Rightarrow$ $Ax = \sum_{k=1}^{\infty} \lambda_{k}
    \sprod{x, x_{k}} x_{k}$ mit $\lambda_{k}\rightarrow0,
    \lambda_{k+1}\leq\lambda_{k}$
    
    Definieren: $Bx := \sum_{k=1}^{\infty} \sqrt{\lambda_{k}}
    \sprod{x,x_{k}} x_{k}$ ($\exists$ für jedes $x\in H$, da
    $\sqrt{\lambda_{k}} \leq c$)
    
    $\Rightarrow$ $B$ ist selbstadjungiert und positiv (einsetzten),
    $B^{2}=A$
    
    $B$ kompakt:
    \begin{equation*}
      \|Bx - \underbrace{\sum_{k=1}^{n} \sqrt{\lambda_{k}}
      \sprod{x, x_{k}} x_{k}}_{=: B_{n}x}\|^{2} =
      \|\sum_{k=n+1}^{\infty}\sqrt{\lambda} \sprod{x, x_{k}}
      x_{k}\|^{2} = \sum_{k=n+1}^{\infty} \lambda_{k}
      |\sprod{x,x_{k}}|^{2} \leq \lambda_{n} \|x\|^{2}
    \end{equation*}
    $\Longrightarrow$ $\|B-B_{n}\|_{L(H)} \leq \sqrt{\lambda_{n}}
    \xrightarrow{n\rightarrow\infty} 0$
    
    Annahme: $C^{2} = A$, $C$ positiv und kompakt, $C=C^{\ast}$
    \begin{equation*}
      \Rightarrow Cx = \sum_{k=1}^{\infty} \nu_{k} \sprod{x,y_{k}} y_{k}
    \end{equation*}
    mit $\nu_{k}\geq 0, \nu_{k+1}\leq \nu_{k}, \nu_{k}\rightarrow0$, ONS
    $(\nu_{k})$, $Cy_{k} = \nu_{k} y_{k}\quad\forall k$
    
    \obda $\nu_{k+1} <\nu <\nu_{1}$
    
    $\Rightarrow$ $C^{2}x = C\big(C(x)\big) = \sum_{k=1}^{\infty}\nu_{k}
    \sprod{Cx, y_{k}} y_{k} =^{C=C^{\ast}} \sum_{k=1}^{\infty}
    \nu_{k}^{2} \sprod{x, y_{k}} y_{k}$
    
    $C^{2}=A \Rightarrow$ $Ax = \sum_{k=1}^{\infty} \nu_{k}^{2} \sprod{x,
    y_{k}} y_{k}$\\
    $\Rightarrow$ $Ay_{l} = \nu_{l}^{2} y_{l} \Rightarrow \nu_{l}^{2} =
    \lambda_{l}$ und $\lambda_{l} = x_{l}$\\
    $\Rightarrow$ $Cx = Bx\quad\forall x \Rightarrow C=B$
  \end{proof}
\end{satz}

\begin{defini}
  $T\in\mathcal{L}(H_{1}, H_{2})$ kompakt $\Rightarrow$
  $T^{\ast}T\in\mathcal{L}(H_{1})$ komapt, selbstadjungiert und positiv.
  
  Setzen: $|T| = (T^{\ast}T)^{\frac12}$
  
  (Vergleiche: $z\in\C \Rightarrow |z| = (z\overline{z})^{\frac12}$)
\end{defini}

\begin{satz}[Polarzerlegung]\label{satz:4}
  $T\in \mathcal{L}(H_{1}, H_{2})$ komapkt. Dann existiert ein
  $U\in\mathcal{L}(H_{1},H_{2})$ mit $T= U |T|$ und
  \begin{equation*}
    \sprod{Ux,Uy} = \begin{cases}
		      \sprod{x,y}&:x,y\in N(T)^{\perp}\\
		      0&: x,y\in N(T)
		    \end{cases}
  \end{equation*}
  
  \begin{proof}
    $|T|$ ist selbstadjungiert und kompakt. Dann bedeutet das:
    \begin{equation}\label{eq:441}
      \sprod{|T|x, |T|y} = \sprod{x, T^{\ast}T y} = \sprod{Tx, Ty}
    \end{equation}
    Dann ist der Nullräume von $T$ und $|T|$ gleich: $N(|T|) = N(T)$.
    
    Wir können also den Raum $H_{1}$ zerlegen in $H_{1} =
    \underline{\overline{R(|T|)}}_{=N(T)^{\perp}}\oplus N(T)$
    
    Wir definieren $U$ zunächst auf $R(|T|)$ durch: $U(|T|x) := Tx$.
    Daraus folgt dann: $U$ ist ein unitärer Operator und bildet ab von
    $R(|T|) \rightarrow R(T)$. eindeutige Fortsetzung auf
    $\overline{R(|T|)} = N(T)^{\perp}$ ist unitär
    $\overline{R(|T|)}\rightarrow \overline{R(T)}$
    
    setzen $U=0$ auf $N(T)$, $x= x_{1}+x_{2}, x_{1}\in N(T)^{\perp},
    x_{2}\in N(T), Ux := Ux_{1}$ $\Longrightarrow$
    $U\in\mathcal{L}(H_{1},H_{2})$ mit $Tx = T(x_{1}+x_{2}) = Tx_{1} =
    U|T|x_{1} + U|T|x_{2} = U|T|x$
  \end{proof}
\end{satz}

\begin{satz}\label{satz:5}
  $T\in\mathcal{L}(H_{1}, H_{2})$ kompakt. Dann existieren ONS
  $(e_{k})_{k}\subset H_{1}$ und $(f_{k})_{k}\subset H_{2}$, sowie Zahlen
  $(s_{k})_{k}$ mit $s_{1}\geq s_{2} \geq \ldots \geq s_{k} \geq
  s_{k+1}\geq \ldots \xrightarrow{k\rightarrow\infty} 0$, so daß $\forall
  x\in H_{1}: Tx = \sum_{k=1}^{\infty} s_{k}\sprod{x, e_{k}} f_{k}$
  
  Die Zahlen $s^{2}_{k}$ sind die entsprechend der Größe und Vielfachheit
  angeordneten Eigenwerte von $T^{\ast}T$. Die Zahlen heißen
  \highl{singuläre Zahlen} (\highl{Singulärwert}) von $T$.
  
  \begin{proof}
    Nach \thref{satz:4} ist $T=U|T|$.
    
    Aus den Betrachtungen in Abschnitt\,\ref{sec:2.2.3} folgt
    \begin{equation*}
      |T|x = \sum_{k=1}^{\infty} s_{k} \sprod{x, e_{k}} e_{k}
    \end{equation*}
    mit $|T|e_{k} = s_{k} e_{k}$ ($\Rightarrow$ $e_{k}\in R(|T|)$)
    $s_{k}\geq s_{k+1} \rightarrow 0$
    
    $\Rightarrow$ $T=U|T| = \sum_{k=1}^{\infty} s_{k}\sprod{x,e_{k}}
    \underbrace{Ue_{k}}_{=: f_{k}\in H_{2}}$ mit $\sprod{f_{k}, f_{l}} =
    \sprod{Ue_{k}, Ue_{l}} = \sprod{e_{k},e_{l}}$
    
    $\Rightarrow$ $(f_{k})$ ONS in $H_{2}$
  \end{proof}
\end{satz}

\begin{bemerk}
  Es gilt: $s_{1} = \|T\|$, denn: $s_{1}^{2}=\|T^{\ast}T\|$ (nach
  Abschnitt\,\ref{sec:2.3.3}) $=\|T\|^{2}$ (nach Abschnitt\,\ref{sec:2.2.2})
\end{bemerk}

\begin{folger}
  $T\in \mathcal{L}(H_{1}, H_{2})$ kompakt. Dann:
  \begin{equation*}
    \begin{CD}
      H_{1} @>T>> H_{2}\\
      @VUVV @AAVA\\
      l_{2} @>S>> l_{2}
    \end{CD}
  \end{equation*}

  $T = VSU$ mit $U\in \mathcal{L}(H_{1}, l_{2})$ mit $\|U\|=1$,
  $V\in\mathcal{L}(l_{2}, H_{2})$ mit $\|V\|=1$
  \begin{equation*}
    S = \begin{pmatrix}s_{1}&&0\\&s_{2}\\0&&\ddots\end{pmatrix}\in
    \mathcal{L}(l_{2})
  \end{equation*}
  mit $\|S\|=\|T\|$, $S(a_{k}) := (s_{k}a_{k})_{k}$
  \highl{"`Diagonaloperator"'}

  \begin{proof} nach \thref{satz:5}
    $Tx = \sum s_{k}\sprod{x,e_{k}} f_{k}$
    Ux = $(\sprod{x, e_{k}})_{k}$ $\|Ux\|\leq \|x\|$ (Bessel)
  
    $V(b_{k}) := \sum_{k=1}^{\infty} b_{k}f_{k}$
    
    $\|V(b_{k})\| = \|b_{k}\|_{l_{2}}$

    $\Rightarrow$ $T=VSU$, $\|T\| \leq \|VSU\| \leq \|V\| \|S\| \|U\| =
    \|S\|$ andererseits: $\|S\| \leq \sup_{k} s_{k} = s_{1} = \|T\|$
  \end{proof}
\end{folger}

