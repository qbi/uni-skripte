\subsection{Maße}

(Überlick in H.\,Triebel: Analysis und math. Physik, Kap.\,13+14)

\begin{defini}
  $\Omega \ne \emptyset$ Menge. Ein System von Teilmengen $\mathcal{A}$
  von $\Omega$ heißt
  \highl[sigma-Algebra@$\sigma$-Algebra]{$\sigma$-Algebra}
  $:\Longleftrightarrow$
  \begin{enumerate}
   \item $\emptyset\in \mathcal{A}, \Omega\in \mathcal{A}$
   \item $A\in \mathcal{A} \Rightarrow \Omega\setminus A\in \mathcal{A}$
   \item $A_{k}\in \mathcal{A}, k\in\N \Rightarrow \bigcup_{k=1}^{\infty}
    A_{k}\in \mathcal{A}$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $A_{k}\in \mathcal{A} \Rightarrow \bigcap_{k=1}^{\infty}
    A_{k}\in \mathcal{A}$
   \item Potenzmenge von $\Omega$ ist $\sigma$-Algebra
  \end{enumerate}
\end{bemerk}

\begin{defini}
  $\mathcal{A}$ sei $\sigma$-Algebra über $\Omega$. Eine Abbildung $\mu \colon
  \mathcal{A} \rightarrow [0,\infty]$ heißt \highl{Maß} auf $\mathcal{A}$
  $:\Longleftrightarrow$
  \begin{enumerate}
   \item $\mu(A)\geq 0, \forall A\in \mathcal{A}$
   \item $\mu(\emptyset) = 0$
   \item $\mu(\bigcup_{k=1}^{\infty} A_{k}) =
    \sum_{k=1}^{\infty}\mu(A_{k})$ falls $A_{k}\in \mathcal{A}$ und
    $A_{k}\cap A_{j} = \emptyset (k\ne j)$
  \end{enumerate}

  $A\in \mathcal{A}$ heißt \highl[Menge!messbar]{messbare Menge}.
  $(\Omega, \mathcal{A}, \mu)$ \highl{Maßraum}
\end{defini}

\begin{defini}
  $(\Omega, \mathcal{A}, \mu)$ Maßraum
  \begin{enumerate}
   \item
    $\mu$ heißt \highl[sigma-endlich@$\sigma$-endlich]{$\sigma$-endlich},
    wenn das System $\{\Omega_{k}\}_{k}\subset \mathcal{A}$ mit
    $\mu(\Omega_{k}) <\infty$
    \begin{gather*}
      \Omega = \bigcup_{k=1}^{\infty} \Omega_{k}
    \end{gather*}
    existiert
   \item
    \highl[mu-endlich@$\mu$-endlich]{$\mu$-endlich}, falls
    $\mu(\Omega)<\infty$
   \item $\mu$ heißt \highl{vollständig} $:\Longleftrightarrow$ $\forall
    N\in \mathcal{A}\colon \mu(N)=0, \forall A\subset N \Longrightarrow A\in
    \mathcal{A}\wedge \mu(A)=0$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item Wenn $\mu(A) = \infty$ zugelassen. Dann für $r\geq0, r +\infty =
    \infty, \infty+\infty = \infty$
   \item $A,B\in \mathcal{A}, A\subset B \Longrightarrow \mu(A) \leq
    \mu(B)$ Monotonie
   \item Beispiel: $\Omega=\N, \mathcal{A}=\mathcal{P}(\N), A\subset \N$
    \begin{gather*}
      \mu(A) := \card(A) (=\text{Anzahl der Elemente})
    \end{gather*}
    $\Longrightarrow$ $\sigma$-endlich vollständiges Maß
  \end{enumerate}
\end{bemerk}

\begin{defini}
  Eine Menge $B\subset \R^{n}$ heißt Borelmenge, wenn $B$ durch abzählbar
  viele Mengenoperationen (Vereinigung, Durchschnitt, Komplement) aus
  offenen Mengen entstanden ist.
\end{defini}

\begin{lemma}
  \begin{enumerate}
   \item Die Familie der Borelmengen $\mathcal{B}$ ist die kleinste
    $\sigma$-Algebra, die alle offenen Teilmengen des $\R^{n}$ enthält.
   \item Eine offene Menge im $\R^{n}$ lässt sich als abzählbare
    Vereinigung offener Rechtecke (Quader) $Q =
    (a_{1},b_{1})\times\ldots\times(a_{n},b_{n}), -\infty < a_{j} < b_{j}
    < \infty (j=1,\ldots,n)$ darstellen.
  \end{enumerate}
\end{lemma}

\begin{satz}
  $\mathcal{B}$ $\sigma$-Algebra der Borelmengen auf $\R^{n}$. Es
  existiert genau ein $\sigma$-endliches Maß $\lambda_{0}$ auf
  $\mathcal{B}$, so dass für jeden offenen Quader
  $Q=(a_{1},b_{1})\times\ldots\times(a_{n},b_{n})$ gilt:
  \begin{gather*}
    \lambda_{0}(Q) = \prod_{j=1}^{n}(b_{j}-a_{j})
  \end{gather*}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item
    $B\in \mathcal{B}$.

    \begin{gather*}
      \lambda_{0}(B) = \inf \{ \sum_{k=1}^{\infty} \lambda_{0}(Q_{k})\colon
         Q_{k}\text{ offene Quader }, B\subset \bigcup_{k=1}^{\infty} Q_{k}\}
    \end{gather*}
   \item $\lambda_{0}$ ist nicht vollständig, aber jedes Maß besitzt eine
    \highl{kleinste vollständige Erweiterung}.
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:2}
  Es gibt eine kleinste vollständige Erweiterung $(\R^{n}, \mathcal{L},
  \lambda)$ von $(\R^{n}, \mathcal{B}, \lambda_{0})$.

  \begin{enumerate}
   \item $(\R^{n}, \mathcal{L}, \lambda)$ vollständiger
    $\sigma$-endlicher Maßraum
   \item $\mathcal{B}\subset \mathcal{L}, \forall B\in \mathcal{B}\colon
    \lambda(B) = \lambda_{0}(B)$
   \item Für alle Maßräume $(\R^{n}, \mathcal{A}, \mu)$ mit den
    Eigenschaften (1) und (2) gilt: $\mathcal{L}\subset \mathcal{A}$ und
    $\mu\big|_{\mathcal{L}} = \lambda$
  \end{enumerate}
\end{satz}

\begin{defini}
  $(\R^{n}, \mathcal{L}, \lambda)$ aus \autoref{satz:2} heißt
  \highl[Maßraum!Lebesgue]{Lebesgue'scher Maßraum}, $\lambda$
  \highl[Maß!Lebesgue]{Lebesgue'sches Maß}.

  $A$ \highl[messbar!Lebesgue]{Lebesgue-messbar} $:\Longleftrightarrow$
  $A\in \mathcal{L}$. $N\in \mathcal{L}$
  \highl[Nullmenge!Lebesgue]{Lebesgue-Nullmenge} $:\Longleftrightarrow$
  $\lambda(N) = 0$

  \textbf{Bezeichnung}: $A\in \mathcal{L}$ $\abs{A} := \lambda(A)$
\end{defini}

\begin{satz}\label{satz:1.3.1/3}
  $(\R^{n}, \mathcal{L}, \lambda)$ Lebesgue'scher Maßraum
  \begin{enumerate}
   \item $N$ \highl{Lebesgue'sche Nullmenge} $:\Longleftrightarrow$ $\forall
    \epsilon>0 \exists (Q_{k})_{k}\text{ offener Quader im $\R^{n}$ mit }
    N\subset \bigcup_{k=1}^{\infty} Q_{k} \wedge \sum_{k=1}^{\infty} \abs{Q_{k}}<\epsilon$.
   \item Jede abzählbare Menge im $\R^{n}$ ist eine Lebesgue'sche Nullmenge.
   \item $\lambda$ ist invariant bzgl. Translation und Rotation, d.\,h.
    \begin{align*}
      x\in\R^{n}, A\in \mathcal{L} &\Longrightarrow \lambda(x+A) =
         \lambda(A)\\
      A\in \mathcal{L}, T\in O(n) &\Longrightarrow \lambda\{y = Tx \colon x\in
         A\} = \lambda(A)
    \end{align*}
   \item $\forall A\in \mathcal{L}\exists\Omega \subset \R^{n}\text{
    offen}: A\subset \Omega \wedge \lambda(\Omega\setminus A) < \epsilon$
  \end{enumerate}
\end{satz}

\subsection{Messbare Fuktionen}
\begin{defini}\label{def:trepfkt}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum.

  \begin{enumerate}
   \item $A\in \mathcal{A}, S\colon A \rightarrow \K$ heißt
    \highl{einfache Funktion} oder \highl{Treppenfunktion} $:\Longleftrightarrow$
    \begin{gather*}
      S(x) = \sum_{j=1}^{r} c_{j} \chi_{A\cap A_{j}}\qquad r\in \N,
         c_{j}\in \K, A_{j}\in \mathcal{A}, A\subset \bigcup_{j=1}^{r} A_{j}\\
      \chi_{A\cap A_{j}} =
         \begin{cases}1& x\in A\cap A_{j}\\0&\text{sonst}\end{cases}
         \qquad(\highl{charakteristische Funktion})
    \end{gather*}
   \item Eine Eigenschaft $E(x)$ gilt auf einer Menge $A \subset \Omega$
    \highl{fast überall}\index{f.\,ü.|see{fast überall}} ($\mu$-fast
    überall, $\mu$-f.\,ü.) $:\Longleftrightarrow$ eine Nullmenge $N$
    ($\mu(N) =0$) existiert und $E(x)$ auf $A\setminus N$ gilt.
  \end{enumerate}

  \todo{Hier könnte noch ein Bild von einer Treppenfunktion hin. Mit
    gnuplot eine Funktion = A in die xy-Ebene (=$\Omega$) legen. Darüber
    einfach ein paar Seulen mit Zufallswerten.}
\end{defini}

\begin{defini}
  $f\colon A\rightarrow\K, A\in \mathcal{A}$ heißt \highl{messbar}
  $:\Longleftrightarrow$ eine Folge von Treppenfunktionen
  $(S_{k})_{k}$ existiert und $S_{k}(x) \rightarrow f(x)$
  $\mu$-f.\,ü. auf $A$.
\end{defini}

\begin{satz}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum,
  $A\in \mathcal{A}$ und $f\colon A \rightarrow\K$.

  Dann sind die folgenden Aussagen äquivalent:
  \begin{enumerate}
   \item $f$ messbar\index{messbar}
   \item Die Urbilder $f^{-1}$ offener Mengen ($O\subset \K$) sind messbar
    ($f^{-1}(O) \in \mathcal{A}$).
   \item Die Urbilder von Borelmengen\index{Borelmengen} über $\K$ sind
    messbar.
  \end{enumerate}
\end{satz}

\begin{satz}
  \begin{enumerate}
   \item $f,g\colon A\rightarrow\K$ messbar $\Longrightarrow$ sind auch $f+g,
    f\cdot g, \frac{f}{g}$ und $\abs{f}^{p}\ (p\in \R)$ messbar
   \item $f_{k}\colon A\rightarrow\R$ messbar und $f_{k}\rightarrow f$
    $\mu$-f.\,ü. auf $A$ $\Longrightarrow$ ist auch $f$ messbar
   \item $f,g\colon A\rightarrow \R$ messbar $\Longrightarrow$ sind auch $f^{+} =
    \max\{0,f(x)\}, f^{-} = \max\{0,-f(x)\},\\\max\{f,g\}$ und
    $\min\{f,g\}$ messbar
   \item $f_{k}\colon A\rightarrow\R$ messbar $\Longrightarrow$ sind auch
    $\sup_{k} f_{k}(x), \inf_{k} f_{k}(x), \limsup\limits_{k\rightarrow\infty} f_{k}$
    und $\liminf_{k\rightarrow\infty} f$ messbar
  \end{enumerate}
\end{satz}

\begin{satz}[Egorov]
  $A\in \mathcal{A}, \mu(A) < \infty$ und $f_{k}\colon A\rightarrow\K$
  messbar für alle $k\in \N$.

  Dann sind folgende Aussagen äquivalent:
  \begin{enumerate}
   \item $f_{k} \rightarrow f$ $\mu$-f.\,ü. auf $A$
   \item $\forall \epsilon>0\ \exists A_{\epsilon}\in \mathcal{A}:
    \mu(A\setminus A_{\epsilon})< \epsilon$ und $f_{k}$ konvergiert
    gleichmäßig gegen $f$ auf $A_{\epsilon}$ ($\sup_{x\in A_{\epsilon}}
    \abs{f_{k}(x)-f(x)} \xrightarrow{k\rightarrow\infty} 0$)
  \end{enumerate}
\end{satz}

\begin{satz}[Lebesguemaß]
  $(\R^{n}, \mathcal{L}, \lambda)$ \help{L oder A? $\rightarrow$ 2./3.
  korregieren}

  \begin{enumerate}
   \item Ist $f\colon \R^{n} \rightarrow \K$ f.\,ü. stetig, dann ist $f$
    \highl{messbar}
   \item Falls der Rand von $\partial A$ eine Nullmenge
    ($\mu(\partial A)=0$) ist, ist $A\in\mathcal{A}$. \help{haut das hin?}
   \item $f\colon \mathcal{A}\rightarrow\K$ ist messbar, falls
    \begin{gather*}
      \tilde{f} = \begin{cases}
                    f&\text{auf }A\\
                    0&\text{sonst}
                  \end{cases}
    \end{gather*}
    f.\,ü. stetig auf $\R^{n}$ ist.
  \end{enumerate}
\end{satz}

\begin{bemerk}[Lusin]
  $\Omega\subset\R^{n}$ offen, $\mu(\Omega)<\infty$,
  $f\colon\Omega\rightarrow\K$ messbar, $f=0$ auf $\R^{n}\setminus\Omega$.

  Dann existiert für alle $\epsilon>0$ $\phi\in C(\Omega)$ \help{das kann
  ich nicht lesen} mit $\norm{\phi}_{\infty} \leq \norm{f}_{\infty}$ und
  $\mu\bigl(\{x\in \Omega\colon f(x)\ne\phi(x)\}\bigr) < \epsilon$
\end{bemerk}

\subsection{Das Lebesgue-Integral}

\begin{defini}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum

  \begin{gather*}
    S(x) = \sum_{j=1}^{r} c_{j} \chi_{A\cap A_{j}}\\
    \int_{A} S(x)\,d\mu = \sum_{j=1}^{r} c_{j} \mu(A\cap A_{j})
  \end{gather*}
  Die Funktion $S(x)$ heißt \highl{Treppenfunktion}
  (vgl.~\autoref{def:trepfkt}). Integral ist unabhängig von der Auswahl der
  $c_{j}$ und $A_{j}$.
\end{defini}

\begin{defini}
  $(\Omega, \mathcal{A}, \mu)$ sei ein vollständiger $\sigma$-endlicher
  Maßraum und $A\subset \mathcal{A}$

  $f\colon A\rightarrow\K$ heißt \highl{integrierbar} $:\Longleftrightarrow$
  \begin{enumerate}
   \item $f$ ist messbar, d.\,h. es gibt eine Folge $(S_{k})_{k}$ von
    Treppenfunktionen mit $S_{k}\rightarrow f$ $\mu$-f.\,ü. auf $A$ und
   \item für alle $\epsilon>0$ ein $n_{0}(\epsilon)$ existiert, so dass
    für alle $n,m\geq n_{0}(\epsilon)$ gilt:
    \begin{gather*}
      \int_{A}\abs{S_{n}(x)-S_{m}(x)}\,d\mu < \epsilon
    \end{gather*}
  \end{enumerate}
\end{defini}

Wir setzen
\begin{gather*}
  \int_{A} f\,d\mu := \lim_{n\rightarrow\infty} \int_{A} S_{n}\,d\mu
\end{gather*}
unabhängig von der Auswahl der Folge $(S_{k})_{k}$)

\begin{bemerk}\label{bem:2}
  \begin{enumerate}
   \item Ist $f=g$ f.\,ü. und $f$ integrierbar, dann ist auch $g$
    integrierbar und
    \begin{gather*}
      \int_{A} g\,d\mu = \int_{A} f\,d\mu
    \end{gather*}
   \item \begin{gather*}\int_{A}d\mu = \mu(A)\end{gather*}
   \item Sei $f\colon\Omega \rightarrow\K$ messbar. Dann ist
    \begin{gather*}
      \int_{A} f\,d\mu = \int_{\Omega} f\chi_{A}\,d\mu
    \end{gather*}
    falls dieses existiert.
  \end{enumerate}
\end{bemerk}

\begin{bemerk}[Äquivalenter Zugang]
  \begin{enumerate}
   \item Ist $f\colon A\rightarrow[0,\infty)$ eine nichtnegative Funktion,
    dann ist
    \begin{gather*}
      \int_{A} f\,d\mu := \sup_{0\leq s\leq f} \int_{A} s\,d\mu
    \end{gather*}
    falls das Supremum existiert.
   \item Bilde jetzt $f\colon A\rightarrow\R$ ab. $f$ lässt sich in einen
    positive Anteil $f^{+} := \max\{0,f(x)\}$ und einen negativen Anteil
    $f^{-}=\max\{0,-f(x)\}$ zerlegen. $f = f^{+} - f^{-}$. Womit man das
    Integral auf die folgende Weise zusammensetzen kann:
    \begin{gather*}
      \int_{A} f\,d\mu = \int_{A} f^{+}\,d\mu - \int_{A} f^{-}\,d\mu
    \end{gather*}
   \item $f$ sein nun eine komplexwertige Funktion ($f\colon A\rightarrow\C$).
    Dann lässt sich $f$ in $f=\Re f+i\Im f$ zerlegen. Das Integral analog
    in:
    \begin{gather*}
      \int_{A} f\,d\mu = \int_{A}\Re f\,d\mu + i\int_{A}\Im f\,d\mu
    \end{gather*}
  \end{enumerate}
\end{bemerk}

\begin{bemerk}
  Sei nun der Lebesgue'sche Maßraum $(\R^{n}, \mathcal{L}, \lambda)$
  gegeben.
  \begin{enumerate}
   \item Für $A\subset\mathcal{L}$ vereinbaren wir folgenden Schreibweise:
    \begin{gather*}
      \int_{A} f\,d\lambda = \int_{A} f(x)\,dx
    \end{gather*}
   \item Ist $f$ die Treppenfunktion
    \begin{gather*}
      f = \begin{cases}
            1& x\in [0,1]\cap\Q\\
            0& x\in [0,1]\setminus\Q
          \end{cases}
    \end{gather*}
    dann existiert das Integral von $f$ auf $[0,1]$ und ist 0.
    \begin{gather*}
      \int_{0}^{1}f(x)\,dx = 0
    \end{gather*}\help{Tolle Sache, aber warum ist das so?}
   \item Ist eine Menge $\Omega\subset\R^{n}$ Jordan-messbar, so ist sie
    auch Lebesgue-messbar.
    \begin{alignat*}{5}
      \exists \int_{\Omega} f\,dx &&\qquad\Longrightarrow\qquad &&
         \exists \int_{\Omega} f\,d\lambda  &&\quad =\quad &&
         \int_{\Omega} f\,dx\\
      \text{Riemann}&&&& \text{Lebesgue}&&&& \text{Riemann}
    \end{alignat*}
   \item Absolut konvergente Riemann-Integrale (uneigentliche) sind
    ebenfalls identisch mit dem Lebesgue-Integral.
   \item Eine Funktion $f\colon\Omega\rightarrow\K$ ist genau dann
    Riemann-Integrierbar, wenn $f$ f.\,ü. stetig im \emph{Lebesgue'schen Sinn}
  \end{enumerate}
\end{bemerk}

