  ($\Longrightarrow$ $A_{k}$ kompakt nach \thref{satz:122}) und
  $\|A_{k}-A\| \xrightarrow{k\rightarrow\infty} 0$ (\help{irgendwie
  spielt \thref{satz:123} hier eine Rolle}) $\Longrightarrow$ $A$
  ist kompakt.
\end{folger}

\begin{bsp}
  $(a_{jk})\in l_{2}(\N\times \N), x= (x_{k})_{k}\in l_{2}(\N)$
  \begin{equation*}
    (Ax)_{j} := \sum_{k=1}^{\infty} a_{jk} x_{k}\qquad(j\in\N)
  \end{equation*}
  $\Longrightarrow$ $A: l_{2} \rightarrow l_{2}$ beschränkt nach
  \thref{bsp:1.4.1/2}
  
  $A$ ist kompakt da:
  \begin{equation*}
    (A_{n}x)_{j} = \begin{cases}(Ax)_{j}&j=1,\ldots,n\\0&j>n\end{cases}
  \end{equation*}
  $\Longrightarrow$ $\dim R(A_{n}) \leq n$ $A_{n}$ kompakt
  
  \begin{align*}
    \|A_{n}x -Ax\|_{2} = (\sum_{j=n+1}^{\infty}\Bigl|\sum_{k=1}^{\infty}
       a_{jk x_{k}}\Bigr|^{2})^{\frac12}\\
    &\leq \underbrace{\Bigl(\sum_{j=n+1}^{\infty}\sum_{k=1}^{\infty}
       |a_{jk}|^{2}\Bigr)^{\frac12}}_{\xrightarrow{n\rightarrow\infty}0}
       \|x\|_{2}\\
    \Longrightarrow& \|A_{n}-A\| \leq \Bigl(\sum_{j=n+1}^{\infty}\sum_{k=1}^{\infty}
       |a_{jk}|^{2}\Bigr) \rightarrow 0
  \end{align*}
