     S'(\R^{n}\times\R^{n}) \taglabel{eq:40}
\end{align*}

Wir klären das doppelte Fragezeichen:
\begin{align*}
  F(f\ast g) &= (2\pi)^{-\frac{n}{2}} (Fg)(\xi) Ff&& f\in S'(\R^{n}),
     g\in E'(\R^{n})\\
  F(f\ast g)(\phi) &= (f\ast g)(F\phi) = (f\otimes g)(\eta(y)(F\phi)(x+y))\\
  &= (2\pi)^{-\frac{n}{2}} (f\oplus g)(\eta(y) \int_{\R^{n}} \phi(\xi)
     e^{-i(x+y)\xi}\,d\xi)\\
  &= (2\pi)^{-\frac{n}{2}} f_{x} ( g_{y}(\int_{\R^{n}} \eta(y)\phi(\xi)
     e^{-i(x+y)\xi}\,d\xi\\
  \taglabel{eq:41} &= (2\pi)^{-\frac{n}{2}} f_{x} ( \int_{\R^{n}}
     \underbrace{g_{y}(\eta(y)e^{-iy\xi})}_{(2\pi)^{-\frac{n}{2}}
     (Fg)(\xi)}\phi(\xi) e^{-ix\xi}\,d\xi\\
  &= (2\pi)^{-\frac{n}{2}} f_{x} ( \underbrace{(2\pi)^{-\frac{n}{2}}
     \int_{\R^{n}} (Fg)(\xi) \phi(\xi) e^{-ix\xi}\,d\xi}_{F((Fg)(\cdot)
     \phi(\cdot))} ) = (2\pi)^{-\frac{n}{2}} f_{x} (F(Fg\cdot\phi)) =
     (2\pi)^{-\frac{n}{2}} (Ff)(Fg\cdot \phi)\\
  &= (2\pi)^{-\frac{n}{2}} \bigl((Fg)(\xi)Ff\bigr)(\phi)
\end{align*}

\begin{satz}[Wärmeleitungsgleichung] % 3
  \begin{gather*}
    \frac{\partial u}{\partial t} - \Delta u = f(x,t)
  \end{gather*}

  \begin{gather*}
    G(x,t) =
       \begin{cases}
         \frac{1}{(4\pi t)^{\frac{n}{2}}} e^{-\frac{\abs{x}^{2}}{4t}}&
         x\in\R^{n}, t>0\\
         0& x\in \R^{n}, t\leq 0
       \end{cases}
  \end{gather*}
  ist Fundamentallösung für die \highl{Wärmeleitungsgleichung}
