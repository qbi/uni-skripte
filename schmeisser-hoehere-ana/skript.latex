\section{Folgenräume und Funktionenräume}
\subsection{Die Räume \texorpdfstring{$l_{p}$}{lp}}

\begin{defini}
  $0<p<\infty$

  \begin{gather*}
    l_{p}=l_{p}(\N) := \{ (a_{k})_{k} \colon
       \sum_{k=1}^{\infty}\abs{a_{k}}^{p}<\infty \}\\
    \norm{a}_{p} = \norm{a_{k}}_{p} :=
       (\sum_{k=1}^{\infty}\abs{a_{k}}^{p})^{\frac1p}\\
    l_{\infty} = l_{\infty}(\N) := \{ (a_{k})_{k}\colon \sup_{k}
       \abs{a_{k}}<\infty \}\\
    \norm{a}_{\infty} = \norm{a_{k}}_{\infty} := \sup_{k}\abs{a_{k}}
  \end{gather*}
\end{defini}

\begin{lemma}[Young'sche Ungleichung]\label{lem:1.2.1/1}
  Für zwei Zahlen $x$ und $y$ mit $0<x,y \in \R$ und die Zahlen $p$ und
  $p'$ mit $1 < p < \infty, \frac1p + \frac1{p'} = 1$ gilt folgende
  Ungleichung:
  \begin{gather*}
    xy \leq \frac{x^{p}}p + \frac{y^{p'}}{p'}
  \end{gather*}

  \begin{proof}
    Der Beweis erfolgt mithilfe der Logarithmusfunktion und der
    Eigenschaft konkaver Funktionen: $t\,f(x)+(t-1)f(y) \leq
    f(tx+(1-t)y)$ mit $t\in(0,1)$
    \begin{gather*}
      \ln(xy) = \ln(x^{\nicefrac{p}{p}}) + \ln(y^{\nicefrac{p'}{p'}}) =
         \frac{1}{p}\ln x^{p} + \frac{1}{p'} \ln y^{p'} \leq
         \ln \Big(\frac{1}{p}x^{p} + \frac{1}{p'}y^{p'}\Big)
    \end{gather*}
    Wendet man auf beide Seiten der Gleichung die Exponentialfunktion an,
    so erhält man die gewünschte Gleichung.
  \end{proof}
\end{lemma}

\begin{lemma}[Hölder'sche Ungleichung]\label{lem:hoelder-sum}
  Gilt für $p$: $1 \leq p < \infty$ und $\frac1p + \frac1{p'} = 1$ ($p' =
  \infty$ falls $p=1$)

  Dann ist
  \begin{gather}\label{eq:hoelder-sum}
    \abs[\Big]{\sum_{k=1}^{n}a_{k} b_{k}}
    \leq \Bigl(\sum_{k=1}^{n} \abs{a_{k}}^{p}\Bigr)^{\frac1p}
    \cdot \Bigl(\sum_{k=1}^{n}\abs{b_{k}}^{p'}\Bigr)^{\frac1{p'}}
  \end{gather}

  \begin{proof}
    o.\,B.\,d.\,A. sei
    \begin{gather*}
      \Bigl(\sum_{k=1}^{n} \abs{a_{k}}^{p}\Bigr)^{\frac1p} =
         \Bigl(\sum_{k=1}^{n} \abs{b_{k}}^{p'}\Bigr)^{\frac1{p'}} = 1\\
      \intertext{Aus der Young'schen Ungleichung (\autoref{lem:1.2.1/1})
        folgt:}
      \abs[\Big]{\sum_{k=1}^{n} a_{k}b_{k}} \leq
         \sum_{k=1}^{n}\abs{a_{k}} \abs{b_{k}} \leq \frac1p
               \underbrace{\sum_{k=1}^{n} \abs{a_{k}}^{p}}_{=1} + \frac1{p'}
               \underbrace{\sum_{k=1}^{n}\abs{b_{k}}^{p'}}_{=1}=\frac1p + \frac1{p'} = 1
    \end{gather*}
  \end{proof}
\end{lemma}

\begin{lemma}[Minkowski'sche Ungleichung]\label{lem:1.2.1/3}
  Es sei $1 \leq p < \infty$

  \begin{gather*}
    \Big(\sum_{k=1}^{n}\abs{a_{k}+b_{k}}^{p}\Big)^{\frac1p} \leq
       \biggl(\sum_{k=1}^{n}\abs{a_{k}}^{p}\biggr)^{\frac1p}+(\sum_{k=1}^{n}
       \abs{b_{k}}^{p})^{\frac1p}
  \end{gather*}

  \begin{proof}
    $1<p<\infty, \frac1p + \frac1{p'} \Leftrightarrow \frac1{p'} =
    \frac{p-1}{p}$

    \begin{gather*}
      \Longrightarrow \sum_{k=1}^{n}\abs{a_{k}+b_{k}}^{p} \leq
         \sum_{k=1}^{n}\abs{a_{k}+b_{k}}^{p-1}\abs{a_{k}} +
         \sum_{k=1}^{n}\abs{a_{k}+b_{k}}^{p-1} \abs{b_{k}}\\
      \text{\autoref{lem:hoelder-sum}} \Longrightarrow \leq
         (\sum_{k=1}^{n}\abs{a_{k}+b_{k}}^{p})^{\frac{p-1}p}
         ((\sum_{k=1}^{n}\abs{a_{k}}^{p})^{\frac1p} +
         (\sum_{k=1}^{n}\abs{b_{k}}^{p})^{\frac1p})\\
      \Longrightarrow (\sum_{k=1}^{n}\abs{a_{k} + b_{k}}^{p})^{\frac1p} \leq
         (\sum_{k=1}^{n}\abs{a_{k}}^{p})^{\frac1p} +
         (\sum_{k=1}^{n}\abs{b_{k}}^{p})^{\frac1p}
    \end{gather*}
  \end{proof}
\end{lemma}

\begin{satz}\label{satz:1.2.1/1}
  \begin{enumerate}
   \item $1\leq p \leq \infty$

    Dann gilt: $(l_{p}, \norm{\cdot}_{p})$ ist ein Banachraum

    Für $1\leq p < \infty$ ist $l_{p}$ separabel. $l_{\infty}$ ist nicht
    separabel.
   \item Die Räume $C_{0} := \{ (a_{k})_{k} \colon \lim_{k\rightarrow\infty}
    a_{k} = 0 \}$ und $c := \{ (a_{k})_{k} \colon \exists
    \lim_{k\rightarrow\infty} a_{k}\}$ sind separabele und abgeschlossene
    Teilräume von $l_{\infty}$
   \item $1\leq p < \infty$

    $A \subset l_{p}$ präkompakt $\Longleftrightarrow$ $A$ beschränkt
    und
    \begin{gather*}
      \sup_{a\in A}\sum_{k=n}^{\infty}
         \abs{a_{k}}^{p}\xrightarrow{n\rightarrow\infty} 0
    \end{gather*}
  \end{enumerate}

  \begin{proof}
    $\norm{a_{k}+b_{k}}_{p} \leq \norm{a_{k}}_{p} + \norm{b_{k}}_{p}$ folgt aus
    \autoref{lem:1.2.1/3}

    Vollständigkeit folgt aus \autoref{satz:1.1.1/2} für $p=\infty$ und analog zu
    Beispiel 1.1.1/4 für $1\leq p <\infty$

    $l_{p} (1\leq p < \infty)$ separabel wie in \autoref{bsp:1.1.1/5}

    $l_{\infty}$ nicht separabel Übung Aufgabe 2, Serie 2

    Aussage für $C_{0}$ siehe \autoref{bsp:1.1.2/1}

    Aussage für $c$: $\{ \alpha = (\alpha_{k})_{k}\colon \alpha_{k}\in \Q,
    \alpha = (\alpha_{1},\alpha_{2},\ldots,\alpha_{n},\alpha_{0},
    \alpha_{0},\ldots), n \in \N \}$

    Beweise von (3) wie in \autoref{bsp:1.1.3/2}
  \end{proof}
\end{satz}

\begin{lemma}\label{lem:1.2.1/4}
  (Aufgabe 1, Blatt 2)
  \begin{enumerate}
   \item $a,b \geq 0, 1\leq p <\infty$ $\Longrightarrow$ $(a+b)^{p}\leq
    2^{p-1}(a^{p}+b^{p})$
   \item $a,b \geq 0, 0 < p \leq 1$ $\Longrightarrow$ $(a+b)^{p} \leq
    a^{p}+b^{p}\leq 2^{1-p} (a+b)^{p}$
  \end{enumerate}

  \todo{beweis einfügen}
\end{lemma}

\begin{folger}
  Für $0<p \leq 1$ und $0 \leq a_{k}$ folgt:
  \begin{gather*}
    \Bigl(\sum_{k=1}^{n} a_{k}\Bigr)^{p} \leq \sum_{k=1}^{n} a_{k}^{p}
  \end{gather*}
\end{folger}

\begin{folger}
  \begin{enumerate}
   \item $0<p<q\leq \infty \Longrightarrow l_{p} \subset l_{q}\ 
    \wedge\ \norm{a_{k}}_{q} \leq \norm{a_{k}}_{p}$\qquad (\highl{Jeusen'sche Ungleichung})
   \item $0 < p <1 \Longrightarrow \norm{a_{k}+b_{k}}_{p} \leq 2^{\frac1p-1}
    (\norm{a_{k}}_{p}+ \norm{b_{k}}_{p})$\\
    (d.\,h. $\norm{\cdot}_{p}$ ist eine \highl{Quasinorm})
    \begin{gather*}
      \norm{a_{k}+b_{k}}_{p}^{p} \leq \norm{a_{k}}_{p}^{p} + \norm{b_{k}}_{p}^{p}
    \end{gather*}
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item
       \begin{gather*}
         \Bigl(\sum_{k=1}^{n}\abs{a_{k}}^{q}\Bigr)^{\frac1q} =
         \biggl(\Bigl(\sum_{k=1}^{n}\abs{a_{k}}^{q}\Bigr)^{\frac{p}{q}}\biggr)^{\frac{1}{p}}
         \leq^{Folg 1} \Bigl(\sum_{k=1}^{n} \abs{a_{k}}^{p}\Bigr)^{\frac1p} (n\rightarrow\infty)
       \end{gather*}
     \item
      \begin{flalign*}
        \sum_{k=1}^{\infty} \abs{ a_{k}+b_{k}}^{p} &\leq^{Lemma 4}
           \sum_{k=1}^{\infty} \abs{a_{k}}^{p} + \sum_{k=1}^{\infty}
           \abs{b_{k}}^{p} \Longleftrightarrow \norm{a_{k}+b_{k}}^{p}_{p} \leq
           \norm{a_{k}}_{p}^{p} + \norm{b_{k}}_{p}^{p}\\
        \biggl(\sum \abs{a_{k}+b_{k}}^{p}\biggr)^{\frac1p}
           &\leq \biggl(\sum \abs{a_{k}}^{p} + \sum \abs{b_{k}}^{p} \biggr)^{\frac1p}\\
           &\leq^{Lemma 4 (1)} 2^{\frac1p-1} \Bigl(
           \biggl(\sum \abs{a_{k}}^{p}\biggr)^{\frac1p}
           + \biggl(\sum \abs{b_{k}}^{p}\biggr)^{\frac1p}\Bigr)
      \end{flalign*}
    \end{enumerate}
  \end{proof}
\end{folger}

\begin{folger}
  $0 < p < 1$ Dann ist $l_{p}$ ein \highl{Quasibanachraum}, d.\,h.
  $\norm{\cdot}_{p}$ ist \highl{Quasinorm} und $l_{p}$ ist vollständig bzgl.
  $\norm{\cdot}_{p}$.

  \begin{proof}
    analog zu $p \geq 1$ in \autoref{satz:1.2.1/1} bzw. zu
    \autoref{bsp:1.1.1/4}
  \end{proof}
\end{folger}

\subsection{Räume stetiger Funktionen}

\begin{defini}
  $X$ metrischer Raum, $E$ Banachraum
  \begin{gather*}
    C(X,E) := \{ f\colon X \rightarrow E\colon f \text{ stetig auf } X\}\\
    B(X,E) := \{ f\colon X \rightarrow E\colon f \text{ beschränkt auf } X\}\\
    \norm{f}_{\infty} = \norm{f}_{B(X,E)} := \sup_{x\in X}\norm{f(x)}_{E}
  \end{gather*}
\end{defini}

\begin{satz}
  $(C(X,E) \cap B(X,E), \norm{\cdot}_{\infty})$ ist abgeschlossener Teilraum
  von $B(X, E)$

  \begin{proof}
    wissen $(B(X,E), \norm{\cdot}_{\infty})$ ist Banachraum

    $C(X,E) \cap B(X,E)$ ist Teilraum, normiert. Es sei $(f_{j})_{j}
    \subset C(X,E) \cap B(X,E)$ Cauchyfolge

    $\Longrightarrow$ $\exists f\in B(X,E)\colon
    \norm{f_{j}-f}_{\infty}\xrightarrow{j\rightarrow\infty} 0$

    Damit bleibt also zu zeigen: $f\in C(X,E)$
    \begin{gather*}
      \epsilon > 0,\ x_{0}\in X,\\
      \norm{f(x_{0})-f(x)}_{E} \leq
         \underbrace{\norm{f(x_{0})-f_{j}(x_{0})}_{E}}_{<\epsilon}
         + \underbrace{\norm{f_{j}(x_{0}) - f_{j}(x)}_{E}}_{<\epsilon}+
          \underbrace{\norm{f_{j}(x) - f(x)}_{E}}_{<\epsilon}
    \end{gather*}
  \end{proof}
\end{satz}

$(X,d)$ metr. Raum, $(E, \norm{\cdot})$ Banachraum, $(B(X,E),
\norm{\cdot}_{\infty})$ Banachraum, $(C(X,E)\cap B(X,E),
\norm{\cdot}_{\infty})$ Banachraum

\begin{defini}
  $U\subset B(X,E) := \{ f\colon X\rightarrow E\colon f\text{ gleichmäßig stetig
  und beschränkt}\}$
\end{defini}

\begin{satz}
  $(U\subset B(X,E), \norm{\cdot}_{\infty})$ abgeschlossener Teilraum von
  $B(X,E)$

  \begin{proof}
    wie bei Satz 1
  \end{proof}
\end{satz}

\begin{bemerk}
  $(X,d)$ kompakter metr. Raum $\Rightarrow$ $U \subset B(X,E) =
  C(X,E) \cap B(X,E) = C(X,E)$ von Interesse $E=\R,\C,
  X=\Omega\subset\R^{n}$ mit $\Omega$ offen und beschränkt

  $\Longrightarrow$ $\overline{\Omega}$ kompakt $\Longrightarrow$
  $C(\overline{\Omega}, E) = U\subset B(\overline{\Omega}, E)$

  \begin{align*}
    f\in U\subset B(\overline{\Omega}, E) &\Longrightarrow f\big|_{\Omega}\in
       U\subset B(\Omega, E)\\
    g \in U\subset B(\Omega, E) &\Longrightarrow \exists! f\in U\subset
       B(\overline{\Omega})\colon f\big|_{\Omega}=g
  \end{align*}
  $C$ stetige Fortsetzung auf $\overline{\Omega}$ (Ü.A.)

  In diesem Sinne: $ U\subset B(\Omega, E) = U\subset B(\overline{\Omega}, E)$
\end{bemerk}

\begin{defini}
  \begin{enumerate}
   \item $\Omega \subset\R^{n}$ offen, beschränkt, $E=\R,\C$
    \begin{gather*}
      C(\Omega) := C(\Omega, E)\\
      C(\overline{\Omega}) := U\subset B(\overline{\Omega}, E)
    \end{gather*}
   \item $\Omega \subset \R^{n}$ offen (nicht notwendig beschränkt)
    \begin{gather*}
      \overline{C}(\Omega) := U\subset B(\Omega, E)
    \end{gather*}

  \end{enumerate}
  Also: $(\overline{C}(\Omega), \norm{\cdot}_{\infty})$ Banachräume
\end{defini}

\begin{defini}
  $(X,d)$ kompakt, metr. Raum $E=\R,\C$

  $A\subset C(X,E)$ heißt \highl{gleichgradig} (gleichmäßig) stetig
  $:\Longleftrightarrow$
  \begin{gather*}
    \forall \epsilon>0 \exists\delta>0 \forall x,x'\in X\colon
       d(x,x')<\delta \colon \sup_{f\in A}\abs{f(x)-f(x')}<\epsilon
  \end{gather*}
\end{defini}

\begin{satz}[Arzela-Ascoli]\label{satz:arzela}
  $(X,d)$ kompakter metrischer Raum, $E=\R,\C$.

  Dann gilt: $A\subset C(X,E)$ präkompakt $\Longleftrightarrow$ $A$ ist
  beschränkt und gleichgradig stetig.

  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] $A$ präkompakt $\Longrightarrow$ $A$ ist
      beschränkt und $\forall \epsilon >0$ existiert ein endliches
      $\epsilon$-Netz (nach~\autoref{satz:1.1.3/2})

      Es sei $\epsilon>0$ und es sei $\{f_{1},\ldots,f_{m}\}\subset
      C(X,E)$ endliches $\epsilon$-Netz für $A$. Weiterhin sei $f\in A$.

      Dann existiert ein $j\in \{1,\ldots,m\}$ mit
      \begin{gather*}
        \norm{f_{j}-f}_{\infty}=\sup_{x\in X}\abs{f_{j}(x) - f(x)}<\epsilon
      \end{gather*}
      und $f_{1},\ldots,f_{m}$ sind gleichm. stetig $\Longrightarrow$
      $\exists\delta\ \forall x,x'\in X$ mit $d(x, x')<\delta\colon
      \sup_{j}\abs{f_{j}(x)- f(x')}<\epsilon$

      $\Longrightarrow$ Für $f\in A$ und $d(x,x')<\delta$ gilt:
      \begin{align*}
        \abs{f(x)-f(x')} &\leq \abs{f(x) -f_{j}(x)}
           +\abs{f_{j}(x)-f_{j}(x')}+\abs{f_{j}(x')-f(x')}\\
        &\leq 2 \underbrace{\norm{f_{j}-f}_{\infty}}_{<\epsilon} +
           \underbrace{\norm{f_{j}(x)-f_{j}(x')}_{\infty}}_{<\epsilon} < 3\epsilon
      \end{align*}
      $\Longrightarrow$ $\sup_{f\in A} \abs{f(x)-f(x')}\leq 3\epsilon$
      $\Longrightarrow$ A gleichm. stetig
     \item["`$\Leftarrow$"'] $(X,d)$ kompakt nach \autoref{fol:1.1.3/1}. Es
      existiert in $X$ eine abzählbare und dichte Teilmenge $M
      =\{x_{1},x_{2},x_{3},\ldots\}\subset X$.

      Es sei $(f_{k})_{k}\in A$ Folge. $A$ beschränkt $\Longrightarrow$
      Jeder Folge $(f_{k}(x_{j}))_{k}$ ist beschränkt in $E$.

      \begin{enumerate}
       \item Wählen Teilfolge $(f_{k}^{1})_{k}\subset (f_{k})_{k}$ mit
        $f_{k}^{1}(x_{1}) \rightarrow y_{1}\in E$
       \item Wähle Teilfolge $(f_{k}^{2})_{k}\in (f_{k}^{1})_{k}$ mit
        $f_{k}^{2}(x_{2}) \rightarrow y_{2}\in E$

        \ldots
       \item Wählen Teilfolge $(f_{k}^{l})_{k}\subset (f_{k}^{l-1})_{k}$
        mit $f_{k}^{l}(x_{l}) \rightarrow y_{l}\in E$
      \end{enumerate}

      Betrachten die Diagonalfolge
      \begin{gather*}
        (g_{k})_{k} := (f_{k}^{k})_{k}\in A
      \end{gather*}

      Nach Konstruktion ist für jedes $x_{l}\in M$ die Folge
      $(g_{k}(x_{l}))_{k}$ konvergent.

      \underline{Wir zeigen:} $(g_{k})$ ist konvergent in $C(X,E)$.

      Es sei $\epsilon>0$. Aus der gleichgradigen Stetigkeit von $A$ folgt:
      \begin{gather}\label{eq:7}
        \exists\delta>0\forall x,x'\colon d(x,x') < \delta \forall j\in \N\colon
           \abs{g_{j}(x)-g_{j}(x')}<\epsilon
      \end{gather}
      $M$ dicht in $X$ $\Longrightarrow$ $X=\bigcup_{l=1}^{\infty}
      K_{\delta}(x_{l})$\\
      $X$ kompakt $\Longrightarrow$ $\exists n$ und
      $X=\bigcup_{l=1}^{n}K_{\delta}(x_{l})$.\\
      Aus der Konvergenz $g_{j}(x^{l})\rightarrow y_{l}$ folgt:
      \begin{gather}\label{eq:8}
        \exists j(\epsilon)\forall j,k\geq j(\epsilon) \forall
           l=1,\ldots,n\colon \abs{g_{j}(x^{l})-g_{n}(x^{l})}<\epsilon
      \end{gather}

      Es sei nun $x\in X$. $\exists l\in \{1,\ldots,n\} \wedge x\in
      K_{\delta}(x_{l})$. Dann gilt für $j,k \geq j(\epsilon)$:
      \begin{gather*}
        \abs{g_{j}(x)-g_{k}(x)}\leq
           \underbrace{\abs{g_{j}(x) - g_{j}(x_{l})}}_{<\epsilon
             \text{nach \autoref{eq:7}}}
           +\underbrace{\abs{g_{j}(x_{l})-g_{k}(x_{l})}}_{<\epsilon
           \text{nach \autoref{eq:8}}}
           +\underbrace{\abs{g_{k}(x_{l})- g_{k}(x)}}_{<\epsilon
           \text{nach \autoref{eq:7}}} < 3\epsilon
      \end{gather*}

      $\Longrightarrow$ $\norm{g_{j}-g_{k}}_{\infty}\leq 3\epsilon$
      $\Longrightarrow$ $(g_{k})$ Cauchyfolge in $C(X,E)$
      $\Longrightarrow$ konvergent, da Banachraum
    \end{description}
  \end{proof}
\end{satz}

\begin{defini}
  $(X,d)$ kompakter metrischer Raum.
  \begin{enumerate}
   \item $A\subset C(X,\K)$ heißt \highl{Unteralgebra}
    $:\Longleftrightarrow$ $A$ ist Teilraum und $\forall f,g\in A$ ist
    $f\cdot g \in A$
   \item $A\subset C(X,\K)$ heißt \highl{punktetrennend}
    $:\Longleftrightarrow$\\ \makebox[1cm]{} $\forall x,y\in X$ mit $x\ne y\ \exists f\in
    A\colon f(x) \ne f(y)$
  \end{enumerate}
\end{defini}

\begin{lemma}\label{lemma:1}
  Es existieren Polynome $(p_{n}(t))_{n}$, so dass
  \begin{gather*}
    \sup_{\abs{t}\leq1} \abs[\big]{p_{n}(t)-\abs{t}} \xrightarrow{n\rightarrow\infty} 0
  \end{gather*}

  \begin{proof}
    setzen $p_{0}(t) := 0, p_{n+1}(t) = p_{n}(t) + \frac12
    (t^{2}-(p_{n}(t))^{2}), n\in \N_{0}$

    Es gilt:
    \begin{gather*}
      \abs{t} - p_{n+1}(t) = (\abs{t}-p_{n}(t)) ( 1- \frac{\abs{t}+p_{n}(t)}{2}
         )\label{eq:11}\\
      \intertext{Außerdem:} 0\leq p_{n}(t) \leq \abs{t} \text{(durch
         Induktion)}\taglabel{eq:12}
    \end{gather*}
    \begin{align*}
      p_{n}(t) \leq \abs{t} &\Longrightarrow \abs{t}+p_{n}(t) \leq 2\abs{t}\\
      &\Longrightarrow 1-\frac{\abs{t}+p_{n}(t)}{2} \geq 1-\frac{2\abs{t}}{2} =
         1-\abs{t} \geq 0\\
      \text{nach \autoref{eq:11}}&\Longrightarrow \abs{t}-p_{n+1}(t) \geq 0
    \end{align*}

    und:
    \begin{align*}
      (2+(n+1)\abs{t})(1-\frac{\abs{t}+p_{n}(t)}{2}) &=
         2+n\abs{t}+\abs{t}-(\abs{t}-\underbrace{p_{n}(t)}_{\geq0})-
         \frac{(n+1)\abs{t}}{2}(\abs{t}+p_{n}(t))\\
      &\leq 2 + n\abs{t}\\
      \Longrightarrow& (1-\frac{\abs{t}+p_{n}(t)}{2}) \leq
         \frac{2+n\abs{t}}{2+(n+1)\abs{t}}\label{eq:13}
    \end{align*}

    Aus \autoref{eq:11} folgt mit \autoref{eq:13} durch Induktion:
    \begin{align}\label{eq:14}
     \begin{split}
        \abs{t}-p_{n+1}(t) &\leq (\abs{t}-p_{n}(t)) \frac{2+n\abs{t}}{2+(n+1)\abs{t}}\\
        & \leq (\abs{t}-p_{n-1}(t))\frac{2+(n+1)\abs{t}}{2+n\abs{t}}
        \frac{2+n\abs{t}}{2+(n+1)\abs{t}}\\
        &\vdots\\
        &\leq (\abs{t}-p_{0}(t)) \frac{2}{2+(n+1)\abs{t}} = \frac{2\abs{t}}{2+(n+1)\abs{t}}
     \end{split}
    \end{align}

    \begin{gather*}
      \abs{t} \leq 1 \text{ und \autoref{eq:14}}  \Longrightarrow 0 \leq \abs{t}-p_{n}(t)
         \leq \frac{2\abs{t}}{2+n\abs{t}} \leq \frac{2\abs{t}}{2\abs{t}+n\abs{t}} =
         \frac{2}{2+n} \xrightarrow{n\rightarrow\infty} 0
    \end{gather*}
  \end{proof}
\end{lemma}

\begin{lemma}\label{lemma:2}
  Sei $A$ Unteralgebra von $C(X,\R)$.

  Dann folgt aus $f\in A$ $\Longrightarrow$ $\abs{f}\in \overline{A}$
  (\highl{Abschluss} von $A$).

  \begin{proof}
    setzen $t:=\frac{f(x)}{\norm{f}_{\infty}} (x\in X) \Longrightarrow
    \abs{t}\leq 1$

    \autoref{lemma:1} $\Longrightarrow$ $\exists p_{n}(t)$ mit
    $\abs[\big]{p_{n}(t)-\abs{t}}\leq \frac{\epsilon}{\norm{f}_{\infty}} (\forall t)$

    $\Longrightarrow \forall x\colon
    \abs{\frac{f(x)}{\norm{f}_{\infty}}-p_{n}(\frac{f(x)}{\norm{f}_{\infty}})}\leq
    \frac{\epsilon}{\norm{f}_{\infty}}$

    $\Longrightarrow \abs{f(x)-\underbrace{\norm{f}_{\infty}
    p_{n}(\frac{f(x)}{\norm{f}_{\infty}})}_{\in A}}\leq \epsilon$
  \end{proof}
\end{lemma}

\begin{satz}[Stone-Weierstraß]\label{satz:weierstr}
  $(X,d)$ kompakter metr. Raum. Es sei $A\subset C(X,\R)$ eine
  punktetrennende Unteralgebra, die die konstanten Funktionen enthält.

  Dann ist $A$ dicht in $C(X,\R)$

  \begin{proof}
    \begin{schritte}
     \item $f,g\in A$. Aus \autoref{lemma:2} folgt:
      \begin{align*}
        \max(f(x), g(x)) &= \frac12(f(x)+g(x) + \abs{f(x)-g(x)})\in \overline{A}\\
        \min(f(x), g(x)) &= \frac12(f(x)+g(x) - \abs{f(x)-g(x)})\in \overline{A}
      \end{align*}
     \item $x,y\in X, y\ne x, \alpha,\beta\in \R \Longrightarrow f\in A\colon
      f(x)=\alpha\wedge f(y) =\beta$

      $A$ punktetrennend $\Longrightarrow$ $\exists g\in A\colon g(x)\ne g(y)$

      setzen $f(t) := \alpha +
      \frac{\beta-\alpha}{g(y)-g(x)}(g(t)-g(x))\in A$, da $1\in A$
      $\Longrightarrow$ $f(x)=\alpha\wedge f(y)=\beta$
     \item (lokale Approximation) $\epsilon>0, f\in C(X,\R), x\in X
      \Longrightarrow \exists g_{x}(t)\in \overline{A}\colon g_{x}(x)=f(x)
      \wedge g_{x}(t) \leq f(t)+\epsilon \alpha t$

      Es sei $z\in X$ (beliebig) $\Longrightarrow$ $\exists h_{z}\in A\colon
      h_{z}(x) =f(x) \wedge h_{z}(z) = f(z)$ Nach 2.\,Schritt

      $h_{z}, f$ stetig $\Longrightarrow$ $\abs{h_{z}(y) -f(y)}\leq \abs{h_{z}(y)
      - \underbrace{f(z)}_{=h_{z}(z)}} + \abs{f(z) - f(y)} \leq \epsilon$
      für alle $y$ mit $d(y,z) \leq \delta_{z}$

      $\Longrightarrow$ $h_{z}(y) \leq f(y) + \epsilon\qquad \forall y\colon
      d(y,z) \leq \delta_{z}$

      Da $X$ kompakt ist, folgt aus dem Satz von Heine-Borel
      (\autoref{satz:heineborel})
      \begin{gather*}
        X = \bigcup_{i=1}^{n} K_{\delta_{z_{i}}}(z_{i})
      \end{gather*}

      Wir definieren jetzt die Funktion $g_{x}(t)$ als
      \begin{gather*}
        g_{x}(t) := \min\{h_{z_{1}}(t),\ldots,h_{z_{n}}(t)\}
      \end{gather*}. Nach Schritt 1 gilt dann: $g_{x}(t) \in \overline{A}$

      $\Longrightarrow$ $g_{x}(t) = \min_{i}\{h_{z_{i}}(x) \} f(x)$ (nach
      Konstruktion) $t\in X$ $\Rightarrow$ $\exists z_{i} \wedge
      d(t,z_{i})<\delta_{z_{i}}$

      $\Longrightarrow$ $g_{x}(t) \leq h_{z_{i}}(t) \leq f(t) + \epsilon$
     \item $\epsilon > 0, f\in C(X,\R)$

      $\Longrightarrow$ $\exists \psi\in A\colon f(t)-\epsilon \leq \psi(t)
      \leq f(t)+\epsilon\quad \forall t\in X$

      Wählen $g_{x}(t)$ aus Schritt 3

      $\Longrightarrow$ $\abs{g_{x}(t)-f(t)}\leq \abs{g_{x}(t) - g_{x}(x)} +
      \abs{\underbrace{g_{x}(x)}_{=f(x)} -f(t)} \leq \epsilon\quad \forall t\colon
      d(t, x) \leq \gamma_{x}$ (wegen der Stetigkeit von $f$ und $g_{x}$)

      $\Rightarrow$ $g_{x}(t) \geq f(t)-\epsilon$ für $d(t,x) \leq
      \gamma_{x}$

      $X$ kompakt, Heine Borel $\Longrightarrow$ $X = \bigcup_{i=1}^{m}
      K_{\gamma_{x_{i}}}(x_{i})$ setzen $\phi(t) :=
      \max\{g_{x_{1}}(t),\ldots,g_{x_{m}}(t)\}\in \overline{A}$ nach
      Schritt 1

      $t\in X$ $\Longrightarrow$ $\exists x_{i}\colon d(t,x_{i})
      <\gamma_{x_{i}}$ und $f(t)-\epsilon \leq g_{x_{i}}(t) \leq \phi(t)
      \leq f(t) + \epsilon\quad \forall t\in X$  $\Rightarrow$
      $\norm{f-\phi}_{\infty} \leq \epsilon$

      $\phi\in \overline{A} \Longrightarrow \exists \psi\in A\colon
      \norm{\phi-\psi}_{\infty} \leq \epsilon \Longrightarrow
      \norm{f-\psi}_{\infty} \leq 2\epsilon$
    \end{schritte}
  \end{proof}
\end{satz}

\begin{folger}
  $\Omega\in \R^{n}$ offen und beschränkt.

  Dann folgt daraus: Die Menge aller Polynome $\mathcal{P}=\{
  p(x)=\sum_{\alpha=(\alpha_{1},\ldots,\alpha_{n})\text{endl.}}
  a_{\alpha}x^{\alpha} : a_{\alpha}\in \R\}$ ist dicht in $C(\overline{\Omega})$.

  Weiterhin gilt: $C(\overline{\Omega})$ ist separabel.

  \begin{proof}
    Man sieht leicht, dass $\mathcal{P}$ eine Unteralgebra ist. $1\in
    \mathcal{P}$ (für $\alpha=0$). $x^{0},y^{0}\in \overline{\Omega}$,
    $x^{0}\ne y^{0}$ $\Longrightarrow$ $\exists j \wedge x^{0}_{j}
    \ne y_{j}^{0}$ $\Longrightarrow$ Polynom $p(x) = x_{j}$ trennt
    $x^{0}$ und $y^{0}$.
  \end{proof}
\end{folger}

\begin{folger}
  Die Menge aller trigonometrischen Polynome $\mathcal{T} := \{ t(x) :=
  a_{0}+\sum_{k=1}^{n} a_{k}\cos x+b_{k}\sin x \colon a_{k},b_{k}\in\R,
  n\in\N\}$ ist dicht in $C_{\text{per}}([-\pi,\pi]) = \{ f\colon [-\pi, \pi]
  \rightarrow \R \colon f \text{stetig}, f(-\pi) = f(\pi)\} \cong C_{per}(\R)
  = \{ f \text{stetig}, \forall x\in \R\colon f(x) = f(x+2\pi) \} \cong
  C(\Pi), \Pi=\frac{\R}{2\pi\Z} = \{\overline{x}\colon \overline{x} = x+2\pi\Z\}$

  $d(\overline{x}, \overline{y}) = \inf\{\abs{x-y+2k\pi}\colon k\in\Z\}$
  $\Longrightarrow$ $(\Pi, d)$ kompakter metrischer Raum

  $f(x) = \overline{f}(\overline{x})$
  \begin{proof}
    Man sieht leicht, dass $\mathcal{T}$ eine Unteralgebra ist und die
    konstantan Funktionen enthält.

    $x_{0}\in (0,\pi), y_{0} \in(-\pi,0)$ $\Longrightarrow$ $\sin x_{0}
    \ne \sin y_{0}$\\
    $x_{0},y_{0}\in [0,\pi] \vee [-\pi,0], x_{0}\ne y_{0}$
    $\Longrightarrow$ $\cos x_{0} \ne \cos y_{0}$
  \end{proof}
\end{folger}

\begin{satz}[komplexwertige Funktionen]
  $(X,d)$ komakter metrischer Raum, $A\subset C(X,\C)$ mit:
  \begin{enumerate}
   \item $A$ Unteralgebra
   \item $1\in A$
   \item $f\in A \Longrightarrow \overline{f}\in A$
   \item $A$ punktetrennend
  \end{enumerate}

  Dann liegt $A$ dicht in $C(X,\C)$.

  \begin{proof}
    $h\in A \Longrightarrow \overline{h}\in A \Longrightarrow \Re h, \Im
    h\in A$

    Setze $A^{\ast} := \{ \Re h\colon h\in A\} \cup \{ \Im h\colon h\in A\} \subset
    C(X,\R)$ $\Longrightarrow$ $A^{\ast}$ Unteralgebra, punktetrennend,
    $1\in A^{\ast}$

    $f\in C(X,\C) \Longrightarrow^{Satz 4}$
    \begin{align*}
      \exists (\phi_{j})\subset A^{\ast}\colon \phi_{i}\rightarrow \Re f\\
      \exists (\psi_{j})\subset A^{\ast}\colon \psi_{i}\rightarrow \Im f
    \end{align*}
    $\Longrightarrow \underbrace{\phi_{i} +i\psi_{j}}_{\in A (wegen satz (3))}
    \rightarrow f$ in $C(X,\C)$
  \end{proof}
\end{satz}

\begin{bsp}
  $S:= \{ z\in \C \colon \abs{z}=1\}, C(S,X), A:= \{ \sum_{k=-n}^{n} c_{k}z^{k}\colon
  c_{k}\in \C, n\in \N_{0}\}\subset C(S,\C)$

  $A$ ist eine Unteralgebra, $1\in A$, $z\in S \Longrightarrow \frac1z\in
  S$, punktetrennend

  $\Longrightarrow$ $A$ dicht in $C(S,\C)$

  Folgerung; für $e^{it}$ $\{ \sum_{k=-n}^{n} c_{k}e^{ikt}\colon c_{k}\in \C,
  n\in \N_{0}\}$ dicht in $C(\Pi, \C)$
\end{bsp}

\begin{bemerk}
  \begin{enumerate}
   \item \highl{Bernstein-Polynome}: $f\in C([0,1])$

    $n\in \N, B_{n} f(x):= \sum_{k=0}^{n} f(\frac{k}{n}) \binom{n}{k}
    x^{n-k} (1-x)^{k} \Longrightarrow \norm{B_{n}f-f}_{\infty}
    \xrightarrow{n\rightarrow\infty} 0$
   \item \highl{Fejér-Mittel}: $f\in C_{per}(\R)$

    $c_{k} := \frac1{2\pi}\int_{-\pi}^{\pi}f(x) e^{-ikx}\,dx$
    Fourierkoeffizient, $k\in\Z$

    $s_{n}f(x) := \sum_{k=-n}^{n} c_{k}e^{ikx}\qquad n\in\N_{0}$
    Partialsummen

    $\sigma_{n} f(x) :=\frac{s_{0}f(x)+\ldots + s_{n}f(x)}{n+1}$
    Fejér-Mittel

    $\Longrightarrow$ $\norm{f-\sigma_{n}f}_{\infty}
    \xrightarrow{n\rightarrow\infty} 0$
  \end{enumerate}
\end{bemerk}

\subsection{Räume differenzierbarer Funktionen}
\label{sec:1.2.3}

\begin{defini}
  $\Omega\subset \R^{n}$ offen, $m\in \N_{0}, \K=\R$ oder $\C$

  \begin{enumerate}
   \item $C^{m}(\Omega) := \{ f\colon \Omega \rightarrow \K \colon \forall \alpha,
    \abs{\alpha} \leq m \exists D^{\alpha}f \in C(\Omega)\}$
   \item $\overline{C}^{m}(\Omega) := \{ f\in C^{m}(\Omega) \colon \forall
    \alpha, \abs{\alpha} \leq m, D^{\alpha}f\in \overline{C}(\Omega)\}$
    gleichm. stetig + beschränkt

    $\norm{f}_{\overline{C}^{m}} := \sum_{\abs{\alpha}\leq m} \norm{D^{m}f}_{\infty}$
   \item $C^{\infty}(\Omega) := \{ f\colon \Omega \rightarrow \K\colon \forall m
    \in \N_{0} f\in C^{m}(\Omega) \}$
   \item $\Omega$ beschränkt, $C^{m}(\overline{\Omega}) := \{ f\in
    C^{m}(\Omega)\colon \forall \alpha, \abs{\alpha}\leq m, \exists \text{stetige
    Fortsetzung von } D^{\alpha}f \text{auf} \overline{\Omega}\}$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  $\Omega$ offen + beschränkt $\Rightarrow$ $\overline{C}^{m}(\Omega) =
  C^{m}(\overline{\Omega})$ setzen $\overline{C}^{0}(\Omega) =
  \overline{C}(\Omega)$
\end{bemerk}

\begin{satz}
  $\Omega\subset \R^{n}$ offen, $m\in \N_{0}$

  Dann ist $\overline{C}^{m}(\Omega)$ bezüglich der Norm
  $\norm{\cdot}_{\overline{C}^{m}}$ ein Banachraum.

  \begin{proof}
    $\norm{\cdot}_{\overline{C}^{m}}$ ist normiert. Wissen
    $\overline{C}(\Omega)$ ist ein Banachraum.

    Es sei $(f_{k})_{k}$ Cauchyfolge in $\overline{C}^{m}(\Omega)$.
    Daraus folgt $\forall \alpha, \abs{\alpha}\leq m \exists f_{\alpha}\in
    \overline{C}(\Omega)\colon
    \norm{D^{\alpha}f_{k}-f_{\alpha}} \xrightarrow{k\rightarrow\infty} 0$

    zeigen: $f_{\alpha} = D^{\alpha} f, \forall \alpha, \abs{\alpha}\leq m$

    Es sei $\alpha = (1,0,\ldots,0) \Longrightarrow \frac{\partial
    f_{k}}{\partial x_{1}} \rightarrow f_{\alpha}$ gleichm. auf $\Omega$

    $\Longrightarrow$ $\int_{x_{1}^{0}}^{n} \frac{\partial f_{k}}{\partial
    x_{1}}(t, x_{2},\ldots,x_{n})\,dt \rightarrow
    \int_{x_{1}^{0}}^{x_{n}} f_{\alpha}(t,x_{2},\ldots,x_{n})\,dt$
    andererseits:

    $(f_{k}(x_{1},\ldots,x_{n})- f_{k}(x_{1}^{0},x_{2},\ldots,x_{n}))
    \rightarrow f_{0}(x_{1},\ldots,x_{n})-
    f_{0}(x_{1}^{0},x_{2},\ldots,x_{n})$

    $\Longrightarrow$ $\exists \frac{\partial f_{0}}{\partial x_{1}}(x) =
    f_{\alpha}(x)\qquad (\alpha = (1,0,\ldots,0))$

    Induktion $\Longrightarrow$ $\exists D^{\alpha}f = f_{\alpha},
    \forall \alpha, \abs{\alpha}\leq m$
  \end{proof}
\end{satz}

\begin{defini}
  \begin{enumerate}
   \item $f \colon\R^{n} \rightarrow \K (=\R/\C)$

    $\supp f:= \{x\in \R^{n}\colon f(x) \ne 0\}$ heißt
    \highl{Träger} (\highl{support}) von $f$
   \item $\Omega$ offen, $C_{0}^{\infty}(\Omega) := \{ f\in C^{\infty}(\Omega)\colon \supp
    f\subset \Omega \text{ kompakt}\}$
  \end{enumerate}
\end{defini}

\begin{satz}\label{satz:1.2.3/2}
  $\Omega\subset \R^{n}$ offen, $K\subset \Omega$ kompakt.

  Dann existiert ein $\phi \in C^{\infty}(\Omega)$ mit:
  \begin{enumerate}
   \item $0\leq \phi(x) \leq 1$
   \item $\phi(x) = 1 \forall x\in K$
   \item $\supp \phi \subset \Omega$ kompakt
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item $\omega(t) := \begin{cases}e^{-\frac{1}{t}}& t>0\\0& t \leq 0\end{cases}$
     \item $\psi(t) := \omega(1+t)\omega(1-t) = \begin{cases}
                                                  e^{-\frac{1}{1-t^{2}}}&\abs{t}<1\\
                                                  0& \abs{t}\geq 1\end{cases}
      \in C_{0}^{\infty}(\R)$

      $\supp \psi = [-1,1], \psi(t) \geq 0$

      $x\in \R^{n}; \psi(x) = \psi(x_{1})\cdot \psi(x_{2})\cdots
      \psi(x_{n}) \in C_{0}^{\infty}(\R^{n})$ $\Longrightarrow$ $\supp
      \psi = [-1,1]^{n}$
     \item $\forall c> 0 \exists f_{c}\in C^{\infty}(\R)$ mit
      \begin{gather*}
        f_{c}(t) = \begin{cases}0& t\in (-\infty,0]\\1& [c,\infty)\end{cases}
      \end{gather*}
      $0 \leq f_{c}(t) \leq 1$

      Es sei $\psi_{c}(t) := \psi(\frac{t-\frac{c}2}{\frac{c}2})$
      $\Longrightarrow$ $\supp \psi_{c} = [0,c]$

      setzen: $f_{c}(t) := \frac{\int_{0}^{t}
      \psi_{c}(t)\,dt}{\int_{0}^{c}\psi_{c}(t)\,dt}$ leistet das
      gewünschte
     \item $\exists g\in C_{0}^{\infty}(\R^{n})$ mit $g(x) > 0$ auf $K$
      und $\supp g\subset \Omega$ kompakt

      Für ein hinreichend kleines $\epsilon_{0}$ und
      $0<\epsilon<\epsilon_{0}$ ist $K^{\epsilon} := \{ x\colon d(x,K) \leq
      \epsilon \} \subset \Omega, K^{\epsilon}$ kompakt

      Wir betrachten offene Würfel $Q_{\epsilon}(x^{0}) = \{ y\in \R^{n}
      \colon \abs{y_{i}-x_{j}^{0}}< \epsilon, j=1,\ldots,n \}$ zentriert um $x_{0}
      \in K$. Wenn wir den Durchmesser dieser Würfel hinreichend klein
      machen, liegen alle diese Würfel in $K^{\epsilon}$. Kantenlänge $2\epsilon$

      Für $\epsilon <\epsilon_{1} < \epsilon_{0}$ ist
      \begin{gather*}
        K\subset \bigcup_{x\in K} Q_{\epsilon}(x) \subset
           K^{\epsilon}\subset \Omega
      \end{gather*}

      Satz nach Heine-Borel $\Longrightarrow$ $\exists
      \{x_{1},\ldots,x_{N}\}\colon K \subset \bigcup_{i=1}^{N}
      Q_{\epsilon}(x_{j}) \subset K^{\epsilon} \subset \Omega$

      Es sei $\psi(x_{1},\ldots,x_{n})$ wie im 2.\,Schritt und definieren
      \begin{gather*}
        g(x) := \sum_{i=1}^{N} \psi(\frac{x-x_{i}}{\epsilon}) \in
           C_{0}^{\infty}(\R^{n})
      \end{gather*}
      und $\supp g = \overline{\bigcup_{j} Q_{\epsilon}(x_{j})}\subset
      K^{\epsilon}, \forall x\in K\colon g(x) > 0$.
     \item Es sei $g(x)$ wie oben.

      $\Rightarrow$ $\exists c:=\min_{x\in K} g(x) > 0$, da $k$ kompakt
      und $g(x) > 0$.

      Es sei $f_{c}$ wie im 2.\,Schritt. Setzen $\phi(x) := f_{c}(g(x))
      \in C^{\infty}(\R^{n})\quad\forall x\in \R^{n}$

      Es ist $\phi(x) = \begin{cases}1 & g(x) \geq c\\0& g(x) \leq 0\end{cases}$

      $x\in \R^{n}\setminus K^{\epsilon}$ $\Longrightarrow$ $g(x) = 0
      \Longrightarrow \phi(x) = 0 \Longrightarrow \supp \phi \subset
      K^{\epsilon} \subset \Omega$
    \end{schritte}
  \end{proof}
\end{satz}

