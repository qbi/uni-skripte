\section{Folgenräume und Funktionenräume}
\subsection{Die Räume \texorpdfstring{$l_{p}$}{lp}}

\begin{defini}
  $0<p<\infty$
  
  \begin{gather*}
    l_{p}=l_{p}(\N) := \{ (a_{k})_{k} :
       \sum_{k=1}^{\infty}|a_{k}|^{p}<\infty \}\\
    \|a\|_{p} = \|a_{k}\|_{p} :=
       (\sum_{k=1}^{\infty}|a_{k}|^{p})^{\frac1p}\\
    l_{\infty} = l_{\infty}(\N) := \{ (a_{k})_{k}: \sup_{k}
       |a_{k}|<\infty \}\\
    \|a\|_{\infty} = \|a_{k}\|_{\infty} := \sup_{k}|a_{k}|
  \end{gather*}
\end{defini}

\begin{lemma}[Young'sche Ungleichung]\label{lem:1.2.1/1}
  (Aufgabe 2, 1. Serie)
  
  $0<x,y \in R, 1<p < \infty, \frac1p + \frac1{p'} = 1$
  
  \begin{equation*}
    \Longrightarrow xy \leq \frac{x^{p}}p + \frac{y^{p'}}{p'}
  \end{equation*}
\end{lemma}

\begin{lemma}[Hölder'sche Ungleichung]\label{lem:hoelder-sum}
  Gilt für $p$: $1 \leq p < \infty$ und $\frac1p + \frac1{p'} = 1$ ($p' =
  \infty$ falls $p=1$)
  
  Dann ist
  \begin{equation}\label{eq:hoelder-sum}
    \Bigl|\sum_{k=1}^{n}a_{k} b_{k}\Bigr|
    \leq \Bigl(\sum_{k=1}^{n} |a_{k}|^{p}\Bigr)^{\frac1p}
    \cdot \Bigl(\sum_{k=1}^{\infty}|b_{k}|^{p'}\Bigr)^{\frac1{p'}}
  \end{equation}
  
  \begin{proof}
    o.B.d.A. sei
    \begin{gather*}
      \Bigl(\sum_{k=1}^{n} |a_{k}|^{p}\Bigr)^{\frac1p} = 
	 \Bigl(\sum_{k=1}^{n} |b_{k}|^{p}\Bigr)^{\frac1{p'}} = 1\\
      \intertext{Aus der Young'schen Ungleichung (\thref{lem:1.2.1/1})
	folgt:}
      \Bigl|\sum_{k=1}^{n} a_{k}b_{k}\Bigr| \leq
	 \sum_{k=1}^{n}|a_{k}| |b_{k}| \leq \frac1p
      	 \underbrace{\sum_{k=1}^{n} |a_{k}|^{p}}_{=1} + \frac1{p'}
      	 \underbrace{\sum_{k=1}^{n}|b_{k}|^{p'}}_{=1}=\frac1p + \frac1{p'} = 1
    \end{gather*}
  \end{proof}
\end{lemma}

\begin{lemma}[Minkowski'sche Ungleichung]\label{lem:1.2.1/3}
  Es sei $1 \leq p < \infty$
  
  \begin{equation*}
    \Big(\sum_{k=1}^{n}|a_{k}+b_{k}|^{p}\Big)^{\frac1p} \leq
       \biggl(\sum_{k=1}^{n}|a_{k}|^{p}\biggr)^{\frac1p}+(\sum_{k=1}^{n}
       |b_{k}|^{p})^{\frac1p}
  \end{equation*}
  
  \begin{proof}
    $1<p<\infty, \frac1p + \frac1{p'} \Leftrightarrow \frac1{p'} =
    \frac{p-1}{p}$
    
    \begin{gather*}
      \Longrightarrow \sum_{k=1}^{n}|a_{k}+b_{k}|^{p} \leq
	 \sum_{k=1}^{n}|a_{k}+b_{k}|^{p-1}|a_{k}| +
	 \sum_{k=1}^{n}|a_{k}+b_{k}|^{p-1} |b_{k}|\\
      \text{\thref{lem:hoelder-sum}} \Longrightarrow \leq
	 (\sum_{k=1}^{n}|a_{k}+b_{k}|^{p})^{\frac{p-1}p}
	 ((\sum_{k=1}^{n}|a_{k}|^{p})^{\frac1p} +
	 (\sum_{k=1}^{n}|b_{k}|^{p})^{\frac1p})\\
      \Longrightarrow (\sum_{k=1}^{n}|a_{k} + b_{k}|^{p})^{\frac1p} \leq
	 (\sum_{k=1}^{n}|a_{k}|^{p})^{\frac1p} +
	 (\sum_{k=1}^{n}|b_{k}|^{p})^{\frac1p}
    \end{gather*}
  \end{proof}
\end{lemma}

\begin{satz}\label{satz:1.2.1/1}
  \begin{enumerate}
   \item $1\leq p \leq \infty$
    
    Dann gilt: $(l_{p}, \|\cdot\|_{p})$ ist ein Banachraum
    
    Für $1\leq p < \infty$ ist $l_{p}$ separabel. $l_{\infty}$ ist nicht
    separabel.
   \item Die Räume $C_{0} := \{ (a_{k})_{k} : \lim_{k\rightarrow\infty}
    a_{k} = 0 \}$ und $c := \{ (a_{k})_{k} : \exists
    \lim_{k\rightarrow\infty} a_{k}\}$ sind separabele und abgeschlossene
    Teilräume von $l_{\infty}$
   \item $1\leq p < \infty$
    
    $A \subset l_{p}$ präkompakt $\Longleftrightarrow$ $A$ beschränkt
    und
    \begin{equation*}
      \sup_{a\in A}\sum_{k=n}^{\infty}
	 |a_{k}|^{p}\xrightarrow{n\rightarrow\infty} 0
    \end{equation*}
  \end{enumerate}
  
  \begin{proof}
    $\|a_{k}+b_{k}\|_{p} \leq \|a_{k}\|_{p} + \|b_{k}\|_{p}$ folgt aus
    \thref{lem:1.2.1/3}
    
    Vollständigkeit folgt aus \thref{satz:1.1.1/2} für $p=\infty$ und analog zu
    Beispiel 1.1.1/4 für $1\leq p <\infty$
    
    $l_{p} (1\leq p < \infty)$ separabel wie in \thref{bsp:1.1.1/5}
    
    $l_{\infty}$ nicht separabel Übung Aufgabe 2, Serie 2
    
    Aussage für $C_{0}$ siehe \thref{bsp:1.1.2/1}
    
    Aussage für $c:$ $\{ \alpha = (\alpha_{k})_{k}: \alpha_{k}\in \Q,
    \alpha = (\alpha_{1},\alpha_{2},\ldots,\alpha_{n},\alpha_{0},
    \alpha_{0},\ldots), n \in \N \}$
    
    Beweise von (3) wie in \thref{bsp:1.1.3/2}
  \end{proof}
\end{satz}

\begin{lemma}\label{lem:1.2.1/4}
  (Aufgabe 1, Blatt 2)
  \begin{enumerate}
