\chapter{Banachräume und beschränkte Operatoren}
\section{Banachräume}
\subsection{Normierte Vektorräume}

% Topologische Räume $\supset$ Metr. Räume $\supset$ Vektorräume
% $\supset$ Banachräume $\supset$ Hilberträume

\textbf{Grunddefinitionen}
\begin{itemize}
 \item
  $E$ \highl{Vektorraum} über einem \highl{Körper} $\K$ mit $\K=\R$ oder $\C$
  (reeller/komplexer Vektorraum)
 \item
  $\{x_{1},\ldots,x_{n}\}\subset E$ \highl[linear!unabhängig]{linear
  unabhängig} $:\Longleftrightarrow$
  $\lambda_{1}x_{1}+\ldots+\lambda_{n}x_{n}=0 \Rightarrow
  \lambda_{1}=\ldots=\lambda_{n}=0$; sonst \highl[linear!abhängig]{linear
  abhängig}
 \item
  $M\subset E$ linear unabhängig $:\Longleftrightarrow$ jede endliche Teilmenge ist linear
  unabhängig
 \item
  $\dim E=n$ $:\Longleftrightarrow$ $\exists n$ linear unabhängige Elemente aus
  $E$ und $n+1$ Elemente sind linear abhängig

  z.\,B. $\K^{n}$ (s. linare Algebra)
 \item
  $\dim E =\infty$ $:\Longleftrightarrow$ $\forall n\in \N\ \exists$ linear unabhängig
  Elemente $\{x_{1},\ldots,x_{n}\}$

  z.\,B. $C([a,b]) := \{ f \big| f\colon [a,b] \rightarrow \K, \text{stetig} \},\;
    \{1,x,x^{2},\ldots,x^{n}\}$ linear unabhängig
 \item
  $M\subset E$. Als Spann (engl.~span) von $M$ bezeichnet man den Teilraum
  von $E$, der durch die Vektoren $x_{1},\ldots,x_{n}$ aufgespannt wird.

  $\Span M := \{x\in E\colon x = \sum_{\text{endl.}} \lambda_{j}x_{j},
  \lambda_{j}\in \K, x_{j}\in M\}$

  Der Spann ist der Durchschnitt aller Teilräume, die $M$ enhalten oder
  anders ausgedrückt, es ist der kleinste Teilraum von $E$, der $M$ enthält.
 \item
  Sei $\dim E = n$. Dann heißen $\{x_{1},\ldots,x_{n}\}$ \highl{Basis}
  $:\Longleftrightarrow$
  \begin{enumerate}
   \item $x_{1},\ldots,x_{n}$ sind linear unabhängig
   \item $\Span\{x_{1},\ldots,x_{n}\} = E$
  \end{enumerate}

  \textbf{Bemerkung}\\ % Das soll so sein; die Bemerkung hatte keine Nummer
  Jeder Vektorraum besitzt eine \highl{Hamelbasis}, d.\,h. es existiert
  eine linear unabhängige Menge von Vektoren, die $E$ aufspannt: $\Span M
  = E$ (o.~Bew.)
\end{itemize}


\begin{defini}
  Ein Vektorraum $E$ heißt \highl{normiert} $:\Leftrightarrow$ eine
  Abbildung $\norm{\cdot}\colon E\rightarrow \R$ mit den Eigenschaften
  \begin{enumerate}
   \item $\norm{x}> 0\qquad \forall x \ne 0$
   \item $\norm{\lambda x} = \norm{\lambda} \norm{x}\qquad \forall \lambda\in
    \K, 0\ne x\in E$
   \item $\norm{x+y} \leq \norm{x}+\norm{y}\qquad \forall x,y\in E$
  \end{enumerate}
  existiert. $\norm{\cdot}$ heißt \highl{Norm}

  Diese abstrakte Definition geht zurück auf Hahn, Banach, Wiener 1922.
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $\norm{x}=0 \Leftrightarrow x=0$
   \item $\abs[\big]{\norm{x}-\norm{y}} \leq \norm{x+y}$
   \item $d(x,y) := \norm{x-y}$ ist eine \highl{Metrik}
  \end{enumerate}
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item $\R^{n}$:
    \begin{align*}
      \norm{x}_{1} &= \sum\abs{x_{j}}\\
      \norm{x}_{2} &= (x_{1}^{2}+\ldots+x_{n}^{2})^{\frac12}\\
      \norm{x}_{\infty} &= \max_{j}\abs{x_{j}}
    \end{align*}

    analog im $\C^{n}$
   \item $C([a,b])$:
    \begin{gather*}
      \norm{f} := \sup_{x\in[a,b]}\abs{f(x)}=\max_{x\in[a,b]}\abs{f(x)}
    \end{gather*}
   \item \index{l1@$l_{1}$} $l_{1}(\N) := \{a \big|
    a=(a_{j})_{j=1}^{\infty} \subset \K:
    \sum_{j=1}^{\infty}\abs{a_{j}}<\infty\}$ ist die Menge aller
    absolutkonvergenten Reihen:
    \begin{gather*}
      \norm{a}_{1} := \sum_{j=1}^{\infty}\abs{a_{j}}
    \end{gather*}
    $\dim l_{1}(\N) = \infty$, da
    $(e_{j}=(0,\ldots,0,
    \underbrace{1}_{\mathclap{j\text{.~Position}}},0,\ldots))$
    mit $j\in\N$ Teilmenge von $l_{1}(\N)$ ist und dieses offensichtlich
    die Dimension\,$\infty$ hat.
  \end{enumerate}
\end{bsp}

\begin{defini}
  $(E,\norm{\cdot})$ normierter Vektorraum, $(x_{j})_{j}$ Folge in $E$

  \begin{enumerate}
   \item Eine Folge $(x_{j})_{j}$ heißt \highl{konvergent}
    $:\Longleftrightarrow$ $\exists x\in E
    \ \forall\epsilon > 0 \exists j_{0}(\epsilon) \forall j\geq
    j_{0}(\epsilon): \norm{x_{j}-x}\leq \epsilon$
   \item Eine Folge $(x_{j})_{j}$ heißt \highl{Cauchyfolge}
    $:\Longleftrightarrow$ $\forall \epsilon >0 \exists j_{0}(\epsilon)
    \forall j,k \geq j_{0}(\epsilon): \norm{x_{j}-x_{k}}\leq \epsilon$
  \end{enumerate}
\end{defini}

\begin{folger}
  \begin{enumerate}
   \item Jede konvergente Folge ist Cauchyfolge
   \item Jede Cauchyfolge ist \highl{beschränkt} ($\exists c>0\ \forall
    j:\norm{x_{j}}\leq c$)
   \item $x_{j}\rightarrow x$ $\Longrightarrow$ $\norm{x_{j}} \rightarrow \norm{x}$
    (Stetigkeit der Norm: $\norm{\cdot}\colon E\rightarrow\R$ stetig)
  \end{enumerate}
\end{folger}

\begin{defini}[Vollständigkeit]
  \begin{enumerate}
   \item $(E, \norm{\cdot})$ normierter Vektorraum heißt
    \highl{vollständig} $:\Longleftrightarrow$ Jede Cauchyfolge in $E$
    ist konvergiert und ihr Grenzwert liegt in $E$.
   \item Ein vollständig normierter Vektorraum heißt \highl{Banachraum}.
  \end{enumerate}
\end{defini}

\begin{bemerk}
  Die Räume $\R^{n}, \C^{n}$ mit der euklidischen Norm $\norm{\cdot}_{2}$
  und der Raum $(C([a,b]), \norm{\cdot}_{\infty})$ mit der Supremumsnorm
  $\norm{f}_{\infty} := \sup_{x\in[a,b]} \abs{f(x)}$ sind Banachräume
  (s.~Analysis~2).
\end{bemerk}

\begin{bemerk}
  Jedoch nicht jeder normierte Vektorraum ist vollständig.
  Beispiele: $\Q, \Q^{n}$
\end{bemerk}

\begin{bsp}
  $\mathcal{P} := \{ p\colon [a,b] \rightarrow \K\colon p \text{ ist
  Polynom}\} \subset C([a,b])$ mit der Norm $\norm{p}_{\infty} = \max\limits_{x\in
  [a,b]} \abs{p(x)}$ ist nicht vollständig.

  \begin{proof}
    Wir wissen $e^{x}=\sum_{k=0}^{\infty}\frac{x^{k}}{k!}$ ist gleichm.
    konvergent auf $[a,b]$. Daraus folgt, die Folge $(p_{n})_{n}$ mit
    $p_{n}(x) = \sum_{k=0}^{n}\frac{x^{k}}{k!} \in \mathcal{P}$ ist eine
    Cauchyfolge bzgl. $\norm{\cdot}_{\infty}$

    Ann.: $\exists p\in \mathcal{P}$ mit $\norm{p_{n}-p} \rightarrow 0
    \Rightarrow$
    \begin{gather*}
      \abs{p(x) - e^{x}} \leq \norm{p(x) - p_{n}(x)}_{\infty}+\norm{p_{n}(x)-e^{x}}_{\infty}
         \xrightarrow{n\rightarrow\infty} 0
    \end{gather*}

    Daraus folgt aber, dass $p(x) = e^{x}$, was ein Widerspruch zu
    unserer Annahme ist, da $e^{x}\notin\mathcal{P}$.
  \end{proof}
\end{bsp}

\begin{bsp}\label{bsp:1.1.1/3}
  Der Raum $C([0,1])$ mit der Norm $\norm{f}_{1} = \int_{0}^{1} \abs{f(t)}\,dt$ ist
  nicht vollständig.

  \begin{proof}
    $m \geq 2$
    \begin{gather*}
      f_{m}(t) =
         \begin{cases}
           0 & 0\leq t < \frac12\\
           m(t-\frac12) & \frac12 \leq t < \frac12+\frac1m =: a_{m}\\
           1 & a_{m} \leq t \leq 1
         \end{cases}
    \end{gather*}

    Sei $n > m$ $\Rightarrow$ $\norm{f_{n}-f_{m}}_{1}\leq \frac1m \rightarrow
    0$ $\Rightarrow$ $(f_{n})_{n}$ ist Cauchyfolge.

    Annahme: Es existiert ein $f\in C([0,1])$ mit $\norm{f_{n}-f}_{1}
    \rightarrow 0$. Dann ist
    \begin{gather*}
      \norm{f_{n}-f}_{1} = \int_{0}^{\frac12}\abs{f(t)}\,dt+
         \int_{\frac12}^{a_{m}}\abs{f_{n}(t)-f(t)}\,dt + \int_{a_{m}}^{1}\abs{1-f(t)}\,dt
    \end{gather*}
    Für $n\rightarrow \infty$ geht $a_{m} \rightarrow \frac12$ und damit
    gilt:
    \begin{gather*}
      \int_{0}^{\frac12}\abs{f(t)}\,dt = 0\quad\text{und}\quad
      \int_{\frac12}^{1}\abs{1-f(t)}\,dt = 0
    \end{gather*}
    Die Funktion $f$ muss also die Gestalt
    \begin{gather*}
      f(t) = \begin{cases}
               0 & \colon0 < t \leq \frac12\\
               1 & \colon\frac12 < t \leq 1
             \end{cases}
    \end{gather*}
    haben, was einen Widerspruch zu der Annahme $f$ sei stetig darstellt.
  \end{proof}
\end{bsp}

\begin{satz}\label{satz:1.1.1/1}
  $(E, \norm{\cdot})$ normierter Vektorraum.

  Dann gilt: $E$ vollständig $\Longleftrightarrow$ $\forall (x_{n})_{n}$ mit
  $\sum\norm{x_{n}}<\infty$ $\Rightarrow$ $\exists x\in E$ mit $x=\sum
  x_{n}$. (jede absolutkonvergente Reihe (bzgl. $\norm{\cdot}$) ist konvergent)

  \begin{proof}
    1.\,Übungsserie Aufgabe 3
  \end{proof}
\end{satz}

% 15.10.04

\begin{satz}\label{satz:1.1.1/2}
  $A\ne \emptyset, B(A) := \{ f\colon A\rightarrow \K\colon \norm{f}_{\infty} :=
  \sup_{x\in A}\abs{f(x)}<\infty \}$

  Dann ist $B$ ist Banachraum bzgl. $\norm{\cdot}$.

  \begin{proof}
    Verwenden \autoref{satz:1.1.1/1}. Es sei $(f_{n})_{n}\subset B(A)$ mit
    $\sum_{n=1}^{\infty} \norm{f_{n}}_{\infty} < \infty$. Weiterhin sei
    $x\in A$.
    \begin{flalign*}
      \Longrightarrow&\abs[\Bigl]{\sum_{k=m}^{n} f_{k}(x)} \leq
         \sum_{k=m}^{n}\norm{f_{k}}_{\infty}\leq
         \epsilon\quad\text{ für }m,n \geq n_{0}(\epsilon)&\\
      \Longrightarrow& \Bigl(\sum_{k=1}^{n}f_{k}(x)\Bigr)_{n}
         \text{ ist C.\,F. in }\K \text{ und da $\K$ vollständig ist }
         \Longrightarrow \exists f(x) = \sum_{k=0}^{\infty} f_{k}(x)&\\
      \Longrightarrow& f\colon A\rightarrow\K \text{ Es gilt: } \abs{f(x)}\leq
         \sum_{k=0}^{\infty}\norm{f_{k}(x)}_{\infty}<\infty&\forall x
    \end{flalign*}
    Somit ist auch $f\in B(A)$, weshalb $B(A)$ vollständig und damit ein
    Banachraum ist.
  \end{proof}
\end{satz}

\begin{folger}
  $A=\N,\ B(\N) = l_{\infty}(\N)=\bigl\{ (a_{n})_{n}\colon
  \norm{a}_{\infty}=\sup_{n}\abs{a_{n}}<\infty\bigr\}$ ist Banachraum.
\end{folger}

\begin{bsp}\index{ln@$l_{n}$}\label{bsp:1.1.1/4}
  \begin{gather*}
    l_{1}(\N) := \{ (a_{n})_{n} \colon \norm{a}_{1}:=
       \sum_{i=1}^{\infty}\abs{a_{i}}<\infty\}
  \end{gather*} ist Banachraum.

  \begin{proof}
    Es sei $(a^{j})_{j\in\N}\subset l_{1}(\N)$ eine Cauchyfolge. Es sei
    $\epsilon>0$.
    \begin{flalign*}
      \Rightarrow\quad &\exists j_{0}(\epsilon)\forall j,k \geq j_{0}(\epsilon):
         \norm{a^{j}-a^{k}}_{1}=\sum_{n=1}^{\infty}\abs{a_{n}^{j}-a_{n}^{k}}
         \leq \epsilon \taglabel{eq:1}&\\
      \Rightarrow\quad &\text{Für jedes feste $n\in \N$ ist $(a_{n}^{j})_{j}$
         Cauchyfolge in $\K$.}&\\
      \Rightarrow\quad & \exists a_{n}\in \K \text{ mit } a_{n}^{j} \rightarrow
         a_{n}\in\K. \text{Setzen } a := (a_{n})_{n}.&
    \end{flalign*}

    Zeigen: $a_{j}\rightarrow a$ in $l_{1}(\N)$. Aus \autoref{eq:1}
    folgt für jedes $N\in\N$:
    \begin{gather*}
      j,k\geq j_{0}(\epsilon)\colon \sum_{n=1}^{N}\abs{a_{n}^{j}-a_{n}^{k}}\leq
         \epsilon\\
      k\rightarrow\infty \Rightarrow \sum_{n=1}^{N}\abs{a_{n}^{j}-a_{n}}\leq
         \epsilon\\
      N\rightarrow\infty \Rightarrow
         \sum_{n=1}^{\infty}\abs{a_{n}^{j}-a_{n}}\leq \epsilon\quad j\geq
         j_{0}(\epsilon)\\
      \sum\abs{a_{n}} \leq \sum\underbrace{\abs{a_{n}-a_{n}^{j}}}_{\leq
         c}+\sum\underbrace{\abs{a_{n}^{j}}}_{\leq c}\leq \infty\\
      \Rightarrow a_{n}^{j} \rightarrow a \in l_{1}(\N)
    \end{gather*}
  \end{proof}
\end{bsp}

\begin{defini}\label{def:1.1.1/4}
  Für einen normierten Vektorraum $(E, \norm{\cdot})$ (oder allgemein einen
  metrischen Raum) gilt:
  \begin{enumerate}
   \item\label{en1} der Teilraum $M\subset E$ heißt \gdwdef{\highl{dicht} in $E$}
    es zu jedem $x\in E$ eine Folge $(x_{j})_{j}\subset M$ gibt,
    deren Grenzwert in $E$ liegt: $x_{j} \rightarrow x \in E$.
   \item $E$ heißt \gdwdef{\highl{separabel}} es eine
    abzählbare und dichte Teilmenge in $E$ gibt.
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $\Q\subset\R$ dicht $\Longrightarrow$ $\R$ separabel
   \item $\Q^{n}\subset\R^{n}$ dicht $\Longrightarrow$ $\R^{n}$ separabel
   \item äquivalent zum \autoref{en1} in \autoref{def:1.1.1/4} ist:
    \begin{gather*}
      \forall \epsilon>0\:\forall x\in E\:\exists x_{\epsilon} \in M\colon
      \norm{x-x_{\epsilon}}<\epsilon \Longleftrightarrow x_{\epsilon}\in
         K_{\epsilon}(x), x\in M
    \end{gather*}
  \end{enumerate}
\end{bemerk}

\begin{bsp}\label{bsp:1.1.1/5}
  $l_{1}(\N)$ ist separabel. ($l_{\infty}(\N)$ ist nicht separabel. siehe Übungsaufgabe)

  \begin{proof}
    Da die Menge $C_{00}(\N):= \{ a\colon \exists n\in \N \ \forall k>n \colon
    a_{k}=0\}$ dicht im $l_{1}(\N)$ liegt \help{wieso?}, lässt sich jedes
    $a = (a_{1},\ldots,a_{n},0,\ldots,0)\in C_{00}(\N)$ durch Folgen aus
    $C_{00}(\N)$ mit rationalen Gliedern approximieren. Die Menge
    $M_{rat} = \{a\colon a\in C_{00}, a_{i}\in\Q\}$ ist eine abzählbare und
    dichte Teilmenge in $l_{1}$, woraus die Behauptung folgt.
  \end{proof}
\end{bsp}

\subsection{Vervollständigung von normierten Vektorraum}

\begin{description}
 \item[Wiederholung:]
  $(E,\norm{\cdot})$ sei normierter Vektorraum. $U\subset E$ heißt
  abgeschlossen $\Leftrightarrow$ $U$ enhält alle Häufungspunkte
  $\Leftrightarrow$ $\forall (x_{j})\subset U, x_{j} \rightarrow x\colon x\in
  U$ $\Leftrightarrow$ $\overline{U}=U$ wobei $\overline{U}:=
  U\cup\{\text{Häufungspunkte von U}\}$ der Abschluss von $U$ ist.

  $U\subset E$ heißt abgeschlossener Teilraum $:\Leftrightarrow$ $U$
  Teilraum und abgeschlossen.
\end{description}

\begin{bemerk}
  $(E,\norm{\cdot})$ Banachraum, $U\subset E$ Teilraum.

  Dann:
  \begin{enumerate}
   \item $U$ abgeschlossen $\Longleftrightarrow$ $(U, \norm{\cdot})$
    vollständig. (Banachraum)
   \item $U$ dicht in $E$ $\Longleftrightarrow$ $\overline{U}=E$
  \end{enumerate}
\end{bemerk}

\begin{bsp}\label{bsp:1.1.2/1}
  \begin{enumerate}
   \item Jede Ebene im $\R^{n}$ ist ein abgeschlossener Teilraum, insbesondere
    sind
    \begin{itemize}
     \item $U=\{ x\in\R^{n}\colon x_{n}=0\}$ abgeschlossener Teilraum des
      $\R^{n}$,
     \item $U = \{x\in\R^{2}\colon ax_{1}+bx_{2}=0\}$ mit $(a,b)\ne0$ ein
      abgeschlossener Teilraum des $\R^{2}$ und
     \item $U=\{x\in\R^{3}\colon a_{1}x_{1}+ a_{2}x_{2}+a_{3}x_{3} =0\}$
      mit $(a_{1},a_{2},a_{3})\ne0$ ein abgeschlossener Teilraum des $\R^{3}$.
    \end{itemize}

   \item Der Raum \highl[$C_{0}$]{$C_{0}(\N)$} der Nullfolgen mit endlichen
    Gliedern ist ein abgeschlossener Teilraum des $l_{\infty}(\N)$.
    Insbesondere ist $C_{0}(\R)$ Banachraum bzgl. der Unendlichnorm
    $\norm{\cdot}_{\infty}$.
    \begin{gather*}
      C_{0}(\N) := \{ (a_{j})_{j}\in l_{\infty}(\N)\colon
         \lim_{j\rightarrow\infty} a_{j} = 0\} \subset l_{\infty}(\N)
    \end{gather*}

    \begin{proof}
      Es sei $(a^{n})_{n}$ eine Folge im $C_{0}(\N)$ mit dem Grenzwert $a\in
      l_{\infty}(\N)$. Da für jedes $\epsilon>0$ und hinreichend großes $j$
      ($j\geq j_{0}(\epsilon)$) sich $a_{j}\in a$ auf folgende Weise
      abschätzen lässt, ist $a$ eine Nullfolge, also in $C_{0}(\N)$, und
      $C_{0}(\N)$ daher abgeschlossen.
      \begin{gather*}
        \abs{a_{j}}\leq \abs{a_{j}-a_{j}^{n}} + \abs{a_{j}^{n}}
           \leq \underbrace{\norm{a-a^{n}}_{\infty}}_{\leq \epsilon, n \geq
           n_{0}(\epsilon)} + \abs{a_{j}^{n}} \leq 2\epsilon
      \end{gather*}
    \end{proof}
   \item $C_{00}(\N)$ liegt dicht in $C_{0}(\N)$. Somit ist $C_{0}(\N)$
    separabel und $\overline{C_{00}(\N)} = C_{0}(\N)\subsetneq
    l_{\infty}(\N)$.

    \begin{proof}
      \begin{description}
       \item["`$\Rightarrow$"'] $(a^{n})_{n}\subset C_{00}(\N),
        a^{n}\rightarrow a\in l_{\infty}(\N)$ $\Rightarrow$ $a\in
        C_{0}(\N)$ wie in \autoref{bsp:1.1.1/5}
       \item["`$\Leftarrow$"'] $a\in C_{0}(\N) \Rightarrow
        \lim_{j\rightarrow\infty} a_{j}=0$

        $ \epsilon > 0 \Rightarrow
        \exists j_{0}(\epsilon) \forall j\geq j_{0}(\epsilon):
        \abs{a_{j}}\leq \epsilon$

        $j \geq j_{0}(\epsilon) \Rightarrow \norm{a-a^{j}}_{\infty}=
        \sup_{k>j}\abs{a_{j}} \leq \epsilon$ wobei
        $a^{j}=(a_{1},\ldots,a_{j},0,\ldots)\in C_{00}(\N)$

      \end{description}
    \end{proof}
  \end{enumerate}
\end{bsp}

\noindent\underline{\textbf{Problem:}} Kann man für \emph{jeden}
normierten Vektorraum $E$ einen Banachraum $F$ mit
$\overline{E}=\overline{F}$ finden?

\begin{defini}
  Für zwei normierte Vektorräume $(E,\norm{\cdot}_{E})$ und $(F,
  \norm{\cdot}_{F})$ werden $E$ und $F$ als \highl{isometrisch isomorph}
  bezeichnet $:\Leftrightarrow$ es eine lineare Abbildung $L\colon E
  \rightarrow F$ mit den folgenden Eigenschaften gibt:
  \begin{itemize}
   \item für alle $x\in E$ ist $\norm{Lx}_{F} = \norm{x}_{E}$
   \item $L$ ist surjektiv ($L(E)=F$)
  \end{itemize}

  $L$ heißt \highl[Isomorphismus!isometrisch]{isometrischer Isomorphismus}.
\end{defini}

\begin{satz}
  \begin{enumerate}
   \item Jeden normierten Vektorraum kann man isometrisch isomorph auf einen
    dichten Teilraum eines geeigneten Banachraums abbilden.
   \item Sämtliche Banachräume mit dieser Eigenschaft sind
    untereinander isometrisch isomorph.
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item konstruieren $F$ mit (1).

      Betrachten dazu alle Cauchyfolgen in $E$. Zwei Cauchyfolgen
      $(x_{j})_{j}$ und $(x_{j}')_{j}$ in $E$ heißen äquivalent
      $:\Leftrightarrow$ ($x_{j}-x_{j}')_{j}$ ist Nullfolge.
      \begin{gather*}
        F := \{ f\colon f \text{ Äquivalenzklasse von C.\,F. bzgl. dieser
           Relation } \}
      \end{gather*}

      $f\sim (x_{j}), g\sim (y_{j}) f+g := $ Äquivalenzklasse
      die $(x_{j}-y_{j})$ enthält.

      $\lambda f := $ Äquivalenzklasse, die $(\lambda x_{j})$ enthält.
      unabhängig von Auswahl des Repräsentanten.

      $0 := $ Äquivalenzklasse, die $(0,0,\ldots)$ (alle Nullfolgen)

      $\norm{f}_{F} := \lim_{j\rightarrow\infty}\norm{x_{j}}_{E}$ (existiert,
      da $(\norm{x_{j}}_{E})$ C.\,F. im $\R$! ( $\abs{\norm{x_{j}}_{E}-
      \norm{x_{k}}_{E}} \leq \norm{x_{j}-x_{k}}$ ))

      $(F, \norm{\cdot}_{F})$ ist also ein normierter Vektorraum.

      Wir setzen nun für $x\in E$ $Lx$, die Äquivalenzklasse, die $(x,x,x,\ldots)$
      enthält, ein und erhalten: $\norm{Lx}_{F} = \norm{x}_{E}$, d.\,h. $L \colon E
      \rightarrow L(E)\subset F$ ist isometrisch isomorph.

      zeigen $\overline{L(E)}=F$, $F$ vollständig $\Rightarrow$ (1)
    \end{schritte}
  \end{proof}
\end{satz}

\begin{bemerk}
  Ein solcher Banachraum heißt \highl{Vervollständigung} von $E$. Alle
  diese Banachräume sind "`Kopien"' und werden nicht unterschieden.
\end{bemerk}

\subsection{Kompaktheit}

$(X, d)$ metrischer Raum

\begin{defini}
  $(X,d)$ metrischer Raum

  $A\subset X$ heißt \highl{kompakt} (folgenkompakt)
  $:\Longleftrightarrow$ jede Folge in $A$ besitzt eine konvergente
  Teilfolge mit Grenzwert in $A$.
\end{defini}

\begin{bemerk}\label{bem:1}
  \begin{enumerate}
   \item
    $A$ kompakt $\Longleftrightarrow$ jede unendliche Teilmenge von $A$
    besitzt einen Häufungspunkt in $A$.
   \item $A\subset X=\K^{n}$ kompakt $\Leftrightarrow$ $A$ beschränkt
    und abgeschlossen.
   \item $X,Y$ metrische Räume, $f\colon X\rightarrow Y$ stetig:

    $A \subset X$ kompakt $\Longrightarrow$
    \begin{enumerate}
     \item $f(A)$ kompakt
     \item $f$ ist gleichmäßig stetig auf $A$
    \end{enumerate}

    Für $Y=\R$ gilt: $\exists \min_{x\in A} f(x), \exists \max_{x\in A} f(x)$
  \end{enumerate}
\end{bemerk}

$r>0, K_{r}(x_{0}) := \{ x\in X\colon d(x, x_{0}) < r\}$ offene Kugel

$x_{0}\in X, U\subset X$ heißt offen $:\Longleftrightarrow$ $\forall
x\in U \exists r>0\colon K_{r}(x)\subset U$

\begin{defini}
  $I$ Indexmenge, $U_{\alpha}$ offen für jedes $\alpha \in I$

  $(U_{\alpha})_{\alpha\in I}$ heißt \highl{offene Überdeckung} von $A$
  $:\Longleftrightarrow$ $A\subset \bigcup_{\alpha\in I} U_{\alpha}$

  $I_{0}\subset I$ $(U_{\beta})_{\beta\in I_{0}}$ Teilüberdeckung von
  $A$ $:\Longleftrightarrow$ $A\subset \bigcup_{\beta\in I_{0}} U_{\beta}$
\end{defini}

\begin{satz}[Heine-Borel]\label{satz:heineborel}
  $(X,d)$ metr. Raum, $A\subset X$

  Dann sind die folgenden beiden Aussagen äquivalent:
  \begin{enumerate}
   \item $A$ ist folgenkompakt
   \item $A$ ist \highl{überdeckunsgkompakt}, d.\,h. aus jeder offenen
    Überdeckung von $A$ kann man eine \highl{endliche Teilüberdeckung}
    auswählen.
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item $A\subset X$ (folgen"~)""kompakt, $(U_{\alpha})_{\alpha}$ offene
      Überdeckung. Dann gilt:

      \begin{gather*}\label{eq:2}
        \exists\lambda>0 \forall
           x\in A \exists U_{\alpha}: K_{\lambda}(x)\subset U_{\alpha}
      \end{gather*}

      Ann: \autoref{eq:2} gilt nicht $\Longrightarrow$
      \begin{gather*}
        \forall n\in \N\exists x_{n}\in A\forall U_{\alpha}:
           K_{\frac1n}(x_{n})\not\subset U_{\alpha}
      \end{gather*}

      Betrachten $(x_{n})_{n\in \N}\subset A$. $A$ kompakt
      $\Longrightarrow$ $\exists$ konv. Teilfolge $(x_{n_{k}})_{k\in\N}$
      mit $x_{n_{k}}\rightarrow x_{0}\in A$

      $\exists \alpha_{0}\in I\colon x_{0}\in U_{\alpha_{0}}$
      $U_{\alpha_{0}}$ offen $\Longrightarrow$ $\exists r_{0}>0
      K_{r_{0}}(x_{0})\subset U_{\alpha_{0}}$

      Wähle $n_{k}$, so dass $d(x_{n_{k}}, x_{0})<\frac{r_{0}}2$ und
      $\frac1{n_{k}}<\frac{r_{0}}2$. Mithilfe der Dreiecksungleichung
      folgt daraus:
      \begin{gather*}
        K_{\frac1{n_{k}}}(x_{n_{k}})\subset K_{r_{0}}(x_{0})\subset U_{\alpha_{0}}
      \end{gather*}
      was einen Widersrpruch zur gemachten Annahme ist.
     \item (1) $\Rightarrow$ (2)\quad Es sei $(U_{\alpha})_{\alpha}$
      offene Überdeckung von $A$. Sei $\lambda$ wie im vorherigen
      Schritt. Wählen
      \begin{gather*}
        y_{1}\in A\\
        y_{2}\in A\setminus K_{\lambda}(y_{1})\\
        y_{3}\in A\setminus (K_{\lambda}(y_{1})\cup K_{\lambda}(y_{2}))\\
        \ldots\\
        y_{n}\in A\setminus \bigcup_{j=1}^{n-1}K_{\lambda}(y_{j})
      \end{gather*}

      Beh.: $\exists n\in \N\colon A\setminus \bigcup_{j=1}^{n-1}
      K_{\lambda}(y_{j})=\emptyset$ ($\Longrightarrow A\subset
      \bigcup_{j=1}^{n} K_{\lambda}(y_{j})\subset \bigcup_{j=1}^{n}
      U_{\alpha_{j}}$)

      Ann: $A\setminus \bigcup_{j=1}^{n-1} K_{\lambda}(y_{j})
      \ne\emptyset\; \forall n\in \N$ $\Rightarrow$ $\exists
      (y_{j})_{j\in \N}\colon d(y_{j}, y_{k}) \geq \lambda>0\; \forall j \ne
      k$ $\Longrightarrow$ $(y_{j})_{j}$ hat keine konvergente Teilfolge
      $\Longrightarrow$ Widerspruch zur Kompaktheit
     \item (2) $\Rightarrow$ (1)\quad $M\subset A$ unendliche Teilmenge

      Ann: $M$ besitzt keinen Häufungspunkt $\Longrightarrow \forall
      x\in M \exists U_{x}$ offen $M\cap U_{x}=\{x\}$

      $M$ abgeschlossen $\Longrightarrow$ $X\setminus M$ offen
      $\Longrightarrow$ $A\subset (\bigcup_{x\in M} U_{x})\cup(X\setminus M)$

      (2) $\Longrightarrow \exists$ endliche Teilüberdeckung, diese
      enthält nur endlich viele Elemente von $M$ $\Longrightarrow$
      Widerspruch zur Annahme
    \end{schritte}
  \end{proof}
\end{satz}

\begin{folger}\label{fol:1.1.3/1}
  $(X,d)$ kompakt $\Longrightarrow$ $X$ ist separabel, d.\,h. $\exists$
  abzählbare dichte Teilmenge in $X$

  \begin{proof}
    $\forall j\in \N$ ist $X = \bigcup_{x\in X} K_{\frac1j}(x)$

    Aus \autoref{satz:heineborel} $\Longrightarrow$ $\exists$ endliche Teilüberdeckung
    \begin{gather}\label{eq:3}
      \big(K_{\frac1j}(x_{k}^{j})\big)_{k=1,\ldots,n_{j}}\qquad\forall j\in\N
    \end{gather}

    $\Longrightarrow$ $M := \{ x_{k}^{j}\colon j\in \N, k=1,\ldots,n_{j}\}$ $M$
    ist abzählbar und dicht in $X$: $x\in X, \epsilon>0$ wähle
    $j>\frac1{\epsilon}$ Aus \autoref{eq:3} folgt $\exists k$ mit $x\in
    K_{\frac1j}(x_{k}^{j}) \Longrightarrow d(x,x_{k}^{j})<\epsilon$
    $\Longrightarrow$ Folgerung
  \end{proof}
\end{folger}

\begin{defini}
  \label{def:praekompakt}
  $(X,d)$ sei ein metrischer Raum. Eine Teilmenge $A\subset X$ heißt
  \gdwdef{\highl{präkompakt}} jede Folge in $A$ eine konvergente Teilfolge
  besitzt (deren Häufungspunkt nicht in $A$ liegen muss).
\end{defini}

\begin{folger}
  \begin{enumerate}
   \item Eine Teilmenge $A$ ist \gdw{\highl{kompakt}} sie präkompakt und
    abgeschlossen ist.
   \item Wenn eine Teilmenge $A$ präkompakt ist, dann sie auch beschränkt.
  \end{enumerate}
\end{folger}

\begin{defini}
  Für ein gegebenes $\epsilon>0$, bezeichnet man die Menge $B\subset X$
  als \highl{$\epsilon$"~Netz} von $A\subset X$ \gdwdef{}
  \begin{gather*}
    A\subset \bigcup_{x\in B}  K_{\epsilon}(x)
  \end{gather*}
\end{defini}

\begin{satz}\label{satz:1.1.3/2}
  Für einen vollständig, metrischen Raum $(X,d)$ gilt:
  $A\subset X$ ist \gdw{präkompakt} für alle $\epsilon>0$ ein
  endliches $\epsilon$-Netz für $A$ existiert.

  \begin{proof}
    \begin{description}
     \item["`$\impliziert$"'] Wir zeigen die umgekehrte
      Aussage\footnote{$A\impliziert B$ ist semantisch äquivalent zu $\neg
      B\impliziert\neg A$}, also dass unter der Prämisse, dass es ein
      $\epsilon>0$ gibt, für das kein endliches $\epsilon$"~Netz in $A$
      existiert, $A$ nicht präkompakt ist.

      Dazu konstruieren wir eine Folge $(x_{j})_{j\in\N}\subset A$, für die
      jeweils zwei Gleider $x_{k}$ und $x_{j}$ (mit $k\ne j$) einen Abstand
      $d(x_{j},x_{k})>\epsilon_{0}$ haben. Keine Teilfolge diese Folge kann
      also konvergieren und somit ist $A$ nicht präkompakt.

      Für $\epsilon_{0}>0$ existiere kein $\epsilon_{0}$"~Netz (Prämisse),
      d.\,h. man kann zu einen Punkt~$x_{1}\in A$ einen Punkt~$x_{2}\in A$
      finden, der nicht zum $\epsilon_{0}$"~Netz gehört. Die Punkte haben also
      den Abstand $d(x_{1},x_{2})>\epsilon_{0}$. Ebenso existiert ein
      $x_{3}\in A$, für das $d(x_{1},x_{3})>\epsilon_{0}$ und
      $d(x_{2},x_{3})>\epsilon_{0}$. Dies lässt sich beliebig fortsetzen zu
      der Folge $(x_{j})_{j}$.

      \help{Warum existiert $x_{3}$?}

     \item["`$\Leftarrow$"'] Es sei $M\subset A$ unendliche Menge.

      Für jedes $\epsilon$ wird $M$ durch endlich viele
      $\epsilon$-Kugeln überdeckt $\Longrightarrow$ Für jedes
      $\epsilon$ existiert eine $\epsilon$-Kugel, die undendlich
      viele Elemente aus $M$ enthält

      $\epsilon=1$ $\Longrightarrow$ $\exists y_{1}\exists$ Folge
      $(x_{j}^{1})_{j}\subset M\colon x_{j}^{1}\ne x_{k}^{1} (j\ne k)$ und
      $(x_{j}^{1})_{j}\subset K_{1}(y_{1})$

      $\epsilon=\frac12$ $\Longrightarrow$ $\exists y_{2}\exists$
      Teilfolge $(x_{j}^{2})_{j}\subset (x_{j}^{1})_{j}\colon
      (x_{j}^{2})_{j}\subset K_{\frac12}(y_{2})$

      \ldots

      $\epsilon=\frac1n$ $\Longrightarrow$ $\exists y_{n}\exists$
      Teilfolge $(x_{j}^{n})_{j}\subset (x_{j}^{n-1})_{j}\colon
      (x_{j}^{n})_{j}\subset K_{\frac1n}(y_{n})$

      Betrachten die Diagonalfolge $(x_{j}^{j})_{j\in \N}\colon j\geq n
      \Longrightarrow x_{j}^{j}\in K_{\frac1n}(y_{n})$

      Für $j,l\geq n$ $\Longrightarrow$ $d(x_{j}^{j},x_{l}^{l})\leq
      d(x_{j}^{j}, y_{n}) + d(x_{l}^{l},y_{n}) < \frac2n$
      $\Longrightarrow$ $(x_{j}^{j})$ ist Cauchyfolge und Teilfolge von
      $(x_{j}^{1})_{j}\subset M$

      X ist vollständig $\Longrightarrow$ $\exists x\in X\colon
      x_{j}^{j}\rightarrow x \Longrightarrow x$ ist HP von $M$
      $\Longrightarrow$ $A$ ist präkompakt.
    \end{description}
  \end{proof}
\end{satz}

\begin{folger}\label{fol:1.1.3/3}
  $(X, d)$ vollst. metr. Raum.

  Dann: $A\subset X$ präkompakt $\Longleftrightarrow$ $\forall
  \epsilon>0$ existiert ein präkompaktes $\epsilon$-Netz für $A$

  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] $A$ präkompakt $\Longrightarrow$ $\exists$
      endliches $\epsilon$-Netz $B_{\epsilon}$: $B_{\epsilon}$
      präkompakt, da endlich.
     \item["`$\Leftarrow$"'] $\exists$ präkompaktes $\epsilon$-Netz
      $B_{\epsilon}$ für $A$.

      \autoref{satz:1.1.3/2} $\Longrightarrow$ $\exists$ endliches
      $\epsilon$-Netz $N_{\epsilon}$ für $B_{\epsilon}$.
      $N_{\epsilon}$ ist $2\epsilon$-Netz für $A$ nach
      Dreickecksungl.
    \end{description}
  \end{proof}
\end{folger}

\begin{bsp}
  Daraus, dass $l_{1}(\N)$ beschränk ist, folgt \emph{nicht}, dass
  $l_{1}(\N)$ präkompakt ist.

  $e^{j}$ sei eine Folge, in der alle bis auf das $j$.~Glied null sind. Das
  $j$.~Glied ist eins. Eine Folge~$(a_{j})_{j}$ dieser Folgen $e^{j}$ ist
  Teilfolge des $l_{1}$.
  \begin{gather*}
    e^{j} = (\underbrace{0,\ldots,0}_{j-1\text{~mal}}, 1,0,\ldots) \text{~mit~} j\in \N\\
    (a_{j})_{j} = ( e^{1}, e^{2}, \dotsc ) \subset l_{1}(\N)
  \end{gather*}

  Für zwei Folgen $e^{j}$ und $e^{k}$ mit $j\ne k$ ist der Abstand $d(e^{j},
  e^{k}) = \norm{e^{j}-e^{k}}_{1}=2$. Die Folge~$(a_{j})_{j}$ ist also eine
  nichtkonvergente Teilfolge von $l_{1}(\N)$ und damit ist der $l_{1}$ nach
  \autoref{def:praekompakt} nicht präkompakt.
\end{bsp}

\begin{bsp}\label{bsp:1.1.3/2}
  $A\subset l_{1}(\N)$ präkompakt $\Longleftrightarrow$ $A$ beschränkt
  und
  \begin{gather}\label{eq:4}
    \sup_{a\in A}\sum_{j=n}^{\infty}\abs{a_{j}}
       \xrightarrow{n\rightarrow\infty} 0
  \end{gather}

  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] $A$ präkompakt $\Longrightarrow$ $A$
      beschränkt

      $\epsilon>0$ $\Longrightarrow$ $\exists$ endliches
      $\epsilon$-Netz $\{b^{1},\ldots,b^{m}\}\subset l_{1}(\N)$

      Es ist
      \begin{gather*}
        \sum_{j=n}^{\infty}\abs{b_{j}^{k}}\leq \epsilon\qquad \text{für}
           n\geq n(\epsilon) \text{ unabhängig von } k\\
        a\in A, n\geq n(\epsilon) \Longrightarrow
           \sum_{j=n}^{\infty}\abs{a_{j}} \leq
           \sum_{j=n}^{\infty}\abs{a_{j}-b_{j}^{k}} +
           \sum_{j=n}^{\infty}\abs{b_{j}^{k}} \leq
           \underbrace{\norm{a-b^{k}}_{1}}_{<\epsilon \text{falls $k$
           passend}} + \epsilon < 2\epsilon
      \end{gather*}
     \item["`$\Leftarrow$"'] Es sei $(a^{k})_{k}\subset A,
      a^{k}=(a^{k}_{1},a^{k}_{2},\ldots)$

      $(a^{k})_{k}$ beschränkt in $l_{1}(\N)$ $\Longrightarrow$
      $(a_{j}^{k})_{k}$ ist beschränkt in $\K$ für jedes $j\in\N$.
      \begin{enumerate}
       \item Wählen Teilfolge $(a^{1,k})_{k}\subset (a^{k})_{k}\colon
        a^{1,k}_{1}\xrightarrow{k\rightarrow\infty} a_{1}$
       \item Wählen Teilfolge $(a^{2,k})_{k}\subset (a^{1,k})_{k}\colon
        a^{2,k}_{2}\xrightarrow{k\rightarrow\infty} a_{2}$

        \ldots
       \item wählen Teilfolge $(a^{l,k})_{k}\subset (a^{l-1,k})_{k}\colon
        a^{l,k}_{l} \xrightarrow{k\rightarrow\infty} a_{l}$

        \ldots
      \end{enumerate}

      Betrachten Diagonalfolge $(b^{k})_{k} = (a^{k,k})_{k}\subset A$:

      \begin{align*}
        l \text{ fest} \Longrightarrow &(b^{k})_{k=l}^{\infty} =
           (a^{k,k})_{k=l}^{\infty}\\
         & (b^{k}_{l})_{k=l}^{\infty} =
           (a^{k,k}_{l})_{k=l}^{\infty}\subset
           (a^{l,k}_{l})_{k=l}^{\infty} \Rightarrow b_{l}^{k}
           \xrightarrow{k\rightarrow\infty} a_{l}\\
        \Longrightarrow& (b_{l}^{k})_{k} \text{ ist konvergent }\qquad\forall l\in\N\\
        \Longrightarrow&
           \sum_{l=1}^{n}\abs{b_{l}^{k}-a_{l}}\qquad\text{konvergent für}
           k\rightarrow\infty\\
        \intertext{wählen $n$ so dass \autoref{eq:4} gilt}\\
        \Longrightarrow& \sum_{l=1}^{\infty}\abs{b_{l}^{k}-b_{l}^{j}} =
           \underbrace{\sum_{l=1}^{n}\abs{b_{l}^{k}-
             b_{l}^{j}}}_{<\epsilon \text{für} j,k>j_{0}(\epsilon)} +
            \underbrace{\sum_{l=n+1}^{\infty}\abs{b_{l}^{k}-
              b_{l}^{j}}}_{<2\epsilon}\\
        \Longrightarrow&  (b^{k})_{k} \text{Cauchyfolge in $l_{1}(\N)$}\\
        \Longrightarrow& \text{Konvergenz}
      \end{align*}
    \end{description}
  \end{proof}
\end{bsp}

\subsection{Endlich-dimensionale Vektorräume}

\begin{bsp}
  \begin{itemize}
   \item $\K^{n} (\K=\R, \K=\C)$
   \item $\mathcal{P}^{n} := \{ p(x)\colon p(x) \text{Polynom vom Grad} \leq
    n\}$

    $\{1,x,\ldots,x^{n}\}$ Basis, $\dim \mathcal{P}=n+1$
   \item $\mathcal{T}^{n} := \{ t(x)\colon t(x)=\sum_{k=-n}^{n} c_{k} e^{ikx},
    c_{k}\in \C\}$

    $\{e^{ikx}\colon k=-n,\ldots,n\}$ Basis, $\dim\mathcal{T}^{n}=2n+1$
  \end{itemize}

  \underline{Lineare Algebra:} $(E,\K) \dim E=n \Longrightarrow E\cong
  \K^{n}$ isomorph
\end{bsp}

\begin{defini}
  $E$ Vektorraum

  Zwei Normen $\norm{\cdot}$ und $\norm{\cdot}^{\ast}$ heißen äquivalent
  $:\Longleftrightarrow$ es zwei Zahlen $c_{1},c_{2}>0$ gibt, so dass für
  alle $x\in E$ gilt:
  \begin{gather*}
    c_{1}\norm{x}^{\ast}\leq \norm{x}\leq c_{2}\norm{x}^{\ast}
  \end{gather*}
\end{defini}

\begin{bemerk}
  Äquivalente Normen erzeugen gleiche Konvergenz und gleiche Topologien,
  d.\,h. $M$ ist offen bzgl. $\norm{\cdot} \Longleftrightarrow M$ ist offen
  bzgl. $\norm{\cdot}^{\ast}$
\end{bemerk}

\begin{lemma}
  $E$ normierter Vektorraum über $\K$, $\{x_{1},\ldots,x_{n}\}$ linear unabhängig

  Dann existiert ein $c>0$, so daß für alle
  $a=(a_{1},\ldots,a_{n})\in\K^{n}$ gilt:
  \begin{gather*}
    \norm{a}_{1} = \abs{a_{1}}+\ldots+\abs{a_{n}} \leq c\:\norm{a_{1}x_{1}+\ldots+
       a_{n}x_{n}}_{E}
  \end{gather*}

  \begin{proof}
    Man betrachte die Funktion $\Phi\colon\K^{n}\rightarrow E$ mit $\Phi(a) :=
    a_{1}x_{1}+ \ldots + a_{n}x_{n}$. $\Phi$ ist linear und stetig, da
    \begin{gather*}
      \norm{\Phi(a)}_{E} \leq \sum_{j=1}^{n}\abs{a_{j}}\
      \max_{k}\bigl\{\norm{x_{k}}_{E}\bigr\} = c_{0}\norm{a}_{1}\\
      \norm{a_{i}-a}_{1}\rightarrow 0 \Longrightarrow
             \norm{\Phi(a_{i})-\Phi(a)}_{E}\rightarrow 0
    \end{gather*}

    % \begin{align*}
    %   \Longrightarrow& \Phi \text{ist linear und stetig, denn}\\
    %   & \norm{\Phi(a)}_{E}\leq
    %          \sum_{j=1}^{n}\abs{a_{j}}\ \max_{k}\bigl\{\norm{x_{k}}_{E}\bigr\} =
    %          c_{0}\norm{a}_{1}\\
    %   & \norm{a_{i}-a}_{1}\rightarrow 0 \Longrightarrow
    %          \norm{\Phi(a_{i})-\Phi(a)}_{E}\rightarrow 0
    % \end{align*}

    Es sei $A=\{ a\in \K^{n}\colon \norm{a}_{1}=1\}$ kompakt im $\K^{n}$.
    Aus der Stetigkeit von $\Phi$ folgt nach \autoref{bem:1}, dass $\Phi(A)
    = \{\Phi(a)\colon \norm{a}_{1}=1\}\subset E$ ebenfalls kompakt ist.

    Die Norm $\norm{\cdot}_{E}\colon E\rightarrow \R$ wiederrum ist eine stetige
    Abbildung, woraus folgt, dass das $\min\Phi(A) = \min\{\norm{\Phi(a)}\colon
    \norm{a}_{1}=1\}$ existiert und $>0$ sein muss.
    \begin{gather}\label{eq:5}
       0 < \tilde{c} := \min\Phi(A) = \min\{\norm{\Phi(a)}\colon \norm{a}_{1}=1\}
    \end{gather}
    Denn aus $\norm{a_{1}x_{1} + \ldots + a_{n}x_{n}}_{E}=0$ folgt, dass
    $a=0$ ist, da die $x_{1},\ldots,x_{n}$ linear unabhängig sind. $a=0$
    ist aber ein Widerspruch zu $\norm{a}_{1}=1$.

    Für alle $a\in\K^{n}$ mit $\norm{a}\ne0$ gilt:
    \begin{align*}
      \norm{a_{1}x_{1}+\ldots+a_{n}x_{n}}_{E} &= \norm{a}_{1}
      \norm[\bigg]{\frac{a_{1}}{\norm{a}_{1}}x_{1}+\ldots+
      \frac{a_{n}}{\norm{a}_{1}}x_{n}}_{E}\\
      &\geq \norm{a}_{1}\tilde{c}&&\text{nach \autoref{eq:5}}\\[3ex]
         \norm{a_{1}} &\leq \frac1{\tilde{c}} \norm{a_{1}x_{1} + \ldots +
         a_{n}x_{n}}
    \end{align*}
  \end{proof}
\end{lemma}

\begin{satz}
  $E$ Vektorraum über $\K$ der Dimension $n$.

  Dann gilt:
  \begin{enumerate}
   \item sämtliche Normen auf $E$ sind zueinander äquivalent.
   \item $E$ ist isometrisch isomorph zum Raum $\K^{n}$ (bei geeigneter
    Normierung)
   \item $E$ ist vollständig und separabel
  \end{enumerate}

  \begin{proof}
    \begin{schritte}
     \item $(E, \norm{\cdot})$ $\dim E =n$, Basis: $\{x_{1},\ldots,x_{n}\}$

      Definieren: $\norm{x}:= \sum_{j=1}^{n}\abs{a_{j}}$ falls: $x=
      \sum_{j=1}^{n}a_{j}x_{j}$

      $\norm{\cdot}$ ist Norm auf $E$, $\norm{x}_{E}= \norm{a_{1}x_{1}+\ldots+
      a_{n}x_{n}}_{E} \leq \norm{x}\underbrace{\max_{j}\norm{x_{j}}_{E}}_{c_{2}}$

      Andererseits nach Lemma: $\norm{x}\leq c \norm{x}_{E} \Longrightarrow
      \norm{\cdot}$ und $\norm{\cdot}_{E}$ sind äquivalent.
     \item Betrachten $(E, \norm{\cdot})$ mit $\norm{\cdot}$ wie im 1.\,Schritt
      und $(\K^{n}, \norm{\cdot}_{1})$

      setzen: $Lx := a =(a_{1},\ldots,a_{n})$, falls x=$a_{1}
      x_{1}+\ldots+ a_{n}x_{n} (L=\Phi^{-1})$

      $\Longrightarrow$ $L\colon E\rightarrow\K^{n}$ surjektiv

      $\norm{Lx}_{1}= \norm{x}$ isometrisch

      $\Longrightarrow$ L:E$\rightarrow$ $\K^{n}$ isometrisch isomorph
     \item $\K^{n}$ vollständig $\Longrightarrow$ $L^{-1}(\K^{n})=E$
      vollständig

      $\{x\colon x=\alpha_{1}x_{1}+\ldots+ \alpha_{n}x_{n}, \alpha_{j}\in\Q\}$
      abzählbar und dicht in $E$
    \end{schritte}
  \end{proof}
\end{satz}

\begin{satz}
  $(E, \norm{\cdot})$ normierter Vektorraum.

  Dann sind folgende Aussagen äquivalent:
  \begin{enumerate}
   \item $E$ endlichdimensional
   \item jede beschränkte Mende ist präkompakt
   \item die abgeschlossene Einheitskugel $B=\{x\in E\colon \norm{x}\leq 1\}$ in
    $E$ ist kompakt.
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] (Satz 1) klar
     \item[$(2)\Rightarrow(3)$] nach Def. der Kompaktheit
     \item[$(3)\Rightarrow(1)$] Es existieren
      $\{x_{1},\ldots,x_{m}\} \subset B$ mit $\bigcup_{i=1}^{m}
      K_{\frac12}(x_{i})\supset B$ nach \autoref{satz:heineborel}

      Setzen $F := \Span \{x_{1},\ldots,x_{n}\}$ $\Rightarrow$ $F\subset
      E$ endlichdimensionaler abgeschlossener Teilraum. Wir setzen: $F=E$

      Ann.: $\exists x\in E\wedge x\notin F$

      Es gilt:
      \begin{gather*}
        d := d(x,F) := \inf_{y\in F}\norm{x-y}>0\qquad\text{da $F$ abgeschlossen}\\
        \text{wählen } y\in F\colon d\leq \norm{x-y}\leq\frac{3}{2}d
           \taglabel{eq:6}\\
        \text{Für } z := \frac{x-y}{\norm{x-y}}\in B \exists x_{i}\colon
           \norm{z-x_{i}}<\frac12\\
        \Longrightarrow x=y+\norm{x-y}z = \underbrace{y +
           \norm{x-y}x_{i}}_{=\tilde{y}\in F} +\norm{x-y}(z-x_{i})\\
        d \leq \norm{x-\tilde{y}}= \norm{x-y}
           \underbrace{\norm{z-x_{i}}}_{<\frac12} \leq \frac12 \norm{x-y}\\
        \Longrightarrow \norm{x-y}\geq 2d\qquad\text{Widerspruch zur
           \autoref{eq:6}}
      \end{gather*}
    \end{description}
  \end{proof}
\end{satz}

