\begin{satz}[Wärmeleitungsgleichung] % 3
  \begin{gather*}
    \frac{\partial u}{\partial t} - \Delta u = f(x,t)
  \end{gather*}

  \begin{gather*}
    G(x,t) =
       \begin{cases}
         \frac{1}{(4\pi t)^{\frac{n}{2}}} \ez^{-\frac{\abs{x}^{2}}{4t}}&
         x\in\R^{n}, t>0\\
         0& x\in \R^{n}, t\leq 0
       \end{cases}
  \end{gather*}
  ist Fundamentallösung für die \highl{Wärmeleitungsgleichung}
  ($\frac{\partial G}{\partial t}- \Delta G = \delta$ in $S'(\R^{n+1})$)
