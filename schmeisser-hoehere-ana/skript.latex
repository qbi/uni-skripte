\subsection{PDGL -- Fouriersche Methode} % 5.4.2

\subsubsection{Laplace-Poisson-Gleichung}
\begin{gather*}
  \Delta u =f\qquad(f\in S(\R^{3}), u \in S'(\R^{3}))\\
  \Leftrightarrow -\abs{\xi}^{2} Fu = Ff
\end{gather*}
Problem: Multiplikation mit $\abs{\xi}^{-2}$ nicht erlaubt in $S'(\R^{3})$!

Ansatz: wir setzen
\begin{gather*}
  (Fv)(\xi) := \frac{Ff(\xi)}{\abs{\xi}^{2}} \in
     L_{1}(\R^{3})\qquad(\Rightarrow v\in C_{0}(\R^{3}))\\
  \abs{\xi}^{2}(F(u+v)) = 0\qquad(\text{in}\ S'(\R^{3}))
\end{gather*}

Daraus folgt:
\begin{gather}
  \label{eq:42} \Delta(u+v) = 0\\
  \label{eq:43} \supp F(u+v) \subset \{0\}
\end{gather}

Nach einem Satz\todo{link: welcher?} folgt daraus, dass $u+v = p(x)$
Polynom und $\Delta p(x)=0$, d.\,h. $u+v$ ist harmonisches Polynom

Es gilt:
\begin{align*}
  v &= F^{-1}(\frac{1}{\abs{\xi}^{2}} Ff) = F^{-1}(F
     F^{-1}(\frac{1}{\abs{\xi}^{2}}) (Ff)(\xi))\\
  &= (2\pi)^{-\frac{3}{2}} (F^{-1} \frac{1}{\abs{\xi}^{2}})\ast f
\end{align*}

\begin{lemma}
  (siehe Übung)
  \begin{gather*}
    (F^{-1} \frac{1}{\abs{\xi}^{2}})(x) = \sqrt{\frac{\pi}{2}}
       \frac{1}{\abs{x}}
  \end{gather*}
\end{lemma}

\begin{satz}
  sämtliche Lösungen von $\Delta u=f$ aus $S'(\R^{3})$ haben die Form
  \begin{gather*}
    u(x) = -v+p = - (2\pi)^{-\frac{3}{2}} \sqrt{\frac{\pi}{2}}
       (\frac{1}{\abs{y}} \ast f) (x) + p(x) = -\frac{1}{4\pi}
       \int_{\R^{3}} \frac{f(y)}{\abs{x-y}}\,dy + p(x)
  \end{gather*}
\end{satz}

\subsubsection{Die Wellengleichung im $\R^{3}$ (Cauchyproblem)}
\index{Cauchyproblem}

\begin{gather}
  \label{eq:44} \frac{\partial^{2} u}{\partial t^{2}} -\Delta u =0\\
  \notag u(x,0)=g(x), \frac{\partial u}{\partial t}(x,0) = f(x)
\end{gather}

\underline{Vorraussetzungen:} $g,f\in D(\R^{3})$, wir setzen
$\hat{u}(\xi,t) := \bigl(F_{x}u(x,t)\bigr)(\xi)$ mit $t$ fest

\autoref{eq:44} $\Leftrightarrow$
\begin{gather*}
   \frac{\partial^{2}}{\partial t^{2}}
     \hat{u}(\xi,t) = -\abs{\xi}^{2} \hat{u}(\xi,t)\\
  \hat{u}(\xi, 0) = \hat{g}(\xi), \frac{\partial}{\partial t}\hat{u}
     (\xi,0) = \hat{f}(\xi)
\end{gather*}
ist ein Anfangswertproblem für gewöhnliche Differentialgleichungen
zweiter Ordnung (bei festem $\xi$!)

$\Rightarrow$
\begin{align*}
  \hat{u}(\xi,t) &= \cos(t\abs{\xi}) \hat{g}(\xi) + \frac{\sin
     t\abs{\xi}}{\abs{\xi}} \hat{f}(\xi)\\
  &= \frac{\partial}{\partial t} (\frac{\sin t\abs{\xi}}{\abs{\xi}})
     \hat{g}(\xi) + \frac{\sin t\abs{\xi}}{\abs{\xi}} \hat{f}(\xi)
\end{align*}

$F_{\xi}^{-1}$, $t$ fest $\Rightarrow$
\begin{gather*}
  u(x,t) = \frac{\partial}{\partial t} (2\pi)^{-\frac{3}{2}} (F_{\xi}^{-1}
     \frac{\sin t\abs{\xi}}{\abs{\xi}})\ast g(x) + (2\pi)^{-\frac{3}{2}}
     (F^{-1}_{\xi} \frac{\sin t\abs{\xi}}{\abs{\xi}}) \ast f(x)
\end{gather*}

\begin{lemma}
  \begin{gather*}
    F^{-1}(\frac{\sin t\abs{\xi}}{\abs{\xi}})(\phi) =
       \sqrt{\frac{\pi}{2}} \frac{1}{t} \int_{\abs{y}=t} \phi\,dS_{y} =
       T(\phi)
  \end{gather*}
  (siehe Übung $t=1$)
\end{lemma}

$g\in S(\R^{3})$ $\Rightarrow$
\begin{align*}
  (T\ast g)(x) &= T_{y}(g(x-y))\\
  &= \sqrt{\frac{\pi}{2}} \frac{1}{t} \int_{\abs{y-x}=t}
     g(y)\,dS_{y}
\end{align*}

\begin{satz}[Lösung von \eqref{eq:44}]
  \begin{gather*}
    u(x,t) = \frac{1}{4\pi t} \int_{\abs{y-x}=t} f(y)\,dS_{Y} +
       \frac{\partial}{\partial t}(\frac{1}{4\pi t} \int_{\abs{y-x}=t}
       g(y)\,dS_{y})
  \end{gather*}
\end{satz}

