  \begin{proof}
    $\sum_{k\ne0} \frac{a_{k}}{{(ik)^{m+2}}}\ez^{ikx}$ ist gleichmäßig
    konvergent in $\R$

    nach \autoref{bem:6} $\Rightarrow$ $D^{m+2}$
    gliedweise:
    \begin{gather*}
      \sum_{k\ne0} D^{m+2} \bigl(\frac{a_{k}}{(ik)^{m+2}}\ez^{ikx}\bigr)
      = \sum_{k\ne0} a_{k}\ez^{ikx} konvergent in D' =_{D'} D^{m+2}f
    \end{gather*}
  \end{proof}
\end{bemerk}

\begin{satz}\label{satz:9}
  $-\infty\leq a<b\leq\infty, k\in\N$

  $T\in D'\bigl((a,b)\bigr)$ mit $D^{(k)}T=0$ $\Longrightarrow$ $T$ ist
  ein Polynom vom Grad $\leq k-1$.

  \begin{proof}
    \begin{description}
     \item[$k=1$:] Es sei $T'=0$ und weiterhin $\omega(\tau)\in
      D\bigl((a,b)\bigr)$ mit $\int_{\omega}\omega(\tau)\,d\tau =1$.

      $\phi\in D(\Omega)$. Setzen
      \begin{gather*}
        \psi(t) := \int_{a}^{t} \Bigl(\phi(\tau)-\omega(\tau)\int_{a}^{b}
        \phi(s)\,ds\Bigr)\,d\tau \in D\bigl((a,b)\bigr) \text{\help{da war eine
        Begründung für $\in$}}
      \end{gather*}
      $\Rightarrow$ $0=T'(\psi) = -T(\psi) =
      -T(\phi(t)-\omega(t)\int_{a}^{b}\phi(s)\,ds) = -T(\phi) +
      \underbrace{T(\omega)}_{=:c}\int_{a}^{b}\phi(s)\,ds$
      $\Longrightarrow$ $T(\phi) = \int_{a}^{b}c\phi(s)\,ds \Rightarrow T=c$
     \item[$k\rightarrow k+1$:]
      Es sei $T^{(k+1)} =0$ $\Longrightarrow$ $(T')^{(k)} =0$
      $\Rightarrow$ $T' = q_{k-1}(t)$ ist ein Polynom vom Grad $q\leq k-1$.

      Es sei $q_{k}' = q_{k-1}$ ein Polynom vom Grad $q_{k}\leq k$
      $\Rightarrow$ $(T-q_{k})' = 0$ $\Longrightarrow^{k=1}$ $T-q_{k} =
      c$ $\Rightarrow$ $T=p_{k}(t)$
    \end{description}
  \end{proof}
\end{satz}

\subsection{Distributionen mit kompakten Träger}

\begin{lemma}\label{lem:6}
  Eine beliebige Distribution $T\in D'(\Omega)$ kann auch wie folgt
  charakterisiert werden:
  \begin{enumerate}
   \item $T:D(\Omega) \rightarrow\C$ linear und
   \item Für alle kompakten Teilmengen $\mathcal{K}\subset\Omega$ muss ein $c>0$
    und ein $m\in\N_{0}$ existieren, so dass für alle $\phi\in D(\Omega)$
    mit $\supp \phi\subset\mathcal{K}$ gilt:
    \begin{gather}\label{eq:33}
      \abs{T(\phi)} \leq c \norm{\phi}_{\mathcal{K},m} = c
      \supp_{x\in\mathcal{K}} \sum_{\abs{\alpha}\leq m}
      \abs{D^{\alpha}\phi(x)}
    \end{gather}
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item["`$\Leftarrow$"'] klar, denn\\
      $\phi_{j} \xrightarrow[D]{}\phi \Rightarrow \exists\mathcal{K}
      \supp\phi_{j}\subset\mathcal{K}, \supp\phi\in\mathcal{K}$

      $\forall m: \norm{\phi_{j}-\phi}_{\mathcal{K},m} \rightarrow0$

      $\Longrightarrow^{\text{\autoref{eq:33}}}$ $\abs{T(\phi_{j}-\phi)}\leq c
      \norm{\phi_{j}-\phi}_{\mathcal{K},m} \rightarrow0$
     \item["`$\Rightarrow$"'] (indirekt). Annahme \autoref{eq:33} gilt
      nicht. Dann würde ein $\mathcal{K}\subset\Omega$ kompakt
      existieren, so dass $\forall l\in\N \exists\phi_{l}\in D(\Omega)$
      mit $\supp \phi_{l}\subset\mathcal{K}$ und
      $\abs{T(\phi_{l})}>l\norm{\phi_{l}}_{\mathcal{K},l} \forall l\in\N$

      setze: $\psi_{l}(x):= \frac{\phi_{l}(x)}{\abs{T(\phi_{l})}}$
      $\Rightarrow$ $\norm{\psi_{l}}_{\mathcal{K},l} < \frac{1}{l}
      \forall l \Longrightarrow^{l\rightarrow\infty} \psi_{l}
      \xrightarrow[D]{} 0$, da $\norm{\psi_{l}}_{\mathcal{K},l} \leq^{l\geq
      m} \norm{\psi}_{\mathcal{K},l} \xrightarrow{l\rightarrow\infty} 0$

      $\Longrightarrow$ $T(\phi_{l}) \xrightarrow{l\rightarrow\infty} 0$
      $\Rightarrow$ Widerspruch zu $\abs{T(\phi_{l})}=1$
    \end{description}
  \end{proof}
\end{lemma}

\begin{lemma}\label{lem:5}
  $T\in D'(\Omega), \supp T=\mathcal{K}\subset\Omega$ kompakt.

  Es sei $\eta\in D(\Omega)$ mit $\eta(x)=1$ auf einer Umgebung von
  $\mathcal{K}$

  $\Longrightarrow$ $\forall\phi\in D(\Omega)$ gilt: $T(\phi) =
  T(\eta\cdot\phi)$ (d.\,h. $T=\eta T$)

  Nur in der Umgebung passiert etwas und wenn ich das abschneide,
  passiert nichts mehr.\help{hat er das so gesagt?}\todo{bildchen}

  \begin{proof}
    $T\big|_{\Omega-\mathcal{K}}=0$ Einschränkung von $T$ auf
    $\Omega-\mathcal{K}$\\
    $\supp(\phi-\eta\phi)\subset\Omega-\mathcal{K}$

    Aus beiden folgt $T(\phi-\eta\phi) = 0$ $\Rightarrow$ $T(\phi) =
    T(\eta\phi)$
  \end{proof}
\end{lemma}

\begin{satz}\label{satz:6}
  $T\in D'(\Omega), \supp T\subset \Omega$ kompakt. Dann $\exists c>0
  \exists m\subset\N_{0}$, so dass
  \begin{gather*}
    \abs{T(\phi)} \leq c \sup_{x\in\Omega} \sum_{\abs{\alpha}\leq m}
    \abs{D^{\alpha}\phi(x)}\qquad\forall\phi\in D(\Omega)
  \end{gather*}

  im Gegensatz zu \autoref{lem:6} hängen die Konstanten nicht von $\mathcal{K}$ ab.
  \begin{proof}
    $\supp T=\mathcal{K}\subset\Omega$ kompakt. Es sei $\eta(x)$ wie in
    \autoref{lem:5} mit $\supp \eta = U$. $\mathcal{K}$ ist so
    beschaffen, dass $\mathcal{K}\subset U^{0}\subset U\subset\Omega$.

    Nach \autoref{lem:6} mit $U$ anstelle von $\mathcal{K}$
    $\Rightarrow$ $\exists c\exists m\in\N_{0}$ mit $\abs{T(\psi)}\leq
    c\sup \sum_{\abs{\alpha}\leq m} \abs{D^{\alpha}\psi(x)}$
    $\forall\psi\in D(\Omega)$ mit $\supp \psi\subset U$

    Wenn $\phi$ ein beliebiges Element von $D(\Omega)$ ist, dann ist
    natürlich der Träger $\supp \eta\phi\subset U$

    $\Longrightarrow$ mit \autoref{lem:5}
    \begin{gather*}
      \abs{T(\phi)} = \abs{T(\eta\phi)} \leq c\sup_{x\in\Omega}
      \sum_{\abs{\alpha}\leq m} \abs{D^{\alpha}(\eta\phi)(x)} \leq^{(+)}
      c' \sup_{x\in\Omega} \sum_{\abs{\alpha}\leq m} \abs{D^{\alpha}\phi(x)}
    \end{gather*}
    (+) Alle Ableitungen von $\eta$ lassen sich nach oben abschätzen, da
    $\eta$ stetig und beschränkt ist; Ableitungen von $\eta\phi$ mit
    Produktregel sind endlich viele Glieder
  \end{proof}
\end{satz}

\begin{satz}
  Es sei $T\in D'(\Omega), \supp T=\{a\}, a\in\Omega$
  ("`\highl{Punktträger}"')

%  Dann können wir zeigen, dass
  $\Rightarrow$ $\exists m\in\N_{0}\exists c_{\alpha}\in\C,
  \abs{\alpha}\leq m$, so dass $T= \sum_{\abs{\alpha}\leq m} c_{\alpha}
