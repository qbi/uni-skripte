\begin{satz}\label{satz:1.4.5/5}
  $\Omega\subset \R^{n}$ offen, $1 \leq p \leq \infty$
  \begin{enumerate}
   \item Die Menge der finiten Treppenfunktionen (kompakter Träger in
    $\Omega$) ist dicht in $L_{p}(\Omega)$.
   \item Die Menge $C_{0}^{\infty}(\Omega)$ liegt dicht in $L_{p}(\Omega)$.
   \item $L_{p}(\Omega)$ ist separabel.
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}[zu~(1)]
     \item
      $\Omega = \bigcup_{j=1}^{\infty} \Omega_{j}$ mit $\Omega_{j} = \{
      x\in \Omega: |x|\leq j \wedge d(x,\partial\Omega) > \frac1j \}$
      $\Longrightarrow$ $\overline{\Omega_{j}}\subset \Omega_{j+1}$
      $f\in L_{p}(\Omega) \Longrightarrow \lim_{j\rightarrow\infty}
      \int_{\Omega\setminus \Omega_{j}} |f(x)|^{p}\, dx =0$
      $\Longrightarrow$ $\chi_{\Omega_{j}}
      f\xrightarrow{j\rightarrow\infty} f$ in $L_{p}(\Omega)$
      
      Nach \thref{satz:17} $\Longrightarrow$ $\chi_{\Omega_{j}}f$ kann
      durch Treppenfunktionen in $L_{p}(\Omega_{j})$ approximiert werden.
      haben Träger in $\overline{\Omega_{j}}$

     \item Nach (1) ausreichend: Approximation von
      Treppenfunktionen $s(x)$ mit $\supp s\subseteq\Omega$ kompakt
      \begin{equation*}
	s(x) = \sum_{j=1}^{r} c_{j}\chi_{A_{j}}\qquad A_{j}\text{ messbar},
	   c_{j}\in \K, \supp A_{j} \subset \supp s
      \end{equation*}
      endliche Summe $\Longrightarrow$ ausreichend Approximation von
      $\chi_{A}$ mit $A$ messbar, $A\subset K \subset \Omega$ kompakt.
      
      Nach \thref{satz:1.3.1/3} existiert eine offene Menge
      $O_{\epsilon}$ mit $A\subset O_{\epsilon} \subset
      \overline{O_{\epsilon}} \subset \Omega$ kompakt und
      $|O_{\epsilon}\setminus A| < \epsilon\ (\forall \epsilon)$. Jede
      offene Menge ist abzählbare Vereinigung offener Würfel.
      $\Longrightarrow$ ausreichend die Approximation von $\chi_{Q}$,
      wobei $Q$ offener Würfel mit $\overline{Q}\subset \Omega$.

      Nach \thref{satz:1.2.3/2} existiert $\phi_{k}\in C_{0}^{\infty}(\R^{n})$
      mit $\phi_{k}(x) = 1$ auf $\overline{Q}, 0\leq \phi_{k}\leq 1,
      \supp \phi_{k} \subset \overline{Q_{k}} \subset \Omega, k\geq
      K_{0}$ wobei $\overline{Q_{k}} 0 \{ x: d(x, \overline{Q}) \leq
      \frac{1}{k} \} \subset \Omega$
      
      $\Longrightarrow$ $\int_{\Omega}|\chi_{Q}- \phi_{k}|^{p}\,dx =
      \int_{\overline{Q_{k}}\setminus Q} \phi_{k}(x)^{p}\,dx \leq
      |\overline{Q_{k}}\setminus Q| = c \frac1k
      \xrightarrow{k\rightarrow\infty}0$

     \item Treppenfunktionen $\sum_{i=1}^{r} c_{j} \chi_{Q_{i}},
      Q_{j}$ Quader in $\Omega$, es ist also außreichend zu zeigen: $c_{j} = \alpha_{j}+ i
      \beta_{j}\quad(\alpha_{j}, \beta_{j} \text{rational})$ Ecken von
      $Q_{j}$ rationale Koordinaten
      
      $\exists \infty$-viele l.u. Treppenfunktionen in $L_{p}(\Omega)$
    \end{enumerate}
  \end{proof}
\end{satz}

