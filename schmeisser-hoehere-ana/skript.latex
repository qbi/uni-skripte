\begin{description}
 \item[\textbf{Wiederholung!}]
  $\Omega\subset\R^{n}$ offen\\
  $D(\Omega) = C_{0}^{\infty}(\Omega)$ mit $\phi_{j}\xrightarrow[D]{}\phi$
  $\Longleftrightarrow$ $\exists K\subset\Omega$ kompakt mit $\supp
  \phi_{j}\subset K, \supp \phi\subset K$ und
  $\norm{\phi_{j}-\phi}_{K,m}\rightarrow0 \forall m\in \N_{0}$\\
  $\norm{K,m} := \sup_{x\in K} \sum_{\abs{\alpha}\leq m}\abs{D^{\alpha}
  \phi(x)}$

  $D^{\alpha}(\Omega) := \{T\colon D(\Omega) \rightarrow \C \text{linear},
  \phi_{j} \xrightarrow[D]{}\phi \Rightarrow T(\phi_{j}) \rightarrow
  T(\phi)\}$\\
  $T_{j} \xrightarrow[D]{} T :\Longleftrightarrow \forall \phi\in
  D(\Omega): T_{j}(\phi) \rightarrow T(\phi)$ konvergent in $D'$

  $f\in L_{1}^{loc}(\Omega) \Rightarrow T_{f} := \int_{\Omega}
  f(x)\phi(x)\,dx$ $\Longrightarrow$ $T_{f}\in D'(\Omega)$

  $f\leftrightarrow T_{f} =f$

  Beispiel für singulär Distribution: $a\in \Omega, \delta_{a}(\phi) :=
  \phi(a)$ "`Delta-Distribution"'

  Träger einer Distribution:
  \begin{gather*}
    \supp T := \{x\colon T_{U(x)\cap\Omega}\ne0 \forall U(x)\}
  \end{gather*}
\end{description}

\subsection{Operationen in \texorpdfstring{$D'(\Omega)$}{D'(Omega)}}

\begin{defini}\label{def:5.1.3/1}
  \begin{enumerate}
   \item $\lambda_{1}, \lambda_{2}\in\C, T_{1},T_{2}\in D'(\Omega)$\\
    \begin{align*}
      (\lambda_{1}T_{1} + \lambda_{2}T_{2})(\phi) &:=
      \lambda_{1}T_{1}(\phi) + \lambda_{2}T_{2}(\phi) &&\forall\phi
      \in D(\Omega)
    \end{align*}
   \item \underline{Differentation}: $\alpha =
    (\alpha_{1},\ldots,\alpha_{n})\in\N_{0}^{n}$\\
    \begin{align*}
      (D^{\alpha}T)(\phi) &:= (-1)^{\alpha} T(D^{\alpha}\phi) &&\phi
      \in D(\Omega)
    \end{align*}
   \item \underline{Multiplikation} mit $a\in C^{\infty}(\Omega)$
    \begin{align*}
      (aT)(\phi) &:= T(a\phi)&&\forall\phi\in D(\Omega)
    \end{align*}
   \item \underline{Affine Transformationen} $A$ reguläre $n\times
    n$-Matrix, $x,b\in\R^{n}, T\in D'(\R^{n})$
    \begin{gather*}
      T(A\cdot+b)(\phi) := \frac{1}{\abs{\det A}}
      T\bigl(\phi(A^{-1}(x-b))\bigr) \help{woher kommt das x auf einmal?}
    \end{gather*}
  \end{enumerate}
\end{defini}

