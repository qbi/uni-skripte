\begin{bemerk}[Gliedweise Differentation]\label{bem:6}
  \begin{gather*}
    \sum_{k=0}^{\infty} T_{k} =_{D'} T \Longrightarrow
    \sum_{k=0}^{\infty} D^{\alpha} T_{k} =_{D'} D^{\alpha} T
  \end{gather*}

  z.\,B. $\sum_{k=0}^{\infty} f_{k}(x) = f(x)$ in $L_{p}(\Omega), 1\leq
  p\leq\infty$ $\Longrightarrow$ $\sum_{k=0}^{\infty}D^{\alpha}f =_{D'}
  D^{\alpha}f$, denn $L_{p}(\Omega)\subset D'(\Omega)$ ist eine stetige
  Einbettung
  \begin{align*}
    \abs{s_{n}(\phi)-f(\phi)} &= \abs{\int_{\Omega}
       (s_{n}-f)(x)\phi(x)\,dx}&&\phi\in D(\Omega), \supp\phi=K kompakt\\
    &\leq \norm{s_{n}-f}_{p} \underbrace{(\int_{K}
       \abs{\phi(x)}^{p'}dx)^{\frac{1}{p'}}}_{=c}
       \xrightarrow{n\rightarrow\infty} 0
  \end{align*}
\end{bemerk}

\begin{bemerk}[Trigonometrische Reihen]
