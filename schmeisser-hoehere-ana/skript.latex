\section{\texorpdfstring{$D(\Omega)\times D'(\Omega)$}{D(Omega)xD'(Omega)}}
\subsection{Definitionen und Beispiele}
$\Omega\subset\R^{n}$ Gebiet, $C_{0}^{\infty}(\Omega)$ wie in
\autoref{sec:1.2.3}

\begin{defini}[Konvergenz in $C_{0}^{\infty}(\Omega)$]
  Seien $\phi_{j}, j\in\N$ und $\phi\in C_{0}^{\infty}$ Dann konvergiert
  $\phi_{j}\rightarrow \phi$ auf $D$, falls gelten:
  \begin{enumerate}
   \item $\exists K\subset\Omega, K$ kompakt, $\supp \phi_{j}\subset K,
    \supp \phi\subset K$
   \item $\forall \alpha\in \N_{0}^{n}$ $D^{\alpha}
    \phi_{j} \xrightarrow{gleichm.} D^{\alpha} \phi$ auf $K$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{itemize}
   \item $D^{\alpha}\phi =
    \frac{\partial^{\alpha}\phi}{\partial^{\alpha_{1}} x_{1}\ldots
      \partial^{\alpha_{n}}x_{n}}, \alpha =
    (\alpha_{1},\ldots,\alpha_{n}), \abs{\alpha} = \abs{\alpha_{1}} + \dotsb
    +\abs{\alpha_{n}}$
    \help{hier fehlt was}
   \item $D(\Omega) = C_{0}^{\infty}(\Omega) + \help{hier fehlt was}$
  \end{itemize}
\end{bemerk}

\begin{bemerk}
  \begin{itemize}
   \item $T\colon D(\Omega) \rightarrow \C$
   \item $T(a_{1}\phi_{1} + a_{2}\phi_{2}) = a_{1}(T\phi_{1}) +
    a_{2}T(\phi_{2}), a_{1}, a_{2}\in \C, \phi_{1}, \phi_{2}\in D(\Omega)$
   \item $\phi_{j} \xrightarrow[D]{} \phi$ $\Longrightarrow$ $T(\phi_{j})
    \rightarrow T(\phi)$
   \item $\bigl[D(\Omega)\bigr]' = D'(\Omega)$
   \item L.\,Schwartz 1950/51
  \end{itemize}
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item reguläre Distributionen: $L_{1}^{loc}$ ($g\in L_{1}^{loc}$,
    falls $\int_{K} g(x)\,dx$ existiert für alle kompakten $K\subset
    \R^{n}$)

    $L_{1}^{loc}(\Omega) \subset D'(\Omega)$ bei geeigneter Intepretation
    von "`$\subset$"'

    $f\in L_{1}^{loc}(\Omega), \phi \in D(\Omega) \Longrightarrow
    f\phi\in L_{1}(\Omega)$
    \help{hier fehlt was}

    $T_{f}(\phi) := \int_{\Omega} f(x) \phi(x)\,dx, \phi\in D(\Omega)$

    Behauptung: $T_{f}\in D'(\Omega)$, linear klar, stetig:
    $\phi_{j} \xrightarrow[D]{}\phi\colon D^{\gamma}\phi_{j} \rightarrow
    D^{\gamma}_{\phi}$ gleichmäßig auf $K$, insbesondere:
    $\phi_{j}\xrightarrow{glm.}\phi$ $\Longrightarrow$ $\exists M>0
    \forall x\in K\forall j\in \N: \abs{\phi_{j}(x)}\leq M$
    $\Longrightarrow$ $\abs{f(x)\phi_{j}(x)} \leq
    \begin{cases}M\abs{f(x)} & x\in M\\0&\text{sonst}\end{cases}\in
    L_{1}(\Omega)$

    Lebesgue $\Longrightarrow$ $T_{f}(\phi_{j}) = \int_{\Omega} f(x)
    \phi_{j}(x)\,dx \xrightarrow{j\rightarrow\infty} \int_{\Omega} f(x)
    \phi(x)\,dx = T_{f}(\phi)$
   \item
  \end{enumerate}
\end{bsp}

