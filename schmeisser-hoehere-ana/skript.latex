  \begin{equation*}\sum_{k=1}^{\infty} |a_{k}|^{p}\,dx<\infty\end{equation*}\\
  Folgen}

\subsection{Maße}

(Überlick in H.~Triebel: Analysis und math. Physik, Kap. 13+14)

\begin{defini}
  $\Omega \not= \emptyset$ Menge. Ein System von Teilmengen $\mathcal{A}$
  von $\Omega$ heißt \highl{$\sigma$-Algebra} $:\Longleftrightarrow$
  \begin{enumerate}
   \item $\emptyset\in \mathcal{A}, \Omega\in \mathcal{A}$
   \item $A\in \mathcal{A} \Rightarrow \Omega\setminus A\in \mathcal{A}$
   \item $A_{k}\in \mathcal{A}, k\in\N \Rightarrow \bigcup_{k=1}^{\infty}
    A_{k}\in \mathcal{A}$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $A_{k}\in \mathcal{A} \Rightarrow \bigcap_{k=1}^{\infty}
    A_{k}\in \mathcal{A}$
   \item Potenzmenge von $\Omega$ ist $\sigma$-Algebra
  \end{enumerate}
\end{bemerk}

\begin{defini}
  $\mathcal{A}$ sei $\sigma$-Algebra über $\Omega$. Eine Abbildung $\mu :
  \mathcal{A} \rightarrow [0,\infty]$ heißt \highl{Maß} auf $\mathcal{A}$
  $:\Longleftrightarrow$
  \begin{enumerate}
   \item $\mu(A)\geq 0, \forall A\in \mathcal{A}$
   \item $\mu(\emptyset) = 0$
   \item $\mu(\bigcup_{k=1}^{\infty} A_{k}) =
    \sum_{k=1}^{\infty}\mu(A_{k})$ falls $A_{k}\in \mathcal{A}$ und
    $A_{k}\cap A_{j} = \emptyset (k\not=j)$
  \end{enumerate}
  
  $A\in \mathcal{A}$ heißt \highl{messbare Menge}. $(\Omega, \mathcal{A}, \mu)$
  \highl{Maßraum}
\end{defini}

\begin{defini}
  $(\Omega, \mathcal{A}, \mu)$ Maßraum
  \begin{enumerate}
   \item 
    $\mu$ heißt \highl{$\sigma$-endlich}, wenn das System $\{
    \Omega_{k}\}_{k}\subset \mathcal{A}$ mit $\mu(\Omega_{k}) <\infty$
    \begin{equation*}
      \Omega = \bigcup_{k=1}^{\infty} \Omega_{k}
    \end{equation*}
    existiert
   \item 
    \highl{$\mu$-endlich}, falls $\mu(\Omega)<\infty$
   \item $\mu$ heißt \highl{vollständig} $:\Longleftrightarrow$ $\forall
    N\in \mathcal{A}: \mu(N)=0, \forall A\subset N \Longrightarrow A\in
    \mathcal{A}\wedge \mu(A)=0$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item Wenn $\mu(A) = \infty$ zugelassen. Dann für $r\geq0, r +\infty =
    \infty, \infty+\infty = \infty$
   \item $A,B\in \mathcal{A}, A\subset B \Longrightarrow \mu(A) \leq
    \mu(B)$ Monotonie
   \item Beispiel: $\Omega=\N, \mathcal{A}=\mathcal{P}(\N), A\subset \N$
    \begin{equation*}
      \mu(A) := \card(A) (=\text{Anzahl der Elemente})
    \end{equation*}
    $\Longrightarrow$ $\sigma$-endlich vollständiges Maß
  \end{enumerate}
\end{bemerk}

\begin{defini}
  Eine Menge $B\subset \R^{n}$ heißt Borelmenge, wenn $B$ durch abzählbar
  viele Mengenoperationen (Vereinigung, Durchschnitt, Komplement) aus
  offenen Mengen entstanden ist.
\end{defini}

\begin{lemma}
  \begin{enumerate}
   \item Die Familie der Borelmengen $\mathcal{B}$ ist die kleinste
    $\sigma$-Algebra, die alle offenen Teilmengen des $\R^{n}$ enthält.
   \item Eine offene Menge im $\R^{n}$ lässt sich als abzählbare
    Vereinigung offener Rechtecke (Quader) $Q =
    (a_{1},b_{1})\times\ldots\times(a_{n},b_{n}), -\infty < a_{j} < b_{j}
    < \infty (j=1,\ldots,n)$ darstellen.
  \end{enumerate}
\end{lemma}

\begin{satz}
  $\mathcal{B}$ $\sigma$-Algebra der Borelmengen auf $\R^{n}$. Es
  existiert genau ein $\sigma$-endliches Maß $\lambda_{0}$ auf
  $\mathcal{B}$, so dass für jeden offenen Quader
  $Q=(a_{1},b_{1})\times\ldots\times(a_{n},b_{n})$ gilt:
  \begin{equation*}
    \lambda_{0}(Q) = \prod_{j=1}^{n}(b_{j}-a_{j})
  \end{equation*}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item 
    $B\subset \mathcal{B}$.

    \begin{equation*}
      \lambda_{0}(B) = \inf \{ \sum_{k=1}^{\infty} \lambda_{0}(Q_{k}):
	 Q_{k}\text{ offene Quader }, B\subset \bigcup_{k=1}^{\infty} Q_{k}\}
    \end{equation*}
   \item $\lambda_{0}$ ist nicht vollständig, aber jedes Maß besitzt eine
    \highl{kleinste vollständige Erweiterung}.
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:2}
  Es gibt eine kleinste vollständige Erweiterung $(\R^{n}, \mathcal{L},
  \lambda)$ von $(\R^{n}, \mathcal{B}, \lambda_{0})$.
  
  \begin{enumerate}
   \item $(\R^{n}, \mathcal{L}, \lambda)$ vollständiger
    $\sigma$-endlicher Maßraum
   \item $\mathcal{B}\subset \mathcal{L}, \forall B\in \mathcal{B}:
    \lambda(B) = \lambda_{0}(B)$
   \item Für alle Maßräume $(\R^{n}, \mathcal{A}, \mu)$ mit den
    Eigenschaften (1) und (2) gilt: $\mathcal{L}\subset \mathcal{A}$ und
    $\mu|_{\mathcal{L}} = \lambda$
  \end{enumerate}
\end{satz}

\begin{defini}
  $(\R^{n}, \mathcal{L}, \lambda)$ aus \thref{satz:2} heißt
  Lebesgue'scher Maßraum, $\lambda$ Lebesgue'sches Maß.
  
  $A$ Lebesgue-messbar $:\Longleftrightarrow$ $A\in \mathcal{L}$. $N\in
  \mathcal{L}$ Lebesgue-Nullmenge $:\Longleftarrow$ $\lambda(N) = 0$
  
  \textbf{Bezeichnung}: $A\in \mathcal{L}$ $|A| := \lambda(A)$
\end{defini}

\begin{satz}\label{satz:1.3.1/3}
  $(\R^{n}, \mathcal{L}, \lambda)$ Lebesgue'scher Maßraum
  \begin{enumerate}
   \item $N$ \highl{Lebesgue'sche Nullmenge} $:\Longleftarrow$ $\forall
    \epsilon>0 \exists (Q_{k})_{k}\text{ offener Quader im $\R^{n}$ mit }
    N\subset \bigcup_{k=1}^{\infty} Q_{k} \wedge \sum_{k=1}^{\infty} |Q_{k}|<\epsilon$.
   \item Jede abzählbare Menge im $\R^{n}$ ist eine Lebesgue'sche Nullmenge.
   \item $\lambda$ ist invariant bzgl. Translation und Rotation, d.~h.
    \begin{align*}
      x\in\R^{n}, A\in \mathcal{L} &\Longrightarrow \lambda(x+A) =
	 \lambda(A)\\
      A\in \mathcal{L}, T\in O(n) &\Longrightarrow \lambda\{y = Tx : x\in
	 A\} = \lambda(A)
    \end{align*}
   \item $\forall A\in \mathcal{L}\exists\Omega \subset \R^{n}\text{
    offen}: A\subset \Omega \wedge \lambda(\Omega\setminus A) < \epsilon$
  \end{enumerate}
\end{satz}

\subsection{Messbare Fuktionen}
\begin{defini}\label{def:trepfkt}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum.
  
  \begin{enumerate}
   \item $A\in \mathcal{A}, S: A \rightarrow \K$ heißt
    \highl{einfache Funktion} oder \highl{Treppenfunktion} $:\Longleftrightarrow$
    \begin{gather*}
      S(x) = \sum_{j=1}^{r} c_{j} \chi_{A\cap A_{j}}\qquad r\in \N,
	 c_{j}\in K, A_{j}\in \mathcal{A}, A\subset \bigcup_{j=1}^{r} A_{j}\\
      \chi_{A\cap A_{j}} =
	 \begin{cases}1& x\in A\cap A_{j}\\0&\text{sonst}\end{cases} 
	 \qquad(\highl{charakteristische Funktion})
    \end{gather*}
   \item Eine Eigenschaft $E(x)$ gilt auf einer Menge $A \subset \Omega$
    \highl{fast überall}\index{f.\,ü.} ($\mu$-fast überall, $\mu$-\fue)
    $:\Longleftrightarrow$ eine Nullmenge $N$ ($\mu(N) =0$) existiert und
    $E(x)$ auf $A\setminus N$ gilt.
  \end{enumerate}
  
  \todo{Hier könnte noch ein Bild von einer Treppenfunktion hin. Mit
    gnuplot eine Funktion = A in die xy-Ebene (=$\Omega$) legen. Darüber
    einfach ein paar Seulen mit Zufallswerten.}
\end{defini}

\begin{defini}
  $f: A\rightarrow\K, A\subset \mathcal{A}$ heißt \highl{messbar}
  $:\Longleftrightarrow$ eine Folge von Treppenfunktionen
  $(S_{k})_{k}$ existiert und $S_{k}(x) \rightarrow f(x)$
  $\mu$-\fue auf $A$.
\end{defini}

\begin{satz}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum, 
  $A\in \mathcal{A}$ und $f:A \rightarrow\K$.
  
  Dann sind die folgenden Aussagen äquivalent:
  \begin{enumerate}
   \item $f$ messbar\index{messbar}
   \item Die Urbilder $f^{-1}$ offener Mengen ($O\subset \K$) sind messbar
    ($f^{-1}(O) \in \mathcal{A}$).
   \item Die Urbilder von Borelmengen\index{Borelmengen} über $\K$ sind
    messbar.
  \end{enumerate}
\end{satz}

\begin{satz}
  \begin{enumerate}
   \item $f,g: A\rightarrow\K$ messbar $\Longrightarrow$ sind auch $f+g,
    f\cdot g, \frac{f}{g}$ und $|f|^{p}\ (p\in \R)$ messbar
   \item $f_{k}:A\rightarrow\R$ messbar und $f_{k}\rightarrow f$
    $\mu$-\fue auf $A$ $\Longrightarrow$ ist auch $f$ messbar
   \item $f,g: A\rightarrow \R$ messbar $\Longrightarrow$ sind auch $f^{+} =
    \max\{0,f(x)\}, f^{-} = \max\{0,-f(x)\},\\\max\{f,g\}$ und
    $\min\{f,g\}$ messbar
   \item $f_{k}: A\rightarrow\R$ messbar $\Longrightarrow$ sind auch
    $\sup_{k} f_{k}(x), \inf_{k} f_{k}(x), \limsup\limits_{k\rightarrow\infty} f_{k}$
    und $\liminf_{k\rightarrow\infty} f$ messbar
  \end{enumerate}
\end{satz}

\begin{satz}[Egorov]
  $A\subset \mathcal{A}, \mu(A) < \infty$ und $f_{k}: A\rightarrow\K$
  messbar für alle $k\in \N$.
  
  Dann sind folgende Aussagen äquivalent:
  \begin{enumerate}
   \item $f_{k} \rightarrow f$ $\mu$-\fue auf $A$
   \item $\forall \epsilon>0\ \exists A_{\epsilon}\in \mathcal{A}:
    \mu(A\setminus A_{\epsilon})< \epsilon$ und $f_{k}$ konvergiert
    gleichmäßig gegen $f$ auf $A_{\epsilon}$ ($\sup_{x\in A_{\epsilon}}
    |f_{k}(x)-f(x)| \xrightarrow{k\rightarrow\infty} 0$)
  \end{enumerate}
\end{satz}

\begin{satz}[Lebesguemaß]
  $(\R^{n}, \mathcal{L}, \lambda)$ \help{L oder A? $\rightarrow$ 2./3.
  korregieren}
  
  \begin{enumerate}
   \item Ist $f: \R^{n} \rightarrow \K$ \fue stetig, dann ist $f$
    \highl{messbar}
   \item Falls der Rand von $\partial A$ eine Nullmenge
    ($\mu(\partial A)=0$) ist, ist $A\in\mathcal{A}$. \help{haut das hin?}
   \item $f: \mathcal{A}\rightarrow\K$ ist messbar, falls
    \begin{equation*}
      \tilde{f} = \begin{cases}
		    f&\text{auf }A\\
		    0&\text{sonst}
		  \end{cases}
    \end{equation*}
    \fue stetig auf $\R^{n}$ ist.
  \end{enumerate}
\end{satz}

\begin{bemerk}[Lusin]
  $\Omega\subset\R^{n}$ offen, $\mu(\Omega)<\infty$,
  $f:\Omega\rightarrow\K$ messbar, $f=0$ auf $\R^{n}\setminus\Omega$.
  
  Dann existiert für alle $\epsilon>0$ $\phi\in C(\Omega)$ \help{das kann
  ich nicht lesen} mit $\|\phi\|_{\infty} \leq \|f\|_{\infty}$ und
  $\mu\bigl(\{x\in \Omega: f(x)\not=\phi(x)\}\bigr) < \epsilon$
\end{bemerk}

\subsection{Das Lebesgue-Integral}

\begin{defini}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum
  
  \begin{gather*}
    S(x) = \sum_{j=1}^{r} c_{j} \chi_{A\cap A_{j}}\\
    \int_{A} S(x)\,d\mu = \sum_{j=1}^{r} c_{j} \mu(A\cap A_{j})
  \end{gather*}
  Die Funktion $S(x)$ heißt \highl{Treppenfunktion}
  (vgl.~\thref{def:trepfkt}) und ist unabhängig von der Auswahl der
  $c_{j}$ und $A_{j}$.
\end{defini}

\begin{defini}
  $(\Omega, \mathcal{A}, \mu)$ sei ein vollständiger $\sigma$-endlicher
  Maßraum und $A\subset \mathcal{A}$
  
  $f:A\rightarrow\K$ heißt \highl{integrierbar} $:\Longleftrightarrow$
  \begin{enumerate}
   \item $f$ ist messbar, d.\,h. es gibt eine Folge $(S_{k})_{k}$ von
    Treppenfunktionen mit $S_{k}\rightarrow f$ $\mu$-\fue auf $A$ und
   \item für alle $\epsilon>0$ ein $n_{0}(\epsilon)$ existiert, so dass
    für alle $n,m\geq n_{0}(\epsilon)$ gilt:
    \begin{equation*}
      \int_{A}|S_{n}(x)-S_{m}(x)|\,d\mu < \epsilon
    \end{equation*}
  \end{enumerate}
\end{defini}

Wir setzen
\begin{equation*}
  \int_{A} f\,d\mu := \lim_{n\rightarrow\infty} \int_{A} S_{n}\,d\mu
\end{equation*}
unabhängig von der Auswahl der Folge $(S_{k})_{k}$)

\begin{bemerk}\label{bem:2}
  \begin{enumerate}
   \item Ist $f=g$ \fue und $f$ integrierbar, dann ist auch $g$
    integrierbar und 
    \begin{equation*}
      \int_{A} g\,d\mu = \int_{A} f\,d\mu
    \end{equation*}
   \item \begin{equation*}\int_{A}d\mu = \mu(A)\end{equation*}
   \item Sei $f:\Omega \rightarrow\K$ messbar. Dann ist
    \begin{equation*}
      \int_{A} f\,d\mu = \int_{\Omega} f\chi_{A}\,d\mu
    \end{equation*}
    falls dieses existiert.
  \end{enumerate}
\end{bemerk}

\begin{bemerk}[Äquivalenter Zugang]
  \begin{enumerate}
   \item Ist $f: A\rightarrow[0,\infty)$ eine nichtnegative Funktion,
    dann ist
    \begin{equation*}
      \int_{A} f\,d\mu := \sup_{0\leq s\leq f} \int_{A} s\,d\mu
    \end{equation*}
    falls das Supremum existiert.
   \item Bilde jetzt $f:A\rightarrow\R$ ab. $f$ lässt sich in einen
    positive Anteil $f^{+} := \max\{0,f(x)\}$ und einen negativen Anteil
    $f^{-}=\max\{0,-f(x)\}$ zerlegen. $f = f^{+} - f^{-}$. Womit man das
    Integral auf die folgende Weise zusammensetzen kann:
    \begin{equation*}
      \int_{A} f\,d\mu = \int_{A} f^{+}\,d\mu - \int_{A} f^{-}\,d\mu
    \end{equation*}
   \item $f$ sein nun eine komplexwertige Funktion ($f:A\rightarrow\C$).
    Dann lässt sich $f$ in $f=\Re f+i\Im f$ zerlegen. Das Integral analog
    in:
    \begin{equation*}
      \int_{A} f\,d\mu = \int_{A}\Re f\,d\mu + i\int_{A}\Im f\,d\mu
    \end{equation*}
  \end{enumerate}
\end{bemerk}

\begin{bemerk}
  Sei nun der Lebesgue'sche Maßraum $(\R^{n}, \mathcal{L}, \lambda)$
  gegeben.
  \begin{enumerate}
   \item Für $A\subset\mathcal{L}$ vereinbaren wir folgenden Schreibweise:
    \begin{equation*}
      \int_{A} f\,d\lambda = \int_{A} f(x)\,dx
    \end{equation*}
   \item Ist $f$ die Treppenfunktion
    \begin{equation*}
      f = \begin{cases}
	    1& x\in [0,1]\cap\Q\\
	    0& x\in [0,1]\setminus\Q
	  \end{cases}
    \end{equation*}
    dann existiert das Integral von $f$ auf $[0,1]$ und ist 0.
    \begin{equation*}
      \int_{0}^{1}f(x)\,dx = 0
    \end{equation*}\help{Tolle Sache, aber warum ist das so?}
   \item Ist eine Menge $\Omega\subset\R^{n}$ Jordan-messbar, so ist sie
    auch Lebesgue-messbar.
    \begin{alignat*}{5}
      \exists \int_{\Omega} f\,dx &&\qquad\Longrightarrow\qquad && 
	 \exists \int_{\Omega} f\,d\lambda  &&\quad =\quad &&
	 \int_{\Omega} f\,dx\\
      \text{Riemann}&&&& \text{Lebesgue}&&&& \text{Riemann}
    \end{alignat*}
   \item Absolut konvergente Riemann-Integrale (uneigentliche) sind
    ebenfalls identisch mit dem Lebesgue-Integral.
   \item Eine Funktion $f:\Omega\rightarrow\K$ ist genau dann
    Riemann-Integrierbar, wenn $f$ \fue stetig im \emph{Lebesgue'schen Sinn}
  \end{enumerate}
  
  \todo{hier gehört noch eine (komplizierte) Zeichnung hin -- Blatt 1.4.8a}
\end{bemerk}

\begin{satz}[Eigenschaften integrierbarer Funktionen]
  Sei $(\Omega, \mathcal{A}, \mu)$ ein vollständiger $\sigma$-endlicher
  Maßraum, $A\subset\mathcal{A}$ und das Maß von $A$ $\mu(A)>0$
  
  \begin{enumerate}
   \item $f,g: A\rightarrow\K$ integrierbar, $\alpha,\beta\in\K$
    \begin{equation*}
      \Longrightarrow \exists \int_{A}(\alpha f +\beta g)\,d\mu =
      \alpha\int_{A} f\,d\mu + \beta\int_{A}g\,d\mu
    \end{equation*}
   \item $f,g: A\rightarrow\K$, $f(x) = g(x)$ \fue, $f$ integrierbar,
    dann ist auch $g$ integrierbar und die Integrale sind gleich.
    (vgl.~\thref{bem:2})
   \item $f,g: A\rightarrow\R$ integrierbar und $f\leq g$ \fue
    \begin{equation*}
      \Longrightarrow \int_{A}f\,d\mu \leq \int_{A}g\,d\mu
    \end{equation*}
   \item $f:A\rightarrow\K$ integrierbar $\Longleftrightarrow$
    $|f|: A\rightarrow\R$ integrierbar und
    \begin{equation*}
      \Bigl|\int_{A}f\,d\mu\Bigr| \leq \int_{A} \bigl|f\bigr|\,d\mu
    \end{equation*}\help{ist das eine Forderung oder eine zweite Aussage}
   \item Ist $f: A\rightarrow\K$ integrierbar, dann gilt:
    \begin{equation*}
      \int_{A} |f(x)|\,d\mu = 0 \Longleftrightarrow f=0\ \fue
    \end{equation*}
   \item $f,g: A\rightarrow\K$, wobei $f$ messbar und $g$ integrierbar
    ist und $|f(x)| \leq |g(x)|$ \fue. So gilt:
    \begin{equation*}
      \exists \int_{A} |f(x)|\,d\mu \leq \int_{A} |g(x)|\,d\mu
    \end{equation*}
  \end{enumerate}
\end{satz}

\begin{bemerk}
  Es gibt kein Analogon zu (4) für Riemann'integrierbare Funktionen.
  Wählt man z.\,B.
  \begin{equation*}
    f(x) = \begin{cases}
	     1& x\in[0,1]\cap\Q\\
	     0& x\in[0,1]\setminus\Q
	   \end{cases}
  \end{equation*}
  dann ist der Betrag $|f(x)|=1$ und somit Riemann'integrierbar, aber
  $f(x)$ selbst ist nicht Riemann'integrierbar. Oder betrachtet man
  $f(x) = \frac{\sin x}{x}$ auf $(0,\infty)$, so existiert zwar das
  (uneigentliche) Riemann-Integral $\int_{0}^{\infty}\frac{\sin
  x}{x}\,dx$, aber das Integral $\int_{0}^{\infty}\bigl|\frac{\sin
  x}{x}\bigr|\,dx=\infty$. Damit ist die Funktion aber auch nicht
  Lebesgue'integrierbar.
\end{bemerk}

\begin{satz}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum
  \begin{enumerate}
   \item $f:\Omega \rightarrow\K$ integrierbar, $E\in \mathcal{A}$.
    \help{ist das hier wirklich ein $\in$ oder soll es ein $\subset$ sein?} Dann:
    \begin{equation*}
      \int_{\Omega} f\,d\mu = \int_{E} f\,d\mu + \int_{E\setminus\Omega}
      f\,d\mu
    \end{equation*}
   \item Ist $f:\Omega\rightarrow\R$ integrierbar und $E\in\mathcal{A}$
    \help{dito}, $f>0$ auf $E$, so folgt aus $\int_{E}f\,d\mu$, dass das
    Maß $\mu(E)=0$
   \item $f:\Omega \rightarrow \K$ integrierbar,
    $\Omega=\bigcup_{k=1}^{\infty} E_{k}$ mit $E_{k}\in\mathcal{A}$ und
    die $E_{k}$ sind paarweise disjunkt ($\forall j\not=k: E_{k}\cap
    E_{j}=\emptyset$):
    \begin{equation*}
      \int_{\Omega} f\,d\mu = \sum_{k=1}^{\infty}\int_{E_{k}} f\,d\mu
      \qquad\text{abs.konv. Reihe}
    \end{equation*}
  \end{enumerate}
\end{satz}

\begin{folger}
  \begin{enumerate}
   \item $E_{1} \subset E_{2} \subset \ldots$ mit
    $\Omega=\bigcup_{k=1}^{\infty} E_{k}$, $E_{k}\in\mathcal{A}$ $\Longrightarrow$
    \begin{equation*}
      \lim_{k\rightarrow\infty}\int_{E_{k}} f\,d\mu = \int_{\Omega} f\,d\mu
    \end{equation*}
   \item Für eine nichtnegative ($w(x) \geq 0$), integrierbare Funktion
    $w:\Omega \rightarrow \R$ und $A\in\mathcal{A}$ ist $\nu$ als das
    \highl{endliche Maß} auf $A$ definiert als:
    \begin{equation*}
      \nu(A) := \int_{A}w(x)\,d\mu
    \end{equation*}
  \end{enumerate}
\end{folger}

\subsection{Konvergenzsätze}

\begin{satz}[Lebesgue]\label{satz:lebesgue}
  Ist $(\Omega, \mathcal{A}, \mu)$ ein vollständiger $\sigma$-endlicher
  Maßraum, $A\in \mathcal{A}, f_{k}: A\rightarrow \K$ messbar und es
  existiert $\lim_{k\rightarrow\infty} f_{k}(x) = f(x)$ \fue Existiert
  weiterhin eine integrierbare Funktion $g:A\rightarrow\K$ und
  $|f_{k}(x)| \leq g(x)$ \fue ($k\in\N$),
  
  dann existiert auch das Integral auf $f$ und Limes und Integral sind
  vertauschbar:
  \begin{equation*}
    \int_{A} f\,d\mu = \int_{A} \bigl(\lim_{k\rightarrow\infty}
       f_{k}(x)\bigr) \,d\mu = \lim_{k\rightarrow\infty} \int_{A} f_{k}\,d\mu
  \end{equation*}
\end{satz}

% <news:d49.421a464d.9a65a@invisibletruth.local>
\begin{bsp}\hspace*{0ex}\\
  \mbox{}\hfill\includegraphics[width=.4\linewidth]{1x}\hfill\mbox{}

  Für die Funktionenfolge
  \begin{equation*}
    f_{k} = \begin{cases}
	      k&: x\in [0,\frac1k]\\
	      0&:\text{sonst}
	    \end{cases}
  \end{equation*}
  existiert der $\lim_{k\rightarrow\infty} f_{k}(x) = 0 = g(x)$ \fue,
  \help{erfüllt das wirklich die Bedingung von \thref{satz:lebesgue}?}
  der Betrag $|f_{k}(x)|\leq g(x) = \nicefrac{1}{x}$ und 
  das Integral $\int_{A} f_{k}(x)\,dx = 1$ für alle $k$ -- die $f_{k}$
  sind also messbar. Aber denoch gilt nicht
  \begin{equation*}
    1 = \lim\int f_{k}\,d\mu \not=\int \lim f_{k}\,d\mu = 0
  \end{equation*}
  da die Funktion $g(x)$ nicht integrierbar ist.\help{stimmt das?}
\end{bsp}

\begin{satz}[B.~Levi, monotone + beschränkte Folgen]\label{satz:blevi}
  $A\subset \mathcal{A}$. Es sei $(f_{k})_{k}$ eine monotone Folge
  integrierbarer, reeller Funktionen $f_{k}: A \rightarrow \R$ und es sei die
  Folge der Integrale $(\int_{A} f_{k}\,d\mu)_{k}$ beschränkt.
  
  Dann existiert \fue auf $A$ der $\lim_{k\rightarrow\infty} f_{k} =:
  f(x)$, $f$ ist integrierbar und es gilt
  \begin{equation*}
    \lim_{k\rightarrow\infty}\int_{A} f_{k}\,d\mu =
       \int_{A}\lim_{k\rightarrow\infty} f_{k}\,d\mu = \int_{A} f\,d\mu
  \end{equation*}
\end{satz}

\begin{bemerk}
  $f_{k}$ sei $\geq 0$ und integrierbar und die
  $\sum_{k=1}^{\infty}\int_{A} f_{k}\,d\mu < \infty$.
  
  Dann existiert $f(x) := \sum_{k=1}^{\infty}f_{k}(x)$ \fue und das
  \begin{equation*}
    \int_{A}\sum_{k=1}^{\infty} f_{k}(x)\,d\mu =
       \sum_{k=1}^{\infty}\int_{A} f_{k}(x)\,d\mu
  \end{equation*}
\end{bemerk}

\begin{satz}[Lemma von Faton]\label{satz:faton-lemma}
  $A\subset \mathcal{A}, f_{k}: A\rightarrow\R$ integrierbar, $f_{k} \geq
  0$ auf $A$. Es sei weiterhin der $\liminf\limits_{k\rightarrow\infty}
  \int_{A} f_{k}\,d\mu < \infty$.
  
  Dann ist $\liminf_{k\rightarrow\infty} f_{k}(x)$ \fue endlich
  und integrierbar und
  \begin{equation*}
    \int_{A}\liminf_{k\rightarrow\infty} f_{k}(x)\,d\mu
    \leq \liminf_{k\rightarrow\infty} \int_{A} f_{k}\,d\mu
  \end{equation*}
\end{satz}

\begin{bemerk}
  Sind die Vorraussetungen die gleichen wie im Lemma von Faton
  (\thref{satz:faton-lemma}) und existiert \fue der
  $\lim_{k\rightarrow\infty} f_{k}(x) =f(x)$, dann ist $f$ integrierbar und
  \begin{equation*}
    \int_{A} f\,d\mu \leq \liminf_{k\rightarrow\infty} \int_{A} f_{k}\,d\mu
  \end{equation*}
\end{bemerk}

\subsection{Die Räume \texorpdfstring{$L_{p}(A,\mu)$}{Lp(A,\textmu)}}

\begin{defini}\index{$L_{p}$}\index{$L_{\infty}$}
  \index{$\|\cdot\|_{p}$}\index{$\|\cdot\|_{\infty}$}
  $(\Omega, \mathcal{A}, \mu)$ vollständiger $\sigma$-endlicher Maßraum,
  $0<p<\infty, A\in \mathcal{A}$
  % \begin{enumerate}
  %  \item
    \begin{align*}
      L_{p}(A, \mu) &:= \{ f: A\rightarrow\K : f\ \text{messbar},
	 \exists\int_{A} (f)^{p}\,d\mu < \infty\}\\
      \|f\|_{p} &:= \Bigl(\int_{A} |f|^{p}\,d\mu\Bigr)^{\frac1p}
	 \\
    % \end{align*}
   % \item 
    % \begin{align*}
      L_{\infty}(A,\mu) &:= \{ f: A\rightarrow\K : f\ \text{messbar},
	 \exists c>0, \exists N, \mu(N) = 0 \ \text{mit}\ |f(x)|\leq c
	 \ \forall x\in A\setminus N\}\\
      \|f\|_{\infty} &:= \inf \{ c: \mu(\{x: f(x) > c\}) = 0\}
    \end{align*}
  % \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item Als "`\highl{wesentliches Supremum}"' $\esssup$ bezeichnet man
    \begin{equation*}
      \esssup_{x\in A}|f(x)| := \|f\|_{\infty} = \inf \bigl\{
      \sup_{x\in A\setminus N} |f(x)|: \mu(N) =0\bigr\}
    \end{equation*}
   \item
      $f\in L_{\infty}(A,\mu) \Longrightarrow N, \mu(N) =0$, so dass
    \begin{align*}
      \|f\|_{\infty} &= \sup_{x\in A\setminus N}|f(x)|\\
      \|f\|_{\infty} = 0 &\Longleftrightarrow \forall x\in A\setminus N:
	 |f(x)|=0 \Longleftrightarrow f(x) = 0\ \fue
    \end{align*}
   \item $p<\infty$ $\Longrightarrow$
    \begin{equation*}
      \|f\|_{p} = \Bigl(\int_{A} |f(x)|^{p}\,d\mu\Bigr)^{\frac1p} = 0
	 \Longleftrightarrow f(x) = 0\ \fue
    \end{equation*}
    
    Übergang zu Äquivalenzklassen: $f\sim g \Longleftrightarrow f=g$ \fue
  \end{enumerate}
\end{bemerk}

\begin{satz}\label{satz:hoelder}
  \begin{enumerate}
   \item Gilt für $p$: $1\leq p \leq \infty$ und $\frac1p + \frac1{p'} =
    1$ ($p' = \infty$, falls $p=1$). Und sei weiterhin $f\in L_{p}(A,\mu)$
    und $g\in L_{p'}(A,\mu)$. Dann ist $f\cdot g\in L_{1}(A, \mu)$ und
    $\|f\cdot g\|_{1} \leq \|f\|_{p}\: \|g\|_{p'}$
    
    Es gilt dann die \highl{Hölder'sche Ungleichung} für Integrale:
    \begin{equation}\label{eq:hoelder-int}
      \int_{A}|f(x) g(x)|\,d\mu \leq
	 \Bigl(\int_{A}|f(x)|^{p}\,d\mu\Bigr)^{\nicefrac1p}
	 \Bigl(\int_{A}|g(x)|^{p'}\,d\mu\Bigr)^{\nicefrac1{p'}}
    \end{equation}
    
    Vergleiche auch: \thref{lem:hoelder-sum} mit der Ungleichung für Summen
    \eqref{eq:hoelder-sum}
   \item $0<p \leq \infty$. Es seien $f,g\in L_{p}(A,\mu)$.
    
    Dann ist $f+g\in L_{p}(A,\mu)$ und
    \begin{align*}
      \|f+g\|_{p} \leq \|f\|_{p} + \|g\|_{p}\ \text{falls } 1\leq p \leq
	 \infty\\
      \intertext{Minkowski'sche Ungleichung \help{Welche der beiden ist
      es?}}
      \|f+g\|_{p}^{p} \leq \|f\|_{p}^{p} + \|g\|_{p}^{p} \leq
	 2^{1-p}\big(\|f\|_{p} + \|g\|_{p}\big)^{p}\ \text{falls}\ 0<p <1
	 \taglabel{eq:minkowski}
    \end{align*}
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}[zu~(1)]
     \item  $1<p<\infty$. Aus \thref{lem:1.2.1/1} folgt:
      \begin{equation*}
	|f(x) g(x)| \leq \frac{|f(x)|^{p}}p + \frac{|g(x)|^{p}}p \in L_{1}(A,\mu)
      \end{equation*}\help{fehlt bei p an g(x) der Strich? im Lemma ist da
      einer}
      $f\cdot g$ messbar $\Longrightarrow$ $f\cdot g\in L_{1}$
      
      \obda\help{wieso gilt obda?}: $\|f\|_{p} = \|g\|_{p'} =1$
      \begin{equation*}
	\Longrightarrow \int_{A} |f(x) g(x)|\,d\mu \leq \frac1p
	   \|f\|_{p}^{p} + \frac1{p'} \help{fehlt hier \|g\|_{p}^{p}?} =
	   \frac1p + \frac1{p'} = 1 = \|f\|_{p} + \|g\|_{p'}
      \end{equation*}
     \item $1\leq p \leq \infty$, \thref{lem:1.2.1/4} $\Longrightarrow$
      $\|f(x) + g(x)\|^{p} \leq 2^{p-1}( |f(x)|^{p} + |g(x)|^{p})\in
      L_{1}(A,\mu)$. Da $f(x) + g(x)$ messbar $\Longrightarrow$
      $\exists\int_{A} |f(x) +g(x)|^{p}\,dx<\infty$
      
      $0<p < 1$ $\Longrightarrow$ $|f(x) +g(x)|$ messbar $\Longrightarrow$
      $f+g\in L_{p}(A,\mu)$ Beweis der Ungleichung analog zum Beweis für
      $l_{p}$ (\thref{lem:1.2.1/3})
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{satz}
  $\mu(A) < 0, 0 < p < q \leq \infty$.
  
  Dann ist $L_{q}(A, \mu) \subset L_{p}(A,\mu)$ und $\|f\|_{p} \leq
  \mu(\Omega)^{\frac1p-\frac1q}\|f\|_{q}$
  
  \begin{proof}
    $u = \frac{q}p>1, \frac1u+\frac1v =1, \frac1v = 1-\frac{p}q$
    \begin{align*}
      \int_{A} |f(x)|^{p}\,d\mu &= \int_{A} |f(x)|^{p} \chi_{A}\,d\mu\\
      &\leq \left(\int_{A}|f(x)|^{q}\,d\mu\right)^{\frac{p}q}\cdot
	 \bigl(\mu(A)\bigr)^{1-\frac{p}q}
    \end{align*}
    $\Longrightarrow$ $\|f\|_{p} \leq \mu(A)^{\frac1p-\frac1q}\|f\|_{p}$
  \end{proof}
\end{satz}

\begin{satz}\label{satz:3}
  $(\Omega, \mathcal{A}, \mu)$ wie oben\help{wo ist oben?},
  $A\in\mathcal{A}, 1 \leq p \leq \infty$. Es sei $(f_{k})_{k}$ eine
  Cauchyfolge in $L_{p}(A,\mu)$.
  
  Dann existiert $f\in L_{p}(A,\mu)$ mit $\|f_{k}-f\|_{p}
  \xrightarrow{k\rightarrow\infty}0$
  
  \begin{proof}
    $1\leq p <\infty$: \thref{satz:1.1.1/1} anwendbar. Es sei
    \begin{equation}
      \sum_{k=1}^{\infty}\|g_{k}(x)\|_{p} < \infty\label{eq:144}
    \end{equation}
    
    \underline{zu zeigen:} $\exists g\in L_{p}(A,\mu)$ mit $g = \sum_{k=1}^{\infty}
    g_{k}(x)$ in $L_{p}(A,\mu)$, d.\,h. $\|\sum_{k=1}^{n} g_{k}-g\|_{p}
    \xrightarrow{n\rightarrow\infty}0$
    
    Es ist $A = \bigcup_{j=1}^{\help{wohin}} A_{j}$ mit $\mu(A_{j})<
    \infty$, da $\mu$ $\sigma$-endlich
    
    $\forall j \Longrightarrow \int_{A_{j}}
    \sum_{k=1}^{n}|g_{k}(x)|\,d\mu \leq \|\sum_{k=1}^{n}
    |g_{k}(x)|\|_{p}\ \mu(A_{j})^{1-\frac1p}\leq c
    \sum_{k=1}^{n}\|g_{k}(x)\|_{p} <^{\eqref{eq:144}} \infty$
    
    Aus dem Satz von B.\,Levi (\thref{satz:blevi}) folgt, dass die Summe
    $\sum_{k=1}^{\infty} |g_{k}(x)|$ \fue auf $A_{j} (\forall j)$
    existiert.
    
    $\Longrightarrow$ $\exists h(x) := \sum_{k=1}^{\infty}
    |g_{k}(x)|$ \fue auf A, $|g(x)| \leq h(x)$
    
    Es gilt:
    \begin{gather*}
      \int_{A} \biggl(\sum_{k}^{n}|g_{k}(x)|_{p}\biggr)^{p}\,d\mu =
	 \Biggl(\biggl\|\sum_{k=1}^{n}|g_{k}(x)| \biggr\|_{p}\Biggr) \leq
	 \biggl(\sum_{k=1}^{n}\|g_{k}\|_{p}\biggr)^{p} <^{\eqref{eq:144}} \infty\\
      \intertext{Nach dem Satz von B.\,Levi (\thref{satz:blevi})}
      \Longrightarrow \exists
	 \int_{A}\biggl(\sum_{k=1}^{\infty}|g_{k}(x)|\biggr)^{p}\,d\mu =
	 \int_{A} h(x)^{p}\,d\mu \Longrightarrow h\in L_{p}(A,\mu)\\
      \Longrightarrow g\in L_{p}(A,\mu)
    \end{gather*}
  \end{proof}
\end{satz}

