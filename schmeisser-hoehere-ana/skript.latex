\chapter{Banachräume und beschränkte Operatoren}
\section{Banachräume}
\subsection{Normierte Vektorräume}

% Topologische Räume $\supset$ Metr. Räume $\supset$ Vektorräume
% $\supset$ Banachräume $\supset$ Hilberträume

\textbf{Grunddefinitionen}
\begin{itemize}
 \item
  $E$ \highl{Vektorraum} über einem \highl{Körper} $\K$ mit $\K=\R$ oder $\C$
  (reeller/komplexer Vektorraum)
 \item
  $\{x_{1},\ldots,x_{n}\}\subset E$ \highl{linear unabhängig} $:\Longleftrightarrow$
  $\lambda_{1}x_{1}+\ldots+\lambda_{n}x_{n}=0 \Rightarrow
  \lambda_{1}=\ldots=\lambda_{n}=0$; sonst \highl{linear abhängig}
 \item 
  $M\subset E$ linear unabhängig $:\Longleftrightarrow$ jede endliche Teilmenge ist linear
  unabhängig
 \item 
  $\dim E=n$ $:\Longleftrightarrow$ $\exists n$ linear unabhängige Elemente aus
  $E$ und $n+1$ Elemente sind linear abhängig

  z.\,B. $\K^{n}$ (s. linare Algebra)
 \item 
  $\dim E =\infty$ $:\Longleftrightarrow$ $\forall n\in \N\ \exists$ linear unabhängig
  Elemente $\{x_{1},\ldots,x_{n}\}$

  z.\,B. $C([a,b]) := \{ f \big| f: [a,b] \rightarrow \K: \text{stetig} \},\;
    \{1,x,x^{2},\ldots,x^{n}\}$ linear unabhängig
 \item 
  $M\subset E$. Als Spann (engl.~span) von $M$ bezeichnet man den Teilraum
  von $E$, der durch die Vektoren $x_{1},\ldots,x_{n}$ aufgespannt wird.
  
  $\Span M := \{x\in E: x = \sum_{\text{endl.}} \lambda_{j}x_{j},
  \lambda_{j}\in \K, x_{j}\in M\}$
  
  Der Spann ist der Durchschnitt aller Teilräume, die $M$ enhalten oder
  anders ausgedrückt, es ist der kleinste Teilraum von $E$, der $M$ enthält.
 \item 
  Sei $\dim E = n$. Dann heißen $\{x_{1},\ldots,x_{n}\}$ \highl{Basis}
  $:\Longleftrightarrow$
  \begin{enumerate}
   \item $x_{1},\ldots,x_{n}$ sind linear unabhängig
   \item $\Span\{x_{1},\ldots,x_{n}\} = E$
  \end{enumerate}

  \textbf{Bemerkung}\\ % Das soll so sein; die Bemerkung hatte keine Nummer
  Jeder Vektorraum besitzt eine \highl{Hamelbasis}, d.\,h. es existiert
  eine linear unabhängige Menge von Vektoren, die $E$ aufspannt: $\Span M
  = E$ (o.~Bew.)
\end{itemize}


\begin{defini}
  Ein Vektorraum $E$ heißt \highl{normiert} $:\Leftrightarrow$ eine
  Abbildung $\|\cdot\|: E\rightarrow \R$ mit den Eigenschaften
  \begin{enumerate}
   \item $\|x\|> 0\qquad \forall x \not= 0$ 
   \item $\|\lambda x\| = \|\lambda\| \|x\|\qquad \forall \lambda\in
    \K, 0\not=x\in E$
   \item $\|x+y\| \leq \|x\|+\|y\|\qquad \forall x,y\in E$
  \end{enumerate}
  existiert. $\|\cdot\|$ heißt \highl{Norm}
  
  Diese abstrakte Definition geht zurück auf Hahn, Banach, Wiener 1922.
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $\|x\|=0 \Leftrightarrow x=0$
   \item $\big|\|x\|-\|y\|\big| \leq \|x+y\|$
   \item $d(x,y) := \|x-y\|$ ist eine \highl{Metrik}
  \end{enumerate}
\end{bemerk}

\begin{bsp}
  \begin{enumerate}
   \item $\R^{n}$:
    \begin{align*}
      \|x\|_{1} &= \sum|x_{j}|\\
      \|x\|_{2} &= (x_{1}^{2}+\ldots+x_{n}^{2})^{\frac12}\\
      \|x\|_{\infty} &= \max_{j}|x_{j}|
    \end{align*}
    
    analog im $\C^{n}$
   \item $C([a,b])$:
    \begin{equation*}
      \|f\| := \sup_{x\in[a,b]}|f(x)|=\max_{x\in[a,b]}|f(x)|
    \end{equation*}
   \item \index{$l_{1}$} $l_{1}(\N) := \{a \big| a=(a_{j})_{j=1}^{\infty} \subset \K:
    \sum_{j=1}^{\infty}|a_{j}|<\infty\}$ ist die Menge aller
    absolutkonvergenten Reihen:
    \begin{equation*}
      \|a\|_{1} := \sum_{j=1}^{\infty}|a_{j}|
    \end{equation*}
    $\dim l_{1}(\N) = \infty$, da
    $(e_{j}=(0,\ldots,0,
    \underbrace{1}_{\makebox[0pt][c]{j\text{-te Position}}},0,\ldots))$
    mit $j\in\N$ Teilmenge von $l_{1}(\N)$ ist und dieses offensichtlich
    die Dimension\,$\infty$ hat.
  \end{enumerate}
\end{bsp}

\begin{defini}
  $(E,\|\cdot\|)$ normierter Vektorraum, $(x_{j})_{j}$ Folge in $E$
  
  \begin{enumerate}
   \item Eine Folge $(x_{j})_{j}$ heißt \highl{konvergent}
    $:\Longleftrightarrow$ $\exists x\in E
    \ \forall\epsilon > 0 \exists j_{0}(\epsilon) \forall j\geq
    j_{0}(\epsilon): \|x_{j}-x\|\leq \epsilon$
   \item Eine Folge $(x_{j})_{j}$ heißt \highl{Cauchyfolge}
    $:\Longleftrightarrow$ $\forall \epsilon >0 \exists j_{0}(\epsilon)
    \forall j,k \geq j_{0}(\epsilon): \|x_{j}-x_{k}\|\leq \epsilon$
  \end{enumerate}
\end{defini}

\begin{folger}
  \begin{enumerate}
   \item Jede konvergente Folge ist Cauchyfolge
   \item Jede Cauchyfolge ist \highl{beschränkt} ($\exists c>0\ \forall
    j:\|x_{j}\|\leq c$)
   \item $x_{j}\rightarrow x$ $\Longrightarrow$ $\|x_{j}\| \rightarrow \|x\|$
    (Stetigkeit der Norm: $\|\cdot\|: E\rightarrow\R$ stetig)
  \end{enumerate}
\end{folger}

\begin{defini}[Vollständigkeit]
  \begin{enumerate}
   \item $(E, \|\cdot\|)$ normierter Vektorraum heißt
    \highl{vollständig} $:\Longleftrightarrow$ Jede Cauchyfolge in $E$
    ist konvergiert und ihr Grenzwert liegt in $E$.
   \item Ein vollständig normierter Vektorraum heißt \highl{Banachraum}.
  \end{enumerate}
\end{defini}

\begin{bemerk}
  Die Räume $\R^{n}, \C^{n}$ mit der Norm $\|\cdot\|_{2}$ und der Raum
  $C([a,b]), \|\cdot\|_{\infty})$ mit der Supremumsnorm $\|f\|_{\infty} :=
  \sup_{x\in[a,b]} |f(x)|$ sind Banachräume (s.~Analysis~2).
\end{bemerk}

\begin{bemerk}
  Jedoch nicht jeder normierte Vektorraum ist vollständig.
  Beispiele: $\Q, \Q^{n}$
\end{bemerk}

\begin{bsp}
  $\mathcal{P} := \{ p: [a,b] \rightarrow \K: p \text{ ist
  Polynom}\} \subset C([a,b])$ mit der Norm $\|p\|_{\infty} = \max\limits_{x\in
  [a,b]} |p(x)|$ ist nicht vollständig.
  
  \begin{proof}
    Wir wissen $e^{x}=\sum_{k=0}^{\infty}\frac{x^{k}}{k!}$ ist gleichm.
    konvergent auf $[a,b]$. Daraus folgt, die Folge $(p_{n})_{n}$ mit
    $p_{n}(x) = \sum_{k=0}^{n}\frac{x^{k}}{k!} \in \mathcal{P}$ ist eine
    Cauchyfolge bzgl. $\|\cdot\|_{\infty}$
    
    Ann.: $\exists p\in \mathcal{P}$ mit $\|p_{n}-p\| \rightarrow 0
    \Rightarrow$
    \begin{equation*}
      |p(x) - e^{x}| \leq \|p(x) -
	 p_{n}(x)\|_{\infty}+\|p_{n}(x)-e^{x}\|_{\infty}
	 \xrightarrow{n\rightarrow\infty} 0
    \end{equation*}
    
    Daraus folgt aber, dass $p(x) = e^{x}$, was ein Widerspruch zu
    unserer Annahme ist, da $e^{x}\not\in\mathcal{P}$.
  \end{proof}
\end{bsp}

\begin{bsp}\label{bsp:1.1.1/3}
  Der Raum $C([0,1])$ mit der Norm $\|f\|_{1} = \int_{0}^{1} |f(t)|\,dt$ ist
  nicht vollständig.
  
  \begin{proof}
    $m \geq 2$
    \begin{equation*}
      f_{m}(t) =
	 \begin{cases}
	   0 & 0\leq t < \frac12\\
	   m(t-\frac12) & \frac12 \leq t < \frac12+\frac1m =: a_{m}\\
	   1 & a_{m} \leq t \leq 1
	 \end{cases}
    \end{equation*}
    
    Sei $n > m$ $\Rightarrow$ $\|f_{n}-f_{m}\|_{1}\leq \frac1m \rightarrow
    0$ $\Rightarrow$ $(f_{n})_{n}$ ist Cauchyfolge.

    Annahme: Es existiert ein $f\in C([0,1])$ mit $\|f_{n}-f\|_{1}
    \rightarrow 0$. Dann ist
    \begin{equation*}
      \|f_{n}-f\|_{1} = \int_{0}^{\frac12}|f(t)|\,dt+
	 \int_{\frac12}^{a_{m}}|f_{n}(t)-f(t)|\,dt + \int_{a_{m}}^{1}|1-f(t)|\,dt
    \end{equation*}
    Für $n\rightarrow \infty$ geht $a_{m} \rightarrow \frac12$ und damit
    gilt:
    \begin{equation*}
      \int_{0}^{\frac12}|f(t)|\,dt = 0\quad\text{und}\quad
      \int_{\frac12}^{1}|1-f(t)|\,dt = 0
    \end{equation*}
    Die Funktion $f$ muss also die Gestalt
    \begin{equation*}
      f(t) = \begin{cases}
	       0 & :0 < t \leq \frac12\\
	       1 & :\frac12 < t \leq 1
	     \end{cases}
    \end{equation*}
    haben, was einen Widerspruch zu der Annahme $f$ sei stetig darstellt.
  \end{proof}
\end{bsp}

\begin{satz}\label{satz:1.1.1/1}
  $(E, \|\cdot\|)$ normierter Vektorraum.
  
  Dann gilt: $E$ vollständig $\Longleftrightarrow$ $\forall (x_{n})_{n}$ mit
  $\sum\|x_{n}\|<\infty$ $\Rightarrow$ $\exists x\in E$ mit $x=\sum
  x_{n}$. (jede absolutkonvergente Reihe (bzgl. $\|\cdot\|$) ist konvergent)
  
  \begin{proof}
    1. Übungsserie Aufgabe 3
  \end{proof}
\end{satz}

% 15.10.04

\begin{satz}\label{satz:1.1.1/2}
  $A\not= \emptyset, B(A) := \{ f: A\rightarrow \K: \|f\|_{\infty} :=
  \sup_{x\in A}|f(x)|<\infty \}$
  
  Dann ist $B$ ist Banachraum bzgl. $\|\cdot\|$.
  
  \begin{proof}
    Verwenden \thref{satz:1.1.1/1}. Es sei $(f_{n})_{n}\subset B(A)$ mit
    $\sum_{n=1}^{\infty} \|f_{n}\|_{\infty} < \infty$. Weiterhin sei
    $x\in A$.
    \begin{flalign*}
      \Longrightarrow&\Bigl|\sum_{k=m}^{n} f_{k}(x)\Bigr| \leq
         \sum_{k=m}^{n}\|f_{k}\|_{\infty}\leq
	 \epsilon\quad\text{ für }m,n \geq n_{0}(\epsilon)&\\
      \Longrightarrow& \Bigl(\sum_{k=1}^{n}f_{k}(x)\Bigr)_{n}
         \text{ ist C.F. in }\K \text{ und da $\K$ vollständig ist }
	 \Longrightarrow \exists f(x) = \sum_{k=0}^{\infty} f_{k}(x)&\\
      \Longrightarrow& f: A\rightarrow\K \text{ Es gilt: } |f(x)|\leq
	 \sum_{k=0}^{\infty}\|f_{k}(x)\|_{\infty}<\infty&\forall x
    \end{flalign*}
    Somit ist auch $f\in B(A)$, weshalb $B(A)$ vollständig und damit ein
    Banachraum ist.
  \end{proof}
\end{satz}

\begin{folger}
  $A=\N,\ B(\N) = l_{\infty}(\N)=\bigl\{ (a_{n})_{n}:
  \|a\|_{\infty}=\sup_{n}|a_{n}|<\infty\bigr\}$ ist Banachraum.
\end{folger}

\begin{bsp}\index{$l_{n}$}\label{bsp:1.1.1/4}
  \begin{equation*}
    l_{1}(\N) := \{ (a_{n})_{n} : \|a\|_{1}:=
       \sum_{i=1}^{\infty}|a_{i}|<\infty\}
  \end{equation*} ist Banachraum.
  
  \begin{proof}
    Es sei $(a^{j})_{j\in\N}\subset l_{1}(\N)$ eine Cauchyfolge. Es sei
    $\epsilon>0$.
    \begin{flalign*}
      \Rightarrow\quad &\exists j_{0}(\epsilon)\forall j,k \geq j_{0}(\epsilon):
	 \|a^{j}-a^{k}\|_{1}=\sum_{n=1}^{\infty}|a_{n}^{j}-a_{n}^{k}|
	 \leq \epsilon \taglabel{eq:1}&\\
      \Rightarrow\quad &\text{Für jedes feste $n\in \N$ ist $(a_{n}^{j})_{j}$
	 Cauchyfolge in $\K$.}&\\
      \Rightarrow\quad & \exists a_{n}\in \K \text{ mit } a_{n}^{j} \rightarrow
	 a_{n}\in\K. \text{Setzen } a := (a_{n})_{n}.&
    \end{flalign*}

    Zeigen: $a_{j}\rightarrow a$ in $l_{1}(\N)$. Aus Gleichung~\eqref{eq:1}
    folgt für jedes $N\in\N$:
    \begin{gather*}
      j,k\geq j_{0}(\epsilon): \sum_{n=1}^{N}|a_{n}^{j}-a_{n}^{k}|\leq
	 \epsilon\\
      k\rightarrow\infty \Rightarrow \sum_{n=1}^{N}|a_{n}^{j}-a_{n}|\leq
	 \epsilon\\
      N\rightarrow\infty \Rightarrow
	 \sum_{n=1}^{\infty}|a_{n}^{j}-a_{n}|\leq \epsilon\quad j\geq
	 j_{0}(\epsilon)\\
      \sum|a_{n}| \leq \sum\underbrace{|a_{n}-a_{n}^{j}|}_{\leq
	 c}+\sum\underbrace{|a_{n}^{j}|}_{\leq c}\leq \infty\\
      \Rightarrow a_{n}^{j} \rightarrow a \in l_{1}(\N)
    \end{gather*}
  \end{proof}
\end{bsp}

\begin{defini}\label{def:1.1.1/4}
  $(E, \|\cdot\|)$ normierter Vektorraum. (oder allgemein ein metrischer Raum)
  \begin{enumerate}
   \item\label{en1} $M\subset E$ heißt \highl{dicht} in $E$
    $:\Longleftrightarrow$ es eine Folge $(x_{j})_{j}\subset M$ gibt,
    deren Grenzwert in $E$ liegt: $x_{j} \rightarrow x \in E$.
   \item $E$ heißt \highl{spearabel} $:\Longleftrightarrow$ es eine
    abzählbare und dichte Teilmenge in $E$ gibt.
  \end{enumerate}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $\Q\subset\R$ dicht $\Longrightarrow$ $\R$ separabel
   \item $\Q^{n}\subset\R^{n}$ dicht $\Longrightarrow$ $\R^{n}$ separabel
   \item äquivalent zum Punkt~\ref{en1} in \thref{def:1.1.1/4} ist:
    \begin{equation*}
      \forall \epsilon>0\:\forall x\in E\:\exists x_{\epsilon} \in M:
      \|x-x_{\epsilon}\|<\epsilon \Longleftrightarrow x_{\epsilon}\in
	 K_{\epsilon}(x), x\in M
    \end{equation*}
  \end{enumerate}
\end{bemerk}

\begin{bsp}\label{bsp:1.1.1/5}
  $l_{1}(\N)$ ist separabel. ($l_{\infty}(\N)$ ist nicht separabel. s. Ü.A.)
  
  \begin{proof}
    Da die Menge $C_{00}(\N):= \{ a: \exists n\in \N \ \forall k>n :
    a_{k}=0\}$ dicht im $l_{1}(\N)$ liegt \help{wieso?}, lässt sich jedes
    $a = (a_{1},\ldots,a_{n},0,\ldots,0)\in C_{00}(\N)$ durch Folgen aus
    $C_{00}(\N)$ mit rationalen Gliedern approximieren. Die Menge
    $M_{rat} = \{a: a\in C_{00}, a_{i}\in\Q\}$ ist eine abzählbare und
    dichte Teilmenge in $l_{1}$, woraus die Behauptung folgt.
  \end{proof}
\end{bsp}

\subsection{Vervollständigung von normierten Vektorraum}

\begin{description}
 \item[Wiederholung:]
  $(E,\|\cdot\|)$ sei normierter Vektorraum. $U\subset E$ heißt
  abgeschlossen $\Leftrightarrow$ $U$ enhält alle Häufungspunkte
  $\Leftrightarrow$ $\forall (x_{j})\subset U, x_{j} \rightarrow x: x\in
  U$ $\Leftrightarrow$ $\overline{U}=U$ wobei $\overline{U}:=
  U\cup\{\text{Häufungspunkte von U}\}$ der Abschluss von $U$ ist.

  $U\subset E$ heißt abgeschlossener Teilraum $:\Leftrightarrow$ $U$
  Teilraum und abgeschlossen.
\end{description}

\begin{bemerk}
  $(E,\|\cdot\|)$ Banachraum, $U\subset E$ Teilraum.
  
  Dann:\todo{hier ist irgendwie viel Platz}
  \begin{enumerate}
   \item $U$ abgeschlossen $\Longleftrightarrow$ $(U, \|\cdot\|)$
    vollständig. (Banachraum)
   \item $U$ dicht in $E$ $\Longleftrightarrow$ $\overline{U}=E$
  \end{enumerate}
\end{bemerk}

\begin{bsp}\label{bsp:1.1.2/1}
  \begin{enumerate}
   \item $U=\{ x\in\R^{n}: x_{n}=0\}$ ist abgeschlossener Teilraum von
    $R^{n}$\\
    $U = \{x\in\R^{2}: ax_{1}+bx_{2}=0\}$ ($(a,b)\not=0$) ist ein
    abgeschlossener Teilraum des $\R^{2}$.\\
    Die Ebene $U=\{x\in\R^{3}: a_{1}x_{1}+ a_{2}x_{2}+a_{3}x_{3} =0\}$
    ($(a_{1},a_{2},a_{3})\not=0$) in $\R^{3}$ ist ein abgeschlossener
    Teilraum.
   \item Der Raum $C_{0}(\N) := \{ a\in l_{\infty}(\N):
    \lim_{j\rightarrow\infty} a_{j} = 0\} \subset l_{\infty}(\N)$ ist
    abgeschl. Teilraum. Insbesondere ist $C_{0}(\R)$ Banachraum bzgl. der
    Unendlichnorm $\|\cdot\|_{\infty}$.
    
    \begin{proof}
      Es sei $(a^{n})_{n}$ Folge im $C_{0}(\N)$ und $a^{n}\rightarrow
      a\in l_{\infty}(\N)$. z.z. $a\in C_{0}(\N)$.
      
      Für $\epsilon>0$ gilt:
      \begin{align*}
	|a_{j}|\leq |a_{j}-a_{j}^{n}| + |a_{j}^{n}|
	   \leq \underbrace{\|a-a^{n}\|_{\infty}}_{\leq \epsilon, n \geq
	   n_{0}(\epsilon)} + |a_{j}^{n}| \leq 2\epsilon && j\leq j_{0}(\epsilon)
      \end{align*}
      $\Rightarrow a\in C_{0}(\N)$
    \end{proof}
   \item $C_{00}(\N)$ liegt dicht in $C_{0}(\N)$. Somit ist $C_{0}(\N)$
    separabel und $\overline{C_{00}(\N)} = C_{0}(\N)\subsetneq
    l_{\infty}(\N)$.   

    \begin{proof}
      \begin{description}
       \item["`$\Rightarrow$"'] $(a^{n})_{n}\subset C_{00}(\N),
	a^{n}\rightarrow a\in l_{\infty}(\N)$ $\Rightarrow$ $a\in
	C_{0}(\N)$ wie in \thref{bsp:1.1.1/5}
       \item["`$\Leftarrow$"'] $a\in C_{0}(\N) \Rightarrow
	\lim_{j\rightarrow\infty} a_{j}=0$
	
	$ \epsilon > 0 \Rightarrow
	\exists j_{0}(\epsilon) \forall j\geq j_{0}(\epsilon):
	|a_{j}|\leq \epsilon$
	
	$j \geq j_{0}(\epsilon) \Rightarrow \|a-a^{j}\|_{\infty}=
	\sup_{k>j}|a_{j}| \leq \epsilon$ wobei
	$a^{j}=(a_{1},\ldots,a_{j},0,\ldots)\in C_{00}(\N)$
	
      \end{description}
    \end{proof}
  \end{enumerate}
\end{bsp}

\noindent\underline{\textbf{Problem:}} Kann man für \emph{jeden}
normierten Vektorraum $E$ einen Banachraum $F$ mit
$\overline{E}=\overline{F}$ finden?

\begin{defini}
  $(E,\|\cdot\|_{E}), (F, \|\cdot\|_{F})$ normierte Vektorraum.
  
  $E$ und $F$ heißten \highl{isometrisch isomorph} $:\Longleftrightarrow$
  $\exists$ lineare Abbildung $L:E \rightarrow F$ mit
  \begin{enumerate}
   \item $\|Lx\|_{F} = \|x\|_{E}\quad\forall x\in E$
   \item $L$ surjektiv ($L(E)=F$)
  \end{enumerate}
  
  L heißt \highl{isometrischer Isomorphismus}
\end{defini}

\begin{satz}
  \begin{enumerate}
   \item Jeden normierten Vektorraum kann man isometrisch isomorph auf einen
    dichten Teilraum eines geeigneten Banachraus abbilden.
   \item Sämtliche Banachräume mit dieser Eigenschaft sind
    untereinander isometrisch isomorph.
  \end{enumerate}
  
  \begin{proof}
    \begin{schritte}
     \item konstruieren $F$ mit (1).
      
      Betrachten dazu alle Cauchyfolgen in $E$. Zwei Cauchyfolgen
      $(x_{j})_{j}$ und $(x_{j}')_{j}$ in $E$ heißen äquivalent
      $:\Leftrightarrow$ ($x_{j}-x_{j}')_{j}$ ist Nullfolge.
      \begin{equation*}
	F := \{ f: f \text{ Äquivalenzklasse von C.F. bzgl. dieser
	   Relation } \}
      \end{equation*}
      
      $f\sim (x_{j}), g\sim (y_{j}) f+g := $ Äquivalenzklasse
      die $(x_{j}-y_{j})$ enthält.
      
      $\lambda f := $ Äquivalenzklasse, die $(\lambda x_{j})$ enthält.
      unabhängig von Auswahl des Repräsentanten.
      
      $0 := $ Äquivalenzklasse, die $(0,0,\ldots)$ (alle Nullfolgen)
      
      $\|f\|_{F} := \lim_{j\rightarrow\infty}\|x_{j}\|_{E}$ (existiert,
      da $(\|x_{j}\|_{E})$ C.F. im $\R$! ( $| \|x_{j}\|_{E}- \|x_{k}\|_{E}
      | \leq \|x_{j}-x_{k}\|$ ))
      
      $(F, \|\cdot\|_{F})$ ist also ein normierter Vektorraum.
      
      Wir setzen nun für $x\in E$ $Lx$, die Äquivalenzklasse, die $(x,x,x,\ldots)$
      enthält, ein und erhalten: $\|Lx\|_{F} = \|x\|_{E}$, d.\,h. $L :E
      \rightarrow L(E)\subset F$ ist isometrisch isomorph.
      
      zeigen $\overline{L(E)}=F$, $F$ vollständig $\Rightarrow$ (1)
    \end{schritte}
  \end{proof}
\end{satz}

\begin{bemerk}
  Ein solcher Banachraum heißt \highl{Vervollständigung} von $E$. Alle
  diese Banachräume sind "`Kopien"' und werden nicht unterschieden.
\end{bemerk}

\subsection{Kompaktheit}

$(X, d)$ metrischer Raum

\begin{defini}
  $(X,d)$ metrischer Raum
  
  $A\subset X$ heißt \highl{kompakt} (folgenkompakt)
  $:\Longleftrightarrow$ jede Folge in $A$ besitzt eine konvergente
  Teilfolge mit Grenzwert in $A$.
\end{defini}

\begin{bemerk}\label{bem:1}
  \begin{enumerate}
   \item 
    $A$ kompakt $\Longleftrightarrow$ jede unendliche Teilmenge von $A$
    besitzt einen Häufungspunkt in $A$.
   \item $A\subset X=\K^{n}$ kompakt $\Leftrightarrow$ $A$ beschränkt
    und abgeschlossen.
   \item $X,Y$ metrische Räume, $f: X\rightarrow Y$ stetig:
    
    $A \subset X$ kompakt $\Longrightarrow$
    \begin{enumerate}
     \item $f(A)$ kompakt
     \item $f$ ist gleichmäßig stetig auf $A$
    \end{enumerate}
    
    Für $Y=\R$ gilt: $\exists \min_{x\in A} f(x), \exists \max_{x\in A} f(x)$
  \end{enumerate}
\end{bemerk}

$r>0, K_{r}(x_{0}) := \{ x\in X: d(x, x_{0}) < r\}$ offene Kugel

$x_{0}\in X, U\subset X$ heißt offen $:\Longleftrightarrow$ $\forall
x\in U \exists r>0: K_{r}(x)\subset U$

\begin{defini}
  $I$ Indexmenge, $U_{\alpha}$ offen für jedes $\alpha \in I$
  
  $(U_{\alpha})_{\alpha\in I}$ heißt \highl{offene Überdeckung} von $A$
  $:\Longleftrightarrow$ $A\subset \bigcup_{\alpha\in I} U_{\alpha}$
  
  $I_{0}\subset I$ $(U_{\beta})_{\beta\in I_{0}}$ Teilüberdeckung von
  $A$ $:\Longleftrightarrow$ $A\subset \bigcup_{\beta\in I_{0}} U_{\beta}$
\end{defini}

\begin{satz}[Heine-Borel]\label{satz:heineborel}
  $(X,d)$ metr. Raum, $A\subset X$
  
  Dann sind die folgenden beiden Aussagen äquivalent:
  \begin{enumerate}
   \item $A$ ist folgenkompakt
   \item $A$ ist \highl{überdeckunsgkompakt}, d.\,h. aus jeder offenen
    Überdeckung von $A$ kann man eine \highl{endliche Teilüberdeckung}
    auswählen.
  \end{enumerate}
  
  \begin{proof}
    \begin{schritte}
     \item $A\subset X$ kompakt, $(U_{\alpha})_{\alpha}$ offene
      Überdeckung. Dann gilt:
      
      \begin{equation*}\label{eq:2}
        \exists\lambda>0 \forall
	   x\in A \exists U_{\alpha}: K_{\lambda}(x)\subset U_{\alpha}
      \end{equation*}
      
      Ann: \eqref{eq:2} gilt nicht $\Longrightarrow$
      \begin{equation*}
	\forall n\in \N\exists x_{n}\in A\forall U_{\alpha}:
	   K_{\frac1n}(x_{n})\not\subset U_{\alpha}
      \end{equation*}
      
      Betrachten $(x_{n})_{n\in \N}\subset A$. $A$ kompakt
      $\Longrightarrow$ $\exists$ konv. Teilfolge $(x_{n_{k}})_{k\in\N}$
      mit $x_{n_{k}}\rightarrow x_{0}\in A$
      
      $\exists \alpha_{0}\in I: x_{0}\in U_{\alpha_{0}}$
      $U_{\alpha_{0}}$ offen $\Longrightarrow$ $\exists r_{0}>0
      K_{r_{0}}(x_{0})\subset U_{\alpha}$
      
      Wähle $n_{k}$, so dass $d(x_{n_{k}}, x_{0})<\frac{r_{0}}2$ und
      $\frac1{n_{k}}<\frac{r_{0}}2$. Mithilfe der Dreiecksungleichung
      folgt daraus:
      \begin{equation*}
	K_{\frac1{n_{k}}}(x_{n_{k}})\subset K_{r_{0}}(x_{0})\subset U_{\alpha_{0}}
      \end{equation*}
      was einen Widersrpruch zur gemachten Annahme ist.
     \item (1) $\Rightarrow$ (2)\quad Es sei $(U_{\alpha})_{\alpha}$
      offene Überdeckung von $A$. Sei $\lambda$ wie im vorherigen
      Schritt. Wählen
      \begin{gather*}
	y_{1}\in A\\
	y_{2}\in A\setminus K_{\lambda}(y_{1})\\
	y_{3}\in A\setminus (K_{\lambda}(y_{1})\cup K_{\lambda}(y_{2}))\\
	\ldots\\
	y_{n}\in A\setminus \bigcup_{j=1}^{n-1}K_{\lambda}(y_{j})
      \end{gather*}
      
      Beh.: $\exists n\in \N: A\setminus \bigcup_{j=1}^{n-1}
      K_{\lambda}(y_{j})=\emptyset$ $(\Longrightarrow A\subset
      \bigcup_{j=1}^{n} K_{\lambda}(y_{j})\subset \bigcup_{j=1}^{n}
      U_{\alpha_{j}}$
      
      Ann: $A\setminus \bigcup_{j=1}^{n-1} K_{\lambda}(y_{j})
      \not=\emptyset\; \forall n\in \N$ $\Rightarrow$ $\exists
      (y_{j})_{j\in \N}: d(y_{j}, y_{k}) \geq \lambda>0\; \forall j \not=
      k$ $\Longrightarrow$ $(y_{j})_{j}$ hat keine konvergente Teilfolge
      $\Longrightarrow$ Widerspruch zur Kompaktheit
     \item (2) $\Rightarrow$ (1)\quad $M\subset A$ unendliche Teilmenge
      
      Ann: $M$ besitzt keinen Häufungspunkt $\Longrightarrow \forall
      x\in M \exists U_{x}$ offen $M\cap U_{x}=\{x\}$
      
      $M$ agbeschlossen $\Longrightarrow$ $X\setminus M$ offen
      $\Longrightarrow$ $A\subset (\bigcup_{x\in M} U_{x})\cup(X\setminus M)$
      
      (2) $\Longrightarrow \exists$ endliche Teilüberdeckung, diese
      enthält nur endlich viele Elemente von $M$ $\Longrightarrow$
      Widerspruch zur Annahme
    \end{schritte}
  \end{proof}
\end{satz}

\begin{folger}\label{fol:1.1.3/1}
  $(X,d)$ kompakt $\Longrightarrow$ $X$ ist separabel, d.\,h. $\exists$
  abzählbare dichte Teilmenge in $X$
  
  \begin{proof}
    $\forall j\in \N$ ist $X = \bigcup_{x\in X} K_{\frac1j}(x)$
    
    Aus \thref{satz:heineborel} $\Longrightarrow$ $\exists$ endliche Teilüberdeckung
    \begin{equation*}
      (K_{\frac1j}(x_{k}^{j})_{k=1,\ldots,n_{j}}\qquad\forall j\in\N
	 \label{gl2}
    \end{equation*}
    
    $\Longrightarrow$ $M := \{ x_{k}^{j}: j\in \N, k=1,\ldots,n_{j}\}$ $M$
    ist abzählbar und dicht in $X$: $x\in X, \epsilon>0$ wähle
    $j>\frac1{\epsilon}$ Aus (\ref{gl2}) folgt $\exists k$ mit $x\in
    K_{\frac1j}(x_{k}^{j}) \Longrightarrow d(x,x_{k}^{j})<\epsilon$
    $\Longrightarrow$ Folgerung
  \end{proof}
\end{folger}

\begin{defini}
  $(X,d)$ metrischer Raum
  
  $A\subset X$ heißt \highl{präkompakt} $:\Longleftrightarrow$ jede
  Folge in $A$ besitzt eine konvergente Teilfolge (deren Häufungspunkt
  nicht in $A$ liegen muss).
\end{defini}

\begin{folger}
  \begin{enumerate}
   \item $A$ kompakt $\Longleftrightarrow$ $A$ präkompakt und
    angeschlossen
   \item $A$ präkompakt $\Longrightarrow$ $A$ beschränkt
  \end{enumerate}
\end{folger}

\begin{defini}
  $\epsilon>0, B\subset X$ heißt \highl{$\epsilon$-Netz} für
  $A\subset X$ $:\Longleftrightarrow$ $A\subset \bigcup_{x\in B}
  K_{\epsilon}(x)$
\end{defini}

\begin{satz}\label{satz:1.1.3/2}
  $(X,d)$ vollständiger metrischer Raum
  
  Dann gilt: $A\subset X$ präkompakt $\Longleftrightarrow \forall
  \epsilon>0 \exists$ für $A$ ein endliches $\epsilon$-Netz.
  
  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] Es sei $A$ präkompakt.
      
      Ann: $\exists \epsilon \wedge \not\exists$ endliches
      $\epsilon_{0}$-Netz für $A$.
      
      Es sei $x_{1}\in A \Longrightarrow \exists x_{2}\in A$ mit
      $d(x_{1},x_{2})>\epsilon_{0}$
      
      $x_{1},x_{2}\in A \Longrightarrow \exists x_{3}\in A$ mit
      $d(x_{1},x_{3})>\epsilon_{0} \wedge d(x_{2},x_{3})>\epsilon_{0}$
      \ldots
      $x_{1},\ldots,x_{j-1}\in A \Longrightarrow \exists x_{j}\in A:
      d(x_{k}, x_{j}) >\epsilon_{0}\quad \forall k=1,\ldots,j-1$
      
      $\Longrightarrow$ $\exists$ Folge $(x_{j})_{j\in\N}\subset A:
      d(x_{j}, x_{k})>\epsilon_{0}\quad\forall j,k$ $\Longrightarrow$
      $(x_{j})\subset A$ hat keine konv. Teilf. -- Widerspruch zur
      Vorraussetzung
     \item["`$\Leftarrow$"'] Es sei $M\subset A$ undenliche Menge.
      
      Für jedes $\epsilon$ wird $M$ durch endlich viele
      $\epsilon$-Kugeln überdeckt $\Longrightarrow$ Für jedes
      $\epsilon$ existiert eine $\epsilon$-Kugel, die undendlich
      viele Elemente aus $M$ enthält
      
      $\epsilon=1$ $\Longrightarrow$ $\exists y_{1}\exists$ Folge
      $(x_{j}^{1})_{j}\in M: x_{j}^{1}\not x_{k}^{1} (j\not= k)$ und
      $(x_{j}^{1})_{j}\subset K_{1}(y_{1})$
      
      $\epsilon=\frac12$ $\Longrightarrow$ $\exists y_{2}\exists$
      Teilfolge $(x_{j}^{2})_{j}\subset (x_{j}^{1})_{j}:
      (x_{j}^{2})_{j}\subset K_{\frac12}(y_{2})$
      
      \ldots
      
      $\epsilon=\frac1n$ $\Longrightarrow$ $\exists y_{n}\exists$
      Teilfolge $(x_{j}^{n})_{j}\subset (x_{j}^{n-1})_{j}:
      (x_{j}^{n})_{j}\subset K_{\frac1n}(y_{n})$
      
      Betrachten die Diagonalfolge $(x_{j}^{j})_{j\in \N}: j\leq n
      \Longrightarrow x_{j}^{j}\in K_{\frac1n}(y_{n})$
      
      Für $j,l\geq n$ $\Longrightarrow$ $d(x_{j}^{j},x_{l}^{l})\leq
      d(x_{j}^{j}, y_{n}) + d(x_{l}^{l},y_{n}) < \frac2n$
      $\Longrightarrow$ $(x_{j}^{j})$ ist Cauchyfolge und Teilfolge von
      $(x_{j}^{1})_{j}\subset M$
      
      X ist vollständig $\Longrightarrow$ $\exists x\in X:
      x_{j}^{j}\rightarrow x \Longrightarrow x$ ist HP von $M$
      $\Longrightarrow$ $A$ ist präkompakt.
    \end{description}
  \end{proof}
\end{satz}

\begin{folger}\label{fol:1.1.3/3}
  $(X, d)$ vollst. metr. Raum.
  
  Dann: $A\subset X$ präkompakt $\Longleftrightarrow$ $\forall
  \epsilon>0$ existiert ein präkompaktes $\epsilon$-Netz für $A$
  
  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] $A$ präkompakt $\Longrightarrow$ $\exists$
      endliches $\epsilon$-Netz $B_{\epsilon}$: $B_{\epsilon}$
      präkompakt, da endlich.
     \item["`$\Leftarrow$"'] $\exists$ präkompaktes $\epsilon$-Netz
      $B_{\epsilon}$ für $A$.
      
      \thref{satz:1.1.3/2} $\Longrightarrow$ $\exists$ endliches
      $\epsilon$-Netz $N_{\epsilon}$ für $B_{\epsilon}$.
      $N_{\epsilon}$ ist $2\epsilon$-Netz für $A$ nach
      Dreickecksungl.
    \end{description}
  \end{proof}
\end{folger}

\begin{bsp}
  Daraus, dass $l_{1}(\N)$ beschränk ist, folgt \emph{nicht}, dass
  $l_{1}(\N)$ präkompakt ist.
  
  $A=\{ e^{j}=(0,\ldots,0,1,0,\ldots): j\in \N\}\subset l_{1}(\N)
  \Longrightarrow \forall j\not= k: d(e^{j}, e^{k}) =
  \|e^{j}-e^{k}\|_{1}=2$. Es existiert also eine nichtkonvergente
  Teilfolge von $l_{1}(\N)$.
\end{bsp}

\begin{bsp}\label{bsp:1.1.3/2}
  $A\subset l_{1}(\N)$ präkompakt $\Longleftrightarrow$ $A$ beschränkt
  und
  \begin{equation*}
    \sup_{a\in A}\sum_{j=n}^{\infty}|a_{j}|
       \xrightarrow{n\rightarrow\infty} 0\label{gl3}
  \end{equation*}
  
  \begin{proof}
    \begin{description}
     \item["`$\Rightarrow$"'] $A$ präkompakt $\Longrightarrow$ $A$
      beschränkt
      
      $\epsilon>0$ $\Longrightarrow$ $\exists$ endliches
      $\epsilon$-Netz $\{b^{1},\ldots,b^{m}\}\subset l_{1}(\N)$
      
      Es ist
      \begin{gather*}
	\sum_{j=n}^{\infty}|b_{j}^{k}|\leq \epsilon\qquad \text{für}
	   n\geq n(\epsilon) \text{ unabhängig von } k\\
	a\in A, n\geq n(\epsilon) \Longrightarrow
	   \sum_{j=n}^{\infty}|a_{j}| \leq
	   \sum_{j=n}^{\infty}|a_{j}-b_{j}^{k}| +
	   \sum_{j=n}^{\infty}|b_{j}^{k}| \leq
	   \underbrace{\|a-b^{k}\|_{1}}_{<\epsilon \text{falls $k$
	   passend}} + \epsilon < 2\epsilon
      \end{gather*}
     \item["`$\Leftarrow$"'] Es sei $(a^{k})_{k}\subset A,
      a^{k}=(a^{k}_{1},a^{k}_{2},\ldots)$
      
      $(a^{k})_{n}$ beschränkt in $l_{1}(\N)$ $\Longrightarrow$
      $(a_{j}^{k})_{k}$ ist beschränkt in $\K$ für jedes $j\in\N$.
      \begin{enumerate}
       \item Wählen Teilfolge $(a^{1,k})_{k}\subset (a^{k})_{k}:
	a^{1,k}_{1}\xrightarrow{k\rightarrow\infty} a_{1}$
       \item Wählen Teilfolge $(a^{2,k})_{k}\subset (a^{1,k})_{k}:
	a^{2,k}_{2}\xrightarrow{k\rightarrow\infty} a_{2}$
	
	\ldots
       \item wählen Teilfolge $(a^{l,k})_{k}\subset (a^{l-1,k})_{k}:
	a^{l,k}_{l} \xrightarrow{k\rightarrow\infty} a_{l}$
	
	\ldots
      \end{enumerate}
      
      Betrachten Diagonalfolge $(b^{k})_{k} = (a^{k,k})_{k}\subset A$:
      
      \begin{align*}
	l \text{ fest} \Longrightarrow &(b^{k})_{k=l}^{\infty} =
	   (a^{k,k})_{k=l}^{\infty}\\
	 & (b^{k}_{l})_{k=l}^{\infty} =
	   (a^{k,k}_{l})_{k=l}^{\infty}\subset
	   (a^{l,k}_{l})_{k=l}^{\infty} \Rightarrow b_{l}^{k}
	   \xrightarrow{k\rightarrow\infty} a_{l}\\
	\Longrightarrow& (b_{l}^{k})_{k} \text{ ist konvergent }\qquad\forall l\in\N\\
	\Longrightarrow&
	   \sum_{l=1}^{n}|b_{l}^{k}-a_{l}|\qquad\text{konvergent für}
	   k\rightarrow\infty\\
	\intertext{wählen $n$ so dass \ref{gl3} gilt}\\
	\Longrightarrow& \sum_{l=1}^{\infty}|b_{l}^{k}-b_{l}^{j}| =
	   \underbrace{\sum_{l=1}^{n}|b_{l}^{k}-
	     b_{l}^{j}|}_{<\epsilon \text{für} j,k>j_{0}(\epsilon)} +
	    \underbrace{\sum_{l=n+1}^{\infty}|b_{l}^{k}-
	      b_{l}^{j}|}_{<2\epsilon}\\
	\Longrightarrow&  (b^{k})_{k} \text{Cauchyfolge in $l_{1}(\N)$}\\
	\Longrightarrow& \text{Konvergenz}
      \end{align*}
    \end{description}
  \end{proof}
\end{bsp}

\subsection{Endlich-dimensionale Vektorräume}

\begin{bsp}
  \begin{itemize}
   \item $\K^{n} (\K=\R, \K=\C)$
   \item $\mathcal{P}^{n} := \{ p(x): p(x) \text{Polynom vom Grad} \leq
    n\}$
    
    $\{1,x,\ldots,x^{n}\}$ Basis, $\dim \mathcal{P}=n+1$
   \item $\mathcal{T}^{n} := \{ t(x): t(x)=\sum_{k=-n}^{n} c_{k} e^{ikx},
    c_{k}\in \C\}$
    
    $\{e^{ikx}: k=-n,\ldots,n\}$ Basis, $\dim\mathcal{T}^{n}=2n+1$
  \end{itemize}
  
  \underline{Lineare Algebra:} $(E,\K) \dim E=n \Longrightarrow E\cong
  \K^{n}$ isomorph
\end{bsp}

\begin{defini}
  $E$ Vektorraum
  
  Zwei Normen $\|\cdot\|$ und $\|\cdot\|^{\ast}$ heißen äquivalent
  $:\Longleftrightarrow$ es zwei Zahlen $c_{1},c_{2}>0$ gibt, so dass für
  alle $x\in E$ gilt:
  \begin{equation*}
    c_{1}\|x\|^{\ast}\leq \|x\|\leq c_{2}\|x\|^{\ast}
  \end{equation*}
\end{defini}

\begin{bemerk}
  Äquivalente Normen erzeugen gleiche Konvergenz und gleiche Topologien,
  d.\,h. $M$ ist offen bzgl. $\|\cdot\| \Longleftrightarrow M$ ist offen
  bzgl. $\|\cdot\|^{\ast}$
\end{bemerk}

\begin{lemma}
  $E$ normierter Vektorraum über $\K$, $\{x_{1},\ldots,x_{n}\}$ linear unabhängig
  
  Dann existiert ein $c>0$, so daß für alle
  $a=(a_{1},\ldots,a_{n})\in\K^{n}$ gilt:
  \begin{equation*}
    \|a\|_{1} = |a_{1}|+\ldots+|a_{n}|\ \leq\ c\:\|a_{1}x_{1}+\ldots+
       a_{n}x_{n}\|_{E}
  \end{equation*}
  
  \begin{proof}
    Man betrachte die Funktion $\Phi:\K^{n}\rightarrow E$ mit $\Phi(a) :=
    a_{1}x_{1}+ \ldots + a_{n}x_{n}$. $\Phi$ ist linear und stetig, da
    \begin{gather*}
      \|\Phi(a)\|_{E} \leq \sum_{j=1}^{n}|a_{j}|\
      \max_{k}\bigl\{\|x_{k}\|_{E}\bigr\} = c_{0}\|a\|_{1}\\
      \|a_{i}-a\|_{1}\rightarrow 0 \Longrightarrow
    	 \|\Phi(a_{i})-\Phi(a)\|_{E}\rightarrow 0
    \end{gather*}

    % \begin{align*}
    %   \Longrightarrow& \Phi \text{ist linear und stetig, denn}\\
    %   & \|\Phi(a)\|_{E}\leq
    % 	 \sum_{j=1}^{n}|a_{j}|\ \max_{k}\bigl\{\|x_{k}\|_{E}\bigr\} =
    % 	 c_{0}\|a\|_{1}\\
    %   & \|a_{i}-a\|_{1}\rightarrow 0 \Longrightarrow
    % 	 \|\Phi(a_{i})-\Phi(a)\|_{E}\rightarrow 0
    % \end{align*}
    
    Es sei $A=\{ a\in \K^{n}: \|a\|_{1}=1\}$ kompakt im $\K^{n}$.
    Aus der Stetigkeit von $\Phi$ folgt nach \thref{bem:1}, dass $\Phi(A)
    = \{\Phi(a): \|a\|_{1}=1\}\subset E$ ebenfalls kompakt ist.
    
    Die Norm $\|\cdot\|_{E}: E\rightarrow \R$ wiederrum ist eine stetige
    Abbildung, woraus folgt, dass das $\min\Phi(A) = \min\{\|\Phi(a)\|:
    \|a\|_{1}=1\}$ existiert und $>0$ sein muss.
    \begin{equation}\label{eq:3}
       0 < \tilde{c} := \min\Phi(A) = \min\{\|\Phi(a)\|: \|a\|_{1}=1\}
    \end{equation}
    Denn aus $\|a_{1}x_{1} + \ldots + a_{n}x_{n}\|_{E}=0$ folgt, dass
    $a=0$ ist, da die $x_{1},\ldots,x_{n}$ linear unabhängig sind. $a=0$
    ist aber ein Widerspruch zu $\|a\|_{1}=1$.

    Für alle $a\in\K^{n}$ mit $\|a\|\not=0$ gilt:
    \begin{align*}
      \|a_{1}x_{1}+\ldots+a_{n}x_{n}\|_{E} &= \|a\|_{1}
      \biggl\|\frac{a_{1}}{\|a\|_{1}}x_{1}+\ldots+
      \frac{a_{n}}{\|a\|_{1}}x_{n}\biggr\|_{E}\\
      &\geq \|a\|_{1}\tilde{c}&&\text{nach \eqref{eq:3}}\\[3ex]
	 \|a_{1}\| &\leq \frac1{\tilde{c}} \|a_{1}x_{1} + \ldots +
	 a_{n}x_{n}\|
    \end{align*}
  \end{proof}
\end{lemma}

\begin{satz}
  $E$ Vektorraum über $\K$ der Dimension $n$.
  
  Dann gilt:
  \begin{enumerate}
   \item sämtliche Normen auf $E$ sind zueinander äquivalent.
   \item $E$ ist isometrisch isomorph zum Raum $\K^{n}$ (bei geeigneter
    Normierung)
   \item $E$ ist vollständig und separabel
  \end{enumerate}
  
  \begin{proof}
    \begin{schritte}
     \item $(E, \|\cdot\|)$ $\dim E =n$, Basis: $\{x_{1},\ldots,x_{n}\}$
      
      Definieren: $\|x\|:= \sum_{j=1}^{n}|a_{j}|$ falls: $x=
      \sum_{j=1}^{n}a_{j}x_{j}$
      
      $\|\cdot\|$ ist Norm auf $E$, $\|x\|_{E}= \|a_{1}x_{1}+\ldots+
      a_{n}x_{n}\|_{E} \leq \|x\|\underbrace{\max_{j}\|x_{j}\|_{E}}_{c_{2}}$
      
      Andererseits nach Lemma: $\|x\|\leq c \|x\|_{E} \Longrightarrow
      \|\cdot\|$ und $\|\cdot\|_{E}$ sind äquivalent.
     \item Betrachten $(E, \|\cdot\|)$ mit $\|\cdot\|$ wie im 1. Schritt
      und $(\K^{n}, \|\cdot\|_{1})$
      
      setzen: $Lx := a =(a_{1},\ldots,a_{n})$, falls x=$a_{1}
      x_{1}+\ldots+ a_{n}x_{n} (L=\Phi^{-1})$
      
      $\Longrightarrow$ $L:E\rightarrow\K^{n}$ surjektiv
      
      $\|Lx\|_{1}= \|x\|$ isometrisch
      
      $\Longrightarrow$ L:E$\rightarrow$ $\K^{n}$ isometrisch isomorph
     \item $\K^{n}$ vollständig $\Longrightarrow$ $L^{-1}(\K^{n})=E$
      vollständig
      
      $\{x: x=\alpha_{1}x_{1}+\ldots+ \alpha_{n}x_{n}, \alpha_{j}\in\Q\}$
      abzählbar und dicht in $E$
    \end{schritte}
  \end{proof}
\end{satz}

\begin{satz}
  $(E, \|\cdot\|)$ normierter Vektorraum.
  
  Dann sind folgende Aussagen äquivalent:
  \begin{enumerate}
   \item $E$ endlichdimensional
   \item jede beschränkte Mende ist präkompakt
   \item die abgeschlossene Einheitskugel $B=\{x\in E: \|x\|\leq 1\}$ in
    $E$ ist kompakt.
  \end{enumerate}
  
  \begin{proof}
    \begin{description}
     \item[$(1)\Rightarrow(2)$] (Satz 1) klar
     \item[$(2)\Rightarrow(3)$] nach Def. der Kompaktheit
     \item[$(3)\Rightarrow(1)$] Es existieren
      $\{x_{1},\ldots,x_{m}\} \subset B$ mit $\bigcup_{i=1}^{m}
      K_{\frac12}(x_{i})\supset B$ nach \thref{satz:heineborel}
      
      Setzen $F := \Span \{x_{1},\ldots,x_{n}\}$ $\Rightarrow$ $F\subset
      E$ endlichdimensionaler abgeschlossener Teilram. Wir setzen: $F=E$
      
      Ann.: $\exists x\in E\wedge x\not\in F$
      
      Es gilt:
      \begin{gather*}
	d := d(x,F) := \inf_{y\in F}\|x-y\|>0\qquad\text{da $F$ abgeschlossen}\\
	\text{wählen } y\in F: d\leq \|x-y\|\leq\frac32d\label{gl5}\\
	\text{Für } z := \frac{x-y}{\|x-y\|}\in B \exists x_{i}:
	   \|z-x_{i}\|<\frac12\\
	\Longrightarrow x=y+\|x-y\|z = \underbrace{y +
	   \|x-y\|x_{i}}_{=\tilde{y}\in F} +\|x-y\|(z-x_{i})\\
	d \leq \|x-\tilde{y}\|= \|x-y\|
	   \underbrace{\|z-x_{i}\|}_{<\frac12} \leq \frac12 \|x-y\|\\
	\Longrightarrow \|x-y\|\geq 2d\qquad\text{Widerspruch zu (\ref{gl5})}
      \end{gather*}
    \end{description}
  \end{proof}
\end{satz}

