\section{Faltung von Distributionen}

\begin{enumerate}
 \item $f,g\in L_{1}(\R^{n}), \phi\in S(\R^{n})$
  \begin{gather*}
    (f*g)(\phi) = \int_{\R^{n}} (f*g)(x) \phi(x)\,dx = \int_{\R^{n}}
       \Bigl(\int_{\R^{n}} f(x-y)g(y)\,dy\Bigr) \phi(x)\,dx
       =^{\todo{Fubini}} = \int_{\R^{n}} \Bigl(\int_{\R^{n}}
       f(x-y)\phi(x)\,dx\Bigr) g(y)\,dy =^{x-y=:z} = \int_{\R^{n}} \Bigl(
       \int_{\R^{n}} f(z)\phi(z+y)\,dz\Bigr) g(y)\,dy =
       \int_{\R^{n}\times\R^{n}} \underbrace{f(x) g(y)}_{f\otimes g(x,y)
       Tensorprodukt} \phi(x+y)\,dxdy \stackrel{=}{?} (f\otimes g) (
       \underbrace{\phi(x+y)}_{\notin S(\R^{n}\times \R^{n}), \in
         C^{\infty}(\R^{n}\times \R^{n})}) =
       \lim_{n\rightarrow\infty} (f\otimes g)(\underbrace{\eta(\frac{x}{n},
       \frac{x}{n}) \phi(x,y)}_{\in S(\R^{n}\times\R^{n})} ) mit \eta\in
       D(\R^{n}\times\R^{n}) \eta(x,y) = 1 \text{~für~} \abs{(x,y)}\leq 1\\
    (f\otimes g)(\psi(x,y)) \text{~für~} \psi\in S(\R^{n}\times\R^{n})=
       f_{*} (g,(\psi(x,y))) \text{Tensorprodukt von Distributionen}
  \end{gather*}

 \item Eine andere Möglichkeit der Verallgemeinerung der Faltung auf
  Distributionen ist $f\in S'(\R^{n}), g\in S(\R^{n})$
  \begin{align*}
    (f*g)(\phi) &= \int_{\R^{n}} \phi(x) \int_{\R^{n}} f(y)g(x-y)\,dy\,dx\\
    &= \int_{\R^{n}} \int_{\R^{n}} \underbrace{g(x-y)}_{=g(-(y-x))}
       \phi(x)\,dx\:f(y)\,dy &&\text{nach \autoref{satz:fubini}}\\
    \intertext{$\tilde{g}(z) := g(-z)$}
    &= \int_{\R^{n}} f(y) (\underbrace{\tilde{g}\times \phi}_{\in
       S(\R^{n})})(y)\,dy = f(\tilde{g}*\phi)
  \end{align*}

 \item $f\in S'(\R^{n}), g\in S(\R^{n})$
  \begin{gather*}
    (f*g)(x) = \int_{\R^{n}} f(y)g(x-y)\,dx = f_{y}(g(x-y))
  \end{gather*}

\end{enumerate}

\begin{lemma}
  $g\in S'(\R^{m}), \psi\in S(\R^{n}\times\R^{m}), \psi=\psi(x,y),
  x\in\R^{n}, y\in\R^{m}$

  $\Longrightarrow$
  \begin{enumerate}
   \item $\phi(x) := g_{y}(\psi(x,y)) \in S(\R^{n})$
   \item $D^{\alpha}\phi(x) = g_{y}(D^{\alpha}_{x}\psi(x,y))$
   \item $\psi_{k} \rightarrow_{S(\R^{n}\times\R^{m})}\psi$
    $\Longrightarrow$ $\phi_{k}(x) = g_{y}(\psi_{k}(x,y)) \rightarrow
    \phi(x)$ in $S'(\R^{n})$
  \end{enumerate}

  \begin{proof} nur die Idee
    zeigen: $\phi\in C^{\infty}$ und (2)
    $\exists k,l: \norm{\phi(x)}_{p,r}\leq c \norm{\psi(x,y)}_{p+k,r+l}
    (\forall p,r\in\N_{0})$
  \end{proof}
\end{lemma}

\begin{lemma}
  Die Menge aller endlichen Linearkombinationen $\{\sum_{endl.}
  \phi_{j}(x)\chi_{j}(y): \phi_{j}\in S(\R^{n}), \chi_{j}\in S(\R^{n})\}$
  liegt dicht in $S(\R^{n}\times\R^{m})$.

  \begin{proof}
    (Idee) Weierstraß'scher Approximationssatz\todo{link?}: Polynome
    $p(x,y)$ dicht in $C^{l}(\mathcal{K}), l\in \N_{0},
    \mathcal{K}\subset \R^{n}\times\R^{n}$ kompakt.

    und der Fakt $D(\R^{n}\times\R^{n})$ dicht in $S(\R^{n}\times\R^{n})$.
  \end{proof}
\end{lemma}

\begin{defini}
  $f\in S'(\R^{n}), g\in S'(\R^{m})$
  \begin{gather*}
    f\otimes g(\psi(x,y)) := f_{x}(g_{y}(\psi(x,y))), \psi\in
       S(\R^{n}\times\R^{m})
  \end{gather*}
  heißt \highl{Tensorprodukt} von $f$ und $g$.

  Klar, nach lemma 1 ist das eine Distribution in
  $S'(\R^{n}\times\R^{m})$ und stetig nach Lemma 1 und nach den
  Anfangsbemerkungen sinnvoll
\end{defini}

\begin{satz} % Satz 3
  \begin{enumerate}
   \item $f\otimes g\in S'(\R^{n}\times\R^{m})$
   \item es existiert genau eine Distribution $h\in
    S'(\R^{n}\times\R^{m})$, sodass $h(\phi(x)\chi(y)) = f(\phi)g(\chi)$.
    Es ist $h= f\otimes g = g\otimes f$
   \item $m(x)\in C^{\infty}(\R^{n})$ polynomial wachsend

    $\Rightarrow$ $m(f\otimes g) = (mf)\otimes g$
   \item $D^{\alpha}_{x}(f\otimes g) = (D^{\alpha} f)\otimes g$
  \end{enumerate}

  \begin{proof}
    \todo{Link: Lemma1} $\Rightarrow$ (1)

    \begin{align*}
      f\otimes g(\sum \phi_{j}\otimes \chi_{j}) = \sum (f\otimes g)
         (\phi_{j}(x)\chi_{j}(y)) = \sum
         f_{x}(g_{y}(\phi_{j}(x)\chi_{j}(y))) = \sum
         f_{x}(g(\chi_{j}(y))\phi(x)) = \sum f_{y}(\phi_{j}(x))
         g_{y}(\chi_{j}(y)) = \ldots = g_{y}(f_{x}(\sum \phi_{j}\otimes
         \chi_{j})) = g\otimes f(\sum \phi_{j}\otimes \chi_{j})
    \end{align*}
    \todo{Lemma 2} $\Rightarrow$ $g\otimes f = f\otimes g$
  \end{proof}
\end{satz}

\begin{defini}
  $f\in S'(\R^{n})), g\in E'(\R^{n})\subset S'(\R^{n})$ ($\supp g$ kompakt)
  Wir wählen uns eine Testfunktion $\eta\in D(\R^{n})$ und $\eta(y) = 1$
  auf einer Umgebung von $\supp g$.

  \begin{align*}
    (f*g)(\phi) := (f\otimes g) (\eta(y)\phi(x+y))&&\phi\in S(\R^{n})
  \end{align*}
  heißt \highl[Faltung!Distribution]{Faltung} von $f$ und $g$.
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item
    $\eta(y)\phi(x+y) \in S(\R^{n}\times\R^{n})$
    \begin{gather*}
      \norm{\eta(y)\phi(x+y)}_{k,l} = \sup_{x,y} (1+\abs{(x,y)}^{k})
         \sum_{\abs{\alpha}+\abs{\beta}\leq l}
         \abs{D^{\alpha}_{x}D^{\beta}_{y}(\eta(y)\phi(x+y))} \leq
         c\sup_{y\in\supp \eta, x} (1+\abs{x+y}^{k}+\abs{y}^{k})
         \sum_{\abs{\gamma}\leq l} \abs{D^{\gamma}\phi(x+y)} \leq c'
         \norm{\phi}_{k,l}
    \end{gather*}

    Daraus folgt, dass die Faltung für alle \ldots definiert ist.

   \item $\delta\in E'(\R^{n})$
    \begin{gather*}
      (f*\delta)(\phi) = (f\otimes \delta)(\eta(y)\phi(x-y)) =
         f_{x}(\delta_{y}(\eta(y)\phi(x+y))) = f_{x}(\eta(0) \phi(x)) =
         f(\phi)\\
      f*\delta = f
    \end{gather*}
  \end{enumerate}
\end{bemerk}

\begin{satz}
  $f\in S'(\R^{n}), g\in E'(\R^{n})$ $\Longrightarrow$
  \begin{enumerate}
   \item $f*g \in S(\R^{n})$
   \item $f_{j} \rightarrow_{S'} f$ $\Rightarrow$ $f_{j}*g
    \rightarrow_{S'} f*g$ und $g_{j} \rightarrow_{E'} g$ $\Rightarrow$
    $f*g_{j} \rightarrow_{S'} f*g$
   \item $D^{\alpha}(f*g) = (D^{\alpha}f)*g = f*(D^{\alpha}g)$
    ($\alpha\in\N_{0}^{n}$) (Ableitung)
   \item (Translationinvarianz der Faltung) $f(\cdot+b) = f*g(\cdot+b) =
    (f*g)(\cdot+b)$, $b\in\R^{n}$
  \end{enumerate}
\end{satz}

\begin{defini}[Faltung von Distributionen mit Testfunktionen]
  $f\in S'(\R^{n}), g\in S(\R^{n})$
  \begin{gather*}
    (f*g)(\phi) := f(\tilde{g}*\phi), \phi\in S(\R^{n})
  \end{gather*}
\end{defini}

\begin{satz}
  Wenn man die Situation $f\in S'(\R^{n}), g\in S(\R^{n})$ hat, gilt
  \begin{enumerate}
   \item $f*g\in S'(\R^{n})\cap C^{\infty}(\R^{n})$ mit der Eigenschaft,
    dass ein $m$ und ein $c_{\alpha}$ für alle $\alpha$ und alle $x$
    existieren und folgendes gilt
    \begin{gather*}
      \abs{D^{\alpha} (f*g)(x)} \leq c_{\alpha}( 1+\abs{x}^{m})
    \end{gather*}

   \item $(f*g)(x) = f_{y}(g(x-y)) \forall x\in\R^{n}$
  \end{enumerate}
\end{satz}

