\begin{enumerate}
 \item $f,g\in L_{1}(\R^{n}), \phi\in S(\R^{n})$
  \begin{gather*}
    (f*g)(\phi) = \int_{\R^{n}} (f*g)(x) \phi(x)\,dx = \int_{\R^{n}}
       \Bigl(\int_{\R^{n}} f(x-y)g(y)\,dy\Bigr) \phi(x)\,dx
       =^{\todo{Fubini}} = \int_{\R^{n}} \Bigl(\int_{\R^{n}}
       f(x-y)\phi(x)\,dx\Bigr) g(y)\,dy =^{x-y=:z} = \int_{\R^{n}} \Bigl(
       \int_{\R^{n}} f(z)\phi(z+y)\,dz\Bigr) g(y)\,dy =
       \int_{\R^{n}\times\R^{n}} \underbrace{f(x) g(y)}_{f\otimes g(x,y)
       Tensorprodukt} \phi(x+y)\,dxdy \stackrel{=}{?} (f\otimes g) (
       \underbrace{\phi(x+y)}_{\notin S(\R^{n}\times \R^{n}), \in
         C^{\infty}(\R^{n}\times \R^{n})}) =
       \lim_{n\rightarrow\infty} (f\otimes g)(\underbrace{\eta(\frac{x}{n},
       \frac{x}{n}) \phi(x,y)}_{\in S(\R^{n}\times\R^{n})} ) mit \eta\in
       D(\R^{n}\times\R^{n}) \eta(x,y) = 1 \text{~für~} \abs{(x,y)}\leq 1\\
    (f\otimes g)(\psi(x,y)) \text{~für~} \psi\in S(\R^{n}\times\R^{n})=
       f_{*} (g,(\psi(x,y))) \text{Tensorprodukt von Distributionen}
  \end{gather*}

 \item Eine andere Möglichkeit der Verallgemeinerung der Faltung auf
  Distributionen ist $f\in S'(\R^{n}), g\in S(\R^{n})$
  \begin{align*}
    (f*g)(\phi) &= \int_{\R^{n}} \phi(x) \int_{\R^{n}} f(y)g(x-y)\,dy\,dx\\
    &= \int_{\R^{n}} \int_{\R^{n}} \underbrace{g(x-y)}_{=g(-(y-x))}
       \phi(x)\,dx\:f(y)\,dy &&\text{nach \autoref{satz:fubini}}\\
    \intertext{$\tilde{g}(z) := g(-z)$}
    &= \int_{\R^{n}} f(y) (\underbrace{\tilde{g}\times \phi}_{\in
       S(\R^{n})})(y)\,dy = f(\tilde{g}*\phi)
  \end{align*}

 \item $f\in S'(\R^{n}), g\in S(\R^{n})$
  \begin{gather*}
    (f*g)(x) = \int_{\R^{n}} f(y)g(x-y)\,dx = f_{y}(g(x-y))
  \end{gather*}

\end{enumerate}

