%satz 2
\begin{satz}[Nullstellen analytischer Funktionen]
  Sei $f$ auf $\Omega$ analytisch und nicht konstant. Dann folgt:
  \begin{enumerate}[a)]
   \item $f(z_{0})= 0\Rightarrow\exists k\in\N\setminus\{0\}$ und Umgebung von
    $z_{0}$ mit $f(z)=(z-z_{0})^{k}g(z)$ mit $g(z)\neq0$.
   \item $f$ an den Stellen $z_{0}$ Nullstelle $k$"~ter Ordnung genau dann,
    wenn $f(z_{0})=f'(z_{0})=\dotsb=f^{(k-1)}(z_{0})=0, f^{(k)}(z_{0})\neq0$.
   \item In jeder kompakten Teilmenge von $\Omega$ hat $f$ höchstens endlich
    viele Nullstellen.
  \end{enumerate}
  \begin{proof}
    Der Beweis lässt sich mit den obigen Sätzen einfach führen.
  \end{proof}
\end{satz}

\paragraph{\textsc{Cauchy}scher Integralsatz für einfache Gebiete}
Sei $f$ holomorph in einem sternförmigen (oder allgemeiner einfachem) Gebiet
$\Omega$ und $\gamma$ ein geschlossener Weg. Dann folgt:
\begin{gather}
  \label{eq:32}
     \int_{\gamma} f(z)\,dz=0
\end{gather}
Dies ergibt sich aus Folgerung 3 in 16.2 \todo{Link einfügen}

\begin{folg}
  Die \autoref{eq:32} gilt auch für einfach gelagerten Weg $\gamma$, d.\,h.
  $\gamma\subset\Omega'\subset\Omega$ mit $\Omega'$ einfach.
\end{folg}

\begin{bemerk}
  Ein Kreisring ist \emph{kein} einfaches Gebiet, denn $\int_{C_{r}(0)}
  \frac{dz}{z}= 2\pi i\neq0$.
\end{bemerk}

\begin{bemerk}
  Sei $f$ holomorph in $\Omega$. Der \textsc{Cauchy}sche Integralsatz gilt
  auch für allgemeinere Gebiete.
\end{bemerk}

\begin{bemerk}
  Sei $f$ holomorph in $\Omega=\C\setminus\{z_{0}\}$. Dann gilt:
  \begin{gather*}
    \int_{\gamma} f(z)\,dz= \int_{C_{r}(z_{0})} f(z)\,dz
  \end{gather*}
  Denn es ist $\int_{\gamma} f(z)\,dz+ \int_{-C_{r}(z_{0})}
  f(z)\,dz=\int_{\gamma} f(z)\,dz- \int_{C_{r}(z_{0})} f(z)\,dz=
  \int_{\partial M} f(z)\,dz=0$.

  Hierbei umläuft $\gamma$ den Punkt $z_{0}$ einfach positiv, d.\,h. der Strahl
  \begin{gather*}
    \{z_{0}+te^{i\phi}\colon t\geq0\}
  \end{gather*}
  trifft $z(t)$ (Parametrisierung von $\gamma$) in genau einem Punkt.
\end{bemerk}

\begin{bsp}
  \begin{gather*}
    \int_{\gamma} \frac{1}{z-z_{0}}\,dz=2\pi i
  \end{gather*}
\end{bsp}

\begin{satz}[Homologiesatz]\label{satz:homologiesatz}
  Sei $\Omega$ ein Gebiet, $f$ holomorph in $\Omega\setminus\{z_{0}\}$ und
  $\gamma$ umlaufe $z_{0}$ einfach positiv. Dann folgt:
  \begin{gather*}
    \int_{\gamma}f(z)\,dz= \int_{C_{r}(z_{0})} f(z)\,dz
  \end{gather*}
  falls das Innere von $\gamma$ und von $C_{r}(z_{0})$ in $\Omega$ liegt.
\end{satz}

\begin{defini}
  Seien $f_{1}, f_{2}$ geschlossene Wege. Diese sind genau dann
  \highl{homolog}, wenn $\int_{\gamma_{1}} f\,dz=\int_{\gamma_{2}}f\,dz$ für
  jedes $f\in C^{1}(\Omega)$.
\end{defini}

