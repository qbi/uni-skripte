\chapter{Holomorphe Funktionen}

\begin{defini}
  Eine offene, zusammenhängende Untermenge $\Omega\subseteq\C$ heißt
  \highl{Gebiet} in $\C$.
\end{defini}

\begin{defini}
  Sei $\Omega \subseteq \C$ ein Gebiet, d.\,h. $\Omega$ ist offen und
  zusammenhängend). Dann heißt $f\colon\Omega \rightarrow \C, z \mapsto f(z)$
  heißt \highl[Funktion!komplexe]{komplexe Funktion} und hat eine Darstellung
  durch zwei reelle Funktionen in zwei reellen Variablen: $f\colon\Omega
  \rightarrow \C, x+iy \mapsto u(x,y) + iv(x,y)$ mit $u,v\in\R$.
\end{defini}

\begin{bsp}
  Für $z\in\C$ ist:
  \begin{align*}
	f(z) &= e^z = \sum^{\infty}_{k=0} \frac{z^k}{k!}\\
	z &= x + iy \Rightarrow e^z = e^{x+iy} = e^x \cdot e^{iy} =
       e^x(\cos(y) + i\sin(y)) = e^x\cos(y) + ie^x\sin(y)\\
	&\Rightarrow u(x,y) = e^x\cos(y) \quad v(x,y) = e^x\sin(y)
\end{align*}
  \end{bsp}

Der Raum $(\C, \abs{\cdot}, +)$ ist ein normierter Raum mit der Betragsfunktion
\begin{gather*}
  \abs{z}= \abs{x+iy}= \sqrt{x^{2}+y^{2}}
\end{gather*}

\begin{defini}
  Sei $U$ eine Umgebung von $a \in \C$ und  $U \setminus \{a\} \subset D_f$.
\begin{gather*}
  \lim_{z \rightarrow a} f(z) = b \Leftrightarrow \forall
     \{z_n\}^{\infty}_{n=0} \subset U \setminus \{a\}\colon z_n \xrightarrow{n
     \rightarrow \infty} a \Rightarrow f(z_n) \xrightarrow{n \rightarrow
     \infty} b
\end{gather*}
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item Äquivalent zur obigen Definition ist:
    \begin{gather*}
	\lim_{z \rightarrow a} f(z) = b \Leftrightarrow \forall \epsilon > 0
	 \exists \delta > 0\colon \abs{f(z)-b} < \epsilon  \forall 0 < \abs{z
	 - a} < \delta
     \end{gather*}
   \item Weiterhin gilt für $h \in \R$:
\begin{gather*}
	\lim_{z \rightarrow a} f(z) = b \Rightarrow \lim_{h \rightarrow 0} f(a
     + h) = b \text{ und } \lim_{h \rightarrow 0} f(a + ih) = b
\end{gather*}
  \end{enumerate}
\end{bemerk}

\begin{bsp}
  Es ist:
  \begin{gather*}
	\lim_{z \rightarrow 0} \frac{e^z - 1}{z} = 1
\end{gather*}
  Denn:
  \begin{align*}
    \abs{\frac{e^z - 1}{z} - 1} &= \abs{\frac{e^z - 1 - z}{z}}=
       \abs{\sum^{\infty}_{k=2} \frac{z^{k-1}}{k!}} \leq \sum^{\infty}_{k=2}
       \abs{\frac{z^{k-1}}{k!}}\\
    &\leq \sum^{\infty}_{k=2} \abs{z^{k-1}} =
       \sum^{\infty}_{k=1} \abs{z}^k = \frac{\abs{z}}{1 - \abs{z}}
\end{align*}
Wir haben hier die geometrische Reihe angewendet, da $\abs{z} \in \R$ und
  $\abs{z} < 1$, falls $z$ nahe genug bei Null ist. Damit folgt:
\begin{gather*}
	0 \leq \lim_{z \rightarrow 0} \abs{\frac{e^z - 1}{z} - 1} \leq
	\lim_{z \rightarrow 0} \frac{\abs{z}}{1 - \abs{z}} = 0
\end{gather*}
\end{bsp}

\begin{satz}[Komplexe Grenzwertsätze]
  Es gelte $\alpha, \beta \in \C$ und $\lim_{z \rightarrow a} f(z) = b$ sowie
  $\lim_{z \rightarrow a} g(z) = c$. Dann folgt:
  \begin{enumerate}
   \item $\lim_{z \rightarrow a} \abs{f(z)} = \abs{b}$
   \item $\lim_{z \rightarrow a} \Re(f(z)) = \Re(b), 
	\lim_{z \rightarrow a} \Im(f(z)) = \Im(b)$
   \item $\lim_{z \rightarrow a} \alpha f(z) + \beta g(z) = \alpha b + \beta c$
   \item $\lim_{z \rightarrow a} f(z) \cdot g(z) = b \cdot c$
   \item $\lim_{z \rightarrow a} \frac{f(z)}{g(z)} = \frac{b}{c}$ , falls $c
    \neq 0$. 
  \end{enumerate}
  \begin{proof}
    Es sei auf den Satz über allgemeine normierte Räume verwiesen. Im
    Komplexen ist der Beweisverlauf analog. Als Neuerung zeigen wir die
    folgende Eigenschaft:
    Für Folgen $\{w_n\}^{\infty}_{n=0} = \{u_n + iv_n\}^{\infty}_{n=0}$ und $w
    = u + iv$ gilt
    \begin{gather*}
	w_n \xrightarrow{n \rightarrow \infty} w \Leftrightarrow u_n
	 \xrightarrow{n \rightarrow \infty} u \wedge v_n \xrightarrow{n
	 \rightarrow \infty} v \Leftrightarrow \Re(w_n) \xrightarrow{n
	 \rightarrow \infty} \Re(w) \wedge \Im(w_n) \xrightarrow{n \rightarrow
	 \infty} \Im(w)
    \end{gather*}
    Das beweist die zweite und dritte Behauptung zugleich.
  \end{proof}
\end{satz}

\begin{defini}
  Sei $a \in D_f \subseteq \C$ und $f$ eine komplexe Funktion. Dann heißt $f$
  an der Stelle $a$ \highl{stetig}, genau dann, wenn $\forall
  \{z_n\}^{\infty}_{n=0} \subset D_f\colon z_n \xrightarrow{n \rightarrow
  \infty} a \Rightarrow f(z_n) \xrightarrow{n \rightarrow \infty} f(a)$.
\end{defini}

\begin{bemerk}
  Äquivalent zur obigen Definition ist: $f$ in $a$ stetig, genau dann, wenn
  $\forall \epsilon > 0 \exists \delta > 0\colon\abs{f(z)-f(a)} < \epsilon$
  für alle $\abs{z-a} < \delta$.
\end{bemerk}

\begin{satz}[Eigenschaften stetiger Funktionen]
  \begin{enumerate}
   \item Seien $f$ und $g$ in $a \in D_f \cap D_g$ stetig und $\alpha, \beta
    \in \C$. Dann sind auch $\abs{f}, \alpha f + \beta g, f \cdot g$ und
    $\frac{f}{g}$ (falls $g(a) \neq 0$) in $a$ stetig.
   \item Sei $f$ in $a \in D_f$ und $g$ in $f(a) \in D_g$ stetig. Dann ist
    auch $g \circ f$ in $a$ stetig.
  \end{enumerate}
  \begin{proof}
    Hier sei auf den entsprechenden Satz im Reellen verwiesen. Der Beweis
    läuft im Komplexen ganz analog.
  \end{proof}
\end{satz}

\chapter{Differenzierung}

\begin{defini}
  Sei $a \in D_f$. Dann heißt $f$ an der Stelle $a$ \highl{differenzierbar},
  genau dann, wenn der Grenzwert
\begin{gather*}
  f'(a) := \lim_{z \rightarrow a} \frac{f(z) - f(a)}{z - a} = \lim_{h
     \rightarrow 0} \frac{f(a + h) - f(a)}{h}
\end{gather*}
existiert, d.\,h. eindeutig bestimmt ist.
Man schreibt statt $f'(a)$ auch gerne $\frac{df}{dz}(a)$.
\end{defini}

\begin{bemerk}
  Die komplexe Differenzierung unterscheidet sich grundsätzlich von der
  Differenzierung im $\R^{2}$, denn
  \begin{gather*}
    \lim_{h \rightarrow 0} \frac{f(a+h)-f(a)}{h} \neq\lim_{h \rightarrow 0}
       \frac{f(a+h)-f(a)}{\abs{h}}
  \end{gather*}
\end{bemerk}

\begin{satz}
  \begin{enumerate}
   \item Seien $f$ und $g$ in $a \in D_f \cap D_g$ differenzierbar und
    $\alpha, \beta \in \C$. Dann sind $f$ und $g$ auch stetig in $a$ und
    $\alpha f + \beta g, f \cdot g$ und $\frac{f}{g}$ (falls $g(a) \neq 0$)
    sind auch in $a$ differenzierbar. Weiterhin gelten dann die, schon aus dem
    Reellen bekannten, Ableitungsregeln:
    \begin{enumerate}
      \item $(\alpha f + \beta g)'(a) = \alpha f'(a) + \beta g'(a)$
      \item $(f \cdot g)'(a) = f'(a) \cdot g(a) + f(a) \cdot g'(a)$
      \item $(\frac{f}{g})'(a) = \frac{f'(a) \cdot g(a) - f(a) \cdot
      g'(a)}{g(a)^2}$
    \end{enumerate}
   \item Sei $f$ in $a \in D_f$ und $g$ in $f(a) \in D_g$ differenzierbar.
    Dann ist auch $g \circ f$ in $a$ differenzierbar. Weiterhin gilt dann
    \highl{Kettenregel}: $(g \circ f)'(a) = (g' \circ f)(a) \cdot f'(a)$.
  \end{enumerate}
  \begin{proof}
    Hier sei auf den entsprechenden Satz im Reellen verwiesen. Der Beweis
    läuft im Komplexen ganz analog.
  \end{proof}
\end{satz}

\begin{bsp}
  \begin{enumerate}
   \item $f(z) = c \Rightarrow f'(z) = 0$
   \item $f(z) = z \Rightarrow f'(z) = 1$
   \item $f(z) = z^n\Rightarrow f'(z) = nz^{n-1}$ für alle $n \in
    \N\setminus\{0\}$
   \item $f(z) = \frac{1}{z} \Rightarrow f'(z) = -\frac{1}{z^2}$ für alle $z
    \neq 0$
   \item $f(z) = e^z \Rightarrow f'(z) = e^z$
  \end{enumerate}
\end{bsp}

\begin{bemerk}
  Im Reellen ist für $\alpha\in\R$ allgemein $(x^{\alpha})'= \alpha
  x^{\alpha-1}$. Denn $(x^{\alpha})'= (e^{\alpha\ln x})'= e^{\alpha\ln x}
  \nicefrac{\alpha}{x}= \frac{\alpha x^{\alpha}}{x}= \alpha x^{\alpha-1}$.

  Im Komplexen ist diese Eigenschaft allgemein \emph{nicht} gültig. Denn $\ln
  x$ hat für $x\in\C$ unendlich viele Werte. Dementsprechend kann $x^{\alpha}$
  nicht mehr als eindeutige Funktion definiert werden.
\end{bemerk}

