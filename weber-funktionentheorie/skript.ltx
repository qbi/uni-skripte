\begin{satz}[Nullstellen analytischer Funktionen]
  Sei $f$ auf $\Omega$ analytisch und nicht konstant. Dann folgt:
  \begin{enumerate}[a)]
   \item $f(z_{0})= 0\Rightarrow\exists k\in\N\setminus\{0\}$ und Umgebung von
    $z_{0}$ mit $f(z)=(z-z_{0})^{k}g(z)$ mit $g(z)\neq0$.
   \item $f$ an den Stellen $z_{0}$ Nullstelle $k$"~ter Ordnung genau dann,
    wenn $f(z_{0})=f'(z_{0})=\dotsb=f^{(k-1)}(z_{0})=0, f^{(k)}(z_{0})\neq0$.
   \item In jeder kompakten Teilmenge von $\Omega$ hat $f$ höchstens endlich
    viele Nullstellen.
  \end{enumerate}
  \begin{proof}
    Der Beweis lässt sich mit den obigen Sätzen einfach führen.
  \end{proof}
\end{satz}

\paragraph{\textsc{Cauchy}scher Integralsatz für einfache Gebiete}
Sei $f$ holomorph in einem sternförmigen (oder allgemeiner einfachem) Gebiet
$\Omega$ und $\gamma$ ein geschlossener Weg. Dann folgt:
\begin{gather}
  \label{eq:1}
     \int_{\gamma} f(z)\,dz=0
\end{gather}
Dies ergibt sich aus Folgerung 3 in 16.2 \todo{Link einfügen}

\begin{folg}
  Die \autoref{eq:1} gilt auch für einfach gelagerten Weg $\gamma$, d.\,h.
  $\gamma\subset\Omega'\subset\Omega$ mit $\Omega'$ einfach.
\end{folg}

\begin{bemerk}
  Ein Kreisring ist \emph{kein} einfaches Gebiet, denn $\int_{C_{r}(0)}
  \frac{dz}{z}= 2\pi i\neq0$.
\end{bemerk}

\begin{bemerk}
  Sei $f$ holomorph in $\Omega$. Der \textsc{Cauchy}sche Integralsatz gilt
  auch für allgemeinere Gebiete.
\end{bemerk}

\begin{bemerk}
  Sei $f$ holomorph in $\Omega=\C\setminus\{z_{0}\}$. Dann gilt:
  \begin{gather*}
    \int_{\gamma} f(z)\,dz= \int_{C_{r}(z_{0})} f(z)\,dz
  \end{gather*}
  Denn es ist $\int_{\gamma} f(z)\,dz+ \int_{-C_{r}(z_{0})}
  f(z)\,dz=\int_{\gamma} f(z)\,dz- \int_{C_{r}(z_{0})} f(z)\,dz=
  \int_{\partial M} f(z)\,dz=0$.

  Hierbei umläuft $\gamma$ den Punkt $z_{0}$ einfach positiv, d.\,h. der Strahl
  \begin{gather*}
    \{z_{0}+te^{i\phi}\colon t\geq0\}
  \end{gather*}
  trifft $z(t)$ (Parametrisierung von $\gamma$) in genau einem Punkt.
\end{bemerk}

\begin{bsp}
  \begin{gather*}
    \int_{\gamma} \frac{1}{z-z_{0}}\,dz=2\pi i
  \end{gather*}
\end{bsp}

\begin{satz}[Homologiesatz]
  Sei $\Omega$ ein Gebiet, $f$ holomorph in $\Omega\setminus\{z_{0}\}$ und
  $\gamma$ umlaufe $z_{0}$ einfach positiv. Dann folgt:
  \begin{gather*}
    \int_{\gamma}f(z)\,dz= \int_{C_{r}(z_{0})} f(z)\,dz
  \end{gather*}
  falls das Innere von $\gamma$ und von $C_{r}(z_{0})$ in $\Omega$ liegt.
\end{satz}

\begin{defini}
  Seien $f_{1}, f_{2}$ geschlossene Wege. Diese sind genau dann
  \highl{homolog}, wenn $\int_{\gamma_{1}} f\,dz=\int_{\gamma_{2}}f\,dz$ für
  jedes $f\in C^{1}(\Omega)$.
\end{defini}

%nr 16.4
\section{\textsc{Cauchy}sche Integralformel}

%satz1
\begin{satz}[\textsc{Cauchy}sche Integralformel für Kreise]
  Sei $f$ holomorph in $\Omega,
  \overline{K_{r}(z_{0})}\subset\Omega\Rightarrow f(z)= \frac{1}{2\pi i}
  \int_{C_{r}(z_{0})} \frac{f(w)}{w-z}\,dw$ für alle $z\in K_{r}(z_{0})$.
\end{satz}

%satz 2
\begin{satz}[\textsc{Cauchy}sche Integralformel für einfach positiv umlaufende
  Wege]
  Sei $\gamma$ ein geschlossener Weg, der jeden von ihm umschlossenen Punkt
  einfach positiv umläuft. Es sei $\gamma$ und sein Inneres in $\Omega$
  enthalten und $f$ holomorph in $\Omega$. Dann folgt:
  \begin{gather*}
    f(z)= \frac{1}{2\pi i} \int_{\gamma} \frac{f(w)}{w-z}\,dw
  \end{gather*}
  \begin{proof}
    Sei $z$ im Inneren von $\gamma$. Dann existiert ein $\epsilon>0$, dass
    $C_{\epsilon}(z)$ und $\gamma$ homolog bezüglich $\Omega\setminus\{z\}$.
    Es folgt, $\frac{1}{2\pi i} \int_{\gamma} \frac{f(w)}{w-z}\,dw=
    \frac{1}{2\pi i} \int_{C_{\epsilon}(z)} \frac{f(w)}{w-z}\,dw=f(z)$.
  \end{proof}
\end{satz}

%satz 3
\begin{satz}[Potenzreihenentwicklung holomorpher Funktionen]
  Sei $f$ holomorph in $\Omega$, also eine $C^{1}$"~Funktion. Dann folgt:
  \begin{enumerate}[a)]
   \item $f$ ist in $\Omega$ analytisch ($\Rightarrow C^{\infty}(\Omega)$)
   \item $f(z)= \sum_{k=0}^{\infty} a_{n}(z-z_{0})^{n}$ ist
    Potenzreihenentwicklung um $z_{0}\in\Omega\Rightarrow$ Konvergenzradius
    dieser Reihe mindestens $R=\abs{(z_{0},\partial\Omega)}$ und
    \begin{gather*}
      a_{n}= \frac{f^{(n)}(z_{0})}{n!}= \frac{1}{2\pi i} \int_{C_{r}(z_{0})}
	 \frac{f(z)}{(z-z_{0})^{n+1}}\,dz\qquad\forall r\colon 0<r<R
    \end{gather*}
    Obiges bezeichnet man als \highl{\textsc{Cauchy}"~Formeln}.
  \end{enumerate}
\end{satz}

\begin{bemerk}
  Im Reellen gibt es keinen, dazu äquivalenten Satz. Eine $C^{1}$"~Funktion in
  $(a,b)\nRightarrow f\in C^{\infty}(a,b)$.
\end{bemerk}


