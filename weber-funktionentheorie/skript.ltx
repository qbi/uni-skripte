   \item $f(z)= \frac{3z^{2}+1}{z+1}, z_{0}=2, z_{0}=i$
   \item $f(z)= \frac{z^{2}}{(z+i)(z-i)^{2}}, z_{0}=0$
  \end{enumerate}

 \item Man bestimme den Konvergenzradius
  \begin{gather*}
    \sum_{k=0}^{\infty} (n^{2}+b^{n})z^{n}\qquad b\in\C
  \end{gather*}
 \item Die Summe $\sum a_{k}z^{k}$ habe den Konvergenzradius $\rho$. Welchen
  Konvergenzradius hat
  \begin{enumerate}
   \item $\sum a_{k} z^{2k}$
   \item $\sum a_{k}^{2} z^{k}$
  \end{enumerate}
 \item Man entwickle in eine Potenzreihe um $z_{0}=0$:
  \begin{enumerate}[a)]
   \item $f(z)= \sin^{2}z$
   \item $f(z)= \cos(z^{2}-1)$
  \end{enumerate}
 \item Man bestimme die Art der Singularitäten:
  \begin{enumerate}[a)]
   \item $\frac{1-\cos z}{z^{2}}$
   \item $e^{\nicefrac{1}{z}} +\nicefrac{1}{z}$
  \end{enumerate}
\end{enumerate}

