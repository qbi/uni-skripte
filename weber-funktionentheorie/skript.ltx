\chapter{\textsc{Cauchy}sche Integralformel}

%satz1
\begin{satz}[\textsc{Cauchy}sche Integralformel für Kreise]
  Sei $f$ holomorph in $\Omega,
  \overline{K_{r}(z_{0})}\subset\Omega\Rightarrow f(z)= \frac{1}{2\pi i}
  \int_{C_{r}(z_{0})} \frac{f(w)}{w-z}\,dw$ für alle $z\in K_{r}(z_{0})$.
\end{satz}

%satz 2
\begin{satz}[\textsc{Cauchy}sche Integralformel für einfach positiv umlaufende
  Wege]
  Sei $\gamma$ ein geschlossener Weg, der jeden von ihm umschlossenen Punkt
  einfach positiv umläuft. Es sei $\gamma$ und sein Inneres in $\Omega$
  enthalten und $f$ holomorph in $\Omega$. Dann folgt:
  \begin{gather*}
    f(z)= \frac{1}{2\pi i} \int_{\gamma} \frac{f(w)}{w-z}\,dw
  \end{gather*}
  \begin{proof}
    Sei $z$ im Inneren von $\gamma$. Dann existiert ein $\epsilon>0$, dass
    $C_{\epsilon}(z)$ und $\gamma$ homolog bezüglich $\Omega\setminus\{z\}$.
    Es folgt, $\frac{1}{2\pi i} \int_{\gamma} \frac{f(w)}{w-z}\,dw=
    \frac{1}{2\pi i} \int_{C_{\epsilon}(z)} \frac{f(w)}{w-z}\,dw=f(z)$.
  \end{proof}
\end{satz}

%satz 3
\begin{satz}[Potenzreihenentwicklung holomorpher Funktionen]\label{satz:43}
  Sei $f$ holomorph in $\Omega$, also eine $C^{1}$"~Funktion. Dann folgt:
  \begin{enumerate}[a)]
   \item $f$ ist in $\Omega$ analytisch ($\Rightarrow C^{\infty}(\Omega)$)
   \item $f(z)= \sum_{k=0}^{\infty} a_{n}(z-z_{0})^{n}$ ist
    Potenzreihenentwicklung um $z_{0}\in\Omega\Rightarrow$ Konvergenzradius
    dieser Reihe mindestens $R=\abs{(z_{0},\partial\Omega)}$ und
    \begin{gather}\label{eq:21}
      a_{n}= \frac{f^{(n)}(z_{0})}{n!}= \frac{1}{2\pi i} \int_{C_{r}(z_{0})}
	 \frac{f(z)}{(z-z_{0})^{n+1}}\,dz\qquad\forall r\colon 0<r<R
    \end{gather}
    Obiges bezeichnet man als \highl{\textsc{Cauchy}"~Formeln}.
  \end{enumerate}
\end{satz}

\begin{bemerk}
  Im Reellen gibt es keinen, dazu äquivalenten Satz. Eine $C^{1}$"~Funktion in
  $(a,b)\nRightarrow f\in C^{\infty}(a,b)$. Betrachten wir beispielsweise die
  Funktion
  \begin{gather*}
    f(x)=
       \begin{cases}
	 x^{2} & x>0\\
	 0 & x\leq0
       \end{cases}
       \qquad
       f'(x)=
       \begin{cases}
	 2x & x>0\\
	 0 & \leq0
       \end{cases}
  \end{gather*}
  Die Ableitung ist stetig, also $f\in C^{1}(\R)$. Doch $f''$ ist in $x=0$
  nicht differenzierbar, also $f\notin\C^{2}(\R)$.

  Es gibt sogar $C^{\infty}$"~Funktionen im Reellen, die trotzdem nicht
  analytisch sind. Ein Beispiel dafür ist:
  \begin{gather*}
    f(x)=
       \begin{cases}
	 e^{-\frac{1}{x^{2}}} & x\neq0\\
	 0 & x=0
       \end{cases}
  \end{gather*}
  Es ist $f\in C^{\infty}(\R)$, aber in $x=0$ nicht analytisch. Denn wäre
  $f(x)= \sum_{k=0}^{\infty} a_{k}x^{k}$, so wäre insbesondere $a_{k} =
  \frac{f^{(k)}(0)}{k!}= 0$. Das heißt, in einer $\epsilon$"~Umgebung um 0
  wäre $f(x)\equiv0$. Das ist ein Widerspruch.
\end{bemerk}

\begin{satz}[Identitätssatz für holomorphe Funktionen]
  Seien $f,g$  holomorph auf $\Omega$ und $z_0 \in \Omega$. Weiterhin gelte
  $f(z_n) = g(z_n)$ für eine Folge $\{z_n\}^{\infty}_{n=1} \subset \Omega
  \setminus \{z_0\}$ mit $z_n \xrightarrow{n \rightarrow \infty} z_0$. Dann
  gilt $f \equiv g$ auf $\Omega$.
  \begin{proof}
    Da $f$ und $g$ holomorph auf $\Omega$ sind, folgt aus
    \autoref{satz:43}~(i), dass $f$ und $g$ auch analytisch auf $\Omega$ sind.
    Der \autoref{satz:34} liefert dann die gewünschte Behauptung.
  \end{proof}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item Es kann nur eine holomorphe Fortsetzung der reellen Funktion $e^x$
    ins Komplexe geben, da die beiden in ganz $\R$ übereinstimmen müssen.
    Diese Fortsetzung lautet:
    \begin{gather*}
      e^z = e^{x+iy}= e^{x}(\cos y+i\sin y)=\sum^{\infty}_{n=0} \frac{z^n}{n!}
    \end{gather*}
    Denn betrachten wir die holomorphe Funktion $f(z)=e^{z}$ mit $z\in\C$.
    Diese stimmt mit $e^{x}$ f+r $x\in\R$ überein. Ist
    $g\colon\C\rightarrow\C$ ebenfalls holomorph, so wählen wir eine beliebige
    in $\R$ konvergente Punktfolge $(z_{n})\subset\R, z_{n}\rightarrow
    z_{0}\in\R$. Dann ist $g(z_{n})=f(z_{n})$. Also gilt nach dem obigen Satz
    die Identität.
   \item Gleiches gilt für die komplexen Fortsetzungen der reellen Funktionen
    $\sin(x)$ und $\cos(x)$:
    \begin{gather*}
      \sin(z) = \sum^{\infty}_{n=0} (-1)^n \frac{z^{2n+1}}{(2n+1)!} \qquad \cos(z) = \sum^{\infty}_{n=0} (-1)^n \frac{z^{2n}}{(2n)!}
    \end{gather*}
   \item Damit gelten die aus dem Reellen bekannten Additionstheoreme auch im
    Komplexen.
   \item Die einzige holomorphe Fortsetzung von $\ln(1 + x)$ für $x < 1$ aus
    dem Reellen ins Komplexe lautet:
    \begin{gather*}
      \ln(1 + z) = \sum^{\infty}_{n=1} (-1)^{n+1} \frac{z^n}{n} \qquad \forall \abs{z} < 1
    \end{gather*}
  \end{enumerate}
\end{bemerk}

\begin{defini}
  Eine auf ganz $\C$ holomorphe Funktion heißt \highl[Funktion!ganze]{ganze
  Funktion}. Insbesondere gilt für ganze Funktionen $f(z)= \sum_{n=0}^{\infty}
  a_{n} z^{n}$ mit $z\in\C$. Der Konvergenzradius ist $R=\infty$.
\end{defini}

\begin{bsp}
  Polynome, $e^{z}, \sin, \cos$ sind ganze Funktionen.
\end{bsp}

\begin{satz}[Satz über Potenzreihen]\label{satz:51}
  Für $z_0 \in \C$ und ein fixiertes $R > 0$ sei die Potenzreihe
  \begin{gather*}
    f(z) := \sum^{\infty}_{n=0} a_n (z - z_0)^n \qquad \forall \abs{z - z_0} <
       R
  \end{gather*}
  gegeben. Weiter setzen wir $M(f,r) := \max \{\abs{f(z)}\colon\abs{z - z_0} =
  r\}$ für $0 < r < R$. Dann gilt:
  \begin{gather*}
	\abs{a_n} \leq \frac{M(f,r)}{r^n} \quad \forall n \in \N
  \end{gather*}
  \begin{proof}
    Aus der Cauchyschen Integralformel (\autoref{eq:21}) \help{oder sind das
    die Cauchy"=Formeln?} und \autoref{satz:24}
    folgt:
    \begin{align*}
      \abs{a_n} &= \abs[\big]{\frac{1}{2\pi i} \int_{C_r(z_0)} \frac{f(z)}{(z
	 - z_0)^{n+1}}\,dz} = \abs[\big]{\frac{1}{2\pi i}} \cdot
	 \abs[\big]{\int_{C_r(z_0)} \frac{f(z)}{(z - z_0)^{n+1}}\,dz}\\
      &\leq \abs[\big]{\frac{-i}{2\pi}} \int_{C_r(z_0)} \abs{\frac{f(z)}{(z -
	 z_0)^{n+1}}}\,dz \leq \frac{1}{2\pi} \cdot L(C_r(z_0)) \cdot
	 \frac{M(f,r)}{\abs{r^{n+1}}}\\
      &= \frac{1}{2\pi} \cdot 2\pi r \cdot \frac{M(f,r)}{r^{n+1}} =
	 \frac{M(f,r)}{r^n}
    \end{align*}
  \end{proof}
\end{satz}

\begin{satz}[Satz von Liouville]\label{satz:52}
  Jede beschränkte ganze Funktion ist konstant. Insbesondere gilt: Jede ganze,
  nicht konstante Funktion ist nicht beschränkt.
  \begin{proof}
    Es gelte $\abs{f(z)} \leq M$ für eine ganze Funktion $f$. Wir können
    o.\,B.\,d.\,A. den Entwicklungspunkt $z_0 = 0$ für $f$ annehmen. Dann gilt
    für ein beliebiges $r > 0$, dass $f$ die Darstellung $f(z) =
    \sum^{\infty}_{n=0} a_n z^n$ für alle $0 < \abs{z} < r$ hat. Mit Hilfe von
    \autoref{satz:51} lässt sich nun folgern, $\abs{a_n}\leq \frac{M}{r^n}$
    für alle $n \in \N$. Der Grenzübergang $r \rightarrow \infty$ liefert
    dann: $a_n = 0$ für alle $n \in \N\setminus \{0\}$. Eingesetzt in die
    Potenzreihenentwicklung von $f$ folgt die Behauptung $f(z) = a_0$.
  \end{proof}
\end{satz}

\begin{bsp}
  Da die Sinusfunktion im Reellen nicht konstant ist, muss sie im Komplexen
  unbeschränkt sein.
\end{bsp}

\begin{satz}[Fundamentalsatz der Algebra]
  Wenn $p$ ein nicht-konstantes Polynom mit komplexen Koeffizienten ist, dann
  hat $p$ wenigstens eine Nullstelle in $\C$. Daraus folgt der Fundamentalsatz
  der Algebra: $p$ lässt sich als Produkt linearer Faktoren schreiben.
  \begin{proof}
    Sei $p(z) = a_0+\dotsb+ a_nz^n$ mit $a_n \neq 0$ für $n \in \N\setminus
    \{0\}$. Dann gilt:
    \begin{gather*}
	\abs{\frac{p(z)}{z^n}} = \frac{\abs{a_0}}{\abs{z}^n} +
	 \frac{\abs{a_1}}{\abs{z}^{n-1}} +\dotsb+
	 \frac{\abs{a_{n-1}}}{\abs{z}} + \abs{a_n}
     \end{gather*}
    Damit folgt im Grenzwert:
    \begin{gather*}
      \lim_{\abs{z}\rightarrow\infty} \abs[\big]{\frac{p(z)}{z^n}} = \abs{a_n}
	 > 0
    \end{gather*}
    Für ein ausreichend großes $R>0$ gilt daher aber auch:
    \begin{gather*}
      \abs[\big]{\frac{p(z)}{z^n}} \geq \frac{\abs{a_n}}{2} > 0 \qquad \forall
	 \abs{z} > R
    \end{gather*}
    Durch einfaches Umstellen erhalten wir damit:
    \begin{gather}\label{eq:51}
      \frac{1}{p(z)} \leq \frac{2}{\abs{a_n}} \cdot \frac{1}{\abs{z}^n} \leq
	 \frac{2}{\abs{a_n}R^n} =: M \qquad \forall \abs{z} > R
    \end{gather}
    Wir nehmen nun an, dass $p(z) \neq 0$ für alle $z \in \C$ wäre. Dann wäre
    $f := \frac{1}{p}$ eine ganze Funktion, weil $p$ als Polynom eine ganze
    Funktion darstellt. Außerdem wäre $f$ beschränkt für $\abs{z} \leq R$,
    weil $f$ stetig ist und $f$ wäre beschränkt für $\abs{z}\geq R$ wegen
    \autoref{eq:51}. Damit ist $f$ eine beschränkte ganze Funktion und wegen
    \autoref{satz:52} somit konstant. Damit ist aber auch $p$ konstant.
    $\lightning$ zur Voraussetzung. Deshalb war die Annahme falsch und $p$ hat
    wenigstens eine Nullstelle in $\C$.
  \end{proof}
\end{satz}

\begin{satz}[Satz von Morera]\label{satz:54}
  Sei $f$ in $\Omega$ stetig und gelte $\int_{\gamma} f(z)\,dz = 0$ für jeden
  einfach gelagerten, geschlossenen Weg\footnote{Es genügt sogar, dass das für
  jeden geschlossenen, in einem sternförmigen Gebiet $\Omega'\subset\Omega$
  liegenden Weg gilt.} $\gamma$ in $\Omega$. Dann ist $f$ holomorph auf
  $\Omega$.
  \begin{proof}
    Sei $K_r(z_0)\subset\Omega$ eine beliebige Kreisscheibe, welche
    trivialerweise sternförmig ist. Nach Vorraussetzung gilt, $\int_{\gamma}
    f(z)\,dz = 0$ für alle in $K_r(z_0)$ liegenden, geschlossenen und einfach
    gelagerten $\gamma$. Damit gilt für einen beliebigen Weg $\gamma' \in
    K_r(z_0)$, dass $\int_{\gamma'} f(z)\,dz$ wegunabhängig ist. Nach
    \autoref{satz:27} besitzt $f$ nun eine Stammfunktion $F$ auf $K_r(z_0)$,
    die außerdem noch holomorph ist. Wegen \autoref{satz:43} ist $F$ damit
    auch analytisch auf $K_r(z_0)$. Mit einer obigen Folgerung folgt nun, dass
    $F$ alle Ableitungen besitzt, welche selber wieder holomorph sind. Damit
    ist im Speziellen $f = F'$ holomorph auf jeder Kreisscheibe $K_r(z_0)
    \subset \Omega$, also auf ganz $\Omega$.
  \end{proof}
\end{satz}

\begin{satz}[Satz über Vertauschung von Differentiation und Grenzübergang]
  \label{satz:55}
  Sei $\{f_n\}^{\infty}_{n=0}$ eine Folge von in $\Omega$ holomorphen
  Funktionen, die kompakt gegen $f$ konvergieren. Dann ist auch $f$ holomorph
  in $\Omega$ und für jedes $k \in \N$ gilt: $f^{(k)}_n (z)
  \xrightarrow{\text{kompakt}} f^{(k)} (z)$.
  \begin{proof}
    Wir zeigen zunächst, dass $f$ holomorph in $\Omega$ ist. Dazu sei $\gamma$
    ein einfach gelagerter geschlossener Weg in $\Omega$. Dann gilt wegen
    \autoref{eq:31}\todo{ist eq:31 oder eq:32 gemeint?} und der dazugehörigen Aussage für jede auf $\Omega$
    holomorphe Funktion $\int_{\gamma} f_n(z)\,dz = 0$. Aus der kompakten
    Konvergenz von $\{f_n\}^{\infty}_{n=0}$ folgt (wie aus Analysis~2
    bekannt), dass $f$ stetig ist. Die Vertauschbarkeit von Grenzübergang und
    Integration folgt aus \autoref{satz:29} und damit folgt auch
    $\int_{\gamma} f(z)\,dz = \int_{\gamma} \lim_{n \rightarrow \infty}
    f_n(z)\,dz = \lim_{n \rightarrow \infty} \int_{\gamma} f_n(z)\,dz = 0$.

    Wegen \autoref{satz:54} gilt nun, dass auch $f$ holomorph auf $\Omega$ ist.
    Bleibt noch die Aussage über die kompakte Konvergenz der Ableitungen zu
    zeigen. Dazu sei $K \subset \Omega$ eine beliebige kompakte Menge. Wir
    setzen $r := \frac{1}{2}\text{dist}(K, \partial \Omega)$ und weiterhin $K_r :=
    \{z \in \C\colon \text{dist}(z, K) \leq r\}$. Dann gilt $K \subset K_r \subset
    \Omega$ und $K_r$ wieder eine kompakte Menge. Weiterhin sei ein $z_0 \in
    K$ fixiert, dann folgt aus \autoref{satz:43} mit den Cauchy-Formeln und
    der Additivität des Integrals:
    \begin{gather*}
      f^{(k)}(z_0) - f^{(k)}_n(z_0) = \frac{k!}{2\pi i} \int_{C_r(z_0)}
	 \frac{f(z) - f_n(z)}{(z - z_0)^{k+1}}\,dz
    \end{gather*}
    Damit gilt dann unter Verwendung von \autoref{satz:24} folgende
    Abschätzung:
    \begin{align*}
      \abs[\big]{f^{(k)}(z_0) - f^{(k)}_n(z_0)} &\leq \frac{k!}{\abs{2\pi i}}
	 \cdot \frac{L(C_r(z_0))}{r^{k+1}} \cdot \max_{z \in C_r(z_0)}
	 \abs{f(z) - f_n(z)}\\
      &= \frac{k!}{2\pi} \cdot \frac{2\pi r}{r^{k+1}} \cdot \max_{z \in
	 C_r(z_0)} \abs{f(z) - f_n(z)}\\
      &= \frac{k!}{r^k} \cdot \max_{z \in C_r(z_0)} \abs{f(z) - f_n(z)}
    \end{align*}
    Und damit gilt auch $f^{(k)}_n \xrightarrow{n \rightarrow \infty} f^{(k)}$
    gleichmäßig auf $K$.
  \end{proof}
\end{satz}

\begin{folg}
  Eine kompakt konvergente Reihe aus holomorphen Funktionen ist beliebig oft
  gliedweise differenzierbar.
\end{folg}

\begin{folg}
  Es sei $f$ stetig auf $\Omega$. Dann sind folgende Aussagen äquivalent:
  \begin{enumerate}
   \item $f$ ist holomorph auf $\Omega$.
   \item $f$ ist analytisch auf $\Omega$.
   \item $f(x + iy) = u(x,y) + iv(x,y)$ mit $u,v \in C^1(\Omega)$ und $u_x =
    v_y$ sowie $u_y = -v_x$.
   \item $\int_{\gamma} f(z)\,dz = 0$ für jeden einfach gelagerten,
    geschlossenen Weg $\gamma$ in $\Omega$.
  \end{enumerate}
  \begin{proof}
    \begin{itemize}
     \item[(i)$\Rightarrow$(ii)] gilt wegen \autoref{satz:43}
     \item[(ii)$\Rightarrow$(i)] gilt wegen \autoref{satz:31}
     \item[(i)$\Rightarrow$(iii)] gilt wegen \autoref{satz:15}
     \item[(i)$\Rightarrow$(iv)] gilt wegen \autoref{folg:21}
     \item[(iv)$\Rightarrow$(i)] gilt wegen \autoref{satz:54}
    \end{itemize}
  \end{proof}
\end{folg}

\begin{bemerk}
  Im Reellen gilt die Aussage "`$f \in C^1(\Omega) \Rightarrow f$ ist in
  Potenzreihe entwickelbar"' übrigens nicht! Ein Gegenbeispiel dazu ist:
  \begin{gather*}
    f(x) =
       \begin{cases}
	 e^{-\frac{1}{x^2}} & x \neq 0 \\
	 0   &  x = 0
	\end{cases}
  \end{gather*}
  Hier gilt $f \in C^{\infty}(\R)$ und außerdem $f^{(k)}(0) = 0$ für alle $k
  \in \N$. Damit kann $f$ in $x_0 = 0$ nicht in eine Potenzreihe entwickelt
  werden. Wenn überhaupt, muss die Potenzreihenentwicklung mit der
  Taylor-Reihe in einer Umgebung von $x_0 = 0$ übereinstimmen. Es gilt aber
  für alle $x \in ]-\epsilon, \epsilon[ \setminus \{0\}$:
  \begin{gather*}
    0 \neq f(x) = \sum^{\infty}_{k=0} \frac{f^{(k)}(0)}{k!} (x - 0)^k =
       \sum^{\infty}_{k=0} \frac{0}{k!} x^k = \sum^{\infty}_{k=0} 0 = 0
  \end{gather*}
\end{bemerk}

\chapter{Laurent"=Entwicklungen}

\begin{defini}\label{def:61}
  \begin{enumerate}
   \item Sei $f$ holomorph und $D_f = \Omega \setminus \{z_0\}$. Dann heißt
    $z_0$ \highl[Singularität!isolierte]{isolierte Singularität}.
   \item  Der Punkt $z_0$ heißt genau dann \highl[Singularität!hebbare]{hebbare
    Singularität}, wenn es eine auf $K_{\epsilon}(z_0)$ holomorphe Funktion
    $g$ mit $f(z) = g(z)$ für alle $z \in K_{\epsilon}(z_0) \setminus \{z_0\}$
    gibt.
   \item Der Punkt $z_0$ heißt genau dann \highl{Polstelle}, wenn
    $\lim_{z\rightarrow z_0}\limits \abs{f(z)} = \infty$.
   \item Der Punkt $z_0$ heißt genau dann
    \highl[Singularität!wesentliche]{wesentliche Singularität}, wenn $z_0$
    weder eine hebbare Singularität noch eine Polstelle ist.
  \end{enumerate}
\end{defini}

\begin{bsp}
  \begin{enumerate}[a)]
   \item Die Funktion $f(z) = \frac{\sin(z)}{z}$ hat in $z_0 = 0$ eine hebbare
    Singularität. Denn $g(z) = 1 - \frac{z^2}{3!} + \frac{z^4}{5!} -
    \frac{z^6}{7!} + \ldots = \sum^{\infty}_{k=0} (-1)^k
    \frac{z^{2k}}{(2k+1)!}$ erfüllt die in \autoref{def:61} geforderten
    Bedingungen.
   \item Die Funktion $f(z) = \frac{z^3 - 1}{z - 1}$ hat in $z_0 = 1$ eine
    hebbare Singularität.
   \item Ebenso hat $f(z) = \frac{e^z - 1}{z}$ in $z_0 = 0$ eine hebbare
    Singularität.
   \item Die Funktion $f(z) = \frac{1}{z}$ hat in $z_0 = 0$ eine Polstelle.
    Denn $\lim_{z \rightarrow 0} \abs{\frac{1}{z}} = \lim_{z \rightarrow 0}
    \frac{1}{\abs{z}} = \infty$.
   \item Die Funktion $f(z) = e^{\frac{1}{z}}$ hat in $z_0 = 0$ eine
    wesentliche Singularität. Denn $\lim_{n \rightarrow \infty}
    f\left(\frac{1}{n}\right) = \lim_{n \rightarrow \infty} e^n = \infty$.
    Aber $\lim_{n \rightarrow \infty} f\left(-\frac{1}{n}\right) = \lim_{n
    \rightarrow \infty} e^{-n} = 0$. Damit ist $z_0 = 0$ keine Polstelle und
    es kann auch keine holomorphe Fortsetzung von $f$ in $z_0$ geben. Wie wir
    später noch sehen werden, ist sogar jede komplexe Zahl als Grenzwert
    möglich. Es gilt zum Beispiel auch: $\lim_{n \rightarrow \infty}
    f\left(\frac{1}{2n\pi i}\right) = \lim_{n \rightarrow \infty} e^{(2n\pi)i}
    = 1$.
  \end{enumerate}
\end{bsp}

\begin{bsp}
  \begin{enumerate}[a)]
   \item Wie wir bereits in \autoref{bsp:31}~(iii) gesehen hatten, gilt,
    $\frac{1}{w - z} = \sum^{\infty}_{n=0} \frac{(z - z_0)^n}{(w -
    z_0)^{n+1}}$ für alle $\abs{z - z_0} < \abs{w - z_0}$.
   \item Aber umgekehrt gilt auch für $\abs{z - z_0} > \abs{w - z_0}$:
    \begin{gather*}
      \frac{1}{w - z} = -\frac{1}{z - w} = -\frac{1}{z - z_0} \cdot \frac{1}{1
	 - \frac{w - z_0}{z - z_0}} = -\frac{1}{z - z_0} \sum^{\infty}_{n=0}
	 \left(\frac{w - z_0}{z - z_0}\right)^n = -\sum^{\infty}_{n=0}
	 \frac{(w - z_0)^n}{(z - z_0)^{n+1}}
    \end{gather*}
   \item Beides zusammen ergibt:
    \begin{gather*}
      \frac{1}{w - z} =
	 \begin{cases}
	   \sum^{\infty}_{n=0} \frac{(z - z_0)^n}{(w - z_0)^{n+1}}  & \abs{z -
	   z_0} < \abs{w - z_0} \\
	   -\sum^{\infty}_{n=0} \frac{(w - z_0)^n}{(z - z_0)^{n+1}} & \abs{z -
	   z_0} > \abs{w - z_0}
	 \end{cases}
    \end{gather*}
    Durch eine Indexverschiebung und anschließendem Übergang von $n$ zu $-n$
    kann man die zweite Gleichung aber auch in einer (noch) ungewöhnlichen
    Form aufschreiben, die uns später noch hilfreich sein wird:
    \begin{gather*}
      \frac{1}{w - z} =
	 \begin{cases}
	   \sum^{\infty}_{n=0} \frac{(z - z_0)^n}{(w - z_0)^{n+1}}    & \abs{z
	   - z_0} < \abs{w - z_0}\\
	   -\sum^{-1}_{n=-\infty} \frac{(z - z_0)^n}{(w - z_0)^{n+1}} & \abs{z
	   - z_0} > \abs{w - z_0}
	 \end{cases}
    \end{gather*}
  \end{enumerate}
\end{bsp}

\begin{defini}
  \begin{enumerate}
   \item Die Reihe $\sum^{\infty}_{n=-\infty} a_n (z - z_0)^n$ heißt
    \highl{Laurentreihe}.
   \item Für eine Laurentreihe $\sum^{\infty}_{n=-\infty} a_n (z - z_0)^n$
    setzen wir
    \begin{align*}
	r(z) &:= \sum^{\infty}_{n=0} a_n (z - z_0)^n\\
	h(z) &:= \sum^{-1}_{n=-\infty} a_n (z - z_0)^n = \sum^{\infty}_{n=1}
	 \frac{a_{-n}}{(z - z_0)^n}
    \end{align*}
    Dabei heißt $r(z)$ \highl{Nebenteil} (oder
    \highl[Teil!regulärer]{regulärer Teil}) und $h(z)$ \highl{Hauptteil} (oder
    \highl[Teil!singulärer]{singulärer Teil}) der Laurentreihe.
   \item Eine Laurentreihe heißt genau dann
    \highl[Laurentreihe!konvergente]{konvergent} in $z$, wenn $r(z)$ und
    $h(z)$ konvergieren.
  \end{enumerate}
\end{defini}

\begin{satz}[Satz über den singulären Teil der Laurentreihe]
  Wir betrachten $\sum_{n=1}^{\infty} \frac{a_{n}}{(z-z_{0})^{n}}$. Dann gilt:
  \begin{enumerate}
   \item Ist die Reihe konvergent für $z=z_{1}$, so ist sie absolut konvergent
    für alle $z$ mit $\abs{z-z_{0}}> \abs{z_{1}-z_{0}}$.
   \item Ist die Reihe divergent in $z=z_{2}$, so ist sie divergent für alle
    $z$ mit $\abs{z-z_{0}}< \abs{z_{1}-z_{0}}$.
  \end{enumerate}
  \begin{proof}
    Zum Beweis führen wir die Aussage auf bereits bekannte
    Konvergenzeigenschaften von Potenzreihen zurück. Dazu setzen wir:
    \begin{gather*}
      w:= \frac{1}{z-z_{0}}\qquad w_{1}:= \frac{1}{z_{1}-z_{0}}\qquad w_{2}:=
	 \frac{1}{z_{2}-z_{0}}
    \end{gather*}
    Damit hat der Hauptteil der Laurentreihe die Gestalt $h(z)=
    \sum_{n=1}^{\infty} a_{-n} w^{n}$.
    \begin{enumerate}
     \item $h(z)=\sum_{n=1}^{\infty} a_{-n} w^{n}_{1}$ konvergiert
      $\Rightarrow\sum_{n=1}^{\infty} a_{-n} w^{n}$ konvergiert absolut für
      alle $\abs{w}<\abs{w_{1}}$. Nun setzt man die Darstellung für $w$ und
      $w_{1}$ ein. Nach der Umstellung der Gleichung erhält man das gewünschte
      Ergebnis.
     \item $h(z)= \sum_{n=1}^{\infty} a_{-n} w^{n}_{2}$ divergiert
      $\Rightarrow\sum_{n=1}^{\infty} a_{-n} w^{n}$ divergiert für alle
      $\abs{w}>\abs{w_{2}}$. Danach wieder Einsetzen und Umstellen.
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item  Diese Konvergenzeigenschaften für den Hauptteil einer Laurentreihe
    sind genau umgekehrt zu den Konvergenzeigenschaften für Potenzreihen von
    früher. Es haben sich nur die Relationszeichen gedreht.
   \item Ist der Hauptteil der Laurentreihe konvergent für $z_{1}$, der
    Nebenteil konvergent für $z_{2}$, so ist die Laurentreihe konvergent im
    Ringgebiet: $\abs{z_{1}-z_{0}}< \abs{z-z_{0}} <\abs{z_{2}-z_{0}}$.
  \end{enumerate}
\end{bemerk}

\begin{satz}[\textsc{Cauchy}"~Formel für Laurentreihen]\label{satz:62}
  Sei $f(z) := \sum^{\infty}_{n=-\infty} a_n (z - z_0)^n$ konvergent für
  $\rho:= \abs{z_{1}-z_{0}} < \abs{z - z_0} <\abs{z_{2}-z_{0}}=\colon R$ mit
  $0 \leq \rho < R \leq \infty$. Dann ist $f(z)$ gleichmäßig konvergent für
  $\rho + \epsilon \leq \abs{z - z_0} \leq R - \mu$ mit $\epsilon, \mu > 0$.
  Insbesondere ist dann $f$ in diesem Kreisring holomorph. Dabei gilt:
  \begin{gather*}
    a_n = \frac{1}{2\pi i} \int_{C_r(z_0)} \frac{f(z)}{(z - z_0)^{n+1}}\,dz
       \qquad \forall \rho < r < R
  \end{gather*}
  \begin{proof}
    Die Tatsache, dass $r(z)$ für $\abs{z - z_0} \leq R - \mu$ gleichmäßig
    konvergent ist, ist schon von den Potenzreihen bekannt. Sei also $\rho +
    \frac{\epsilon}{2} = \abs{z_2 - z_0}$. Da $f$ konvergent für dieses $z_2$
    ist, folgt, dass $f(z_2) := \sum^{\infty}_{n=1} \frac{a_{-n}}{(z_2 -
    z_0)^n}$ absolut konvergent ist. Dies impliziert, dass
    $\left\{\frac{a_{-n}}{(z_2 - z_0)^n}\right\}^{\infty}_{n=1}$ eine
    Nullfolge darstellt. Da die Glieder jeder konvergenten Folge auch
    beschränkt sind, gilt, $\frac{\abs{a_{-n}}}{\abs{z_2 - z_0}^n} \leq M
    \Leftrightarrow \abs{a_{-n}} \leq M \cdot \abs{z_2 - z_0}^n$. Sei nun
    weiterhin $\rho + \epsilon \leq \abs{z - z_0}$. Dann lässt sich folgende
    Abschätzung treffen:
    \begin{gather*}
      \abs[\big]{\frac{a_{-n}}{(z - z_0)^n}} = \frac{\abs{a_{-n}}}{\abs{z_2 -
	 z_0}^n} \cdot \frac{\abs{z_2 - z_0}^n}{\abs{z - z_0}^n}\leq M
	 \abs[\big]{\frac{z_2 - z_0}{z - z_0}}^n \leq M \left(\frac{\rho +
	 \frac{\epsilon}{2}}{\rho + \epsilon}\right)^n
    \end{gather*}
    Mit dem \textsc{Weierstraß}schen Majorantenkriterium folgt nun, dass
    $h(z)$ gleichmäßig konvergent ist für $\abs{z - z_0} \geq \rho +
    \epsilon$.

    Sei nun $K$ eine kompakte Teilmenge von $\rho < \abs{z - z_0} < R$. Dann
    liegt $K$ in einem Kreisring obiger Art und wie wir bereits gesehen haben,
    sind $r(z)$ und $h(z)$ damit kompakt konvergent gegen $f$. Mit
    \autoref{satz:55} folgt dann, dass $f$ auch holomorph ist. Aus
    \autoref{satz:29} folgt aufgrund der kompakten Konvergenz, dass
    Integration und Grenzwertbildung vertauscht werden dürfen. Daher gilt:
    \begin{gather*}
      \int_{C_r(z_0)} \frac{f(w)}{(w - z_0)^{k+1}}\,dw =
	 \sum^{\infty}_{n=-\infty} a_n \int_{C_r(z_0)} (w - z_0)^{n-k-1}\,dw
    \end{gather*}
    Der \autoref{satz:26} liefert dann, dass nur der Summand mit $n = k$ übrig
    bleibt, was uns zu folgender Gleichung bringt:
    \begin{gather*}
      \int_{C_r(z_0)} \frac{f(w)}{(w - z_0)^{k+1}}\,dw = 2\pi i \cdot a_k
    \end{gather*}
    Die Division durch $2\pi i$ liefert das gewünschte Ergebnis.
  \end{proof}
\end{satz}

\begin{satz}[Identitätssatz für Laurentreihen]
  Sei $\rho:= \abs{z_{1}-z_{0}}, R:= \abs{z_{2}-z_{0}}$. Gilt nun
  \begin{gather*}
    \sum^{\infty}_{n=-\infty} a_n (z - z_0)^n = \sum^{\infty}_{n=-\infty} b_n
       (z - z_0)^n \text{ für } \rho<\abs{z-z_{0}}<R
  \end{gather*}
  so sind $a_{n}=b_{n}$ für alle $n\in\Z$.
  \begin{proof}
    Der Beweis folgt sofort aus der in \autoref{satz:62} gezeigten Formel für
    die Koeffizienten der Laurentreihe. Man braucht nur das gleiche $r$
    betrachten.
  \end{proof}
\end{satz}

