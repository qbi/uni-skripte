\subsection{Vervollst"andigung von Ma"sr"aumen}

($\Omega$, F, m) beliebiger Ma"sraum

\begin{defini}\\
  $F^{m} = \{ A\subseteq\Omega: \exists A_{1},A_{2}\in F, A_{1}\subseteq
  A \subseteq A_{2}, m(A_{2}\setminus A_{1})=0\}$
\end{defini}

\begin{lemma}\\
  $F^{m}$ ist eine $\sigma$-Algebra, $F\subseteq F^{m}$
  
  \begin{proof}\\
    $F^{m}$ ist $\sigma$-Algebra:
    \begin{enumerate}[1)]
     \item $\Omega$\in $F^{m}$, da $\Omega$\in F
     \item A\in $F^{m}$ $\Rightarrow$ $A^{C}\in F^{m}$: klar, da
      m($A_{1}^{C}\setminus A_{2}^{C})=m(A_{2}\setminus A_{1})=0$
     \item $A_{n}\in F^{m}, n\geq 1 \Rightarrow
      \cup_{m=1}^{\infty}A_{n}\in F^{m}$: $\exists A_{n}^{(n)}\subseteq
      A_{n}\subseteq A_{n}^{(n)}, A_{i}^{(n)}\in F,
      m(A_{2}^{(n)}\setminus A_{1}^{(n)}=0, n=1$
      
      A:= $\cup_{n=1}^{\infty}A_{n}, A_{i}:=\cup_{n=1}^{\infty}A_{i}^{(n)}\in
      F, A_{1}\subseteq A \subseteq A_{2}, m(A_{2}\setminus A_{1}) \leq
      m(\cup_{n=1}^{\infty}(A_{2}^{(n)}\setminus A_{1}^{(n)}) \leq
      \sum_{n=1}^{\infty} m(A_{2}^{\infty}\setminus A_{1}^{(n)}=0$
      
      $\Rightarrow$ A \in $F^{m}$
    \end{enumerate}
  \end{proof}
\end{lemma}

\begin{defini}\\
  m(A)=m($A_{1}$) (=m($A_{2}$)), $A_{1}\subseteq A\subseteq A_{2}, A_{1},
  A_{2}\in F, m(A_2\setminus A_{1}) =0 A\in F^{m}$
\end{defini}

\begin{lemma}\\
  \begin{enumerate}[(i)]
   \item Definition von m auf $F^{m}$ korrekt
   \item m ist ein Ma"s auf $F^{m}$ (Fortsetzung von m auf F)
  \end{enumerate}
  
  \begin{proof}\\
    \begin{enumerate}[(i)]
     \item $A_{1}\subseteq A \subseteq A_{2}, A_{1}, A_{2}\in F,
      m(A_{2}\setminus A_{2})=0$
      $A_{1}'\subseteq A \subseteq A_{2}', A_{1}', A_{2}' \in F,
      m(A_{2}'\setminus A_{1}')=0$
      
      $\Rightarrow A_1\setminus A_{1}' \subseteq A_{2}'\setminus A_{1}'
      \Rightarrow m(A_{1}\setminus A_{1}') \leq m(A_{2}' \setminus
      A_{1}')=0$
      
      Es folgt m($A_{1}$) = \underbrace{m($A_{1}\setminus A_{1}')}_{=0} + m(A_{1}\cap
      A_{1}') = m(A_{1}\cap A_{1}')=\ldots=m(A_{1}')$
     \item
      \begin{enumerate}[(a)]
       \item m($\emptyset$) =0
       \item m($A_{n})\in F^{m}$ paarw. disjunkt, A :=
	$\cup_{n=1}^{\infty}A_{n}$ $\exists A_{1}^{(n)}\subseteq A_{n}
	\subseteq A_{2}^{(n)} : A_{1}^{(n)},A_{2}^{(n)}\in F,
	m(A_{2}^{(n)}\setminus A_{1}^{(n)})=0, n\geq 1$
	
	$ A_{i}:= \cup_{n=1}^{\infty}A_{i}^{(n)}\in F, A_{1}\subseteq A
	\subseteq A_{2}, m(A_{2}\setminus A_{1}) = 0$
	
	Es gilt: m(A) = (nach defini) m($A_{1}$) = $\sum_{n=1}^{\infty}
	m(A_{1}^{(n)} = \sum_{n=1}^{\infty} m(A_{n}}$ (wegen
	$\sigma$-Additivit"at von m auf F, $A_{1}^{(n)}, n\geq 1,$ paarw. disj.
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{lemma}

\begin{defini}\\
