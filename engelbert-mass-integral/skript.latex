\chapter{Monotoner Klassensatz}
$\Omega\not=\emptyset$, $\mathcal{M}\subseteq\mathcal{P}(\Omega)$

\begin{defini}
  \begin{enumerate}[(i)]
   \item $\mathcal{M}$ heißt monotone Klasse, falls
    \begin{enumerate}[a)]
     \item $A_{n}\in\mathcal{M}, A_{n}\subseteq A_{n+1}, n\ge1\Rightarrow
      \bigcup_{n=1}^{\infty}A_{n}\in\mathcal{M}$
     \item $A_{n}\in\mathcal{M}, A_{n+1}\subseteq A_{n}, n\ge1
      \Rightarrow\bigcap_{n=1}^{\infty}A_{n}\in\mathcal{M}$
    \end{enumerate}
   \item $S\subseteq\mathcal{P}(\Omega)$. Dann bezeichne
    $\mathfrak{M}(S)$ die kleinste monotone Klasse, die S enthält:\\
    $\mathfrak{M}(S)=\bigcap_{\substack{\mathfrak{M}\, \text{monotone Klasse}\\
    S\subseteq \mathfrak{M}}}\mathcal{M}$
  \end{enumerate}
\end{defini}

\begin{satz}
  $\CF\in \mathcal{P}(\Omega)$
