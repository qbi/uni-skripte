    \begin{gather}
      \Rightarrow |f+g|\leq(|f|^{p}+|g|^{p})^{\frac1p}2^{\frac1q} ( auch
	 gültig für |f+g|=0)\\
      = |f+g|^{p} \leq (|f|^{p}+|g|^{p})2^{\frac{p}q} \Rightarrow
	 |f+g|^{p} integrierbar \Rightarrow f+g\in L_{p}
    \end{gather}

    Dreiecksungleicheung:
    \begin{align}
      \|f+g\|^{p}_{p} &=\int_{\Omega}|f+g|^{p}\,\mathfrak{m} =
	 \int_{\Omega}|f+g||f+g|^{p-1}\,dm\\
      &\leq\int_{\Omega}|f||f+g|^{p-1}\,\mathfrak{m} +
	 \int_{\Omega}|g||f+g|^{p-1}\,dm\\
	 &\leq(\int_{\Omega}|f|^{p}\,\mathfrak{m})^{\frac1p}
	 (\int_{\Omega}|f+g|^{(p-1)q}\,\mathfrak{m})^{\frac1q} +
	 (\int_{\Omega}|g|^{p}\,\mathfrak{m})^{\frac1q}
	 (\int_{\Omega}|f+g|^{(p-1)q}\,\mathfrak{m})^{\frac1q} (Höldersche Ungleichung)\\
      &=(\|f\|_{p}+\|g\|_{p})(\int_{\Omega}|f+g|^{p}\,\mathfrak{m})^{\frac1q}\\
      Ann:. \int_{\Omega}|f+g|^{p}\,dm\not=0\\
	 &=(\int_{\Omega}|f+g|^{p}\,\mathfrak{m})^{1-\frac1q=1\frac1p}
	 \leq\|f\|_{p}+\|g\|_{p} \Rightarrow \|f\|_{p}+\|g\|_{p}
	 \Rightarrow \|f+g\|_{p}\leq \|f\|_{p}+\|g\|_{p}
    \end{align}
  \end{description}
\end{proof}

\begin{satz} % Satz 3
  $1\leq p<\infty, (\Omega, \mathcal{\CF}, \mathcal{m})$ Maßraum

  Dann istt $L_{p}(\Omega, \mathcal{\CF},\mathcal{m})$ ein Banachraum (vollständig
  nromierter linearer Raum)

  \begin{proof}
    \begin{enumerate}
     \item $L_{p}\subseteq L_{0}$ linearer Unterraum (Anwendung von Satz 2)

      $f\in L_{p}, \alpha\in\R \Rightarrow \alpha f\in L_{p}:
      \int_{\Omega}|\alpha f|^{p}\,\mathfrak{m} =
      |\alpha|^{p}\underbrace{\int_{\Omega}|f|^{p}\,\mathfrak{m}}_{<\infty} <\infty$
     \item $\|\cdot\|$ ist eine Norm
      \begin{enumerate}
       \item $\|f\|_{p}\geq0, \|f\|_{p}=\int_{\Omega}|f|^{p}\,\mathfrak{m}=0
	\Leftrightarrow |f|=0 m-fü \Leftrightarrow f=0$  (im Sinne
	der Äquivalenzklassenbildung)
       \item $\|\alpha f\|_{p}=|\alpha|\|f\|_{p}, \alpha\in\R, f\in
	L_{p}$ klar
       \item $\|f+g\|_{p} \leq \|f\|_{p}+\|g\|_{p}, f,g\in L_{p}$ (Satz 2)
      \end{enumerate}
     \item ($L_{p},\|\cdot\|_{p})$ vollständiger normierter Raum

      Es sei ($f_{n}$) eine Cauchy-Folge in $L_{p}$:
      $\lim_{n,m\rightarrow\infty} \|f_{n}-f_{m}\|_{p}=0$

      Es existiert $(n_{k})\colon \|f_{n}-f_{m}\|_{p}\leq\frac{1}{2^k},
      \forall n,m\geq n_{k}$

      $n_{k}\leq n_{k+1}$
      Es folgt: $\|f_{n_{k+1}}-f_{n_{k}}\|_{p}\leq\frac{1}{2^{k}}, k=1,2,\ldots$
      $g := \sum_{k=1}^{\infty} |f_{n_{k+1}}-f_{n_{k}}|$ messbare Funktion

      zeigen: $g\in L_{p}$
      \begin{gather}
	\int_{\Omega}|g|^{p}\,\mathfrak{m} = \int_{\Omega}\lim_{m\rightarrow\infty}
	   (\sum_{k=1}^{m}|f_{n_{k-1}}-f_{n_{k}}|)^{p}\,\mathfrak{m} =^{B.\,Levi}
	   \lim_{m\rightarrow\infty} \int_{\Omega}
	   (\sum_{k=1}^{m}|f_{n_{k-1}}-f_{n_{k}})^{p}\,dm\\
	   =\lim_{n\rightarrow\infty}
	   \|\sum_{k=1}^{m}|f_{n_{k-1}}-f_{n_{k}}| \|^{p}_{p} \leq^{Dreiecksungl.}
	   \lim_{m\rightarrow\infty}
	   (\sum_{k=1}^{m}\underbrace{\|f_{n_{k-1}}-f_{n_{k}}\|_{p}}_{\leq\frac1{2^{k}}}
	   )^{p} \leq1
      \end{gather}

      $\Rightarrow$ $0\leq g<\infty$ m-fü
      $\Rightarrow$ $\Omega_{0}\in \mathcal{\CF}, m(\Omega^{\complement})=0$ und
      $0\leq g(x)<\infty$ $\forall x\in\Omega_{0}$

      \begin{gather*}
        f(x) =
      \begin{cases}
	f_{n_{0}}(x)+\sum_{k=1}^{\infty}(f_{n_{k+1}}(x)-f_{n_{k}}(x)&
	x\in\Omega_{\Omega}\\
        0 & x\in\Omega_{0}^{\complement}
      \end{cases}
      \end{gather*}
      Beh.: $f\in L_{p}$ und $\|f_{n}-f\|_{p}$
      $\rightarrow_{n\rightarrow \infty}0$
      \begin{enumerate}
       \item $f\in L_{p}$: |f|$\leq$ |$f_{m}|+g\in L_{p}$
       \item ||$f_{n}-f||_{p} \rightarrow^{n\rightarrow\infty}0$

	g.\,z.\,z: ||$f_{n_{m}}-f||_{p}\rightarrow^{m\rightarrow\infty}0$ (da
	($f_{n}$) Cauchy-Folge)
	||$f_{n_{m}}-f||_{p}=(\int_{\Omega}|f_{n_{m}}-f|
	-f_{n_{1}}-\sum_{k=1}^{\infty} (f_{n_{k+1}}-f_{n_{k}})|^{p}\,\mathfrak{m})^{\frac1p}$
	=($\int_{\Omega}|\sum_{k=1}^{\infty}(f_{n_{k-1}}-f_{k})|^{p}\,dm
	=^{B.\,Levi}\lim_{s\rightarrow\infty}(\int_{\Omega}
	\sum_{k=1}^{s}(f_{n_{k-1}}-f_{n_{k}})|^{p}\,\mathfrak{m})^{\frac1p} =
	\lim_{s\rightarrow\infty}\sum_{k=1}^{s}
	\underbrace{||f_{n_{k-1}}-f_{n_{k}}||_{p}}_{\leq \frac1{2^{k}}}
	\leq\sum_{k=m}^{\infty}\frac1{2^{k}}\rightarrow^{n\rightarrow\infty}0$
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{satz}

