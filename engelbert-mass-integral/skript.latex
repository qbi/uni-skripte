   \item F^{m} hei"st Vervollst"andigung von F bgzl. m
   \item ($\Omega$, $F^{m}$, m) hei"st Vervollst"andigung des Ma"sraumes
    ($\Omega$, F, m)
  \end{enumerate}
\end{defini}

\begin{defini}\\
  Es sei (M, dM, $\mu$) ein Ma"sraum. (M, dM, $\mu$) hei"st vollst"andig,
  falls f"ur alle A$\subseteq$$\Omega$, f"ur die exist. eine Menge B
  \in dM mit A$\subseteq$ B und $\mu(B)=0$, stets folgt A\in dM (und
  $\mu(A)=0$ gilt)
\end{defini}

\begin{bemerk}\\
  ($\Omega$, $F^{m}$, m) ist vollst"andig.
\end{bemerk}

\begin{satz}\\
  Sei $m$ ein Pr"ama"s auf ($\Omega$, dA), dA Algebra, F:=$\sigma$(dA)
  
  $\bar{m}$ auf $\bar{F}$ seien definiert wie im Beweis des
  Fortsetzungssatzes (insbesondere ist $\bar{m}$ ein Ma"s auf F)
  
  Dann gilt:
  \begin{enumerate}[(i)]
   \item $F^{\bar{m}} \subseteq \bar{F}$
   \item Sei $m$ endlich. Dann gilt $F^{\bar{m}}=\bar{F}$
  \end{enumerate}
  
  \begin{proof}\\
    \begin{enumerate}[(i)]
     \item Sei A\in $F^{\bar{m}}. \Rightarrow \exists A_{1}, A_{2}\in F:
      A_{1}\subseteq A\subseteq A_{2}, \bar{m}(A_{2}\setminus A_{1})=0$
      
      A=$\underbrace{A_{1}}_{\in F\subseteq \bar{F}}\cup(A\setminus A_{1})$
      
      ggz. A$\setminus$$A_{1}\in \bar{F}$
      
      Es gilt: A$\setminus$$A_{1}\subseteq A_{2}\setminus A_{1}\in F,
      \bar{m}(A_{2}\setminus A_{1})=0$
      
      deshalb ggz.: C$\subseteq$B, B \in F, $\bar{m}(B)=0 \Rightarrow
      C\in \bar{F}$
      
      $m^{\ast}(C\cap E)+m^{\ast}(C^{C}\cap E) \leq
      \underbrace{m^{\ast}(B)}_{=\bar{m}(B)=0}+m^{\ast}(C^{C}\cap E) \leq
      m^{\ast}(E)$
      
      umgekehrte Ungleichung folgt aus der Subadditivit"at von $m^{\ast}$
      (au"seres Ma"s sogar stark subadditiv)
      
      $\Rightarrow$ C \in $\bar{F}$
     \item $m$ sei endl., A\in $\bar{F}$ z.z.: A\in $F^{\bar{m}}$
      folgerung zum Fortsetzungssatz:
      \begin{gather}\\
	\sup_{B\subseteq A, B\in dA_{\delta}} \bar{m}(B) = \bar{m}(A) =
	   \inf_{A\subseteq B, B\in dA_{\sigma}} \bar{m}(B)
      \end{gather}
      
      Analog zum Beweis von Satz 4.1 folgt hieraus:
      \begin{gather}\\
	\exists A_{1}\in dA_{\delta\sigma}, A_{2}\in dA_{\sigma\delta}:
	   A_{1}\subseteq A \subseteq A_{2}, \bar{m}(A_{1}) = \bar{m}(A)
	   = \bar{m}(A_{2}) (\Rightarrow \bar{m}(A_{2}\setminus A_{1})=0)\\
	dA_{\delta\sigma}, dA_{\sigma\delta}\subseteq F \Rightarrow
	   A_{1}, A_{2}\in F\\
	\Rightarrow A \in F^{\bar{m}}
      \end{gather}
    \end{enumerate}
  \end{proof}
\end{satz}

\section{Konstruktion von Pr"ama"sen}
$\omega\not\emptyset$

\begin{defini}\\
  dC $\subseteq$ dP($\Omega$) Mengensystem, dC hei"st dann eine
  Halbalgebra, falls gilt:
  \begin{enumerate}
   \item $\Omega\in dC$
   \item dC ist ein $\pi$-System
   \item A\in dC $\Rightarrow$ $A^{C}=\cup_{i=1}^{n}A_{i},
    A_{1},\ldots,A_{n}\in dC$ paarw. disj.
  \end{enumerate}
\end{defini}

\begin{bemerk}\\
  $\emptyset\in dC$
\end{bemerk}

\begin{beispiel}\\
  $\Omega=\R, dC=\{[a,b)\cap \R: -\infty\leq a \leq b<+\infty\}$
  Halbalgebra
\end{beispiel}

\begin{lemma}\\
  dC sei halbalgebra, dA := $\alpha$(dC) di von dC erzeugte Algebra
  
  Dann gilt: A\in dA $\Leftrightarrow$ A = $\cup_{i=1}^{n}A_{i},
  A_{1},\ldots,A_{n}\in dC$ paarw. disj.
\end{lemma}

\begin{satz}\\
  Sei m eine nicht negative Mengenfunktion auf einer Halbarlgebra. dC,
  $m(\emptyset)=0$, $m$ additiv
  
  \begin{enumerate}
   \item Es exist. genau ein Inhalt m' (additive mengenfnk. mit
    m($\emptyset$)=0) auf dA := $\alpha(dC)$ mit m'(A)=m(A), A\in dC
    
    Es gilt ($\ast$): m'(A)= $\sum_{i=1}^{n}m(A_{i})$, A\in dA, A=$\cup_{i=1}^{n}A_{i},
    A_{1},\ldots,A_{n}\in dC$ paarw. disj.
   \item Falls m  $\sigma$-additiv auf dC ist, dann ist auch m'
    $\sigma$-additiv auf dA (und damit ein Pr"ama"s).
  \end{enumerate}
  
  \begin{proof}\\
    \begin{enumerate}
     \item Definieren Mengenfunktion m' gem"a"s ($\ast$) auf dA
      
      Definition ist korrekt. A=
      $\cup_{i=1}^{m}A_{i}=\cup_{j=1}^{m}B_{m}, A_{1},\ldots,A_{n}\in dC$
      paarw. dj. $B_{1},\ldots,B_{m}\in dC$
      
      $\Rightarrow A_{i}=\cup_{j=1}^{m}\underbrace{(A_{i}\cap
      B_{j})}_{\in dC} i=1,\ldots,n$
      
      $\sum_{i=1}^{n} m(A_{i})=\sum_{i=1}^{n}\sum_{j=1}^{m}m(A_{i}\cap
      B_{j})$ (Additiv. von m auf dC)
      $=\sum_{j=1}^{m}\underbrace{\sum_{i=1}^{n} m(A_{i}\cap
      B_{j})}_{m(B_{j})}=\sum_{j=1}^{m}m(B_{j})$
      
      $\cup_{i=1}^{n}\underbrace{(A_{i}\cap B_{j})}_{\in dC \text{pw. dj}}=B_{j}$
      
     \item m' ist additiv auf dA
      $A^{(j)j}\in dA, j=1,\ldots,m$ pw. dj. A:= $\cup_{j=1}^{m}A^{(j)}$
      
      zz.: m'(A) = $\sum_{j=1}^{m}m'(A^{(j)})$
      
      Darstellung von $A^{(j)}, A: A^{(n)}=\cup_{i=1}^{n_{j}}A_{i}^{(j)},
      A_{1}^{(j)},\ldots,A_{n_{j}}^{(j)}\in dC$ pw.dj.
      A=$\cup_{i=1}^{n}A_{i}, A_{1},\ldots,A_{n}\in dC$ pw.dj.
      
      Es gilt: $\underbrace{A_{i}}_{\in dC}=\cup_{j=1}^{m}\cup_{k=1}^{n_{j}}\underbrace{(A_{i}\cap
      A_{k}^{(j)})}_{\in dC}$
      
      m'(A) := $\sum_{i=1}^{n} m(A_{i}) =(**)
      \sum_{i=1}^{n}\sum_{j=1}^{m}\sum_{k=1}^{n_{j}} m(A_{i}\cap
      A_{k}^{(j)})$ (Additivit"at von m auf dC)
      = $\sum_{j=1}^{m}\sum_{k=1}^{n_{j}}\sum_{i=1}^{n} m(A_{i}\cap
      A_{k}^{(j)}) = \sum_{j=1}^{m}\sum_{k=1}^{n_{j}} m(A_{k}^{(j)})$
      
      \Rightarrow m'(A)=$\sum_{j=1}^{m} m'(A^{(j)})$
     \item m' \sigma-additiv auf dA, falls m \sigma-add. auf dC
      
      Beweis analog f"ur den Fall, das der Index m=$+\infty$
    \end{enumerate}
  \end{proof}
\end{satz}

