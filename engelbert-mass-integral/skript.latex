\section{Vervollständigung von Maßräumen}

$(\Omega, \CF, m)$ beliebiger Maßraum

\begin{defini}
  $\CF^{m} = \{ A\subseteq\Omega: \exists A_{1},A_{2}\in \CF, A_{1}\subseteq
  A \subseteq A_{2}, m(A_{2}\setminus A_{1})=0\}$
\end{defini}

\begin{lemma}
  $\CF^{m}$ ist eine $\sigma$-Algebra, $\CF\subseteq \CF^{m}$

  \begin{proof}
    $\CF^{m}$ ist $\sigma$-Algebra:
    \begin{enumerate}[1)]
     \item $\Omega\in \CF^{m}$, da $\Omega\in \CF$
     \item $A\in \CF^{m}$ $\Rightarrow$ $A^{\complement}\in \CF^{m}$: klar, da
      $m(A_{1}^{\complement}\setminus A_{2}^{\complement})=m(A_{2}\setminus A_{1})=0$
     \item $A_{n}\in \CF^{m}, n\geq 1 \Rightarrow
      \bigcup_{m=1}^{\infty}A_{n}\in \CF^{m}$: $\exists A_{n}^{(n)}\subseteq
      A_{n}\subseteq A_{n}^{(n)}, A_{i}^{(n)}\in \CF,
      m(A_{2}^{(n)}\setminus A_{1}^{(n)}=0, n=1$

      A:= $\bigcup_{n=1}^{\infty}A_{n}, A_{i}:=\bigcup_{n=1}^{\infty}A_{i}^{(n)}\in
      \CF, A_{1}\subseteq A \subseteq A_{2}, m(A_{2}\setminus A_{1}) \leq
      m(\bigcup_{n=1}^{\infty}(A_{2}^{(n)}\setminus A_{1}^{(n)}) \leq
      \sum_{n=1}^{\infty} m(A_{2}^{\infty}\setminus A_{1}^{(n)}=0$

      $\Rightarrow$ $A \in \CF^{m}$
    \end{enumerate}
  \end{proof}
\end{lemma}

\begin{defini}
  m(A)=m($A_{1}$) (=m($A_{2}$)), $A_{1}\subseteq A\subseteq A_{2}, A_{1},
  A_{2}\in \CF, m(A_2\setminus A_{1}) =0 A\in \CF^{m}$
\end{defini}

\begin{lemma}
  \begin{enumerate}[(i)]
   \item Definition von m auf $\CF^{m}$ korrekt
   \item m ist ein Maß auf $\CF^{m}$ (Fortsetzung von $m$ auf $\CF$)
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}[(i)]
     \item $A_{1}\subseteq A \subseteq A_{2}, A_{1}, A_{2}\in \CF,
      m(A_{2}\setminus A_{2})=0$
      $A_{1}'\subseteq A \subseteq A_{2}', A_{1}', A_{2}' \in \CF,
      m(A_{2}'\setminus A_{1}')=0$

      $\Rightarrow A_1\setminus A_{1}' \subseteq A_{2}'\setminus A_{1}'
      \Rightarrow m(A_{1}\setminus A_{1}') \leq m(A_{2}' \setminus
      A_{1}')=0$

      Es folgt $m(A_{1}) = \underbrace{m(A_{1}\setminus A_{1}')}_{=0} + m(A_{1}\cap
      A_{1}') = m(A_{1}\cap A_{1}')=\ldots=m(A_{1}')$
     \item
      \begin{enumerate}[(a)]
       \item m($\emptyset$) =0
       \item m($A_{n})\in \CF^{m}$ paarw. disjunkt, A :=
	$\bigcup_{n=1}^{\infty}A_{n}$ $\exists A_{1}^{(n)}\subseteq A_{n}
	\subseteq A_{2}^{(n)} : A_{1}^{(n)},A_{2}^{(n)}\in \CF,
	m(A_{2}^{(n)}\setminus A_{1}^{(n)})=0, n\geq 1$

	$ A_{i}:= \bigcup_{n=1}^{\infty}A_{i}^{(n)}\in \CF, A_{1}\subseteq A
	\subseteq A_{2}, m(A_{2}\setminus A_{1}) = 0$

	Es gilt: $m(A) =^{(\text{nach Definition})} m(A_{1}) = \sum_{n=1}^{\infty}
	m(A_{1}^{(n)} = \sum_{n=1}^{\infty} m(A_{n})$ (wegen
	$\sigma$-Additivität von $m$ auf $\CF$, $A_{1}^{(n)}, n\geq 1,$ paarw. disj.)
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{lemma}

\begin{defini}
  \begin{enumerate}[(i)]
   \item $\CF^{m}$ heißt Vervollständigung von $\CF$ bgzl. $m$
   \item $(\Omega, \CF^{m}, m)$ heißt Vervollständigung des Maßraumes
    $(\Omega, \CF, m)$
  \end{enumerate}
\end{defini}

\begin{defini}
  Es sei $(M, \mathfrak{M}, \mu)$ ein Maßraum. $(M, \mathfrak{M}, \mu)$ heißt vollständig,
  falls für alle $A\subseteq \Omega$, für die exist. eine Menge $B
  \in \mathfrak{M}$ mit $A\subseteq B$ und $\mu(B)=0$, stets folgt $A\in \mathfrak{M}$ (und
  $\mu(A)=0$ gilt)
\end{defini}

\begin{bemerk}
  $(\Omega, \CF^{m}, m)$ ist vollständig.
\end{bemerk}

\begin{satz}
  Sei $m$ ein Prämaß auf $(\Omega, \mathfrak{A})$, $\mathfrak{A}$ Algebra, $\CF:=\sigma(\mathfrak{A})$

  $\bar{m}$ auf $\bar{\CF}$ seien definiert wie im Beweis des
  Fortsetzungssatzes (insbesondere ist $\bar{m}$ ein Maß auf $\CF$)

  Dann gilt:
  \begin{enumerate}[(i)]
   \item $\CF^{\bar{m}} \subseteq \bar{\CF}$
   \item Sei $m$ endlich. Dann gilt $\CF^{\bar{m}}=\bar{\CF}$
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}[(i)]
     \item Sei $A\in \CF^{\bar{m}}. \Rightarrow \exists A_{1}, A_{2}\in \CF\colon
      A_{1}\subseteq A\subseteq A_{2}, \bar{m}(A_{2}\setminus A_{1})=0$

      $A=\underbrace{A_{1}}_{\in \CF\subseteq \bar{\CF}}\cup(A\setminus A_{1})$

      ggz. $A\setminus A_{1}\in \bar{\CF}$

      Es gilt: $A\setminus A_{1}\subseteq A_{2}\setminus A_{1}\in \CF,
      \bar{m}(A_{2}\setminus A_{1})=0$

      deshalb ggz.: $C\subseteq B, B \in \CF, \bar{m}(B)=0 \Rightarrow
      C\in \bar{\CF}$

      $m^{\ast}(C\cap E)+m^{\ast}(C^{\complement}\cap E) \leq
      \underbrace{m^{\ast}(B)}_{=\bar{m}(B)=0}+m^{\ast}(C^{\complement}\cap E) \leq
      m^{\ast}(E)$

      umgekehrte Ungleichung folgt aus der Subadditivität von $m^{\ast}$
      (außeres Maß sogar stark subadditiv)

      $\Rightarrow$ $C \in \bar{\CF}$
     \item $m$ sei endl., $A\in \bar{\CF}$ z.\,z.: $A\in \CF^{\bar{m}}$
      folgerung zum Fortsetzungssatz:
      \begin{gather}
	\sup_{B\subseteq A, B\in \mathfrak{A}_{\delta}} \bar{m}(B) = \bar{m}(A) =
	   \inf_{A\subseteq B, B\in \mathfrak{A}_{\sigma}} \bar{m}(B)
      \end{gather}

      Analog zum Beweis von Satz 4.1 folgt hieraus:
      \begin{gather}
	\exists A_{1}\in \mathfrak{A}_{\delta\sigma}, A_{2}\in \mathfrak{A}_{\sigma\delta}:
	   A_{1}\subseteq A \subseteq A_{2}, \bar{m}(A_{1}) = \bar{m}(A)
	   = \bar{m}(A_{2}) (\Rightarrow \bar{m}(A_{2}\setminus A_{1})=0)\\
	\mathfrak{A}_{\delta\sigma}, \mathfrak{A}_{\sigma\delta}\subseteq \CF \Rightarrow
	   A_{1}, A_{2}\in \CF\\
	\Rightarrow A \in \CF^{\bar{m}}
      \end{gather}
    \end{enumerate}
  \end{proof}
\end{satz}

\chapter{Konstruktion von Prämaßen}
$\Omega\ne\emptyset$

\begin{defini}
  $\mathfrak{C} \subseteq \mathfrak{P}(\Omega)$ Mengensystem, $\mathfrak{C}$ heißt dann eine
  Halbalgebra, falls gilt:
  \begin{enumerate}
   \item $\Omega\in \mathfrak{C}$
   \item $\mathfrak{C}$ ist ein $\pi$-System
   \item $A\in \mathfrak{C}$ $\Rightarrow$ $A^{\complement}=\bigcup_{i=1}^{n}A_{i},
    A_{1},\ldots,A_{n}\in \mathfrak{C}$ paarw. disj.
  \end{enumerate}
\end{defini}

\begin{bemerk}
  $\emptyset\in \mathfrak{C}$
\end{bemerk}

\begin{bsp}
  $\Omega=\R, \mathfrak{C}=\{[a,b)\cap \R: -\infty\leq a \leq b<+\infty\}$
  Halbalgebra
\end{bsp}

\begin{lemma}
  $\mathfrak{C}$ sei halbalgebra, $\mathfrak{A} := \alpha(\mathfrak{C})$ die von $\mathfrak{C}$ erzeugte Algebra

  Dann gilt: $A\in \mathfrak{A} \Leftrightarrow A = \bigcup_{i=1}^{n}A_{i},
  A_{1},\ldots,A_{n}\in \mathfrak{C}$ paarw. disj.
\end{lemma}

\begin{satz}
  Sei m eine nicht negative Mengenfunktion auf einer Halbarlgebra. $\mathfrak{C}$,
  $m(\emptyset)=0$, $m$ additiv

  \begin{enumerate}
   \item Es exist. genau ein Inhalt $m'$ (additive mengenfnk. mit
    $m(\emptyset)=0$) auf $\mathfrak{A} := \alpha(\mathfrak{C})$ mit $m'(A)=m(A)$, $A\in \mathfrak{C}$

    Es gilt ($\ast$): $m'(A)= \sum_{i=1}^{n}m(A_{i})$, $A\in \mathfrak{A}, A=\bigcup_{i=1}^{n}A_{i},
    A_{1},\ldots,A_{n}\in \mathfrak{C}$ paarw. disj.
   \item Falls $m$  $\sigma$-additiv auf $\mathfrak{C}$ ist, dann ist auch $m'$
    $\sigma$-additiv auf $\mathfrak{A}$ (und damit ein Prämaß).
  \end{enumerate}

  \begin{proof}
    \begin{enumerate}
     \item Definieren Mengenfunktion $m'$ gemäß ($\ast$) auf $\mathfrak{A}$

      Definition ist korrekt. $A=
      \bigcup_{i=1}^{m}A_{i}=\bigcup_{j=1}^{m}B_{m}, A_{1},\ldots,A_{n}\in \mathfrak{C}$
      paarw. dj. $B_{1},\ldots,B_{m}\in \mathfrak{C}$

      $\Rightarrow A_{i}=\bigcup_{j=1}^{m}\underbrace{(A_{i}\cap
      B_{j})}_{\in \mathfrak{C}} i=1,\ldots,n$

      $\sum_{i=1}^{n} m(A_{i})=\sum_{i=1}^{n}\sum_{j=1}^{m}m(A_{i}\cap
      B_{j})$ (Additiv. von $m$ auf $\mathfrak{C}$)
      $=\sum_{j=1}^{m}\underbrace{\sum_{i=1}^{n} m(A_{i}\cap
      B_{j})}_{m(B_{j})}=\sum_{j=1}^{m}m(B_{j})$

      $\bigcup_{i=1}^{n}\underbrace{(A_{i}\cap B_{j})}_{\in \mathfrak{C} \text{pw. dj}}=B_{j}$

     \item $m'$ ist additiv auf $\mathfrak{A}$
      $A^{(j)j}\in \mathfrak{A}, j=1,\ldots,m$ pw. dj. A:= $\bigcup_{j=1}^{m}A^{(j)}$

      z.\,z.: m'(A) = $\sum_{j=1}^{m}m'(A^{(j)})$

      Darstellung von $A^{(j)}, A: A^{(n)}=\bigcup_{i=1}^{n_{j}}A_{i}^{(j)},
      A_{1}^{(j)},\ldots,A_{n_{j}}^{(j)}\in \mathfrak{C}$ pw.\,dj.
      A=$\bigcup_{i=1}^{n}A_{i}, A_{1},\ldots,A_{n}\in \mathfrak{C}$ pw.\,dj.

      Es gilt: $\underbrace{A_{i}}_{\in \mathfrak{C}}=\bigcup_{j=1}^{m}\bigcup_{k=1}^{n_{j}}\underbrace{(A_{i}\cap
      A_{k}^{(j)})}_{\in \mathfrak{C}}$

      m'(A) := $\sum_{i=1}^{n} m(A_{i}) =(**)
      \sum_{i=1}^{n}\sum_{j=1}^{m}\sum_{k=1}^{n_{j}} m(A_{i}\cap
      A_{k}^{(j)})$ (Additivität von $m$ auf $\mathfrak{C}$)
      = $\sum_{j=1}^{m}\sum_{k=1}^{n_{j}}\sum_{i=1}^{n} m(A_{i}\cap
      A_{k}^{(j)}) = \sum_{j=1}^{m}\sum_{k=1}^{n_{j}} m(A_{k}^{(j)})$

      $\Rightarrow$ $m'(A)=\sum_{j=1}^{m} m'(A^{(j)})$
     \item $m'$ $\sigma$-additiv auf $\mathfrak{A}$, falls $m$ $\sigma$-additiv auf $\mathfrak{C}$

      Beweis analog für den Fall, dass der Index $m=+\infty$ ist.
    \end{enumerate}
  \end{proof}
\end{satz}

