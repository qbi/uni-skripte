  Eigenschaften:
  \begin{enumerate}[E1]
   \item $f=1_{A}, A\in \CF \Rightarrow \int_{\Omega}fdm = m(A)$
   \item (Positivität) %\CFehlt was
   \item (Linearität)
    $f,g$ nichtnegative messbare Funktionen, $a,b\in\R$ und $a,b\geq0$

    $\Rightarrow$ $af+bg$ nichtnegative messbare Funktion und es gilt:
    $\int_{\Omega}(af+bg)dm = a\int_{\Omega}f dm + b\int_{\Omega}g dm$

    \begin{proof}
      $(f_{n})$ und $(g_{n})$ Folgen von nichtnegativen einfachen
      Funktionen, $f_{n}\uparrow f$ und $g_{n}\uparrow g$

      $\Rightarrow$ $af_{n}+bg_{n}$ nichtneg. einfach,
      $af_{n}+bg_{n}\uparrow af+bg$\\
      $\Rightarrow \int_{\Omega}(af+bg)\mathfrak{m}:=
      \lim_{n\rightarrow\infty}\int_{\Omega} (af_{n}+bg_{n})dm
      =^{E1.4.}\lim_{n\rightarrow\infty}
      (a\int_{\Omega}f_{n}\mathfrak{m}+b\int_{\Omega} g_{n}\mathfrak{m})\\
      = a\int_{\Omega}f \mathfrak{m}+b\int_{\Omega} gdm$
    \end{proof}
   \item (monotonie) f,g nichtnegativ messbar, f$\leq$g $\Rightarrow$
      $\int_{\Omega}fdm \leq \int_{\Omega}g \mathfrak{m}$

      \begin{proof}
	Anwendung von Satz~2: $f_{n}\uparrow f, g_{n}\uparrow g, f_{n}, g_{n}$ nicht
	negativ und einfach $\Rightarrow$ $\lim_{n\rightarrow\infty}
	f_{n} \leq\lim_{n\rightarrow\infty} g_{n} \Rightarrow^{Satz
	2}\lim_{n\rightarrow\infty}\int f_{n}dm
	\leq\lim_{n\rightarrow\infty} \int g_{n}\mathfrak{m} \Rightarrow
	\int_{\Omega}fdm \leq \int_{\Omega}gdm$
      \end{proof}
   \item f $\geq$ 0 messbar

    $\int_{\Omega}$ fdm = 0 $\Leftrightarrow$ $f=0$ $m$-fast überall
    ($m(\{f>0\})=0$)

    \begin{proof}
      \begin{enumerate}
       \item "`$\Rightarrow$"' (Kontraposition)
	Ann.: m(\{f>0\})>0

	Sei ($f_{n}$) Folge nichtneg. einfacher Funktionen, $f_{n}\uparrow f$
	0<m(\{f>0\})=m($\bigcup_{n=1}^{\infty}\{f_{n}>0\}) =
	\lim_{n\rightarrow\infty} m(\{f_{n}> 0\}) \Rightarrow \exists n:
	m(\{f_{n}>0\}) > 0 \Rightarrow^{E1.6} \int_{\Omega} f_{n} \mathfrak{m} > 0
	\Rightarrow \int_{\Omega} fdm \geq \int_{\Omega}f_{n}\mathfrak{m}>0$
       \item "`$\Leftarrow$"'

	Es gelte: $f=0$ $m$-fast überall
	Sei ($f_{n}$) Folge nichtneg. einfacher Funktionen,
	$f_{n}\uparrow f\Rightarrow f_{n}=0$ $m$-fast überall
	$\Rightarrow^{E1.6}\int_{\Omega} f_{n}\mathfrak{m} =0,\forall n \Rightarrow
	\int_{\Omega} fdm := \lim_{n\rightarrow\infty} \int_{\Omega}
	f_{n}\mathfrak{m} =0$
      \end{enumerate}
    \end{proof}
   \item f$\geq$0 messbar
    \begin{gather}
      \int_{\Omega}fdm < \infty \Rightarrow f<\infty \text{m-fast
	 überall} (d.\,h. m(\{f=\infty\})=0)
    \end{gather}

    \begin{proof}
       f$\geq \infty 1_{\{f=\infty\}} \Rightarrow^{E4}
      \infty>\int_{\Omega} fdm \geq \int_{\Omega} \infty
      1_{\{f=\infty\}}\mathfrak{m} = \infty m(\{f=\infty\})$
    \end{proof}
  \end{enumerate}

  \section{Integrierbare Funktion}
  $(\Omega, \mathcal{\CF}, m)$ beliebieger Maßraum, f sei messbare Funktion auf
  $(\Omega, \mathcal{\CF})$, f=$f^{+}-f^{-}$
  $f^{+}=\max\{f,0\} positiver Teil von f, f^{-}=\max\{0,-f\} negativer
  Teil$ $f^{+},f^{-} nichtnegative messbare Funktionen$

  \begin{defini}
    \begin{enumerate}[(i)]
     \item Wir sagen, dass das Integral $\int_{\Omega}fdm$
      \emph{existiert}, falls $\int_{\Omega} f^{+}<\infty \vee
      \int_{\Omega} f^{-}<\infty$.

      Wir setzen:
      \begin{gather}
	\int_{\Omega} fdm := \int_{\Omega}f^{+}\mathfrak{m} - \int_{\Omega}f^{-}dm
      \end{gather}
     \item f heiß't integrierbar, falls f messbar ist und
      $\int_{\Omega}fdm$ existiert sowie endlich ist ($\Leftrightarrow$
      $\int_{\Omega}f^{+}\mathfrak{m}, \int_{\Omega}f^{-}\mathfrak{m} < \infty$
    \end{enumerate}
  \end{defini}

  Eigenschaften:
  \begin{enumerate}[E1]
   \item f=$1_{A}, A\in \mathcal{\CF} \Rightarrow \int_{\Omega} fdm = m(A)$
   \item Positivität f$\geq$0 messbar $\Rightarrow$ $\int_{\Omega}fdm
    \geq 0$
   \item Lineariotät $f,g$ messbare Funktionen, $\int_{\Omega} f\,dm,
    \int_{\Omega} g\,dm$ exitsieren, $a,b \in\R$
    \begin{gather}
      a\int_{\Omega}fdm+ b\int_{\Omega}g\,dm
    \end{gather} sinnvoll nach Rechenregeln mit $\infty$

    Dann gilt: $\int_{\Omega}(af+bg)\mathfrak{m}$ exitsiert und es gilt:
    $\int_{\Omega}(af+bg)\mathfrak{m} =\int_{\Omega}fdm+b\int_{\Omega}gdm$
  \end{enumerate}

  % \CFehlt was!

  Berechnung: $\int_{\Omega}(f+g)^{+}\mathfrak{m} = \int_{\Omega} (f+g)\cdot
  1_{\{f+g\geq 0\}}\mathfrak{m} = \int_{\Omega} (f^{+}-f^{-}+g^{+}-g^{-})\cdot
  1_{\{f+g\geq0\}} \mathfrak{m} = \int_{\Omega}\underbrace{[(f^{+}+g^{+})
  1_{\{f+g\geq0\}} - (f^{-}+g^{-})1^{\{f+g\geq 0\}}]}_{\geq0}\mathfrak{m}$

  Zwischenschritt: $\int_{\Omega} (f^{-}+g^{-}) 1_{\{f+g\geq0\}}\mathfrak{m} <+\infty$
  \begin{enumerate}[1. Fall]
   \item Sei (3) erfüllt $\Rightarrow$ $\int_{\Omega}
    (f^{-}+g^{-})1_{\{f+g\geq0\}}\mathfrak{m} \leq_{Monotonie} \int_{\Omega}
    (f^{-}+g^{-})\mathfrak{m} < \infty$
   \item Sei (2) erfüllt
    \begin{gather}
      \int_{\Omega} (f^{-}+g^{-})1_{\{f+g\geq0\}} \mathfrak{m} \leq^{Monotonie} \int_{\Omega}
	 (f^{+}+g^{+})1_{\{f+g\geq0\}}\mathfrak{m} \leq \int_{\Omega}
	 (f^{+}+g^{+})\mathfrak{m} < +\infty
    \end{gather}
  \end{enumerate}

  Anwendung von II:
  $\Rightarrow \int_{\Omega}[f+g]^{+}\mathfrak{m} =
  \int_{\Omega}(f^{+}+g^{+})1_{\{f+g\geq0\}}\mathfrak{m} -
  \int_{\Omega}(f^{-}+g^{-})1_{\{f+g\geq0\}} \mathfrak{m} =
  \int_{\Omega}f^+1_{\{f+g\geq0\}} \mathfrak{m}+ \int_{\Omega}
  g^{+}1_{\{f+g\geq0\}} \mathfrak{m} - \int_{\Omega} f^{-}1_{\{f+g\geq0\}}\mathfrak{m} -
  \int_{\Omega} g^{-}1_{\{f+g\geq0\}}\mathfrak{m}$

  Analog: $\int_{\Omega}(f+g)^{-}\mathfrak{m} = \int_{\Omega}
  (-f-g)1_{\{f+g\geq0\}}\mathfrak{m} = \ldots =
  \int_{\Omega}f^{-}1_{\{f+g\geq0\}}dm
  +\int_{\Omega}g^{-}1_{\{f+g\geq0\}}dm
  -\int_{\Omega}f^{+}1_{\{f+g\geq0\}}\mathfrak{m}- \int_{\Omega}g^{+}1_{\{f+g\geq0\}}\mathfrak{m}$

  Es folgt aus Alternative (2) oder (3)
  \begin{gather}
    \int_{\Omega}(f+g)^{+}\mathfrak{m}<+\infty \text{oder} \int_{\Omega}
       (f+g)^{-}\mathfrak{m} < +\infty
  \end{gather}
  Daraus folgt:
  \begin{gather}
    \exists\int_{\Omega} (f+g)\mathfrak{m} \text{und}\\ \int_{\Omega}(f+g)\mathfrak{m} :=
       \int_{\Omega}(f+g)^{+}\mathfrak{m} - \int_{\Omega}(f+g)^{-} \mathfrak{m} =
       \int_{\Omega}f^{+}\mathfrak{m} + \int_{\Omega} g^{+}\mathfrak{m} -\int_{\Omega}f^{-}dm
       -\int_{\Omega}g^{-}\mathfrak{m} = \int_{\Omega}f\,\mathfrak{m}+\int_{\Omega} g\,dm
  \end{gather}

  E4 (Monotonie)
  f,g messbar, f$\leq$g, $\int_{\Omega}f\,\mathfrak{m}, \int_{\Omega}g\,\mathfrak{m}$
  existieren.

  Dann folgt:
  \begin{gather}
    \int_{\Omega} f\,dm\leq\int_{\Omega} g\,dm
  \end{gather}

  \begin{proof}
    o.\,B.\,d.\,A.:$\int_{\Omega}f\,\mathfrak{m}>-\infty, \int_{\Omega} g\,\mathfrak{m}<+\infty$

    $\Rightarrow$ m(\{f=-$\infty$\}) = m(\{$f^{-}=+\infty\})=0$ (da
    $\int_{\Omega}f^{-}\mathfrak{m}<+\infty)$

    m(\{g=+$\infty$\})=m(\{$g^{+}=+\infty\})=0$

    Setzen:
    \begin{gather*}
      h=\begin{cases}
		g-f& auf \{-\infty<f,g<+\infty\}\\
		0& sonst
	      \end{cases}, h\geq0
    \end{gather*}

    $0\leq_{Positiv.} \int_{\Omega}h\,\mathfrak{m} =_{Linearität} \int_{\Omega}
    g\,1^{\{-\infty<f,g<+\infty\}}\mathfrak{m}-\int_{\Omega}f\,1^{\{-\infty<f,g<+\infty\}}dm
    =\int_{\Omega}g\,\mathfrak{m} -\int_{\Omega} f\,\mathfrak{m} $
  \end{proof}

  E5 $f,g$ integrierbar, $f\leq g$

  \begin{gather}
    \int_{\Omega} f\,\mathfrak{m} = \int_{\Omega}g\,\mathfrak{m} \Leftrightarrow f=g\quad
       \text{$m$-fast überall}
  \end{gather}

  \begin{proof}
    h sei wie in Beiweis von E4 definiert

    \begin{align}
      \int_{\Omega}h\,\mathfrak{m} = \int_{\Omega}g\,\mathfrak{m}-\int_{\Omega}f\,\mathfrak{m} =0
	 &\Leftrightarrow h=0 \text{~$m$-fast überall} (E2.5)\\
	 &\Leftrightarrow f=g \text{~$m$-fast überall}
    \end{align}
  \end{proof}

  E6 f messbar

  \begin{gather}
    f \text{integrierbar} \Leftrightarrow \int_{\Omega}|f|\,\mathfrak{m}<+\infty
  \end{gather}

  \begin{proof}
    \begin{gather}
      \int_{\Omega}|f|\,\mathfrak{m}=\int_{\Omega}(f^{+}+f^{-})\mathfrak{m} =
	 \int_{\Omega}f^{+}\mathfrak{m}+\int_{\Omega} f^{-}dm
    \end{gather}
  \end{proof}

  E7 f messbar

  f integrierbar $\Rightarrow$ |f| <$+\infty$ $m$-fast überall

  \begin{proof}
    \begin{gather}
      \int_{\Omega}|f|\mathfrak{m}<+\infty \Rightarrow^{E2.6} |f|<+\infty
         \text{~$m$-fast überall}
    \end{gather}
  \end{proof}

  E8 f messbar, f=0 $m$-fast überall

  $\Rightarrow \exists\int_{\Omega} f\,\mathfrak{m}$ und $\int_{\Omega}f\,\mathfrak{m} = 0$

  \begin{gather}
    f=f^{+}+f^{-} \Rightarrow f^{+}=0 \text{~$m$-fast überall}, f^{-}=0 \text{~$m$-fast überall}\\
    \Rightarrow \int_{\Omega} f^{+}\,\mathfrak{m}=0, \int_{\Omega}f^{-}\,\mathfrak{m}=0 (\text{nach
       E2.5})\\
    \Rightarrow \int_{\Omega}f\,\mathfrak{m} =\int_{\Omega}f^{+}\,\mathfrak{m} -\int_{\Omega}
       f^{-}\,\mathfrak{m} =0
  \end{gather}

  \begin{folgerung}
    $A\in \mathcal{\CF}$, $\mathcal{m}(A^{\complement}) = 0$, $\exists\int_{\Omega}f\,\mathfrak{m}$

    $\Rightarrow$ $\int_{\Omega}f\cdot 1^{A}\,\mathfrak{m}=\int_{\Omega}f\,\mathfrak{m}$

    \begin{proof}
      $\int_{\Omega}f\cdot1_{A}\,\mathfrak{m}=\int_{\Omega}f\,\mathfrak{m}-\overbrace{\int_{\Omega}
      \underbrace{f\cdot1_{A^{\complement}}}_{=0}\,\mathfrak{m}}^{=0 (E8)}$
    \end{proof}
  \end{folgerung}

  \section{Stetigkeitseigenschaften der Integrals}

  Riemann-Integral: Stetigkeit bzw. gleichmäßiger Konvergenz der
  Integranden

  $(\Omega,\mathcal{\CF},\mathcal{m})$ beliebiger Maßraum

  \begin{satz}[Satz von B.~Levi über monotone Konvergenz]
    Sei ($f_{n})$ eine wachsende Folge nichtnegativer messbarer
    Funktionen, f=$\lim_{n\rightarrow\infty}f_{n}$

    Dann gilt:
    \begin{gather}
      \lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,\mathfrak{m} =\int_{\Omega}f\,dm
    \end{gather}

    \begin{proof}
      \begin{enumerate}
       \item "`$f_{n}\leq f$"' $\Rightarrow^{Monotonie}
	\int_{\Omega}f_{n}\,\mathfrak{m} \leq \int_{\Omega}f\,\mathfrak{m} \Rightarrow
	\lim_{n\rightarrow\infty}\int_{\Omega} f_{n}\,\mathfrak{m} \leq\int_{\Omega}f\,\mathfrak{m}$
       \item "`$f_{n}\geq f$"'
	$\exists \lim_{m\rightarrow\infty}f_{n,m}\uparrow f_{n}, f_{n,m}$
	nichtnegativ einfach

	g:=$\max_{1\leq n\leq m}f_{n,m}$ einfache messbare Funktion
	\begin{enumerate}
	 \item $g_{m}$ einfach
	 \item $f_{n,m} \leq g_{m} \leq f_{m}, 1\leq n\leq m$
	 \item $g_{m} \leq g_{m+1}, \forall m\geq1$
	 \item $\lim_{m\rightarrow\infty} g_{m}=f$ (da über (b) eingeschachtelt)
	\end{enumerate}

	$\Rightarrow\int_{\Omega}f\,\mathfrak{m} :=
	\lim_{m\rightarrow\infty}\int_{\Omega} \underbrace{g_{m}}_{\leq
	f_{m}}\,\mathfrak{m} \leq \lim_{m\rightarrow\infty}\int_{\Omega}f_{m}\,\mathfrak{m}$
      \end{enumerate}
    \end{proof}
  \end{satz}

  \begin{folgerung}
    Es sein ($f_{n})$ eine monoton wachsende Folge messbarer Funktionen,
    $f=\lim_{n\rightarrow\infty} f_{n}$. Es gelte:
    \begin{enumerate}
     \item g $\leq$ $f_{n}$ $m$-fast überall
     \item $\int_{\Omega} g^{-}\mathfrak{m}<+\infty$
    \end{enumerate}

    Dann folgt:
    \begin{gather}
      \int_{\Omega}f\,\mathfrak{m}=\lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm
    \end{gather}

    \begin{proof}
      o.\,B.\,d.\,A. $g^{+}=0$ $h_{n}:=(f_{n}-g)\cdot 1_{\{-\infty<g\leq f_{n}\}}
      \geq0, \lim_{n\rightarrow\infty} h_{n}=(f-g)\cdot 1_{\{-\infty<g\leq f_{n}\}}$

      $\Rightarrow^{B Levi}$
      \begin{align}
	\lim_{n\rightarrow\infty}\int_{\Omega} h_{n}\,dm
	   &=\int_{\Omega}(f-g)\cdot 1_{\{-\infty<g\leq f_{n}\}}\,dm\\
	&=\int_{\Omega}(f-g)\,\mathfrak{m}=\int_{\Omega}f\,\mathfrak{m}-\int_{\Omega}g\,dm\\
	=\lim_{n\rightarrow\infty}\int_{\Omega}(f_{n}-g)\,\mathfrak{m} &=
	    \lim_{n\rightarrow\infty}(\int_{\Omega}f_{n}\,\mathfrak{m}-\int_{\Omega}g\,\mathfrak{m})
      \end{align}

      $\rightarrow$
    \end{proof}
  \end{folgerung}

  \begin{satz}[Lemma von Faton]
    Sei ($f_{n}$) eine Folge \emph{nichtnegativer} messbarer Funktionen.

    Dann folgt:
    \begin{gather}
      \int_{\Omega} \liminf_{n\rightarrow\infty} f_{n}\,\mathfrak{m} \leq
	 \liminf_{n\rightarrow\infty} \int_{\Omega}f_{n}\,dm
    \end{gather}

    \begin{proof}
      \begin{align}
	\int_{\Omega}\liminf_{n\rightarrow\infty} f_{n}\,dm
	   &=\int_{\Omega}\lim_{n\rightarrow\infty} \inf_{k\geq n} f_{k} =
	   \lim_{n\rightarrow\infty}\int_{\Omega}\int_{k\geq n} f_{k}\,dm
	   (B.\,Levi)\\
	[\int_{\Omega}\inf_{k\geq n} f_{k}\,
	   dm\leq^{Monotonie} \int_{\Omega}f_{j}\,\mathfrak{m},\forall j\geq n
	   (\inf_{k\geq n} f_{k} \leq f_{j}, j\geq n)]\\
	&\leq\lim_{n\rightarrow\infty}\inf_{j\geq
	   n}\int_{\Omega}f_{j}\,\mathfrak{m} =
	   \liminf_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm
      \end{align}
    \end{proof}
  \end{satz}

  \begin{folgerung} % Folgerugn 1
    ($f_{n}$) Folge messbarer Funktionen mit:
    \begin{enumerate}
     \item g $\leq$ $f_{n}$ $m$-fast überall für eine messbare Funktion $g$
     \item $\int_{\Omega}g^{-}\,\mathfrak{m}<+\infty$
    \end{enumerate}
  \end{folgerung}

  Dann folgt: Es existieren $\int_{\Omega}f_{n}\,\mathfrak{m}$ und
  $\int_{\Omega}\liminf f_{n}\,\mathfrak{m}$ und es gilt:
  \begin{gather}
    \int_{\Omega}\liminf_{n\rightarrow\infty} f_{n}\,\mathfrak{m} \leq
       \liminf_{n\rightarrow\infty} \int_{\Omega} f_{n}\,dm
  \end{gather}

  \begin{proof}
    \begin{enumerate}
     \item g $\leq$ $f_{n}$ $m$-fast überall $\Rightarrow$ $f^{-}_{n}\leq g^{-}$
      $m$-fast überall $\Rightarrow$ $\int_{\Omega}f^{-}_{n}\,dm\leq+\infty$,
      ($\liminf_{n\rightarrow\infty} f_{n})^{-}\leq g^{-} $ $\Rightarrow$
      $\int_{\Omega}(\liminf_{n\rightarrow\infty}f_{n})^{-}\,\mathfrak{m}<\infty$
     \item A=$\bigcap_{n=1}^{\infty}\{-\infty<g\leq f_{n}\}, m(A^{\complement})=0$
      $h_{n}=(f_{n}-g)\cdot 1_{A} \Rightarrow h_{n}\geq 0$ messbar
      $\Rightarrow$ $\int_{\Omega}\underbrace{\liminf_{n\rightarrow\infty} h_{n}}_{=
      (\liminf f_{n}-g)\cdot1_{A}}\,dm
      \leq\liminf_{n\rightarrow\infty}\int_{\Omega}h_{n}\,dm
      = \int_{\Omega}\liminf_{n\rightarrow\infty}\,\mathfrak{m} -\int_{\Omega}g\,dm
      \leq\liminf_{n\rightarrow\infty}(\int_{\Omega}f_{n}\,dm
      -\int_{\Omega}g\,gm)$
    \end{enumerate}
  \end{proof}

  \begin{folgerung}
    Sei ($f_{n})$ Folge messbarer Funktionen mit den Eigenschaften:
    \begin{enumerate}
     \item $f_{n}\leq g$ $m$-fast überall, n$\geq$1, g messbar
     \item $\int_{\Omega}g^{+}\,\mathfrak{m}<\infty$
    \end{enumerate}

    Dann existieren: $\int_{\Omega}f_{n}\,\mathfrak{m}$ und
    $\int_{\Omega}\limsup_{n\rightarrow\infty}f_{n}\,\mathfrak{m}$ und es gilt:
    \begin{gather}
      \limsup_{n\rightarrow\infty}\int_{\Omega}f_{n}\,\mathfrak{m} \leq
	 \int_{\Omega}\limsup_{n\rightarrow\infty} f_{n}\,dm
    \end{gather}

    \begin{proof}
      Anwendung von Folgerung 1 für die Folge ($g_{n})$ mit $g_{n}=-f_{n}$
      $\limsup_{n\rightarrow\infty}f_{n}=-\liminf_{n\rightarrow\infty}g_{n}$
    \end{proof}
  \end{folgerung}

  \subsection{Konvergenz $m$-fast überall}
  $(f_{n})$ Folge messbarer Funktionen,
  $\Omega_{0}:=\{\omega\in\Omega:\exists\lim_{n\rightarrow\infty}
  f(\omega) (in \hat{\R})\}\in \mathcal{\CF}$

  ($\lim_{n\rightarrow\infty} f_{n})(\omega) = \{ %\}
  \begin{cases}
    \lim_{n\rightarrow\infty}f_{n}(\omega)&\omega\in\Omega_{0}\\
    0&\omega\not\in\Omega_{0}
  \end{cases}$
  $\lim_{n\rightarrow\infty}f_{n}$ messbar auf $(\Omega,\mathcal{\CF})$

  \begin{defini}
    ($f_{n}$) heißt $m$-fast überall konvergent, falls m($\Omega_{0}^{\complement})=0$
  \end{defini}

  \begin{satz}[Satz von Lebesgue über majorisierte Konvergenz] %satz 3
    Sei ($f_{n})$ einen Folge messbarer Funktionen auf $(\Omega,\mathcal{\CF})$ mit
    folgenden Eigenschaften:
    \begin{enumerate}
     \item ($f_{n})$ konvergiert $m$-fast überall
     \item Es existiert eine messbare Funktion g auf $(\Omega,\mathcal{\CF})$ mit
      \begin{enumerate}
       \item |$f_{n}|\leq g$ $m$-fast überall, n$\geq$1
       \item $\int_{\Omega}g\,\mathfrak{m}<\infty$
      \end{enumerate}
    \end{enumerate}

    Dann folgt:
    \begin{enumerate}
     \item $f_{n}$ ist integrierbar, n$\geq$1
     \item $\exists\lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,\mathfrak{m}$
     \item $\lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,\mathfrak{m} =
      \int_{\Omega}\lim_{n\rightarrow\infty}f_{n}\,\mathfrak{m}$
    \end{enumerate}

    \begin{proof}
      -g$\leq f_{n}\leq g$ $m$-fast überall, g,-g sind integrierbar
      $\rightarrow$ $\limsup_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm
      \leq^{Farton} \int_{\Omega}\limsup_{n\rightarrow\infty}f_{n}\,\mathfrak{m} =
      \lim_{n\rightarrow\infty}f_{n}\,\mathfrak{m}$ (wegen der $m$-fast überall Konvergenz
      von ($f_{n})$) = $\int_{\Omega}\liminf_{n\rightarrow\infty}
      f_{n}\,\mathfrak{m} \leq^{Faton} \liminf_{n\rightarrow\infty}\int_{\Omega}
      f_{n}\,\mathfrak{m} \leq\limsup_{n\rightarrow\infty}\int_{\Omega}f_{n}\,\mathfrak{m}$
    \end{proof}
  \end{satz}

  \chapter{Das Lebesgue-Integral}
  $(\R, \mathcal{B}^{\lambda}(\R), \lambda)$, Maßraum, $\lambda$ Lebesgue-Maß,
  $\mathcal{B}(\R)$ $\sigma$-Algebra der Lebesgue-messbaren Mengen
  ($\mathcal{B}^{\lambda}(\R)$ ist die Vervollständigung von $\mathcal{B}(\R)$ bzgl.
  $\lambda$)

  \begin{defini}
    $\int_{\R}f\,d\lambda$ bzw. $\int_{\R}f(x)\,dx$ heißt
    \emph{Lebesgue-Integral} von $f$ (sofern das Integral existiert),
    $\int_{A}f\,d\lambda =\int_{\R}f\cdot1_{A}\,d\lambda, A\in
    \mathcal{B}^{\lambda}(\R)$
  \end{defini}

  \begin{lemma}
    $f\colon\R\rightarrow \hat{\R}$

    $f$ ist \emph{Lebesgue-messbar} $\Leftrightarrow$ Es existierten
    Borel-messbare Funktionen $f_{1},f_{2}$ mit $f_{1}\leq f\leq f_{2}$
    und $\lambda(\{f_{1}\not=f_{2}\})=0$

    (Übungsaufgabe)
  \end{lemma}

  Zielstellung: Vergleichen von Lebesgue-Integral und Riemann-Integral

  \begin{defini}
    $f\colon\R\rightarrow\hat{\R}$, $f$ heißt \emph{$\lambda$-f.ü. stetig} auf
    $[a,b]$, falls eine Menge $N\subseteq[a,b]$ existiert mit $N \in
    \mathcal{B}^{\lambda}(\R), \lambda(N)=0$ und $f$ ist stetig in allen Punkten
    $x\in[a,b]\cap N^{\complement}$
  \end{defini}

  \begin{satz}
    $f\colon\R\rightarrow \hat{\R}$ beschränk, $|f|\leq c$, $c>0$,
    $[a,b]\subseteq\R$ endliches Intervall

    $f$ Riemann-integrierbar auf $[a,b]$ $\Leftrightarrow$ $f$ ist
    $\lambda$-f.ü. stetig auf $[a,b]$

    Es gilt dann:
    \begin{enumerate}
     \item $f$ ist Lebesgue-integrierbar
     \item $\int_{[a,b]}f\,d\lambda = R-\int^{b}_{a}f(x)\,dx$
    \end{enumerate}

    \begin{proof}
      $f\colon\R\rightarrow \hat{\R}$ beschränkt $|f|\leq c$
      o.\,B.\,d.\,A. $f(x)=0, x\not\in[a,b], bzw. f=f\cdot 1_{[a,b]}$

      $fZ_{n}=\{t_{0}^{(n)},\ldots,t_{m_{n}}^{(n)}\}$ Zerlegung von
      $[a,b], a=t_{0}^{(n)}<\ldots<t_{m_{n}}^{n}=b$

      $fZ_{n+1}$ sei feiner als $fZ_{n}$

      Darbouxsche Summen:
      \begin{gather}
	I_{k}^{(n)} :=
	   \begin{cases}
	     [t_{k}^{(n)}, t_{k+1}^{(n)})& k=0,\ldots,m_{n}-2\\
	     [t_{k}^{(n)}, t_{k+1}^{(n)}& k=m_{n}-1
	   \end{cases}
      \end{gather}

      $\underline{S}_{n}:=\sum_{k=0}^{m_{n}-1}\inf_{y\in I_{k}^{(n)}} f(y)
      (t_{k+1}^{(n)}- (t_{k}^{(n)})$
      $\overline{S}_{n}:=\sum_{k=0}^{m_{n}-1}\sup_{y\in I_{k}^{(n)}} f(y)
      (t_{k+1}^{(n)}- (t_{k}^{(n)})$
    \end{proof}
  \end{satz}

