\subsection{Produkte von Wahrscheinlichkeitsmaßen}

$(\Omega_{1}, \mathcal{\CF}_{1}, P_{1})$ und $(\Omega_{2}, \mathcal{\CF}_{2}, P_{2})$
Wahrscheinlichkeitsräume

$\Omega = \Omega_{1}\times\Omega_{2}, \mathcal{\CF} = \mathcal{\CF}_{1}\otimes \mathcal{\CF}_{2},
\omega=(\omega_{1}, \omega_{2}) \in \Omega$

Projektionsabbildungen
$X_{1}(\omega) = \omega_{1}, X_{2}(\omega)=\omega_{2}$

\begin{lemma} %3
  $X_{i}:(\Omega,\mathcal{\CF})\rightarrow(\Omega_{i},\mathcal{\CF}_{i})$ messbar, (i=1,2)

  \begin{proof}
    \begin{description}
     \item[i=1] $B\in \mathcal{\CF}_{1}$: $X_{1}^{-1}(B)=B\times\Omega_{2}\in
      \mathcal{\CF}_{1}\times \mathcal{\CF}_{2}\subseteq \mathcal{\CF}$

      $p$ sei Wahrscheinlichkeitsmaß auf $(\Omega, \mathcal{\CF})$ $\Rightarrow$ $X_{i}: (\Omega,
      \mathcal{\CF}, P) \rightarrow (\Omega_{i}, \mathcal{\CF}_{i})$ zufällige Variable,
      $P_{X_{i}}:$ Verteilung von X bzgl. P (Bildmaß)
    \end{description}
  \end{proof}
\end{lemma}

\begin{satz}
  \begin{enumerate}
   \item $P_{1}\otimes P_{2}$ ist ein Wahrscheinlichkeitsmaß auf $(\Omega, \mathcal{\CF})$
   \item Es sei $P$ irgendein Wahrscheinlichkeitsmaß auf  $(\Omega, \mathcal{\CF})$ mit $P_{X_{i}} =
    P_{i} (i=1,2)$. Dann sind folgende Aussagen äquivalent:
    \begin{enumerate}
     \item \begin{gather}P = P_{1}\otimes P_{2}\end{gather}
     \item\label{BBB} $X_{1}$ und $X_{2}$ sind unabhängig bzgl. $P$
     \item\label{CCC} $X_{1}^{-1}(\CF_{1})$ und $X_{2}^{-1}(\mathcal{\CF}_{2})$ unabhängig bzgl.
      $P$
    \end{enumerate}
  \end{enumerate}

  \begin{proof}
    \begin{description}
     \item[(\ref{BBB}) $\Leftrightarrow$ (\ref{CCC}):] klar
     \item[(a) $\Leftrightarrow$ (b)]
      \begin{align}
	P = P_{1}\otimes P_{2} &\Leftrightarrow^{S.2} P(A_{1}\times A_{2}) =
	   P_{1}(A_{1}) P_{2}(A_{2}), \forall A_{1}\in \mathcal{\CF}_{1}, A_{2}\in
	   \mathcal{\CF}_{2}\\
	&\Leftrightarrow P(\underbrace{\{X_{1}\in
	   A_{1}\}}_{=A_{1}\times\Omega_{2}} \cap \underbrace{\{X_{2}\in
	   A_{2}\}}_{=\Omega_{1}\times A_{2}}) =
	   P_{X_{1}}(A_{1}) P_{X_{2}}(A_{2}) = P(\{X_{1}\in A_{1}\})
	   P(\{X_{2}\in A_{2}\})\\
	&\Leftrightarrow X_{1}, X_{2} unabh. bzgl. P
      \end{align}
    \end{description}
  \end{proof}
\end{satz}

\begin{bemerk}
  \ref{BBB} bedeutet:
  \begin{gather}
    P(\{X_{1}\in A_{1}\}\cap\{X_{2}\in A_{2}\}) = P(\{X_{1}\in
       A_{1}\})\cdot P (\{X_{2}\in A_{2}\})\quad \forall A_{1}\in \mathcal{\CF}_{1},
       A_{2}\in \mathcal{\CF}_{2}
  \end{gather}

  \ref{CCC} bedeutet:
  \begin{gather}
    P(B_{1}\cap B_{2}) = P(B_{1})\cdot P(B_{2})\quad \forall B_{1}\in
       X_{1}^{-1}(\mathcal{\CF}_{1}) (\Leftrightarrow B_{1}=\{X_{1}\in A_{1}\},
       A_{1}\in \mathcal{\CF}_{1}), B_{2}\in X_{2}^{-1}(\mathcal{\CF}_{2}) (\Leftrightarrow
       B_{1}=\{X_{2}\in A_{2}\}, A_{2}\in \mathcal{\CF}_{2})
  \end{gather}
\end{bemerk}

\subsection{Schnittfunktion}

\begin{gather}
  f\colon\Omega = \Omega_{1}\times\Omega_{2} \rightarrow \hat{\R}
\end{gather}

\begin{defini} % 4
  $x\in \Omega_{1}, f_{x}(y) = f(x,y), y\in \Omega_{2}, f_{x}$ heißt
  \emph{Schnitt} von f in $x\in \Omega_{1}$

  $y\in \Omega_{2}, f^{y}(x) = f(x,y), x\in\Omega_{1}, f^{y}$ heißt
  \emph{Schnitt} von f in $y\in \Omega_{2}$
\end{defini}

\begin{lemma} % 4
  Sei $f:(\Omega, \mathcal{\CF}) = (\Omega_{1}\times \Omega_{2}, \mathcal{\CF}_{1}\otimes
  \mathcal{\CF}_{2}) \rightarrow (\hat{\R}, \mathcal{B}(\hat{\R}))$ messbar

  Dann  ist $f_{x}: (\Omega_{2}, \mathcal{\CF}_{2}) \rightarrow (\hat{\R},
  \mathcal{B}(\hat{\R}))$ sowie  $f^{y}:(\Omega_{1}, \mathcal{\CF}_{1}) \rightarrow
  (\hat{\R}, \mathcal{B}(\hat{\R}))$ messbar

  \begin{proof}
    \begin{gather}
      A=\{(x,y)\in \Omega: f(x,y) < r\}, r\in \R \Rightarrow A\in
	 \mathcal{\CF}_{1}\otimes \mathcal{\CF}_{2}\\
      \Rightarrow^{Lemma 2} A_{x}\in \mathcal{\CF}_{2}, A^{y}\in \mathcal{\CF}_{1}, \text{es
	 gilt: } A_{x}=\{y\in\Omega_{2}: f_{x}(y) < r\} =
	 f^{-1}_{x}([-\infty, r))\in \mathcal{\CF}_{2}\forall r\in \R \Rightarrow
	 f_{x} messbar
    \end{gather}

    analog: $\mathcal{\CF}_{1}\ni A^{y}= (f^{y})^{-1}([-\infty,r))$
  \end{proof}
\end{lemma}

\begin{satz}[Satz von Fubini] % 5
  ($\Omega_{1}, \mathcal{\CF}_{1}, \mathcal{m}_{1})$ und ($\Omega_{2}, \mathcal{\CF}_{2}, \mathcal{m}_{2})$
  $\sigma$-endliche Maßräume, $(\Omega, \mathcal{\CF}, \mathcal{m})$ = ($\Omega_{1}\times
  \Omega_{2}, \mathcal{\CF}_{1}\otimes \mathcal{\CF}_{2}, \mathcal{m}_{1}\otimes \mathcal{m}_{2})$

  f sei eine messbare Funktion auf ($\Omega, \mathcal{\CF})$, f nichtnegativ.

  Dann folgt: Das Integral $\int_{\Omega_{2}}
  \underbrace{f(x,y)}_{f_{x}(y)} \mathcal{m}_{2}(dy)$ ist eine messbare Funktion
  in $x$ auf ($\Omega_{1}, \mathcal{\CF}_{1})$ und es gilt:
  \begin{gather}
    \int_{\Omega} f(x,y) \mathcal{m}(d(x,y)) =
       \int_{\Omega_{1}}(\int_{\Omega_{2}}f(x,y) m_{2}(dy)) m_{1}(dx)
  \end{gather}

  \begin{proof}
    \begin{enumerate}
     \item f = $1_{A}, A\in \mathcal{\CF}_{1}\otimes \mathcal{\CF}_{2}$
      \begin{align}
	\int_{\Omega} f(x,y) m(d(x,y)) &= \int_{\Omega} 1_{A}(x,y)
	   \mathcal{m}(d(x,y)) = \mathcal{m}(A) = \int_{\Omega_{1}}
	   \underbrace{\mathcal{m}_{2}(A_{x})}_{messbar in x (Satz 1)} \mathcal{m}_{1}(dx)\\
	& \int_{\Omega_{1}}(\int_{\Omega_{2}}1_{A}(y) \mathcal{m}_{2}(dy)) \mathcal{m}(dx)
	   = \int_{\Omega_{1}}\underbrace{(\int_{\Omega_{2}}f(x,y)
	   m_{2}(dy))}_{messbar in x}\mathcal{m}_{1}(dx)
      \end{align}
     \item f sei nichtnegative einfache Funktion, d.\,h.
      \begin{gather}
	f = \sum_{k=1}^{n}c_{k} 1_{A_{k}}, c_{1},\ldots,c_{n}\geq 0,
	   A_{1},\ldots,A_{n}\in \mathcal{\CF}_{1}\otimes \mathcal{\CF}_{2}
      \end{gather}

      \begin{align}
	\int_{\Omega}f(x,y) \mathcal{m}(d(x,y)) &=
	   \sum_{k=1}^{n}c_{k}\int_{\Omega}1_{A}(x,y) m(d(x,y))\\
	&= \sum_{k=1}^{n}c_{k}\int_{\Omega_{1}}(\int_{\Omega_{2}}
	   1_{A_{k}}(x,y) m_{2}(dy))m_{1}(dx) = \int_{\Omega_{1}}
	   \underbrace{(\sum_{k=1}^{n}c_{k} \int_{\Omega_{2}}1_{A_{k}}(x,y)
	   \mathcal{m}_{2}(dy))}_{messbar in x}m_{1}(dx)\\
	&= \int_{\Omega_{1}}\underbrace{(\int_{\Omega_{2}}\sum_{k=1}^{n} c_{k}
	   1_{A_{k}}(x,y) m_{2}(dy))}_{messbar in x}m_{1}(dx)
      \end{align}
     \item f messbar auf $(\Omega, \mathcal{\CF})$, f$\geq$0

      $\exists f_{n}: f_{n}\geq0$ und einfach,
      $f_{n}\uparrow_{n\rightarrow\infty}f$

      $\Rightarrow$
      \begin{gather}
	\int_{\Omega}f(x,y) m(d(x,y)) =
	   \lim_{n\rightarrow\infty}\int_{\Omega}f_{n}(x,y) m(d(x,y))
	   =^{2)}
	   \lim_{n\rightarrow\infty}\int_{\Omega_{1}}(\int_{\Omega_{2}}
	   f_{n}(x,y) m_{2}(dy))m_{1}(dx) =^{B. Levi} \int_{\Omega_{1}}
	   (\lim_{n\rightarrow\infty} \int_{\Omega_{2}} f_{n}(x,y)
	   m_{2}(dy)) m_{1}(dx) =^{B. Levi}
	   \int_{\Omega_{1}}\underbrace{(\int_{\Omega_{2}}f(x,y) m_{2}(dy))}_{messbar
	   in x} m_{1}(dx)
      \end{gather}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{satz} % 5':
  f sei messbar auf $(\Omega, \mathcal{\CF})$, $\exists\int_{\Omega} f(x,y)m(d(x,y))$

  Dann existiert $\int_{\Omega_{1}}(\int_{\Omega_{2}}f(x,y) m_{2}(dy))
  m_{1}(dx)$ und es gilt:
  \begin{gather}
    \int_{\Omega}f(x,y) m(d(x,y)) =
       \int_{\Omega_{1}}(\int_{\Omega_{2}}f(x,y) m_{2}(dy)) m_{1}(dx)
  \end{gather}

  \begin{proof}
    \begin{gather}
      \exists\int_{\Omega}f(x,y) m(d(x,y)) \Leftrightarrow
	 \int_{\Omega}f^{+}(x,y) m(d(x,y))<\infty oder
	 \int_{\Omega}f^{-}(x,y) m(d(x,y))<\infty
    \end{gather}

    Ann.: $\int_{\Omega}f^{+}(x,y) m(d(x,y))<\infty$

    \begin{align}
      \int_{\Omega} f(x,y) m(d(x,y)) &=
	 \underbrace{\int_{\Omega}f^{+}(x,y) m(d(x,y))}_{<\infty}
	 -\int_{\Omega}f^{-}(x,y) m(d(x,y))\\
      &=^{Satz 5} \int_{\Omega_{1}}(\underbrace{\int_{\Omega_{2}}
	 f^{+}(x,y)m_{2}(dy))}_{<\infty m_{1}-f.ü.}m_{1}(dx)-
	 \int_{\Omega_{1}}(\int_{\Omega_{2}} f^{-}(x,y)m_{2}(dy))m_{1}(dx)
    \end{align}
    N=\{$x\in \Omega_{1}: \int_{\Omega_{2}}f^{+}(x,y) m_{2}(dy)=\infty\},
    N\in \mathcal{\CF}_{1}, m_{1}(N)=0$
    \begin{align}
      &= \int_{\Omega_{1}}
	 1_{N^{\complement}}(x)(\int_{\Omega_{2}}f^{+}(x,y)m_{2}(dy)) m_{1}(dx) -
	 \int_{\Omega_{1}}
	 1_{N^{\complement}}(x)(\int_{\Omega_{2}}f^{-}(x,y)m_{2}(dy)) m_{1}(dx)\\
      &=^{Linearität} \int_{\Omega_{1}}
	 1_{N^{\complement}}(x)(\int_{\Omega_{2}}f^{+}(x,y)m_{2}(dy)-
	 \int_{\Omega_{2}}f^{+}(x,y)m_{2}(dy)) m_{1}(dx)\\
      &= \int_{\Omega_{1}}\underbrace{1_{N^{\complement}}(x) (\int_{\Omega_{2}}f(x,y)
	 m_{2}(dy))}_{g(x) aus Bemerkung} m_{1}(dx)\\
      &= \int_{\Omega_{1}}(\int_{\Omega_{2}} f(x,y) m_{2}(dy))m_{1}(dx)
	 (Iteriertes Integral existiert)
    \end{align}
  \end{proof}
\end{satz}

\begin{bemerk}
  (Iterierte Integral und dessen Existenz)

  Das iterierte Integral $\int_{\Omega_{1}}(\int_{\Omega_{2}} f(x,y)
  m_{2}(dy))m_{1}(dx)$ existiert (gemäß Definition), falls:
  \begin{enumerate}
   \item
    \begin{gather}
      \exists\int_{\Omega_{2}}f(x,y) m_{2}(dy) m_{1}-f.\,ü., d.\,h. es exist.
	 N\in \mathcal{\CF}_{1}, m_{1}(N) = 0 und \int_{\Omega_{1}} f(x,y) m_{2}(dy)
	 exists \forall x\in N^{\complement}
    \end{gather}
   \item
    \begin{gather*}
      g(x) =
         \begin{cases}
           \int_{\Omega_{2}} f(x,y) m_{2}(dy) & x\in N^{\complement}\\
           0& x\in N
         \end{cases}
    \end{gather*}

    $g$ messbar
   \item $\exists\int_{\Omega_{1}}g(x) m_{1}(dx)$
  \end{enumerate}
\end{bemerk}

\begin{folgerung}
  $f$ messbar auf $(\Omega, \mathcal{\CF})$

  Aus Satz 5 folgt:
  \begin{gather}
    \exists\int_{\Omega} f(x,y) m(d(x,y)) \Leftrightarrow
       \int_{\Omega_{1}}(\int_{\Omega_{2}}f^{+}(x,y)
       m_{2}(dx))m_{1}(dx)<\infty oder
       \int_{\Omega_{1}}(\int_{\Omega_{2}}f^{-}(x,y) m_{2}(dx))m_{1}(dx)<\infty
  \end{gather}
\end{folgerung}

