\begin{satz}[Radon-Nicodym] % 3
  $\mu$, cm ma"se auf ($\Omega$, cF), cm sei $\sigma$-endlich.
  
  Dann sind die folgenden Bedingungen "aquivalent:
  \begin{enumerate}
   \item $\mu$ << m
   \item es existiert eine Dichte f von $\mu$ bzgl. m
  \end{enumerate}
  
  \begin{proof}
    (b)$\rightarrow$(a): klar
    
    Umkehrung: zus"atzliche Vorraussetzung: $\mu$ $\sigma$-endlich
    \begin{enumerate}[1.Schritt]
     \item Reduktion auf den Fall, da"s $\mu$ und cm endlich sind
      $\exists$ $\Omega$_{n}$$\in cF, paarweise disjunkt,
      $\Omega=\bigcup_{n=1}^{\infty}\Omega_{n}$ mit m($\Omega$_{n}$$),
      $\mu$($\Omega_{n}$) < $\infty$
      
      $m_{n}(A) := m(A\cap\Omega_{n}), \mu_{n}(A)=\mu(A\cap\Omega_{n}),
      A\in cF, n\geq1$ $cm_{n}, \mu_{n}$ endliche Ma"se, $\mu_{n} << cm_{n}$
      
      Ann.: Satz g"ultig f"ur endliche Ma"se
      $\Rightarrow$ $\exists$ Dichte $f_{n}$ von $\mu_{n}$ bgzl. $cm_{n}$
      
      Ansatz: f:=$\sum_{n=1}^{\infty}f_{n}\cdot1_{\Omega_{n}}$
      
      f$\geq$0 me"sbar
      $\mu(A)$=$\sum_{n=1}^{\infty}\mu(A\cap\Omega_{n})
      =\sum_{n=1}^{\infty}\int_{A}f_{n}\,dm =^{Lemma
      1} =\sum_{n=1}^{\infty}\int_{A}f_{n}\cdot1_{\Omega_{n}}\,dm
      =^{B.Levi} \int_{A}\sum_{n=1}^{\infty} f_{n}\cdot1_{\Omega_{n}}\,dm
      = \int_{A}\sum_{n=1}^{\infty} f\,dm, A\in cF$
     \item Seien $\mu$, cm endlich
      $\mathcal{H}=\{h: h\geq 0 \text{me"sbar}, \int_{A}h\,dm\leq\mu(A),
      \forall A\in \mathcal{F}\}$
      
      \begin{enumerate}
       \item $\mathcal{H}\not0: 0\in\mathcal{H}$
       \item f,g\in $\mathcal{H} \Rightarrow f\cup g\in\mathcal{H}:
	\int_{A}(f\cup g)\,dm=\int_{A\cap\{f\leq g\}} g\,dm+
	\int_{A\cap\{g<f\}} f\,dm \leq \mu(A\cap\{f\leq g\})+
	\mu(A\cap\{g < f\}) = \mu(A), A\in \mathcal{F}$
	
	c:= $\sup_{h\in \mathcal{H}}\int_{\Omega}h\,dm, c\leq \mu(\Omega)
	< \infty$
	
	$\exists f_{n}' \in \mathcal{H}$ mit
	c=$\sup_{n\geq1}\int_{\Omega}f_{n}'\,dm$, $f_{n}
	=\max\{f_{1}',\ldots,f_{n}'\}\in \mathcal{H}$
	$f_{n}\leq f_{n+1}, f:=\lim_{n\rightarrow\infty}f_{n}$
	
	Beh.: f\in $\mathcal{H}$ und c=$\int_{\Omega} f\,dm$
	wiseen: $\int_{A}f_{n}\,dm \leq \mu(A), A\in \mathcal{F}
	\rightarrow^{n-\infty}^{B.Levi} \int_{A}f\,dm \leq \mu(A)$
	$\Rightarrow$ f\in $\mathcal{H}$
	
	c=$\sup_{n\geq1}\int_{\Omega}f_{n}'\,d\mathcal{m}\leq

	\sup_{n\geq1}\int_{\Omega}f_{n}\,d\mathcal{m}
	=\lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,d\mathcal{m}
	=^{B.Levi} \int_{\Omega} \,d\mathcal{m}\leq c$
	
	Es gilt: f ist Dichte von $\mu$ bzgl. $\mathcal{}$
	zeigen: $\mu<<\mathcal{m} \Rightarrow f$ Dichte
	$\tau(A) := \mu(A)-\int_{A}f\,d\mathcal{m}, A\in \mathcal{F},
	\tau$ Ma"s auf ($\Omega, \mathcal{F}$)
	
	z.z. $\mu$<< $\mathcal{m} \Rightarrow \tau\equiv0$
	
	Kontraposition: $\tau(\Omega) >0 \Rightarrow \mu \not<< \mathcal{m}$
	oBdA: $\mathcal{m}(\Omega)>0$
	
	$\alpha \mathcal{m}(\Omega) < \tau(\Omega),
	\alpha=\frac{\tau(\Omega)}{2\mathcal{m}(\Omega)}$
	
	Anwendung von Lemma 2 auf $\nu$=$\alpha$$\mathcal{m}, \tau:
	\exists \Omega'\in \mathcal{F}, \alpha\mathcal{m}(\Omega')<
	\tau(\Omega'), \alpha\mathcal{m}(A) \leq \tau(A), A\in
	\mathcal{F}, A\subseteq\Omega'$
	
	Ansatz: f'=f+$\alpha\cdot1_{\Omega'}$
	\begin{enumerate}
	 \item f'\in $\mathcal{H}:
	  \int_{A}f'\d\mathcal{m}=\int_{A}f\,d\mathcal{m}+
	  \int_{A}\alpha\cdot1_{\Omega'}\,d\mathcal{m}=\int_{A}f\,d\mathcal{m}
	  +\underbrace{\alpha\mathcal{m}(A\cap\Omega')}_{\tau(A\cap\Omega')} \leq\int_{A} f
	  \,d\mathcal{m}+\tau(A) =\mu(A)$
	 \item c = $\int_{\Omega} f'\,d\mathcal{m}:
	  c=\int_{\Omega}f\,d\mathcal{m}\leq\int_{\Omega}f'\,d\mathcal{m}\leq^{f\in\mathcal{H}} c$
	 \item c=$\int_{\Omega}f'\,d\mathcal{m}=\int_{\Omega}
	  f\,d\mathcal{m}+ \int_{\Omega}\alpha 1_{\Omega'}\,d\mathcal{m}=
	  c+ \alpha \mathcal{m}(\Omega') \Rightarrow \mathcal{m}(\Omega')=0$
	 \item $0<\tau(\Omega') = \mu(\Omega') -\int_{\Omega'}
	  f\,d\mathcal{m}=\mu(\Omega') \Rightarrow \mu(\Omega')>0$
	\end{enumerate}
	
	Somit gezeigt $\mu$ $\not$<< $\mathcal{m}$
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{beispiel}
  m beliebiges ma"s auf ($\Omega$, cF), cm $\not=$ 0 f\equiv$\infty$:
  $\mu$(A) =$\int_{A} f\,dm, A\in cF$
  $\mu$ Ma"s: $\mu$(A) =
  \begin{case}0&cm(A)=0\\$\infty$&cm(A)>0\end{case} A\in cF, $\mu$ ist
  nicht $\sigma$-endlich
\end{beispiel}

\begin{notation}
  \frac{d$\mu$}{d$\mathcal{m}}$ Dichte von $\mu$ bgzl. $\mathcal{m}$
  (sofern Dichte existiert)
\end{notation}

\begin{satz} %4
  Es seien $\mu$, $\mathcal{m}$ Ma"se auf ($\Omega$, $\mathcal{F})$ und
  $frac{d\mu}{d\mathcal{m}}$ sei Dichte, h $\geq$0 me"sbar.
  
  Dann gilt:
  \begin{gather}
    \int_{\Omega}h\,d\mu = \int_{\Omega}
       h(frac{d\mu}{d\mathcal{m}})d\mathcal{m}
  \end{gather}
  
  \begin{proof}(Standardbeweis)
    \begin{enumerate}
     \item h=$1_{A}, A\in \mathcal{F}$
     \item h einfach, h$\geq$ 0
     \item h $\geq$ 0 me"sbar: $\exists$ $h_{n}|^h, h_{n} \geq0$ einfach
    \end{enumerate}
  \end{proof}
\end{satz}

\subsection{Singul"are Ma"se}
$\mu$, $\mathcal{m}$ Ma"se auf ($\Omega$,$\mathcal{F})$

\begin{definition} %3
  $\mu$ hei"st singul"ar zu $\mathcal{m}$ ($\mu$|_ $\mathcal{m})$, falls
  exist A\in $\mathcal{F}$ mit $\mu$(A)=$\mathcal{m}(A^{C})=0$.
\end{definition}

\begin{beispiel}
  ($\Omega$, $\mathcal{F}=(\R, \mathcal{B}(\R)), \lambda$ Lebesgue-Ma"s,
  $\mathcal{m}$ diskretes Ma"s
  $\mathcal{m}=\sum_{i=1}^{\infty}\alpha_{i}\delta_{x_{i}},
  x_{1},x_{2},\ldots\in\R$
  $\mathcal{m}|_\lambda$
\end{beispiel}

\begin{satz}[Zerlegungssatz von Hahn-Lebesgue]
  $\mu$, $\mathcal{m}$ $\sigma$-endlich
  
  Dann existiert eine eindeutige Zerlegung $\mu =\nu+\lambda$
  \begin{enumerate}
   \item $\nu\ll\mathcal{m}$
   \item $\lambda\perp\mathcal{m}$
  \end{enumerate}
  
  \begin{proof}
    $\mathcal{m}+\mu$ $\sigma$-endlich, $\mu\ll\mathcal{m}+\mu$
    
    Satz von Radon-Nicodym:
    \begin{gather}
      f=\frac{d\mu}{d(\mathcal{m}+\mu)}\quad\text{Dichte}
    \end{gather}
    
    Ansatz: $\nu(A) := \mu(A\cap\{f<1\}), \lambda(A) :=
    \mu(A\cap\{f\geq1\}), A\in \mathcal{F}$
    $\mu$ = $\nu+ \lambda, \nu, \lambda$ erf"ullen die gew"unschten
    Eigenschaften
  \end{proof}
\end{satz}

