\chapter{Maße auf metrischen Räumen}

\begin{satz}
  $(\Omega, \mathcal{\CF})$ messbarer Raum, $\mathcal{K}\subseteq \mathcal{\CF}, \mathcal{M}' = \mathcal{m}(\mathcal{K})$ (kleinste
  monotone Klasse $\supseteq \mathcal{K}$).

  Es gilt:
  \begin{enumerate}
   \item $\emptyset,\Omega\in \mathcal{K}$
   \item $A,B\in \mathcal{K}$ $\Rightarrow$ $A\cup B\in \mathcal{K}$
   \item $A\in \mathcal{K}$ $\Rightarrow$ $A^{\complement}\in \mathcal{M}'$
  \end{enumerate}

  $\Rightarrow$ $\mathcal{M}'=\mathcal{\CF}$

  \begin{proof}
    Übungsaufgabe
  \end{proof}
\end{satz}

\begin{satz}
  $\mathcal{m}$ sei ein $\sigma$-endliches Maß auf $(\Omega,\mathcal{\CF})$, $\mathcal{K}\subseteq\mathcal{\CF}$
  wie in Satz 1.

  Dann gilt: (*) $\mathcal{m}(A) = \sup_{\CF\subseteq A, \CF\in \mathcal{K}_{\delta}} \mathcal{m}(\CF),
  \mathcal{K}_{\delta} = \{A=\bigcap_{n=1}^{\infty}A_{n}: A_{n} \in \mathcal{K},
  A_{n+1}\subseteq A_{n}, n\geq1\}$

  \begin{proof}
    \begin{enumerate}[I]
     \item $\mathcal{m}$ sei endlich $\mathcal{M}=\{A\in \mathcal{\CF}: (*) erfüllt\}$
      Dann gilt:
      \begin{enumerate}
       \item $\mathcal{K}\subseteq \mathcal{M}$
       \item2 $\mathcal{M}$ monotone Klasse
      \end{enumerate}
      $\Rightarrow^{Satz 1} \mathcal{\CF}\subseteq \mathcal{M}$

      Beiweis von 2:
      \begin{enumerate}
       \item
	$A_{n} \subseteq A_{n+1}, A_{n}\in \mathcal{M}, A=\bigcup_{n=1}^{\infty}A_{n}$

	$\mathcal{m}(A)=\sup_{n\geq1} \mathcal{m}(A_{n})=\sup_{n\geq1}\sup_{\CF\subseteq
	A_{n}, \CF\in K_{\delta}} \mathcal{m}(\CF) \leq \sup_{n\geq1}\sup_{\CF\subseteq
	A, \CF\in K_{\delta}} \mathcal{m}(\CF) \leq \mathcal{m}(A)$

       \item $A_{n+1}\subseteq A_{n}\in \mathcal{M}, n\geq1,
	A=\bigcap_{n=1}^{\infty}A_{n}$
	$\exists \CF_n\in \mathcal{K}_{\delta}: \mathcal{m}(A_{n})-m(\CF_{n})<\frac\epsilon{2^n}$
	$\CF_{n}\subseteq A_{n}$
	$\CF_{n}' = \bigcap_{k=1}^{n}\CF_{k}, \CF_{n}'\subseteq A_{n}, \CF_{n}'\in
	\mathcal{K}_{\delta} \mathcal{m}(A_{n}\setminus \CF_{n}')=
	\mathcal{m}(A_{n}\cap(\bigcap_{k=1}^{n}\CF_{k})^{\complement}) =
	m(\bigcup_{k=1}^{n}(A_{n}\setminus \CF_{k}) \leq
	m(\bigcup_{k=1}^{n}(A_{k}\setminus \CF_{k}) \leq \sum_{k=1}^{n}
	\underbrace{\mathcal{m}(A_{k}\setminus \CF_{k})}_{\frac\epsilon{2^{k}}}<\epsilon$

	Wählen $\CF:= \cap_{n=1}^{\infty}$
	$\CF_{n}=\bigcap_{n=1}^{\infty}\CF_{n}' \Rightarrow \CF\in
	\mathcal{K}_{\delta}=(\mathcal{K}_{\delta})_{\delta}, \CF\uparrow$

	$\mathcal{m}(A)-\mathcal{m}(\CF) =
	\lim_{n\rightarrow\infty}[\underbrace{m(A_{n})-m(\CF_{n}')}_{< \epsilon}] < \epsilon$
      \end{enumerate}
     \item $\mathcal{m}$ $\sigma$-endlich
      $\exists \Omega_{n}\in \mathcal{\CF}$, $\Omega_{n}\subseteq \Omega_{n+1}$
      $\Omega=\bigcup_{n=1}^{\infty} \Omega_{n}, \mathcal{m}(\Omega_{n}) <\infty$

      $\mathcal{m}_{n}(A):= \mathcal{m}(A\cap\Omega_{n}), A\in \mathcal{\CF}$

      $\mathcal{m}_{n}$ endliches Maß

      $\mathcal{m}(A)=\sup_{n\geq1}\mathcal{m}(A\cap\Omega_{n}) =\sup_{n\geq1} \mathcal{m}_{n}(A)
      =^{I} \sup_{n\geq1}\sup_{\CF\subseteq A, \CF\in \mathcal{K}_{\delta}} \mathcal{m}_{n}(\CF)
      \leq \sup_{n\geq1}\sup_{\CF\subseteq A, \CF\in \mathcal{K}_{\delta}} \mathcal{m}(\CF)
      \leq m(A)$ $\Rightarrow$ (*)
    \end{enumerate}
  \end{proof}
\end{satz}

$(E,\rho)$ metrischer Raum, $\mathcal{\CF}=\{\CF\subseteq E\colon \CF abgeschlossen\}$
$\mathcal{B}(E)=\sigma(\mathcal{\CF})$

Eigenschaften von $\mathcal{\CF}$:
\begin{enumerate}
 \item $\emptyset, E \in \mathcal{\CF}$
 \item $\CF_{1}, \CF_{2}\in \CF \Rightarrow \CF_{1}\cup \CF_{2}, \CF_{1}\cap \CF_{2}\in
  \mathcal{\CF}$
 \item $\CF\in \mathcal{\CF}$ $\Rightarrow$ $\CF^{\complement}\in \mathcal{m}(\mathcal{\CF}): \CF=\{x\in E: \rho(x,\CF) = 0\}$

  $\CF^{\complement}=\{x\in E: \rho(x, \CF)>0\} = \bigcup_{n=1}^{\infty}\underbrace{\{x\in E:
  \rho(x,\CF)\geq\frac1n\}}_{abgeschlossen}\in \mathcal{\CF}_{\sigma}\subseteq \mathcal{m}(\mathcal{\CF})$
 \item $\mathcal{\CF}_{\delta}=\mathcal{\CF}$
\end{enumerate}

\begin{satz}
  Sei m ein $\sigma$-endliches Maß auf $(E, \mathcal{B}(E))$.

  Dann folgt $\mathcal{m}(A) = \sup_{\CF\subseteq A, \CF\in \mathcal{\CF}} \mathcal{m}(\CF), A\in B(E)$
\end{satz}

\begin{satz}[Approximation von Außen]
  $m$ beliebiges Maß auf $(E,B(E))$, $A\in B(E)$ fest gewählt

  $\mathcal{G}=\{G\subseteq E: G opffen\}$
  (*) $\mathcal{m}(A)=\inf_{A\subseteq G, G\in \mathcal{G}} m(G)$ $\Leftrightarrow$ Falls
  $m(A)<\infty$, dann exist $G_{0}\supseteq A$, offen mit $m(G_{0})<\infty$
\end{satz}

\begin{folgerung}
  $m$ endliches Maß auf $(E,B(E))$

  Dann gilt: $m(A) =\inf_{A\subseteq G, G\in \mathcal{G}} m(G), A\in B(E)$
\end{folgerung}

\begin{folgerung}
  $\lambda$ Lebesgue-Maß auf $(\R, B(\R))$ $\Rightarrow$ $\lambda(A) =
  \inf_{A\subseteq G, G\in \mathcal{G}} \lambda(G), A\in B(\R)$

  \begin{proof}
    $A\in B(\R)$, $A\cap[n,n1)$, $n\in\Z$

    $\underbrace{A\cap[n,n+1)}_{erfüllt Bedingung in Satz 4}\subseteq$(n-1,n+1) offen

    Es exits. $G_{n}$ offen, $A\cap [n,n+1)\subseteq G_{n}$ und
    $\lambda(G_{n})-\lambda(A\cap[n,n-1))<\frac{\epsilon}{2^{|n|}}$

    $\lambda(A)=\sum_{n \text{ganzz.}} \lambda(A\cap[n,n-1))\geq\sum_{n \text{ganzz.}}
    [\lambda(G_{n})-\frac\epsilon{2^{|n|}}]\geq \lambda(\bigcup_{n \text{ganzz.}}
    G_{n})-3\epsilon = \lambda(G)-3\epsilon, G=\bigcup_{n \text{ganzz.}} G_{n}$
    offen, $G\supseteq A$
  \end{proof}
