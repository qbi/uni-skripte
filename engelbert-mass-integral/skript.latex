\section{Der Satz von Daniell} % Kapitel 13

\begin{description}
 \item[Problem] Zusammenhang zwischen linearen Funktionalen und Ma"sen
\end{description}

($\Omega$, $\mathcal{F}$) me"sbarer Raum, $\mathbb{B}(\Omega,
\mathcal{F})$ linearer Raum der beschr"ankten me"sbaren Funktionen
H$\subseteq\mathbb{B}(\Omega,\mathcal{F})$ mit folgenden Eigenschaften:
\begin{enumerate}
 \item $H$ linearer Raum
 \item $1_{\Omega}\in H$
 \item $H$ abgeschlossen gegen"uber $\wedge,\vee$ (Minimum, Maximum)
 \item $\sigma(H)=\mathcal{F}$, $\sigma(H)$ kleinste $\sigma$-Algebra
  ($\subseteq$$\mathcal{F})$, da"s f:($\Omega$,$\sigma(H))\rightarrow \R$
  me"sbar ist $\forall f\in H$
\end{enumerate}

\begin{satz}[Daniell]
  I:H$\rightarrow$R Abbildung mit folgenden Eigenschaften:
  \begin{enumerate}
   \item I ist linear
   \item I ist positiv, f\in H, f$\geq$0 $\Rightarrow$ I(f) $\geq$0
   \item I ist stetig bzgl. monotoner Konvergenz: $f_{n}\in H, f_{n}|v
    0(n\rightarrow\infty)$ punktweise $\Rightarrow$
    $\lim_{n\rightarrow\infty}I(f_{n})=0$
  \end{enumerate}
  
  Dann existiert ein endlichen Ma"s $\mathcal{m}$ auf
  ($\Omega$,$\mathcal{F})$ mit 
  \begin{gather}
    I(f) = \int_{\Omega}f\,d\mathcal{m}, f\in H
  \end{gather}
  
  Insbesondere:
  \begin{gather}
    \mathcal{m}(\Omega) = I(1_{\Omega})
  \end{gather}
  
  Das Ma"s $\mathcal{m}$ ist eindeutig bestimmt
  
  \begin{proof}
    siehe Neven II.7.1 oder P.A.Meyer: Probability and Potentials, T II.2.4
  \end{proof}
\end{satz}

\begin{folgerung}[Fortsetzungssatz von Caratheodory]
  $\mathcal{m}$ Pr"ama"s auf ($\Omega$, $\mathcal{A}$) $\mathcal{A}$
  Algebra, $\mathcal{F}=\sigma(\mathcal{A})$
  
  H=$\mathcal{L}(\{1_{A}: A\in\mathcal{A}\})$ (lineare H"ulle)
  Eigenschaften 1-4 erf"ullt
  
  \begin{gather}
    I(f) = \sum_{k=1}^{n}c_{k}\mathcal{m}(A_{k}), f\in H,
       f=\sum_{k=1}^{n}c_{k}1_{A_{k}}
  \end{gather}
  $I$ erf"ullt (i)-(iii) von Satz Daniell
  
  $\xrightarrow^{Satz 1}$ Es existiert ein Ma"s $\bar{\mathcal{m}}$ auf
  ($\Omega$, $\mathcal{F})$ mit:
  \begin{gather}
    I(f)= \int_{\Omega}f\,d\bar{\mathcal{m}}
  \end{gather}
  insbesondere:
  \begin{gather}
    f = 1_{A}, A\in \mathcal{A} \Rightarrow \mathcal{m}(A)
       =\bar{\mathcal{m}}(A)
  \end{gather}
\end{folgerung}

\begin{satz}[Rieszscher Darstellungssatz (1909) von F. Riesz]
  K kompakt, hausdorffsch ($\forall x,y\in K \exists
  \varepsilon_{x},\varepsilon_{y}: K_{\varepsilon_{x}}(x)\cap
  K_{\varepsilon_{y}}(y) = \emptyset$)
  
  C(K)  Raum aller stetigen (beschr"ankten!) Funktionen auf K,
  $\mathcal{B}_{0}(K)=\sigma(C(K))$ $\sigma$-Algebra der Bairschen Mengen
  I: C(K)$\rightarrow$\R Abbildung mit:
  \begin{enumerate}
   \item $I$ lineares Funktional
   \item $I$ ist positiv
  \end{enumerate}
  
  Dann existiert genau ein (endliches) Ma"s auf $\mathcal{m}$ auf $(K,
  \mathcal{B}_{0}(K))$ mit $I(f) = \int_{K}f\,d\mathcal{m}, f\in C(K)$
  
  \begin{proof}
    $\|f\| = \sup_{x\in K} |f(x)|, f\in C(K)$ ist eine Norm
    
    \begin{enumerate}
     \item $I$ positiv \Rightarrow $I$ stetig (=beschr"ankt) bzgl.
      $\|\cdot\|$ 
      \begin{enumerate}
       \item I positiv $\Rightarrow$ I monoton:
	f,g \in C(K), f$\geq$ g $\Rightarrow$ f-g\in C(K), f-g$\geq$ 0
	$\Rightarrow$ I(f-g) $\geq$ 0 $\Rightarrow$ I(f) $\geq$ I(g)
       \item f$\leq$ \|f\|$\cdot$$1_{K}$ $\Rightarrow^{Monotonie}
	I(f)\leq I(\|f\|1_{K})=\|f\|\underbrace{I(1_{K})}_{|\cdot|<\infty}$ 
	
	-f$\leq$ \|f\|$\cdot$$1_{K}$ $\Rightarrow^{Monotonie}
	I(-f)\leq I(\|f\|1_{K})=\|f\|\underbrace{I(1_{K})}_{|\cdot|<\infty}$ 
	
	|I(f)| = \max\{I(f), -I(f)\} = \max\{I(f), I(-f)\} $\leq$ \|f\|
	I($1_{K}$), f\in C(K) $\Rightarrow$ I beschr"ankt $\Rightarrow$ I stetig
      \end{enumerate}
     \item I stetig bzgl. \|$\cdot$\| $\rightarrow$ I stetig bzgl.
      monotoner Konvergenz $f_{n}\in C(K), f_{n}(x) |v 0
      (n\rightarrow\infty) \rightarrow^{Satz von Dini} ||f_{n}||
      \xrightarrow^{n\rightarrow\infty}0 \rightarrow^{nach I} I(f_{n})
      \rightarrow^{n\rightarrow\infty}0$
      
      $F_{n}= \{f_{n}\geq \varepsilon\}$ abgeschlossen, $F_{n}\supseteq
      F_{n+1}, \bigcap_{n=1}^{\infty}F_{n}=\emptyset$
      
      $\Rightarrow^{K beschr"ankt} \exists n: F_{n}=\emptyset$ 
    \end{enumerate}
    
    Alle Vorraussetzungen f"ur den Satz von Daniell sind erf"ullt
    $\Rightarrow$ Beh.
  \end{proof}
\end{satz}

\section{Die S"atze von Egoroff und Lusin}

\begin{satz}[D.F. Egoroff]
  $(\Omega, \mathcal{F}, \mathcal{m})$, endlicher Ma"sraum, $f_{n},
  f$ me"sbare Funktionen, $f_{n}\rightarrow^{x\rightarrow\infty} f$
  $\mathcal{m}$-f"u
  
  Dann folgt:
  \begin{gather}
    \forall\varepsilon>0 \exists A_{\varepsilon}\in \mathcal{F}:
       \mathcal{m}(A_{\varepsilon})<\varepsilon, f_{n}\rightarrow f
       \text{gleichm"a"sig auf } A_{\varepsilon}
  \end{gather}.
  
  \begin{proof}
    \begin{gather}
      N^{C}=\{\omega\in\Omega: f_{n}(\omega)
	 \rightarrow^{n\rightarrow\infty} f(\omega)\}, N\in \mathcal{F},
	 \mathcal{m}(N)=0\\
      F_{nk}= \bigcup_{j=n}^{\infty}\{\omega\in\Omega:
	 |f_{j}(\omega)-f(\omega)|\geq 2^{-k}\}\cap N^{C},
	 F_{nk}|v\emptyset (n\rightarrow\infty)\label{XXx}
    \end{gather}
    
    $\varepsilon>0$ Es existiert ein n(k) mit
    $\mathcal{m}(F_{n(k)\,k})<\varepsilon 2^{-k}$
    
    \begin{gather}
      A_{\varepsilon} = N\cup \bigcup_{k=1}^{\infty} F_{n(k)\,k},
	 A_{\varepsilon}\in \mathcal{F}\\
      \mathcal{m}(A_{\varepsilon})\leq
	 \sum_{k=1}^{\infty}\mathcal{m}(F_{n(k)\,k})<\varepsilon\\
      A_{\varepsilon}^{C} = N^{C}\cap \bigcap_{k=1}^{\infty}
	 F_{n(k)\,k}^{C} \subseteq N^{C}\cap F_{n(k)\,k}^{C}\\
      \omega\in A_{\varepsilon}^{C}\Rightarrow
	 |f_{j}(\omega)-f(\omega)|<2^{-k}, j \geq n(k)
    \end{gather}
  \end{proof}
\end{satz}

\begin{satz}[N. Lusin]
  $(\Omega,\rho), (E,d)$ metrische R"aume, $E$ separabel, $\mathcal{m}$
  endliches Ma"s auf $(\Omega, \mathcal{B}(\Omega))$, $f: (\Omega,
  \mathcal{B}(\Omega)) \rightarrow (E, \mathcal{B}(E))$.
  
  Dann folgt: F"ur alle $\varepsilon>0$ existiert eine abgeschlossene
  Menge $F\subseteq\Omega$ mit den folgenden Eigenschaften:
  \begin{enumerate}
   \item $f_{|F}$ (Einschr"ankung von $f$ auf $F$) ist  stetig
   \item $\mathcal{m}(F^{C})<\varepsilon$
  \end{enumerate}
  
  \begin{proof}
    $\{a_{1}, a_{2},\ldots\}$ abz"ahlbare dichte Menge in $E$
    
    \begin{gather}
      B_{nk} = f^{-1}(S_{2^{-k}}(a_{n})), n,k=1,2,\ldots\\
      D_{nk} = B_{nk}\setminus\bigcup_{j=1}^{\infty} B_{nk},
	 n,k=1,2,\ldots\\
      D_{nk} \text{paarw. disj.}, n=1,2,\ldots, k \text{fest}\\
      A_{nk}= \bigcup_{j=1}^{\infty} D_{jk} =
	 \bigcup_{j=1}^{\infty}B_{jk} |^\Omega (n\rightarrow\infty)
    \end{gather}
    
    Beweis f"ur letzte Gleichung:
    \begin{gather}
      \bigcup_{n=1}^{\infty} A_{nk} = \bigcup_{n=1}^{\infty} B_{nk} =
	 \bigcup_{n=1}^{\infty} f^{-1}(S_{2^{-k}}(a_{n})) =
	 f^{-1}(\underbrace{\bigcup_{n=1}^{\infty}
	 S_{2^{-k}}(a_{n})}_{=E}) = \Omega
    \end{gather}
    Es existiert mit $\mathcal{m}(A_{n(k)\,k}^{C})<\varepsilon\cdot
    2^{-k-1}, k=1,2,\ldots$

    Es existiert $F_{jk}\subseteq D_{jk}, F_{jk}$ abgeschlossen,
    $\mathcal{m}(D_{jk}\supset F_{jk}) < \varepsilon2^{k-j-1}$. Nach Satz
    I.10.3
    \begin{gather}
      F := \bigcap_{k=1}^{\infty}\bigcup_{j=1}^{n(k)} F_{jk}
    \end{gather} abgeschlossen
    
    \begin{enumerate}[I.]
     \item zeigen: $\mathcal{m}(F^{C}) < \varepsilon$
      
      \begin{gather}
	\mathcal{m}(F^{C}) =
	   \mathcal{m}(\bigcup_{k=1}^{\infty}\bigcap_{j=1}^{n(k)}
	   F_{jk}^{C}) \leq
	   \sum_{k=1}^{\infty}\mathcal{m}(\bigcap_{j=1}^{n(k)}F_{jk}^{C})
	   = \sum_{k=1}^{\infty} \mathcal{m}((\bigcup_{j=1}^{n(k)}
	   F_{jk})^{C}) =
	   \sum_{k=1}^{\infty}[\mathcal{m}(\bigcup_{j=1}^{n(k)} D_{jk}
	   \cap (\bigcup_{j=1}^{n(k)}
	   F_{jk})^{C})+\mathcal{m}((\bigcup_{j=1}^{n(k)} D_{jk})^{C}\cap
	   (\bigcup_{j=1}^{n(k)} F_{jk})^{C})] <
	   \sum_{k=1}^{\infty}[\mathcal{m}(\bigcup_{j=1}^{n(k)}
	   (D_{jk}\setminus F_{jk}))+\varepsilon2^{-k-1}]\\
	= \sum_{k=1}^{\infty}[\sum_{j=1}^{n(k)}
	   \underbrace{\mathcal{m}(D_{jk} \setminus
	   F_{jk})}_{<\varepsilon 2^{-k-j-1}}+\varepsilon+2^{-k-1}] < \varepsilon
      \end{gather}
     \item zeigen $f_{|F}$ stetig
      
      $F\cap F_{1k},\ldots,F \cap F_{n(k)k}$ Zerlegung von $F$ $f_{k} :=
      \sum_{j=1}^{n(k)} a_{j}\cdot1_{F\cap F_{jk}}$ auf $F$
      
      Beh.: $f_{k}$ stetig (auf $F$)
      
      \begin{gather}
	x, x_{l}\in F, x_{l}\rightarrow^{l\rightarrow\infty}x \Rightarrow
	   \exists j, \exists l_{0} l \geq l_{0}: x_{l}\in F \cap F_{jk}\\
	\Rightarrow f_{k}(x) = a_{j} ( j so bestimmt, da"s x\in F\cap F_{jk})\\
	f_{k}(x_{l}) = a_{j}, l\geq l_{0} \Rightarrow
	   \lim_{l\rightarrow\infty} f_{k}(x_{l}) = f_{k}(x)
      \end{gather}
      
      beh.: $d(f_{k}(x), f(x)) \leq 2^{-k}, x\in F$
      
      \begin{gather}
	x\in F \Rightarrow \exists j=1,\ldots,n(k): x\in F\cap F_{jk}
	   \Rightarrow f_{k}(x) = a_{j}\\
	x \in F\cap F_{jk} \subseteq D_{jk} \subseteq B_{jk} =
	   f^{-1}(S_{2^{-k}}(a_{j})) \Rightarrow f(x) \in
	   S_{2^{-k}}(a_{j}) \Rightarrow d(f(x), a_{j}) < 2^{-k}
      \end{gather}
      
      Es folgt $f_{k}\rightarrow^{k\rightarrow\infty}f_{|F}$
      gleichm"a"sig $\Rightarrow$ $f_{|F}$ stetig
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{beispiel}
  \begin{gather}
    \int_{0}^{\infty}\frac{\sin x}x\,dx (=\frac{\pi}2)
  \end{gather}
  
  \begin{itemize}
    Das Integral existiert nicht als Lebesgue-Integral:
    \begin{gather}
      f(x) = \frac{\sin x}x \Rightarrow \int_{0}^{\infty}
	 f^{+}(x)\lambda(dx) = \infty, \int_{0}^{\infty}
	 f^{-}(x)\lambda(dx) = \infty
    \end{gather}
    
    \begin{align}
      \int_{0}^{\infty} f^{+}(x)\lambda(dx) =
	 \sum_{k=0}^{\infty}\int_{2\pi k}^{2\pi k+\pi}f(x)\,dx \geq
	 \sum_{k=0}^{\infty} \frac{1}{2\pi k+\pi} \int_{0}^{\pi} \sin
	 x\,dx \geq \int_{0}^{\pi} \sin x\,dx \sum_{k=0}^{\infty}
	 \frac{1}{2\pi k+\pi} (Harmonische Reihe) = \infty 
    \end{align}
    
    Analog: $\int_{0}^{\infty}f^{-}(x)\lambda(dx) = \infty$
   \item Integral existiert als uneigentliches Riemann-Integral
    
    zu zeigen:
    $\int_{0}^{a}\frac{\sin x}x\,dx$ ist Cauchy-Folge
    
    \begin{gather}
      \int_{a}^{b}\frac{\sin x}{x}\, dx = [-\frac{\cos x}{x}]_{a}^{b} -
	 \int_{a}^{b}(-\cos x)(\frac{1}{x^{2}}\,dx = \frac{\cos a}{a} -
	 \frac{\cos b}{b} - \int_{a}^{b}\frac{\cos x}{x^{2}}\,dx\\
      |\int_{a}^{b}\frac{\sin x}{x} \leq \frac1a+\frac1b +
	 \int_{a}^{b}\frac1{x^{2}}\,dx \leq \frac1a +
	 \frac1b+\frac1a+\frac1b \rightarrow^{a,b\rightarrow\infty}0
    \end{gather}
    $\Rightarrow$ obiges Integral $\int_{a}^{b}\frac{\sin x}{x}\,dx$ ist CF.
  \end{itemize}
\end{beispiel}
