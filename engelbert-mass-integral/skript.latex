\begin{align}
  &t: [a,b] \rightarrow t([a,b])\quad \text{stetig diff'bar}\\
  &f: t([a,b]) \rightarrow \R\quad \text{statig}
\end{align}
\begin{gather}
  \int_{a}^{b}f(t(x))t'(x)\,dx=\int_{t(a)}^{t(b)}f(y)\,dy, \quad y=t(x)
\end{gather}

Sei $t$ zus"atzlich umkehrbar eindeutig:
\begin{gather}
  \int_{a}^{b}f(t(x))|t'(x)|\,dx=\int_{t(a)}^{t(b)}f(y)\,dy, \quad y=t(x)
\end{gather}
\begin{proof}
  \begin{enumerate}[1.Fall]
   \item $t$ streng monoton wachsend
   \item $t$ streng monoton fallend, da t'(x) < 0 und t(a) > t(b)
  \end{enumerate}
\end{proof}

\begin{satz} % 1
  \begin{enumerate}
   \item Das Lebesgue-Ma"s $\lambda$^{n}$$ auf $(\R^{n},
    \mathcal{B}(\R^{n}))$ ist verschiebungsinvariant
   \item Es sei $\mathcal{m}$ ein Ma"s auf $(\R^{n},
    \mathcal{B}(\R^{n}))$ mit den folgenden Eigenschaften:
    \begin{enumerate}
     \item $\mathcal{m}$ ist verschiebungsinvariant
     \item $\mathcal{m}([0,1]^{n})=1$
    \end{enumerate}
    Dann gilt: $\mathcal{m}=\lambda^{n}$
  \end{enumerate}
  
  \begin{proof}
    vgl. Satz II.8.2+3, "Ubungsaufgabe (Serie 11)
  \end{proof}
\end{satz}

\begin{lemma}
  Es existiert eine Zerlegung 
  \begin{gather}
    T=S\circ O
  \end{gather}
  mit $S$ selbstadjungierter lienarer Operator mit nichtnegativen
  Eigenwerten und einer orthogonalen Transformation $O$.
  
  \begin{proof}
    "Ubungsserie
  \end{proof}
\end{lemma}

\begin{satz}
  $\lambda^{n}$ sei das Lebesgue-ma"s auf $(\R^{n}, \mathcal{B}(\R))$,
  $T$ lineare Abbildung des $\R^{n}$ in sich.
  
  \begin{enumerate}
   \item A\in $\mathcal{B}(\R^{n}) \Rightarrow T(A)\in
    \mathcal{B}^{\lambda^{n}}(\R^{n})$ (Vervollst"andigung von
    $\mathcal{B}(\R^{n})$)
   \item A\in $\mathcal{B}(\R^{n}) \Rightarrow \lambda^{n}(T(A))=|det
    T|\,\lambda^{n}(A)$
  \end{enumerate}
  
  \begin{proof}
    \begin{enumerate}
     \item Es gelte zun"achst T($\R^{n}$) = $\R^{n}$
      
      $T^{-1}$ Umkehrabbildung: $T^{-1}$ ist linear und somit stetig
      
      \begin{gather}
	T(A) = (T^{-1})^{-1}(A)
      \end{gather}(volle Urbild von $A$ bzgl. $T^{-1}$)
      
      $\Rightarrow$ T(A)\in $\mathcal{B}(\R^{n})$
     \item dim(T($\R^{n}$)) < n:
      
      $H=T(\R^{n})$ abgeschlossener Unterraum $\Rightarrow$ $H\in
      \mathcal{B}(\R^{n})$
      
      z.z.: $\lambda^{n}(H) = 0$ (Daraus folgt wegen T(A)$\subseteq$ H:
      T(A)\in $\mathcal{B}^{\lambda^{n}}(\R^{n})$ und $\lambda^{n}(T(A))=0$)
      
      $dim(H)=k < n$, sei $x_{1},\ldots,x_{k}$ orthonormierte Basis von
      $H$, $x_{1},\ldots,x_{k},x_{k+1},\ldots,x_{n}$ Erg"anzung einer
      orthonormierten Basis des $\R^{n}$
      
      setze $Ox_{i} := e_{i}, i=1,\ldots,n, e_{1},\ldots,e_{n}$ kanonische
      Basis des $\R^{n}$, $O$ definiert eine orthogonale Transformation
      des $\R^{n}$
      
      \begin{gather}
	O(H)=\{y\in\R^{n}: y=(y_{1},\ldots,y_{n})^{T},y_{k+1}=\ldots=y_{n}=0\}
      \end{gather}

      Es gilt:
      \begin{gather}
	\lambda^{n}(O(H)) = \lambda^{n}(H) (vgl. kommender Punkt c)
      \end{gather}
      
      g.z.z.: $\lambda^{n}(O(H))=0$
      \begin{gather}
        O(H)=\bigcup_{N=1}^{\infty}[-N,N]^{k}\times\{0\}\times\ldots\times\{0\}
      \end{gather}
      $\lambda^{n}([-N,N]^{k}\times\{0\}\times\ldots\times\{0\})=
      (2N)^{k} \bigcap_{i=k+1}^{n}\lambda(\{0\})=0$
     \item T sei orthogonal
      $\mu$(A) := $\lambda^{n}(\underbrace{T(A)}_{\in \mathcal{B}(\R)}),
      A\in \mathcal{B}(\R^{n})$
      
      $\mu$ Ma"s auf $(\R^{n}, \mathcal{B}(\R^{n}))$, $\mu$
      verschiebungsinvariant, 0 < $\mu$([0,1]^{n})<$\infty$
      
      \begin{gather}
	\frac{\mu}{\mu([0,1]^{n})}
      \end{gather} verschiedungsinvariant und geeicht
      
      $\Rightarrow^{Satz 1} $
      \begin{gather}
	\frac{\mu}{\mu([0,1]^{n})} = \lambda^{n}\\
	\lambda^{n}(T(A)) = \mu([0,1]^n)\lambda^{n}(A), A\in \mathcal{B}(\R^{n})\\
	B := \{ x\in $\R^{n}$: <x,x> = \|x\|^{2} < 1\}
      \end{gather}
      
      $T(B)=B$ (T,$T^{-1}$ orthogonal)
      T(B)=\{T(x): x\in B\} $\subseteq$ B
      
      z.z. $T(x) \in B$, d.h. $<T(x), T(x)>=<x,x> < 1$
      
      Einsetzen von $B$: $\lambda^{n}(T(B)) = \lambda^{n}(B) = \mu([0,1]^{n}) \lambda^{n}(B)$
      $\Rightarrow$ $\mu([0,1]^{n})=1$ $\Rightarrow$ Beh.
     \item Sei T eine Diagonalmatrix
      \begin{gather}
	T = \begin{pmatrix}
	      \lambda_{1}&&0\\
	      0& \ddots & 0\\
	      0&& \lambda_{n}
	    \end{pmatrix}
      \end{gather}
      $\mu$(A) = $\lambda^{n}(T(A)), A\in \mathcal{B}(\R^{n})$, $\mu$
      verschiebungsinvariant, 0 < $\mu$([0,1]^{n})<$\infty$
      
      $\Rightarrow^{Satz 1}$
      \begin{gather}
	\frac{\mu}{\mu([0,1]^{n})} = \lambda^{n} \Rightarrow
	   \lambda^{n}(T(A)) = \mu([0,1]^{n})\lambda^{n}(A),
	   A\in\mathcal{B}(\R^{n})\\
	\mu([0,1]^{n}) = \lambda^{n}(T([0,1]^{n})) = \lambda^{n}
	   (Rechtecke der Seitenl"ange |\lambda_{i}|) =
	   \prod_{i=1}^{n}|\lambda_{i}| = |\det T|
      \end{gather}
      
     \item T beliebige lineare Transformation
      \begin{enumerate}
       \item $dim(T(\R^{n})) < n: A\in \mathcal{B}(\R^{n}) \Rightarrow
	T(A) \in \mathcal{B}^{\lambda^{n}}(\R^{n})$ und
	$\lambda^{n}(T(A)) =^{b} 0 = |det(T)|\lambda^{n}(A)$
       \item $T(\R^{n}) = \R^{n}$
	Lemma 1: $T=S\circ O$, $S$ selbstadjungiert mit nichtnegativen
	Eigenwerten, $O$ orthogonal
	
	$S= U\circ D\circ U^{T}$, $U$ orthogonal, $D$ Diagonalmatrix
	(Hauptachsentransformation)
	$\Rightarrow$ Darstellung $T= U\circ D\circ
	\underbrace{U^{T}\circ O}_{= \vartilde{O}}$
	
	\begin{align}
	  \lambda^{n}(T(A)) =
	     \lambda^{n}(U(\underbrace{D(\vartilde{O}(A))}_{\in
	     \mathcal{B}(\R^{n})}))\\
	  =^{c} \lambda^{n}(D(\underbrace{\tilde{O}(A)}_{\in
	     \mathcal{B}(\R^{n})}))\\
	  =^{d} |det D| \lambda^{n}(\vartilde{O}(A)) =^{c}|det D| \lambda^{n}(A)
	\end{align}
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{folgerung}
  Das Lebesgue-ma"s $\lambda^{n}$ auf ($\R^{n}, \mathcal{B}(\R^{n}))$ ist
  invariant bzgl. beliebiger Bewegungen (Drehungen, Spiegelung,
  Translationen)
\end{folgerung}

\subsection{Nichtlineare Transformationen}

$G\subseteq \R^{n} \text{offen}, t: G\rightarrow\R^{n} \text{Abbildung},
G':= t(G), t=(t_{1},\ldots,t_{n})$

Vorraussetzungen:
\begin{enumerate}
 \item $t$ umkehrbar eindeutig
 \item $t$ stetig diff'bar,
  \begin{gather}
    t'(x) = \big(\frac{\partial t_{i}(x)}{\partial
       x_{i}}\big)_{i=1,\ldots,n}
  \end{gather}
 \item F(x) = det t'(x) $\not=0, x\in G$
\end{enumerate}

\begin{bemerkung}
  Es gelten dann auch:
  \begin{itemize}
   \item Umkehrabbildung $t^{-1}$ ist stetig diff'bar,
    ($t^{-1})'(t(x))=(t'(x))^{-1, x\in G}$
   \item $t^{-1}$ stetig
   \item $G' = t(G)$ offen
  \end{itemize}
  
  Folgerungen aus em Satz "uber impliziete Funktionen, Vorraussetzungen
  sind symetrisch in t und $t^{-1}$
\end{bemerkung}

\begin{lemma} %2
  Sei $r$ eine wachsende Funktion f"ur die gilt:
  \begin{gather}
    \lim_{a|v 0} r(a) = 0
  \end{gather}
  
  \begin{gather}
    |R_{x}(z) \leq n\, r(|z-x|)|z-x|, z\in S_{a}(x)\subseteq G
  \end{gather}
  
  \begin{proof}
    \begin{align}
      t_{i}(z)-t_{i}(x) &=
	 \sum_{k=1}^{n}[t_{i}(z_{1},\ldots,z_{k},x_{k+1},\ldots,x_{n}) -
	 t(z_{1},\ldots,z_{k-1},x_{k},\ldots,x_{n})]\\
      &= \sum_{k=1}^{n} \frac{\partial t_{i}}{\partial x_{k}}
	 (z_{1},\ldots, z_{k-1},x_{k}+\theta_{k}(z_{k}-x_{k}),
	 x_{k+1},\ldots,x_{n})(z_{k}-x_{k}), \quad \theta_{k}\in [0,1],
	 k=1,\ldots,n (Mittelwertsatz)
    \end{align}
    
    Vergleich mit Taylorentwicklung in folgenden Satz:
    \begin{gather}
      R_{x}^{i}(z) = \sum_{k=1}^{n}[\frac{\partial t_{i}}{\partial
	 x_{k}}(z_{1},\ldots,z_{k-1},x_{k}+\theta_{k}(z_{k}-x_{k}),
	 x_{k+1},\ldots,x_{n})-\frac{\partial t_{i}}{\partial
	 x_{k}}(x)](z_{k}-x_{k})\\
      r(|z-x|) := \max_{i,k=1,\ldots,n}\sup_{y\in G,
	 |y-x|\leq|z-x|}|\frac{\partial t_{i}}{\partial
	 x_{k}}(y)-\frac{\partial t_{i}}{\partial x_{k}}(x)|\\
      |R_{x}(z)| \leq n\,r(|z-x|) |z-x|
    \end{gather}
    f"ur $a|v 0: r(a) |v 0$ (Gleichm. stetigkeit der pariellen Ableitungen), $r$ wachsend
  \end{proof}
\end{lemma}

\begin{satz} %3
  \begin{gather}
    A\in \mathcal{B}(\R^{n}), A\subseteq G \Rightarrow \lambda^{n}(t(A)) =
       \int_{A}|F(x)|\lambda^{n}(dx)
  \end{gather}
  
  \begin{proof}
    \begin{enumerate}[I]
     \item Zus"atzliche Annahme:
      \begin{enumerate}
	\setcounter{item}{3}
       \item $G$ beschr"ankt
       \item $det t'(x)$, $det (t^{-1})'(x)$ beschr"ankt
       \item partielle Ableitung von $t$ bzw. $t^{-1}$ sind gleichm"a"sig
	stetig und beschr"ankt auf $G$ bzw. $G'$
      \end{enumerate}
      
      Annahme: Sei Satz 3 g"ultig unter 1-6
      Betrachten $t: G\rightarrow G' mit 1-3$
      Es existiere $G_{m}|^ G: G_{m}$ offen, beschr"ankt,
      $\bar{G_{m}}\subseteq G (\Rightarrow \bar{G_{m}}$ kompakt)
      t: $G_{m} \rightarrow G'_{m} \rightarrow G_{m}'=t(G_{m}),
      \bar{G'_{m}}$ kompakt
      
      $\Rightarrow t: G_m \rightarrow G'_{m}$ erf"ullt 1-6
      
      Anwendung von Satz 3 f"ur $G_{m}:$
      \begin{gather}
	\lambda^{n}(t(A\cap G_{m})) = \int_{A\cap G_{m}}|det t'(x)|
	   \lambda^{n}(dx),\quad A\in \mathcal{B}(\R^{n}), A\subseteq G\\
	|v m\rightarrow\infty |v m\rightarrow\infty\\
	\lambda^{n}(t(A)) = \int_{A}|det t'(x)|\lambda^{n}(dx)
      \end{gather}
     \item $t:G\rightarrow G'$ erf"ulle 1-6
      
      x\in $\R^{n}: |x|=\max_{i=1,\ldots,n}|x_{i}|, x=(x_{1},\ldots,x_{n})$
      $|\cdot|$ Norm, $S_{a}(x) = \times_{i=1}^{n}(x_{i}-a, x_{i}+a)$
      (Vollkugel mit Zentrum $x$ und Radius $a>0$)
      
      Taylorentwicklung:
      w"ahlen $a>0$ so dass $z\in S_{a}(x) \subseteq G$
      \begin{gather}
	t(z) = t(x) + t'(x)(z-x)+R_{x}(z), x\in G
      \end{gather}
      
      insert Lemma 2
     \item $I_{m}\{x=(x_{1},\ldots,x_{n})\in \R^{n}:
      x_{i}=\frac{k_{i}}{2^{m}}, k_{i}\in \Z, i=1,\ldots,n\}$
      
      x$\in$ $I_{m}: W_{m}(x) = \times_{i=1}^{n}[x_{i},
      x_{i}+\frac1{2^{m}})$
      
      \begin{lemma} %3
	x$\in$ $I_{m}, W_{m}(x)\subseteq G, x'=(x_{1}',\ldots,x_{n}'),
	x_{i}'= x_{i}+\frac1{2^{m}}, i=1,\ldots,n$ (x' Mittelpukt von $W_{m}(x)$)
	
	b=($b_{1},\ldots,b_{n})^{T} := (t'(x))^{-1}t(x')$
	
	$\Rightarrow$ Es existiert
	$\varepsilon_{m}|v_{m\rightarrow\infty}0: t(W_{m}(x)) \subseteq
	t'(x')(\times_{i=1}^{n}[b_{i}-\frac{1+\varepsilon_{m}}{2^{m+1}},
	b_{i}+\frac{1+\varepsilon_{m}}{2^{m+1}}]$
	
	\begin{proof}
	  z\in $W_{m}(x)$ t(z)-t($x') = t'(x')(z-x')+R_{x'}(z)$
	  
	  $\Rightarrow$ (t'(x'))^{-1} t(z) -b = z-x'+(t'(x'))^{-1}($R_{x'}(z))$
	  
	  A Matrix, A=(a_{ij}) |Ax| $\leq$ $n\max_{i,j=1,\ldots,n}|a_{j}| |x|$
	  \begin{align}
	    |(t'(x'))^{-1}(t(z))-b| &\leq |z-x'|+|(t'(x'))^{-1} R_{x'}(z)|\\
	    &\leq \underbrace{|z-x'|}_{\frac1{2^{m+1}}}+n\, 
	       \underbrace{\max_{i,j=1,\ldots,n}|\frac{\partial
	       s_{i}}{\partial x_{j}}(t(x'))|}_{\leq M} |R_{x'}(z)| (s=
	       (s_{1},\ldots,s_{n})=t^{-1})\\
	    &\leq \frac1{2^{m+1}}+n\cdot M\cdot n\cdot
	       r(\underbrace{|z-x'|}_{\leq \frac1{2^{m+1}}})
	       \underbrace{|z-x'|}_{\leq \frac1{2^{m+1}}}\\
	    & \frac{1}{2^{m+1}}+n^{2}\cdot M\cdot
	       r(\frac{1}{2^{m+1}})\frac{1}{2^{m+1}} (Lemma 2)\\
	    &= \frac{1}{2^{m+1}}[1+\varepsilon_{m}]
	  \end{align}
	  $\Rightarrow$ (t'(x'))^{-1}(t(z))\in
	  $\times$_{i=1}^{n}[$b_{i}-\frac{1}{2^{m+1}}(1+\varepsilon_{m}),
	  b_{i}+\frac{1}{2^{m+1}}(1+\epsilon_{m})$
	  $\Rightarrow$ t(z$\in$ $W_{m}(z)$)$\in t'(x') (\times_{i=1}^{n}[$b_{i}-\frac{1}{2^{m+1}}(1+\varepsilon_{m}),
	  b_{i}+\frac{1}{2^{m+1}}(1+\epsilon_{m})
	\end{proof}
      \end{lemma}
      
      Setzen $\lambda = \lambda^{n}$
      
      $\lambda(t(G)) = \lambda(t(\bigcup_{m=1}^{\infty}G_{m})) =
      \lambda(\bigcup_{m=1}^{\infty}t(G_{m})) = \lim_{m\rightarrow\infty}
      \lambda(t(G_{m})) =
      \lim_{m\rightarrow\infty}\lambda(t(\bigcup_{x\in G,
      W_{m}(x)\subseteq G}W_{m}(G))) =
      \lim_{m\rightarrow\infty}\lambda(\bigcup_{x\in G, W_{m}(x)\subseteq
      G} t(W_{m}(x))) = \lim_{m\rightarrow \infty}\sum_{x\in G,
      W_{m}(x)\subseteq G} \lambda(t(W_{m}(x))) \leq^{Lemma 3}
      \liminf_{m\rightarrow\infty} \sum_{x\in G, W_{m}(x)\subseteq G}
      \lambda(t'(x')(\times_{i=1}^{n}[b_{i}-
      \frac{1+\varepsilon_{m}}{2^{m+1}},
      b_{i}+\frac{1+\varepsilon_{m}}{2^{m+1}}])) =
      \lim_{m\rightarrow\infty} \sum_{x\in G, W_m(x)\subseteq G} |det
      t'(x'))| (\frac{1+\varepsilon_{m}}{2^{m+1}})^{n} =
      \liminf_{m\rightarrow\infty} \sum_{x \in G, W_{m}(x)\subseteq
      G}|det t'(x')|\lambda(W_{m}(x))(1+\varepsilon_{m})^{n} =
      \liminf_{m\rightarrow\infty}\int_{\R^{n}} F_{m}(z) \lambda(dz)
      (1+\varepsilon_{m})^{n}$
      
      $F_{m}(z) = \sum_{x\in G, W_{m}(x)\subseteq G} |det t'(x')|
      1_{W_{m}(x)}(z)$
      
      $=\int_{\R^{n}}|det t'(z)|\cdot 1_{G}(z) \lambda(dz)$
      
      (Anwendung des Satzes von Lebesgue:
      \begin{enumerate}
       \item $\lim_{m\rightarrow\infty}F_{m}(z) = |det t'(z)|\cdot
	1_{G}(z), z\in \R^{n}$
       \item 0$\leq F_{m}(z) \leq \sup_{y\in G}|det t'(y)|\cdot 1_{G}(z)$
	integrierbar bzgl. $\lambda$
      \end{enumerate})
      
      also: $(1) \lambda(t(G)) \leq \int_{G}|det t'(z)\lambda(dz)$
      
      U$\subseteq$ G offen: $t_{|U}: U\rightarrow t(U)$ erf"ullt alle
      Vorraussetzungen, folglich erhalten wir wie (1): (2) $\lambda(t(U))
      \leq \int_{U}|det t'(z)| \lambda(dz)$
     \item A$\subseteq$ G, A\in $\mathcal{B}(G)$
      $\lambda(A) = \inf_{A\subseteq U\subseteq G, U \text{offen}}
      \lambda(U)$ (Satz I.10.4, Folgerung 1)
      
      $\varepsilon > 0$
      W"ahlen $U$ offen: $A\subseteq U\subseteq G$ mit $\lambda(U) \leq
      \lambda(A)+\varepsilon, \lambda(A)<\infty$
      
      $\lambda(t(A))\leq\lambda(t(U))\lambda^{(2)}\int_{U}|det
      t'(z)|\lambda(dz)=\int_{A}|det t'(z)|\lambda(z)+\underbrace{\int_{U\setminus
      A}\underbrace{|det t'(z)|}_{\leq c} \lambda(dz)}_{c\,
      \lambda(U\setminus A)\leq c\,\varepsilon}$
      
      (3) $\lambda(t(A)) \leq \int_{A}|det t'(z)|\lambda(dz), A\in
      \mathcal{B}(G), A\subseteq G$
     \item s=$t^{-1}$ Umkehrabbildung, s: G'$\rightarrow$ G mit 1)-6)
      $\lambda_{s}$ Bildma"s auf (G, $\mathcal{B}(G))$ von
      $\lambda_{|G'}$ bzgl $s$
      $\lambda_{s}(A)=\lambda(t(A)), A \in \mathcal{B}(G), A\subseteq G$
      $\lambda_{s}(A) = \lambda_{|G'}(s^{-1}(A))=\lambda_{|G'}(t(A))$
      
      Aus (3) folgt:  $\lambda_{s} \ll \lambda_{|G}$ Sei f(z) = \frac{d
      $\lambda_{s}$}{d $\lambda_{|G}}(z)$ Dichte
      
      $\Rightarrow$ $\lambda(t(A)) = \int_{A}f(z) \lambda(dz)
      \leq^{(3)}\int_{A}|det t'(z)| \lambda(dz), A\in \mathcal{G}$
      $\Rightarrow \int_{A} [|det t'(z)|-f(z)]\lambda(dz) \geq 0, \forall
      A\in \mathcal{B}(G)$
      
      $A_{0}:= \{z\in G: |det t'(z)|-f(z)<0\}$
      $\Rightarrow$ $\int_{A}[|det t'(z)|-f(z)]\lambda(dz) \leq 0
      \Rightarrow =0$
      
      $\int_{G}[|det t'(z)|-f(z)] 1_{A_{z}}(z)\lambda(dz) = 0 \Rightarrow
      [|det t'(z)|-f(z)] 1_{A_{z}}(z)=0$ $\lambda$ f"u. auf $G$
      
      $\Rightarrow$ (4) f(z) $\leq$ |det t'(z)| $\lambda$-f"u. auf G
     \item (Vorraussetung der Rollen von s und t)
      $\lambda_{t}$ Bildma"s auf (G', $\mathcal{B}(G))$ von
      $\lambda_{|G}$ bzgl. $t$
      
      analog zu V: $\lambda_{t}\ll \lambda_{|G'}$ und \frac{d
      $\lambda_{t}$}{d$\lambda_{|G'}}(y) \leq |det s'(y)|, y \in G',
      \lambda$-f"u
      s'(y)-($t^{-1})'(y) = (t'(t^{-1}(y)))^{-1}$
      
      (5) \frac{d $\lambda_{t}$}{d $\lambda_{|G'}}(y) |det
      t'(t^{-1}(y))|^{-1}, y\in G', \lambda-$f"u.
     \item Zusammenstellung
      $\lambda$(t(A)) $\leq^{(3)} \int_{A}|det t'(z)| \lambda(dz)
      =^{Transformationssatz 6.2} \int_{t(A)} |det
      t'(t^{-1}(y))|\lambda_{t}(dy) =^{S.11.4} \int_{t(A)}|det
      t'(t^{-1}(y))|\underbrace{\frac{d \lambda_{t}(y)}{d
      \lambda_{|G'}}(y)}_{\leq^{(4)} |det t'(t^{-1}(y))|^{-1}}
      \lambda(dy) \leq \int_{t(A)}\lambda(dy)\leq \lambda(t(A))$
      
      $\Rightarrow$ $\lambda(t(A)) = \int_{A}|det t'(z)| \lambda(dz),
      A\in \mathcal{B}(G)$
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{bemerkung}
  G$\subseteq$ $\R^{n}, t:G\rightarrow \R^{n}$
  
  $G^{(0)}$ Kern von $G$ (Menge der inneren Punkte von $G$)
  
  \begin{enumerate}
   \item $t_{|G^{(0)}}$ erf"ullt die Bedingungen 1)-3)
   \item $\lambda(G\setminus G^{(0)}) = 0, \lambda(t(G)\setminus
    t(G^{(0)})=0$
    
    insbesondere:
    $\lambda(\partial G)=0, \lambda(\partial t(G))=0$
    
    t stetig $\Rightarrow$ (t(G))^{(0)}=t($G^{(0)}$), $\partial t(G) =
    \bar{t(G)}-t(G^{(0)}$
  \end{enumerate}
  $\Rightarrow$ Unter diesen Vorraussetzungen gelten sinngem"a"s die
  S"atze 3-5
\end{bemerkung}

\begin{bemerkung}
  \begin{enumerate}
   \item
    \begin{gather}
      t(A) = (t^{-1})^{-1}(A)\in \mathcal{B}(\R^{n})
    \end{gather}
   \item $t$ sei linear, $t(x) = Tx$, $T$ $n\times n$-Matrix
    $t'(x)=T \Rightarrow \lambda^{n}(t(A))=\int_{A}|det
    T|\lambda^{n}(dx)=|det T|\lambda^{n}(A)$ 
    vgl. Satz 2
   \item $\lambda^{n}(t(A)) = \lambda^{n}_{t^{-1}} (A), A\in
    \mathcal{B}(\R^{n})$
  \end{enumerate}
  
  $\lambda^{n}_{t^{-1}}$ Bildma"s des Ma"ses $\lambda^{n}_{|G'}$
  (Einschr"ankung von $\lambda^{n}$ auf $G'$, Ma"s auf (G',
  $\mathcal{B}(G')), \lambda^{n}_{t^{-1}}$ Ma"s auf (G, $\mathcal{B}(G))$
\end{bemerkung}

\begin{satz} %4
  (Folgerung aus Satz 3)
  
  Sei $\lambda^{n}_{t^{-1}}$ das Bildma"s von $\lambda^{n}_{|G'}$ auf
  $(G, \mathcal{B}(G))$.
  
  Dann gilt:
  \begin{gather}
    \lambda^{n}_{t^{-1}}\ll \lambda^{n}_{|G}
  \end{gather}
  und
  \begin{gather}
    \frac{d \lambda^{n}_{t^{-1}}}{d \lambda^{n}_{|G}}=|det t'(x)|
       (=|F(x)|), x\in G
  \end{gather}
\end{satz}

\begin{satz}[Substitutionsregel f"ur das Lebesgue-Integral] %5
  Sei $f$ me"sbar auf $(G', \mathcal{B}(G'))$.
  
  Dann folgt:
  \begin{gather}
    \int_{G}f(y) \lambda^{n}(dy) = \int_{G} f(t(x)) |det t'(x)
       \lambda^{n}(dx)
  \end{gather}
  (falls eines der beiden Integrale existiert, so existiert auch das
  zweite und es gilt die Gleichheit)

  \begin{proof}
    oBdA $f\geq 0$
    
    \begin{gather}
      \int_{G'}f(y) \lambda^{n}(dy) = \int_{G'}
	 f(t(t^{-1}(y)))\lambda^{n}(dy) = \int_{G} f(t(x))
	 \lambda^{n}_{t^{-1}}(dx) =^{Satz 11.4} \int_{G} f(t(x)) \frac{d
	 \lambda^{n}_{t^{-1}}}{d \lambda^{n}_{|G}}(x)\, d\lambda^{n}_{|G}
	 = \int_{G} f(t(x)) |F(x)| \lambda^{n}(dx)
    \end{gather}
  \end{proof}
\end{satz}

