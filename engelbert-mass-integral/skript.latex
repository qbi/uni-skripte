\subsection{Eindeutigkeitssatz f"ur ma"se}
$(\Omega, F)$ me"sbarere Raum, $m_{1}, m_{2}$ Ma"se auf $(\Omega, F)$

\underline{Problem:} dS\subseteq F Mengensystem, m_{1}(A)=m_{2}, \forall A\in dS
\xrightarrow{?} m_{1}=m_{2} (auf F)

\begin{defini}\\
  Ein Ma"s $m$ auf $(\Omega, F)$ hei"st endlich, falls
  $m(\Omega)<+\infty$ (und somit $m(A)<+\infty, \forall A\in F$) gilt.
\end{defini}

\begin{satz}\\
  Es seien $m_{1}, m_{2}$ \emph{endliche} Ma"se auf $(\Omega, F),
  dS\subseteq 2^{\Omega}$ mit folgenden Eigenschaften:
  \begin{itemize}
   \item dS $\pi$-System
   \item $\Omega\in dS$
   \item $\sigma(dS)=F$
   \item $m_{1}(A)=m_{2}(A), A\in dS$
  \end{itemize}
  
  Dann folgt: $m_{1}=m_{2}$ (auf F)
  
  \begin{proof}\\
    $\mathcal{D}=\{A\in F: m_{1}(A)=m_{2}(A)\}$
    \begin{enumerate}[a)]
     \item dS\in cD
     \item cD ist ein Dynkin-System:
      \begin{enumerate}[(i)]
       \item $\Omega\in dS\subseteq cD \Rightarrow \Omega\in cD$
       \item A\in cD $\Rightarrow A^{C}\in cD:
	m_{1}(A)=m_{1}(\Omega)-m_{1}(A) = m_{2}(\Omega)-m_{2}(A)=m_{2}(A^{C})$
       \item $A_{k}\in cD, k\geq1, $ paarw. disj. $\Rightarrow$
	$m_{1}(\cup_{k=1}^\infty
	m_{1}(A_{k})=\sum_{k=1}^{\infty}m_{2}(A_{k})
	=m_{2}(\cup_{k=1}^{\infty}A_{k}) \Rightarrow
	\cup_{k=1}^{\infty}A_{k}\in cD$
	
	$\xrightarrow{S. I.5.2.} \sigma(dS)\in cD \Rightarrow F\subseteq cD$
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{defini}\\
  \begin{enumerate}[(i)]
   \item dS$\subseteq 2^{\Omega}, m$ Ma"s auf ($\Omega$, F), m hei"st
    $\sigma$-endlich \emph{auf} dS, falls $\exists \Omega_{n}\in dS$ mit
    $\Omega_{n}\subseteq\Omega_{n+1}, n\geq1$ und
    $\Omega=\cup_{n=1}^{\infty}\Omega_{n}$, sowie m($\Omega_{n}$)<+$\infty$
   \item m hei"st $\sigma$-endl., falls m $\sigma$-endl auf F ist
  \end{enumerate}
\end{defini}

\begin{satz}\\
  Seien $m_{1},m_{2}$ MA"se auf ($\Omega, F$), dS$\subseteq$ $2^{\Omega}$
  mit folgenden Vorraussetzungen:
  \begin{enumerate}[1)]
   \item dS $\pi$-System
   \item $m_{1}, m_{2}$ sind $\sigma$-endl. \emph{auf dS}
   \item $\sigma(dS)=F$
   \item $m_{1}(A)=m_{2}(A), A\in dS$
  \end{enumerate}
  
  Dann gilt: $m_{1}=m_{2}$ (auf F)

  % 30.4.
  \begin{proof}
    $\exists \Omega_{i}^{(n)}\in dS,
    \Omega_{i}^{(n)}\uparrow_{n\rightarrow\infty} \Omega,
    dM_{i}(\Omega_{i}^{(n)})<+\infty, \forall n\geq1, i=1,2$
    
    $\Omega^{(n)}:=\Omega_{1}^{(n)}\cap\Omega_{2}\in dS,
    \Omega^{(n)}\uparrow_{n\rightarrow\infty}\Omega,
    m_{1}(\Omega^{(n)})<+\infty, m_{2}(\Omega^{(n)})<+\infty$
    
    m_{1}^{(n)},m_{2}^{(n)} endliche Ma"se auf (\Omega,F), dS' :=
    dS$\cup$\{$\Omega$\}
    
    $m_{1},m_{2},dS'$ erf"ullen Vorraussetzung von Satz 1 $\Rightarrow$
    $m_{1}^{(n)}=m_{2}^{(n)} auf F \forall n$
    $\Rightarrow m_{1}(A\cap\Omega^{(n)})=m_{2}(A\cap\Omega^{(n)}), A\in
    F$\\
    \downarrown$n\rightarrow$$\infty$
    \downarrown$n\rightarrow$$\infty$ (stetigkeit von unten von $m_{1},
    m_{2}$)\\
    $m_{1}(A)=m_{2}(A), A\in F$
  \end{proof}
\end{satz}

\subsection{Fortsetzungssatz von Caratheodory}
C. Caratheodory (1873-1950)

\begin{satz}[Fortsetzungssatz]
