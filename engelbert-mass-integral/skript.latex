  Berechnung: $\int_{\Omega}(f+g)^{+}dm = \int_{\Omega} (f+g)\cdot
  1_{\{f+g\geq 0\}}dm = \int_{\Omega} (f^{+}-f^{-}+g^{+}-g^{-})\cdot
  1_{\{f+g\geq0\}} dm = \int_{\Omega}\underbrace{[(f^{+}+g^{+})
  1_{\{f+g\geq0\}} - (f^{-}+g^{-})1^{\{f+g\geq 0\}}]}_{\geq0}dm$
  
  Zwischenschritt: $\int_{\Omega} (f^{-}+g^{-}) 1_{\{f+g\geq0\}}dm <+\infty$
  \begin{enumerate}[1. Fall]
   \item Sei (3) erf"ullt $\Rightarrow$ $\int_{\Omega}
    (f^{-}+g^{-})1_{\{f+g\geq0\}}dm \leq_{Monotonie} \int_{\Omega}
    (f^{-}+g^{-})dm < \infty$
   \item Sei (2) erf"ullt
    \begin{gather}
      \int_{\Omega} (f^{-}+g^{-})1_{\{f+g\geq0\}} dm \leq^{Monotonie} \int_{\Omega}
	 (f^{+}+g^{+})1_{\{f+g\geq0\}}dm \leq \int_{\Omega}
	 (f^{+}+g^{+})dm < +\infty
    \end{gather}
  \end{enumerate}
  
  Anwendung von II:
  $\Rightarrow$$\int_{\Omega}[f+g]^{+}dm =
  \int_{\Omega}(f^{+}+g^{+})1_{\{f+g\geq0\}}dm -
  \int_{\Omega}(f^{-}+g^{-})1_{\{f+g\geq0\}} dm =
  \int_{\Omega}f^+1_{\{f+g\geq0\}} dm+ \int_{\Omega}
  g^{+}1_{\{f+g\geq0\}} dm - \int_{\Omega} f^{-}1_{\{f+g\geq0\}}dm -
  \int_{\Omega} g^{-}1_{\{f+g\geq0\}}dm$
  
  Analog: $\int_{\Omega}(f+g)^{-}dm = \int_{\Omega}
  (-f-g)1_{\{f+g\geq0\}}dm = \ldots =
  \int_{\Omega}f^{-}1_{\{f+g\geq0\}}dm
  +\int_{\Omega}g^{-}1_{\{f+g\geq0\}}dm
  -\int_{\Omega}f^{+}1_{\{f+g\geq0\}}dm- \int_{\Omega}g^{+}1_{\{f+g\geq0\}}dm$
  
  Es folgt aus Alternative (2) oder (3)
  \begin{gather}
    \int_{\Omega}(f+g)^{+}dm<+\infty \text{oder} \int_{\Omega}
       (f+g)^{-}dm < +\infty
  \end{gather}
  Daraus folgt:
  \begin{gather}
    \exists\int_{\Omega} (f+g)dm \text{und}\\ \int_{\Omega}(f+g)dm :=
       \int_{\Omega}(f+g)^{+}dm - \int_{\Omega}(f+g)^{-} dm =
       \int_{\Omega}f^{+}dm + \int_{\Omega} g^{+}dm -\int_{\Omega}f^{-}dm
       -\int_{\Omega}g^{-}dm = \int_{\Omega}f\,dm+\int_{\Omega} g\,dm
  \end{gather}
  
  E4 (Monotonie)
  f,g me"sbar, f$\leq$g, $\int_{\Omega}f\,dm, \int_{\Omega}g\,dm$
  existieren.
  
  Dann folgt:
  \begin{gather}
    \int_{\Omega} f\,dm\leq\int_{\Omega} g\,dm
  \end{gather}
  
  \begin{proof}
    oBdA.:$\int_{\Omega}f\,dm>-\infty, \int_{\Omega} g\,dm<+\infty$
    
    $\Rightarrow$ m(\{f=-$\infty$\}) = m(\{$f^{-}=+\infty\})=0$ (da
    $\int_{\Omega}f^{-}dm<+\infty)$
    
    m(\{g=+$\infty$\})=m(\{$g^{+}=+\infty\})=0$
    
    Setzen: h=\begin{case}
		g-f& auf \{-$\infty$<f,g<+$\infty$\}\\
		0& sonst
	      \end{case}, h$\geq$0
    
    0$\leq_{Positiv.}$$\int_{\Omega}h\,dm =_{Linearit"at} \int_{\Omega}
    g\,1^{\{-\infty<f,g<+\infty\}}dm-\int_{\Omega}f\,1^{\{-\infty<f,g<+\infty\}}dm
    =\int_{\Omega}g\,dm -\int_{\Omega} f\,dm $
  \end{proof}
  
  E5 f,g integrierbar, f$\leq$g
  
  \begin{gather}
    \int_{\Omega} f\,dm = \int_{\Omega}g\,dm \Leftrightarrow f=g\quad m
       fast "uberall
  \end{gather}
  
  \begin{proof}
    h sei wie in Beiweis von E4 definiert

    \begin{align}
      \int_{\Omega}h\,dm = \int_{\Omega}g\,dm-\int_{\Omega}f\,dm =0
	 &\Leftrightarrow h=0 m- fast "uberall (E2.5)\\
	 &\Leftrightarrow f=g m- fast "uberall
    \end{align}
  \end{proof}
  
  E6 f me"sbar
  
  \begin{gather}
    f \text{integrierbar} \Leftrightarrow \int_{\Omega}|f|\,dm<+\infty
  \end{gather}
  
  \begin{proof}
    \begin{gather}
      \int_{\Omega}|f|\,dm=\int_{\Omega}(f^{+}+f^{-})dm =
	 \int_{\Omega}f^{+}dm+\int_{\Omega} f^{-}dm
    \end{gather}
  \end{proof}
  
  E7 f me"sbar
  
  f integrierbar $\Rightarrow$ |f| <$+\infty$ m-fast "uberall
  
  \begin{proof}
    \begin{gather}
      \int_{\Omega}|f|dm<+\infty \Rightarrow^{E2.6} |f|<+\infty m-fast
	 "uberall
    \end{gather}
  \end{proof}
  
  E8 f me"sbar, f=0 m-fast "uberall
  
  $\Rightarrow \exists\int_{\Omega} f\,dm$ und $\int_{\Omega}f\,dm = 0$
  
  \begin{gather}
    f=f^{+}+f^{-} \Rightarrow f^{+}=0 m-f."u., f^{-}=0 m-f."u.\\
    \Rightarrow \int_{\Omega} f^{+}\,dm=0, \int_{\Omega}f^{-}\,dm=0 (nach
       E2.5)\\
    \Rightarrow \int_{\Omega}f\,dm =\int_{\Omega}f^{+}\,dm -\int_{\Omega}
       f^{-}\,dm =0
  \end{gather}
  
  \begin{folgerung}
    A\in cF, cm($A^{C}) = 0, \exists\int_{\Omega}f\,dm$
    
    $\Rightarrow$ $\int_{\Omega}f\cdot 1^{A}\,dm=\int_{\Omega}f\,dm$
    
    \begin{proof}
      $\int_{\Omega}f\cdot1_{A}\,dm=\int_{\Omega}f\,dm-\overbrace{\int_{\Omega}
      \underbrace{f\cdot1_{A^{C}}}_{=0}\,dm}^{=0 (E8)}$
    \end{proof}
  \end{folgerung}
  
  \subsection{Stetigkeitseigenschaften der Integrals}
  
  Riemann-Integral: Stetigkeit bzw. gleichm"a"siger Konvergenz der
  Integranden
  
  ($\Omega$,cF,cm) beliebiger Ma"sraum
  
  \begin{satz}[Satz von B.~Levi "uber monotone Konvergenz]
    Sei ($f_{n})$ eine wachsende Folge nichtnegativer me"sbarer
    Funktionen, f=$\lim_{n\rightarrow\infty}f_{n}$
    
    Dann gilt:
    \begin{gather}
      \lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm =\int_{\Omega}f\,dm
    \end{gather}
    
    \begin{proof}
      \begin{enumerate}
       \item ``$f_{n}\leq f$'' $\Rightarrow^{Monotonie}
	\int_{\Omega}f_{n}\,dm \leq \int_{\Omega}f\,dm \Rightarrow
	\lim_{n\rightarrow\infty}\int_{\Omega} f_{n}\,dm \leq\int_{\Omega}f\,dm$
       \item ``$f_{n}\geq f$
	$\exists \lim_{m\rightarrow\infty}f_{n,m}|^f_{n}, f_{n,m}$
	nichtnegativ einfach
	
	g:=$\max_{1\leq n\leq m}f_{n,m}$ einfache me"sbare Funktion
	\begin{enumerate}
	 \item $g_{m}$ einfach
	 \item $f_{n,m} \leq g_{m} \leq f_{m}, 1\leq n\leq m$
	 \item $g_{m} \leq g_{m+1}, \forall m\geq1$
	 \item $\lim_{m\rightarrow\infty} g_{m}=f$ (da "uber (b) eingeschachtelt)
	\end{enumerate}
	
	$\Rightarrow\int_{\Omega}f\,dm :=
	\lim_{m\rightarrow\infty}\int_{\Omega} \underbrace{g_{m}}_{\leq
	f_{m}}\,dm \leq \lim_{m\rightarrow\infty}\int_{\Omega}f_{m}\,dm$
      \end{enumerate}
    \end{proof}
  \end{satz}
  
  \begin{folgerung}
    Es sein ($f_{n})$ eine monoton wachsende Folge me"sbarer Funktionen,
    $f=\lim_{n\rightarrow\infty} f_{n}$. Es gelte:
    \begin{enumerate}
     \item g $\leq$ $f_{n}$ m-f."u.
     \item $\int_{\Omega} g^{-}dm<+\infty$
    \end{enumerate}
    
    Dann folgt:
    \begin{gather}
      \int_{\Omega}f\,dm=\lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm
    \end{gather}
    
    \begin{proof}
      oBdA. $g^{+}=0$ $h_{n}:=(f_{n}-g)\cdot 1_{\{-\infty<g\leq f_{n}\}}
      \geq0, lim_{n\rightarrow\infty} h_{n}=(f-g)\cdot 1_{\{-\infty<g\leq f_{n}\}}$
      
      $\Rightarrow^{B Levi}$
      \begin{align}
	\lim_{n\rightarrow\infty}\int_{\Omega} h_{n}\,dm
	   &=\int_{\Omega}(f-g)\cdot 1_{\{-\infty<g\leq f_{n}\}}\,dm\\
	&=\int_{\Omega}(f-g)\,dm=\int_{\Omega}f\,dm-\int_{\Omega}g\,dm\\
	=\lim_{n\rightarrow\infty}\int_{\Omega}(f_{n}-g)\,dm &=
	    \lim_{n\rightarrow\infty}(\int_{\Omega}f_{n}\,dm-\int_{\Omega}g\,dm)
      \end{align}
      
      $\rightarrow$
    \end{proof}
  \end{folgerung}
  
  \begin{satz}[Lemma von Faton]
    Sei ($f_{n}$) eine Folge \emph{nichtnegativer} me"sbarer Funktionen.
    
    Dann folgt:
    \begin{gather}
      \int_{\Omega} \liminf_{n\rightarrow\infty} f_{n}\,dm \leq
	 \liminf_{n\rightarrow\infty} \int_{\Omega}f_{n}\,dm
    \end{gather}
    
    \begin{proof}
      \begin{align}
	\int_{\Omega}\liminf_{n\rightarrow\infty} f_{n}\,dm
	   &=\int_{\Omega}\lim_{n\rightarrow\infty} \inf_{k\geq n} f_{k} =
	   \lim_{n\rightarrow\infty}\int_{\Omega}\int_{k\geq n} f_{k}\,dm
	   (B.Levi)\\
	[\int_{\Omega}\inf_{k\geq n} f_{k}\,
	   dm\leq^{Monotonie} \int_{\Omega}f_{j}\,dm,\forall j\geq n
	   (\inf_{k\geq n} f_{k} \leq f_{j}, j\geq n)]\\
	&\leq\lim_{n\rightarrow\infty}\inf_{j\geq
	   n}\int_{\Omega}f_{j}\,dm =
	   \liminf_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm
      \end{align}
    \end{proof}
  \end{satz}
  
  \begin{folgerung} % Folgerugn 1
    ($f_{n}$) Folge me"sbarer Funktionen mit:
    \begin{enumerate}
     \item g $\leq$ $f_{n}$ m-f."u. f"ur eine me"sbare Funktion $g$
     \item $\int_{\Omega}g^{-}\,dm<+\infty$
    \end{enumerate}
  \end{folgerung}
  
  Dann folgt: Es existieren $\int_{\Omega}f_{n}\,dm$ und
  $\int_{\Omega}\liminf f_{n}\,dm$ und es gilt:
  \begin{gather}
    \int_{\Omega}\liminf_{n\rightarrow\infty} f_{n}\,dm \leq
       \liminf_{n\rightarrow\infty} \int_{\Omega} f_{n}\,dm
  \end{gather}
  
  \begin{proof}
    \begin{enumerate}
     \item g $\leq$ $f_{n}$ m-f."u. $\Rightarrow$ $f^{-}_{n}\leq g^{-}$
      m-f."u. $\Rightarrow$ $\int_{\Omega}f^{-}_{n}\,dm\leq+\infty$,
      ($\liminf_{n\rightarrow\infty} f_{n})^{-}\leq g^{-} $ $\Rightarrow$
      $\int_{\Omega}(\liminf_{n\rightarrow\infty}f_{n})^{-}\,dm<\infty$
     \item A=$\cap_{n=1}^{\infty}\{-\infty<g\leq f_{n}\}, m(A^{C})=0$
      $h_{n}=(f_{n}-g)\cdot 1_{A} \Rightarrow h_{n}\geq 0$ me"sbar
      $\Rightarrow$ $\int_{\Omega}\underbrace{\liminf_{n\rightarrow\infty} h_{n}}_{=
      (\liminf f_{n}-g)\cdot1_{A}}\,dm
      \leq\liminf_{n\rightarrow\infty}\int_{\Omega}h_{n}\,dm
      = \int_{\Omega}\liminf_{n\rightarrow\infty}\,dm -\int_{\Omega}g\,dm
      \leq\liminf_{n\rightarrow\infty}(\int_{\Omega}f_{n}\,dm
      -\int_{\Omega}g\,gm)$
    \end{enumerate}
  \end{proof}
  
  \begin{folgerung}
    Sei ($f_{n})$ Folge me"sbarer Funktionen mit den Eigenschaften:
    \begin{enumerate}
     \item $f_{n}\leq g$ m-f."u., n$\geq$1, g me"sbar
     \item $\int_{\Omega}g^{+}\,dm<\infty$
    \end{enumerate}
    
    Dann existieren: $\int_{\Omega}f_{n}\,dm$ und
    $\int_{\Omega}\limsup_{n\rightarrow\infty}f_{n}\,dm$ und es gilt:
    \begin{gather}
      \limsup_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm \leq
	 \int_{\Omega}\limsup_{n\rightarrow\infty} f_{n}\,dm
    \end{gather}
    
    \begin{proof}
      Anwendung von Folgerung 1 f"ur die Folge ($g_{n})$ mit $g_{n}=-f_{n}$
      $\limsup_{n\rightarrow\infty}f_{n}=-\liminf_{n\rightarrow\infty}g_{n}$
    \end{proof}
  \end{folgerung}
  
  \subsubsection{Konvergenz m-f."u.}
  ($f_{n}$) Folge me"sbarer Funktionen,
  $\Omega_{0}:=\{\omega\in\Omega:\exists\lim_{n\rightarrow\infty}
  f(\omega) (in \hat{\R})\}\in cF$
  
  ($lim_{n\rightarrow\infty} f_{n})(\omega) = \{ %\}
  \begin{case}
    \lim_{n\rightarrow\infty}f_{n}(\omega)&\omega\in\Omega_{0}\\
    0&\omega\not\in\Omega_{0}
  \end{case}$
  $\lim_{n\rightarrow\infty}f_{n}$ me"sbar auf ($\Omega$,cF)
  
  \begin{defini}
    ($f_{n}$) hei"st m-f."u. konvergent, falls m($\Omega_{0}^{C})=0$
  \end{defini}

  \begin{satz}[Satz von Lebesgue "uber majorisierte Konvergenz] %satz 3
    Sei ($f_{n})$ einen Folge me"sbarer Funktionen auf ($\Omega$,cF) mit
    folgenden Eigenschaften:
    \begin{enumerate}
     \item ($f_{n})$ konvergiert m-f."u.
     \item Es existiert eine me"sbare Funktion g auf ($\Omega$,cF) mit
      \begin{enumerate}
       \item |$f_{n}|\leq g$ m-f."u., n$\geq$1
       \item $\int_{\Omega}g\,dm<\infty$
      \end{enumerate}
    \end{enumerate}
    
    Dann folgt:
    \begin{enumerate}
     \item $f_{n}$ ist integrierbar, n$\geq$1
     \item $\exists\lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm$
     \item $\lim_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm =
      \int_{\Omega}\lim_{n\rightarrow\infty}f_{n}\,dm$ 
    \end{enumerate}
    
    \begin{proof}
      -g$\leq$$f_{n}\leq g$ m-f."u., g,-g sind integrierbar
      $\rightarrow$ $\limsup_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm
      \leq^{Farton} \int_{\Omega}\limsup_{n\rightarrow\infty}f_{n}\,dm =
      \lim_{n\rightarrow\infty}f_{n}\,dm$ (wegen der m-f."u. Konvergenz
      von ($f_{n})$) = $\int_{\Omega}\liminf_{n\rightarrow\infty}
      f_{n}\,dm \leq^{Faton} \liminf_{n\rightarrow\infty}\int_{\Omega}
      f_{n}\,dm \leq\limsup_{n\rightarrow\infty}\int_{\Omega}f_{n}\,dm$ 
    \end{proof}
  \end{satz}
  
  \section{Das Lebesgue-Integral}
  (\R, $cB^{\lambda}$(\R), $\lambda$), Ma"sraum, $\lambda$ Lebesgue-Ma"s,
  cB(\R) $\sigma$-Algebra der Lebesgue-me"sbaren mengen
  ($cB^{\lambda}(\R)$ ist die Vervollst"andigung von cB(\R) bzgl.
  $\lambda$)
  
  \begin{defini}
    $\int_{\R}f\,d\lambda$ bzw. $\int_{\R}f(x)\,dx$ hei"st
    \emph{Lebesgue-Integral} von $f$ (sofern das Integral existiert),
    $\int_{A}f\,d\lambda =\int_{\R}f\cdot1_{A}\,d\lambda, A\in
    cB^{\lambda}(\R)$
  \end{defini}

  \begin{lemma}
    f:\R$\rightarrow$$\hat{\R}$
    
    f ist \emph{Lebesgue-me"sbar} $\Leftrightarrow$ Es existierten
    Borel-me"sbare Funktionen $f_{1},f_{2}$ mit $f_{1}\leq f\leq f_{2}$
    und $\lambda(\{f_{1}\not=f_{2}\})=0$
    
    ("Ubungsaufgabe)
  \end{lemma}
  
  Zielstellung: Vergleichen von Lebesgue-Integral und Riemann-Integral
  
  \begin{defini}
    f:\R$\rightarrow\hat{\R}$, f hei"st \emph{$\lambda$-f."u. stetig} auf
    [a,b], falls eine Menge N$\subseteq$[a,b] existiert mit N \in
    $cB^{\lambda}(\R), \lambda(N)=0$ und f ist stetig in allen Punkten
    x\in[a,b]$\capN^{C}$
  \end{defini}
  
  \begin{satz}
    f:\R$\rightarrow$$\hat{\R}$ beschr"ank, |f|$\leq$c, c>0,
    [a,b]$\subseteq$\R endliches Intervall
    
    f Riemann-integrierbar au [a,b] $\Leftrightarrow$ f ist
    $\lambda$-f."u. stetig auf [a,b]
    
    Es gilt dann:
    \begin{enumerate}
     \item f ist Lebesgue-integrierbar
     \item $\int_{[a,b]}f\,d\lambda = R-\int^{b}_{a}f(x)\,dx$
    \end{enumerate}
    
    \begin{proof}
      f:\R$\rightarrow$$\hat{\R}$ beschr"ankt |f|$\leq$c
      oBdA. f(x)=0, x\not\in[a,b], bzw. f=f$\cdot$$1_{[a,b]}$
      
      $fZ_{n}$=\{$t_{0}^{(n)},\ldots,t_{m_{n}}^{(n)}\}$ Zerlegung von
      [a,b], a=$t_{0}^{(n)}<\ldots<t_{m_{n}}^{n}=b$
      
      $fZ_{n+1}$ sei feiner als $fZ_{n}$
      
      Darbouxsche Summen:
      \begin{gather}
	I_{k}^{(n)} :=
	   \begin{case}
	     [t_{k}^{(n)}, t_{k+1}^{(n)})& k=0,\ldots,m_{n}-2\\
	     [t_{k}^{(n)}, t_{k+1}^{(n)}& k=m_{n}-1
	   \end{case}
      \end{gather}
      
      $\underline{S}_{n}:=\sum_{k=0}^{m_{n}-1}\inf_{y\in I_{k}^{(n)}} f(y)
      (t_{k+1}^{(n)}- (t_{k}^{(n)})$
      $\overline{S}_{n}:=\sum_{k=0}^{m_{n}-1}\sup_{y\in I_{k}^{(n)}} f(y)
      (t_{k+1}^{(n)}- (t_{k}^{(n)})$
    \end{proof}
  \end{satz}

