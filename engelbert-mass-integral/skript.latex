% 19.5.

\begin{defini}
	Das Maß $\lambda$ auf $(\R,\mathcal{B})$ mit $\lambda([a,b])=b-a$ heißt 
	Lebesgue-Maß.
\end{defini}

\begin{lemma}
  $\lambda$ ist auf $\mathcal{B}(\R)$ verschiebungsinvariant:
  \begin{gather}
    A \in \mathcal{B}(\R), x\in \R \Rightarrow x+A\in \mathcal{B}(\R)
       \wedge \lambda(x+A) = \lambda(A)
  \end{gather}

  \begin{proof}
    als Übungsaufgabe: Anwendung von Satz 8.2
  \end{proof}
\end{lemma}

\begin{satz}
  Es existiert $V\subset\R$ mit $V\not\in \mathcal{B}^{\lambda}(\R)$.

  \begin{proof}
    $a,b\in \R\colon a\sim b$ $\Leftrightarrow$ $a-b\in\Q$

    $\Q_{a}$ Restklasse bzgl. $\sim$ mit $a\in\Q$, $(\Q_{a})_{a\in\R}$
    Zerlegung von $\R$: $\bigcup_{a\in\R}\Q_{a}=\R$ $a\not\sim b$ $\Rightarrow$
    $\Q_{a}\cap\Q_{b}=\emptyset$

    $V$ sei eine Auswahlmenge für $(Q_{a})_{a\in\R}$, dann besteht $V\cap\Q_{a}$
    aus genau einem Element.

    $V_{r}:= r+V, r\in\Q$

    Eigenschaften
    \begin{enumerate}
     \item $r_{1}\not= r_{2}\Rightarrow
      V_{r_{1}}\cap V_{r_{2}}=\emptyset,\quad r_{1},r_{2}\in\Q$

      Ann.: $x\in V_{r_{1}}\cap V_{r_{2}}\Rightarrow x=r_{1}+a=r_{2}+b,
      a,b\in V$ (a und b sind auswahlelemente) $ a-b = r_{1}-r_{2}\in\Q
      \Rightarrow a\sim b$

      $\Rightarrow a=b \Rightarrow r_{1}=r_{2}$
     \item $\bigcup_{r\in\Q} V_{r}=\R : x\in\R \Rightarrow \exists a\in\R:
      x\in\Q_{a}$, o.\,B.\,d.\,A.: $a\in V$

      $x\in a+\Q\Rightarrow x=a+r, r\in\Q$

      $\Rightarrow r\in V_{r}$

      Annahme: $V\in \mathcal{B}^{\lambda}(\R)$
      \begin{description}
       \item[1. Fall: $\lambda(V)=0$] $\Rightarrow$
	$+\infty=\lambda(\R)=\lambda(\bigcup_{r\in\Q}V_{r}) =
	\sum_{r\in\Q}
	\lambda(V_{r})=\sum_{r\in\Q}\lambda(r+v)=\sum_{r\in\Q}\lambda(V) =0$

	Widerspruch!
       \item[2. Fall: $\lambda(V)>0$]
	$\lambda(V)=\lim_{n\rightarrow\infty} \lambda(V\cap[-n,+n])
	\Rightarrow \exists n:\lambda(V\cap[-n,+n])>0$
	2n=$\lambda$([-n,n])$\geq$
	$\lambda$($\bigcup_{k=1}^{\infty}V_{\frac1{2^k}}\cap [-n,n]) =
	\sum_{k=1}^{\infty}\lambda(V_{\frac1{2^{k}}}\cap[-n,n])\geq\sum_{k=1}^{\infty}
	 \lambda(V_{\frac1{2^k}}\cap[-n+\frac1{2^k},n] =
	\sum_{k=1}^{\infty}[\lambda(V_{\frac1{2^k}}\cap[-n+\frac1{2^k},n+\frac1{2^k})
	-\underbrace{\lambda(V_{\frac1{2^k}}\cap(n,n+\frac1{2^k}])}_{\leq\frac1{2^k}} \geq
	\sum_{k=1}^{\infty} \lambda(\frac1{2^k}+V\cap[-n,n])-1 =
	\sum_{k=1}^{\infty} \underbrace{\lambda(V\cap[-n,n])}_{>0}-1 = +\infty$
      \end{description}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{folgerung}
  $\card(\{A\in\R\colon A\not\in \mathcal{B}(\R)\}) = 2^{\complement}$

  \begin{proof}
    $M=\{V'\subseteq\R\colon V'\text{ Auswahlmenge von } (Q_{a})_{a\in\R}\}$
    ($V'\in M$ $\Rightarrow$ $V'\not\in \mathcal{B}^{\lambda}(\R)$)

    $\mathrm{Mächtigkeit} (M)=  \mathrm{Mächtigkeit} (\times_{a\in\R}\Q_{a})=
    \prod_{a\in\R} \card(\Q_{a}) \\= \prod_{a\in\R} \card(\Q) =
    \card(\Q)^{\card(\R)}\geq 2^{\complement}$
  \end{proof}
\end{folgerung}

