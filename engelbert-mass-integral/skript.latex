\subsubsection{Produkte von WK-Ma"sens}

($\Omega_{1}$, $cF_{1}$, $P_{1}$) und ($\Omega_{2}$, $cF_{2}$, $P_{2}$)
WK-R"aume

$\Omega = \Omega_{1}\times\Omega_{2}, cF = cF_{1}\otimes cF_{2},
\omega=(\omega_{1}, \omega_{2}) \in \Omega$

Projektionsabbildungen
$X_{1}(\omega) = \omega_{1}, X_{2}(\omega)=\omega_{2}$

\begin{lemma} %3
  $X_{i}:(\Omega,cF)\rightarrow(\Omega_{i},cF_{i})$ me"sbar, (i=1,2)
  
  \begin{proof}
    \begin{description}
     \item[i=1] B\in $cF_{1}$: $X_{1}^{-1}(B)=B\times\Omega_{2}\in
      cF_{1}\times cF_{2}\subseteq cF$
      
      $p$ sei WK-Ma"s auf ($\Omega$, cF) $\Rightarrow$ $X_{i}: (\Omega,
      cF, P) \rightarrow (\Omega_{i}, cF_{i})$ zuf"allige Variable,
      $P_{X_{i}}:$ Verteilung von X bzgl. P (Bildma"s)
    \end{description}
  \end{proof}
\end{lemma}

\begin{satz}
  \begin{enumerate}
   \item $P_{1}\otimes P_{2}$ ist ein WK-Ma"s auf ( $\Omega$, cF)
   \item Es sei P irgendein WK-Ma"s auf  ($\Omega$, cF) mit $P_{X_{i}} =
    P_{i} (i=1,2)$. Dann sind folgende Aussagen "aquivalent:
    \begin{enumerate}
     \item \begin{gather}P = P_{1}\otimes P_{2}\end{gather}
     \item\label{BBB} $X_{1}$ und $X_{2}$ sind unabh"angig bzgl. $P$
     \item\label{CCC} $X_{1}^{-1}(F_{1})$ und $X_{2}^{-1}(cF_{2})$ unabh"angig bzgl.
      $P$
    \end{enumerate}
  \end{enumerate}
  
  \begin{proof}
    \begin{description}
     \item[(\ref{BBB}) $\Leftrightarrow$ (\ref{CCC}):] klar
     \item[(a) $\Leftrightarrow$ (b)]
      \begin{align}
	P = P_{1}\otimes P_{2} &\Leftrightarrow^{S.2} P(A_{1}\times A_{2}) =
	   P_{1}(A_{1}) P_{2}(A_{2}), \forall A_{1}\in cF_{1}, A_{2}\in
	   cF_{2}\\
	&\Leftrightarrow P(\underbrace{\{X_{1}\in
	   A_{1}\}}_{=A_{1}\times\Omega_{2}} \cap \underbrace{\{X_{2}\in 
	   A_{2}\}}_{=\Omega_{1}\times A_{2}}) =
	   P_{X_{1}}(A_{1}) P_{X_{2}}(A_{2}) = P(\{X_{1}\in A_{1}\})
	   P(\{X_{2}\in A_{2}\})\\
	&\Leftrightarrow X_{1}, X_{2} unabh. bzgl. P
      \end{align}
    \end{description}
  \end{proof}
\end{satz}

\begin{bemerkung}
  \ref{BBB} bedeutet:
  \begin{gather}
    P(\{X_{1}\in A_{1}\}\cap\{X_{2}\in A_{2}\}) = P(\{X_{1}\in
       A_{1}\})\cdot P (\{X_{2}\in A_{2}\})\quad \forall A_{1}\in cF_{1},
       A_{2}\in cF_{2}
  \end{gather}
  
  \ref{CCC} bedeutet:
  \begin{gather}
    P(B_{1}\cap B_{2}) = P(B_{1})\cdot P(B_{2})\quad \forall B_{1}\in
       X_{1}^{-1}(cF_{1}) (\Leftrightarrow B_{1}=\{X_{1}\in A_{1}\},
       A_{1}\in cF_{1}), B_{2}\in X_{2}^{-1}(cF_{2}) (\Leftrightarrow
       B_{1}=\{X_{2}\in A_{2}\}, A_{2}\in cF_{2})
  \end{gather}
\end{bemerkung}

\subsubsection{Schnittfunktion}

\begin{gather}
  f:\Omega = \Omega_{1}\times\Omega_{2} \rightarrow \hat{\R}
\end{gather}

\begin{defini} % 4
  x\in $\Omega_{1}, f_{x}(y) = f(x,y), y\in \Omega_{2}, f_{x}$ hei"st
  \emph{Schnitt} von f in $x\in \Omega_{1}$
  
  y\in $\Omega_{2}, f^{y}(x) = f(x,y), x\in\Omega_{1}, f^{y}$ hei"st
  \emph{Schnitt} von f in $y\in \Omega_{2}$
\end{defini}

\begin{lemma} % 4
  Sei $f:(\Omega, cF) = (\Omega_{1}\times \Omega_{2}, cF_{1}\otimes
  cF_{2}) \rightarrow (\hat{\R}, cB(\hat{\R}))$ me"sbar
  
  Dann  ist $f_{x}: (\Omega_{2}, cF_{2}) \rightarrow (\hat{\R},
  cB(\hat{\R}))$ sowie  $f^{y}:(\Omega_{1}, cF_{1}) \rightarrow
  (\hat{\R}, cB(\hat{\R}))$ me"sbar
  
  \begin{proof}
    \begin{gather}
      A=\{(x,y)\in \Omega: f(x,y) < r\}, r\in \R \Rightarrow A\in
	 cF_{1}\otimes cF_{2}\\
      \Rightarrow^{Lemma 2} A_{x}\in cF_{2}, A^{y}\in cF_{1}, \text{es
	 gilt: } A_{x}=\{y\in\Omega_{2}: f_{x}(y) < r\} =
	 f^{-1}_{x}([-\infty, r))\in cF_{2}\forall r\in \R \Rightarrow
	 f_{x} me"sbar
    \end{gather}
    
    analog: $cF_{1}\ni A^{y}= (f^{y})^{-1}([-\infty,r))$
  \end{proof}
\end{lemma}

\begin{satz}[Satz von Fubini] % 5
  ($\Omega_{1}, cF_{1}, cm_{1})$ und ($\Omega_{2}, cF_{2}, cm_{2})$
  $\sigma$-endliche Ma"sr"aume, ($\Omega$, cF, cm) = ($\Omega_{1}\times
  \Omega_{2}, cF_{1}\otimes cF_{2}, cm_{1}\otimes cm_{2})$
  
  f sei eine me"sbare Funktion auf ($\Omega, cF)$, f nichtnegativ.
  
  Dann folgt: Das Integral $\int_{\Omega_{2}}
  \underbrace{f(x,y)}_{f_{x}(y)} cm_{2}(dy)$ ist eine me"sbare Funktion
  in $x$ auf ($\Omega_{1}, cF_{1})$ und es gilt:
  \begin{gather}
    \int_{\Omega} f(x,y) cm(d(x,y)) =
       \int_{\Omega_{1}}(\int_{\Omega_{2}}f(x,y) m_{2}(dy)) m_{1}(dx)
  \end{gather}
  
  \begin{proof}
    \begin{enumerate}
     \item f = $1_{A}, A\in cF_{1}\otimes cF_{2}$
      \begin{align}
	\int_{\Omega} f(x,y) m(d(x,y)) &= \int_{\Omega} 1_{A}(x,y)
	   cm(d(x,y)) = cm(A) = \int_{\Omega_{1}}
	   \underbrace{cm_{2}(A_{x})}_{me"sbar in x (Satz 1)} cm_{1}(dx)\\
	& \int_{\Omega_{1}}(\int_{\Omega_{2}}1_{A}(y) cm_{2}(dy)) cm(dx)
	   = \int_{\Omega_{1}}\underbrace{(\int_{\Omega_{2}}f(x,y)
	   m_{2}(dy))}_{me"sbar in x}cm_{1}(dx)
      \end{align}
     \item f sei nichtnegative einfache Funktion, d.h.
      \begin{gather}
	f = \sum_{k=1}^{n}c_{k} 1_{A_{k}}, c_{1},\ldots,c_{n}\geq 0,
	   A_{1},\ldots,A_{n}\in cF_{1}\otimes cF_{2}
      \end{gather}
      
      \begin{align}
	\int_{\Omega}f(x,y) cm(d(x,y)) &=
	   \sum_{k=1}^{n}c_{k}\int_{\Omega}1_{A}(x,y) m(d(x,y))\\
	&= \sum_{k=1}^{n}c_{k}\int_{\Omega_{1}}(\int_{\Omega_{2}}
	   1_{A_{k}}(x,y) m_{2}(dy))m_{1}(dx) = \int_{\Omega_{1}}
	   \underbrace{(\sum_{k=1}^{n}c_{k} \int_{\Omega_{2}}1_{A_{k}}(x,y)
	   cm_{2}(dy))}_{me"sbar in x}m_{1}(dx)\\
	&= \int_{\Omega_{1}}\underbrace{(\int_{\Omega_{2}}\sum_{k=1}^{n} c_{k}
	   1_{A_{k}}(x,y) m_{2}(dy))}_{me"sbar in x}m_{1}(dx)
      \end{align}
     \item f me"sbar auf ($\Omega$, cF), f$\geq$0
      
      $\exists f_{n}: f_{n}\geq0$ und einfach,
      $f_{n}|^_{n\rightarrow\infty}f$

      $\Rightarrow$
      \begin{gather}
	\int_{\Omega}f(x,y) m(d(x,y)) =
	   \lim_{n\rightarrow\infty}\int_{\Omega}f_{n}(x,y) m(d(x,y))
	   =^{2)}
	   \lim_{n\rightarrow\infty}\int_{\Omega_{1}}(\int_{\Omega_{2}}
	   f_{n}(x,y) m_{2}(dy))m_{1}(dx) =^{B. Levi} \int_{\Omega_{1}}
	   (\lim_{n\rightarrow\infty} \int_{\Omega_{2}} f_{n}(x,y)
	   m_{2}(dy)) m_{1}(dx) =^{B. Levi}
	   \int_{\Omega_{1}}\underbrace{(\int_{\Omega_{2}}f(x,y) m_{2}(dy))}_{me"sbar
	   in x} m_{1}(dx)
      \end{gather}
    \end{enumerate}
  \end{proof}
\end{satz}
  
\begin{satz} % 5':
  f sei me"sbar auf ($\Omega$, cF), $\exists\int_{\Omega} f(x,y)m(d(x,y))$
  
  Dann existiert $\int_{\Omega_{1}}(\int_{\Omega_{2}}f(x,y) m_{2}(dy))
  m_{1}(dx)$ und es gilt:
  \begin{gather}
    \int_{\Omega}f(x,y) m(d(x,y)) =
       \int_{\Omega_{1}}(\int_{\Omega_{2}}f(x,y) m_{2}(dy)) m_{1}(dx)
  \end{gather}
  
  \begin{proof}
    \begin{gather}
      \exists\int_{\Omega}f(x,y) m(d(x,y)) \Leftrightarrow
	 \int_{\Omega}f^{+}(x,y) m(d(x,y))<\infty oder
	 \int_{\Omega}f^{-}(x,y) m(d(x,y))<\infty
    \end{gather}
    
    Ann.: $\int_{\Omega}f^{+}(x,y) m(d(x,y))<\infty$
    
    \begin{align}
      \int_{\Omega} f(x,y) m(d(x,y)) &=
	 \underbrace{\int_{\Omega}f^{+}(x,y) m(d(x,y))}_{<\infty}
	 -\int_{\Omega}f^{-}(x,y) m(d(x,y))\\
      &=^{Satz 5} \int_{\Omega_{1}}(\underbrace{\int_{\Omega_{2}}
	 f^{+}(x,y)m_{2}(dy))}_{<\infty m_{1}-f."u.}m_{1}(dx)-
	 \int_{\Omega_{1}}(\int_{\Omega_{2}} f^{-}(x,y)m_{2}(dy))m_{1}(dx)
    \end{align}
    N=\{x\in $\Omega_{1}: \int_{\Omega_{2}}f^{+}(x,y) m_{2}(dy)=\infty\},
    N\in cF_{1}, m_{1}(N)=0$
    \begin{align}
      &= \int_{\Omega_{1}}
	 1_{N^{C}}(x)(\int_{\Omega_{2}}f^{+}(x,y)m_{2}(dy)) m_{1}(dx) -
	 \int_{\Omega_{1}}
	 1_{N^{C}}(x)(\int_{\Omega_{2}}f^{-}(x,y)m_{2}(dy)) m_{1}(dx)\\
      &=^{Linearit"at} \int_{\Omega_{1}}
	 1_{N^{C}}(x)(\int_{\Omega_{2}}f^{+}(x,y)m_{2}(dy)-
	 \int_{\Omega_{2}}f^{+}(x,y)m_{2}(dy)) m_{1}(dx)\\
      &= \int_{\Omega_{1}}\underbrace{1_{N^{C}}(x) (\int_{\Omega_{2}}f(x,y)
	 m_{2}(dy))}_{g(x) aus Bemerkung} m_{1}(dx)\\
      &= \int_{\Omega_{1}}(\int_{\Omega_{2}} f(x,y) m_{2}(dy))m_{1}(dx)
	 (Iteriertes Integral existiert)
    \end{align}
  \end{proof}
\end{satz}

