Ungleichung von Young
\begin{gather}
  1<p, q<\infty, \frac1p+\frac1q=1, r_{1},r_{2},t_{1},t_{2}\in\R\\
  \Rightarrow
     |r_{1}t_{1}+r_{2}t_{2}|\leq(|r_{1}|^{p}+|r_{2}|^{p})^{\frac1p}
     (|t_{1}|^{q}+|t_{2}|^{q})^{\frac1q}
\end{gather}

\begin{proof} % Beweis von Satz 2
  $f,g\in L_{p}=L_{p}(\Omega, cF, cm)$
  
  z.z.: $f+g\in L_{p}$ und $\|f+g\|_{p}\leq\|f\|_{p}+\|g\|_{p}$
  
  \begin{description}
   \item[1.Fall] $p=1$
    \begin{gather}
      \|f+g\|_{q}=\int_{\Omega}|f+g|\,dm \leq
	 \int_{\Omega}(|f|+|g|)\,dm =
	 \underbrace{\int_{\Omega}|f|\,dm}_{<\infty} +\underbrace{\int_{\Omega}
	 |g|\,dm}_{<\infty} = \|f\|_{q}+\|g\|_{q} <\infty
    \end{gather}
   \item[2.Fall] $1<p<\infty$
    zeigen zun"achst: $f+g\in L_{p}$
    \begin{gather}
      |f+g|^{p}\leq|f+g||f+g|^{p-1}\leq(|f|+|g|)|f+g|^{p-1} =
	 |f||f+g|^{p-1}+|g||f+g|^{p-1} (r_{1}=|f|, r_{2}=|g|,
	 t_{1}=t_{2}=|f+g|^{-1})\\
      \leq^{Lemma
	2}(|f|^{p}+|g|^{p})^{\frac1p}(2|f+g|^{(p-1)q})^{\frac1q} =
	 (|f|^{p}+|g|^{p})^{\frac1p} 2^{\frac1q}|f+g|^{p-1}
    \end{gather}
    Vorraussetzung: $|f+g|\not=0$, Multiplikation mit $|f+g|^{1-p}$
    \begin{gather}
      \Rightarrow |f+g|\leq(|f|^{p}+|g|^{p})^{\frac1p}2^{\frac1q} ( auch
	 g"ultig f"ur |f+g|=0)\\
      = |f+g|^{p} \leq (|f|^{p}+|g|^{p})2^{\frac{p}q} \Rightarrow
	 |f+g|^{p} integrierbar \Rightarrow f+g\in L_{p}
    \end{gather}
    
    Dreiecksungleicheung:
    \begin{align}
      \|f+g\|^{p}_{p} &=\int_{\Omega}|f+g|^{p}\,dm =
	 \int_{\Omega}|f+g||f+g|^{p-1}\,dm\\
      &\leq\int_{\Omega}|f||f+g|^{p-1}\,dm +
	 \int_{\Omega}|g||f+g|^{p-1}\,dm\\
	 &\leq(\int_{\Omega}|f|^{p}\,dm)^{\frac1p}
	 (\int_{\Omega}|f+g|^{(p-1)q}\,dm)^{\frac1q} +
	 (\int_{\Omega}|g|^{p}\,dm)^{\frac1q}
	 (\int_{\Omega}|f+g|^{(p-1)q}\,dm)^{\frac1q} (H"oldersche Ungleichung)\\
      &=(\|f\|_{p}+\|g\|_{p})(\int_{\Omega}|f+g|^{p}\,dm)^{\frac1q}\\
      Ann:. \int_{\Omega}|f+g|^{p}\,dm\not=0\\
	 &=(\int_{\Omega}|f+g|^{p}\,dm)^{1-\frac1q=1\frac1p}
	 \leq\|f\|_{p}+\|g\|_{p} \Rightarrow \|f\|_{p}+\|g\|_{p}
	 \Rightarrow \|f+g\|_{p}\leq \|f\|_{p}+\|g\|_{p}
    \end{align}
  \end{description}
\end{proof}

\begin{satz} % Satz 3
  $1\leqp<\infty, (\Omega, cF, cm)$ Ma"sraum
  
  Dann istt $L_{p}(\Omega, cF,cm)$ ein Banachraum (vollst"andig
  nromierter linearer Raum)
  
  \begin{proof}
    \begin{enumerate}
     \item $L_{p}\subseteq L_{0}$ linearer Unterraum (Anwendung von Satz 2)
      
      $f\in L_{p}, \alpha\in\R \Rightarrow \alpha f\in L_{p}:
      \int_{\Omega}|\alpha f|^{p}\,dm =
      |\alpha|^{p}\underbrace{\int_{\Omega}|f|^{p}\,dm}_{<\infty} <\infty$
     \item $\|\cdot\|$ ist eine Norm
      \begin{enumerate}
       \item $\|f\|_{p}\geq0, \|f\|_{p}=\int_{\Omega}|f|^{p}\,dm=0
	\Leftrightarrow |f|=0 m-f"u \Leftrightarrow f=0$  (im Sinne
	der "Aquivalenzklassenbildung)
       \item $\|\alpha f\|_{p}=|\alpha|\|f\|_{p}, \alpha\in\R, f\in
	L_{p}$ klar
       \item $\|f+g\|_{p} \leq \|f\|_{p}+\|g\|_{p}, f,g\in L_{p}$ (Satz 2)
      \end{enumerate}
     \item ($L_{p},\|\cdot\|_{p})$ vollst"andiger normierter Raum
      
      Es sei ($f_{n}$) eine Cauchy-Folge in $L_{p}$:
      $\lim_{n,m\rightarrow\infty} \|f_{n}-f_{m}\|_{p}=0$
      
      Es existiert ($n_{k}$): \|$f_{n}-f_{m}\|_{p}\leq\frac1{2^k},
      \forall n,m\geq n_{k}$
      
      $n_{k}\leq n_{k+1}$
      Es folgt: \|$f_{n_{k+1}}-f_{n_{k}}\|_{p}\leq\frac1{2^{k}}}, k=1,2,\ldots$
      g := $\sum_{k=1}^{\infty} |f_{n_{k+1}}-f_{n_{k}}|$ me"sbare Funktion
      
      zeigen: g\in $L_{p}$
      \begin{gather}
	\int_{\Omega}|g|^{p}\,dm = \int_{\Omega}\lim_{m\rightarrow\infty}
	   (\sum_{k=1}^{m}|f_{n_{k-1}}-f_{n_{k}}|)^{p}\,dm =^{B.Levi}
	   \lim_{m\rightarrow\infty} \int_{\Omega}
	   (\sum_{k=1}^{m}|f_{n_{k-1}}-f_{n_{k}})^{p}\,dm\\
	   &=\lim_{n\rightarrow\infty}
	   \|\sum_{k=1}^{m}|f_{n_{k-1}}-f_{n_{k}}| \|^{p}_{p} \leq^{Dreiecksungl.}
	   \lim_{m\rightarrow\infty}
	   (\sum_{k=1}^{m}\underbrace{\|f_{n_{k-1}}-f_{n_{k}}\|_{p}}_{\leq\frac1{2^{k}}} 
	   )^{p} \leq1
      \end{gather}
      
      $\Rightarrow$ 0$\leq$g<$\infty$ m-f"u
      $\Rightarrow$ $\Omega_{0}\in cF, m(\Omega^{C})=0$ und
      0$\leq$g(x)<$\infty$ $\forall$ x\in$\Omega_{0}$
      
      f(x) =
      \begin{case}
	$f_{n_{0}}(x)+\sum_{k=1}^{\infty}(f_{n_{k+1}}(x)-f_{n_{k}}(x)&
	x\in\Omega_{\Omega}\\ 0 & x\in\Omega_{0}^{C}$
      \end{case}
      
      Beh.: f\in $L_{p}$ und \|$f_{n}$-f\|_{p}
      $\rightarrow$_{n$\rightarrow$$\infty$}0
      \begin{enumerate}
       \item f\in $L_{p}$: |f|$\leq$ |$f_{m}|+g\in L_{p}$
       \item ||$f_{n}-f||_{p} \rightarrown^{n\rightarrow\infty}0$
	
	g.z.z: ||$f_{n_{m}}-f||_{p}\rightarrow^{m\rightarrow\infty}0$ (da
	($f_{n}$) Cauchy-Folge)
	||$f_{n_{m}}-f||_{p}=(\int_{\Omega}|f_{n_{m}}-f|
	-f_{n_{1}}-\sum_{k=1}^{\infty} (f_{n_{k+1}}-f_{n_{k}})|^{p}\,dm)^{\frac1p}$
	=($\int_{\Omega}|\sum_{k=1}^{\infty}(f_{n_{k-1}}-f_{k})|^{p}\,dm
	=^{B.Levi}\lim_{s\rightarrow\infty}(\int_{\Omega}
	\sum_{k=1}^{s}(f_{n_{k-1}}-f_{n_{k}})|^{p}\,dm)^{\frac1p} =
	\lim_{s\rightarrow\infty}\sum_{k=1}^{s}
	\underbrace{||f_{n_{k-1}}-f_{n_{k}}||_{p}}_{\leq \frac1{2^{k}}}
	\leq\sum_{k=m}^{\infty}\frac1{2^{k}}\rightarrow^{n\rightarrow\infty}0$
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{satz}

