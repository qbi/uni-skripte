\chapter{Messbare Funktionen}

Seien $(\Omega, \CF), (E, \FE)$ messbare Räume.

\begin{defini}
  Eine Abbildung $f\colon\Omega\rightarrow E$ heißt \emph{messbare
    Abbildung\index{Abbildung!messbare}} von $(\Omega, \CF)$ in $(E,
  \FE)$, falls für alle $B\in \FE$ das volle Urbild
  $f^{-1}(B)={\omega\in\Omega:f(\omega)\in B}\in \CF$

  Notation: $f\colon(\Omega,\CF)\rightarrow(E,\FE)$ messbar
\end{defini}

\section{Eigenschaften messbarer Funktionen}
\begin{enumerate}
\item Die konstante Funktion $f(\omega) = c\in E, \omega\in\Omega$
  ist stets messbar\\
  \begin{proof}
    \begin{gather*}
      f^{-1}(B)=
      \begin{cases}
        \emptyset & c\not\in B\\
        \Omega &c\in B
      \end{cases}
      \qquad \in \CF
    \end{gather*}
  \end{proof}
\item Hintereinanderausführung messbarer Funktionen
  $f\colon(\Omega,\CF)\rightarrow(E_{1}, e_{1})$ messbar,
  $g:(E_{1},e_{1})\rightarrow(E_{2},e_{2})$ messbar\\
  $g\circ f$ Hintereinanderausführung
  $\longrightarrow$ $g\circ f$ $(\Omega,\CF)$
  $\rightarrow$ ($E_{2}, e_{2}$) messbar\\

  Beweis: $B\in e_{2}$: ($g\circ f)^{-1}$(B)=$f^{-1}$($\underbrace{g^{-1}(B)}_{\in e_{1}})\in \CF$
 \item Einschränkung messbarer Funktionen
  $f\colon(\Omega,\CF)\rightarrow(E,\FE)$ messbar,
  $\Omega_{0}\subseteq\Omega$, $f_{|\Omega_{0}}:= f(\omega)$,
  $\omega\in\Omega_{0}$ (Einschränkung von f auf $\Omega_{0}$
  $\longrightarrow f_{|\Omega_{0}}:(\Omega_{0},\CF\cap\Omega_{0})\rightarrow
   (E,\FE)$ messbar, ($\Omega_{0}, \CF\cap\Omega_{0})$ Spur von ($\Omega,\CF)$
  auf $\Omega_{0}$\\
Beweis: $B\in \FE: f^{-1}_{|\Omega_{0}}(B)=\underbrace{f^{-1}(B)}_{\in
  \CF}\cap\Omega_{0}\in \CF\cap\Omega_{0}$
 \item Kriterium für Messbarkeit
  ($\Omega,\CF), (E,e), \mathfrak{S} \subseteq e,
  \sigma(\mathfrak{S})=\mathfrak{e} (\mathfrak{S}$
  erzeugt $\mathfrak{e}$ bzw. $\mathfrak{S}$ ist ein Erzeugendensystem für $\mathfrak{E}$)
  $f\colon\Omega\rightarrow E$
  $f\colon(\Omega,\CF)\rightarrow(E,\mathfrak{E})$ messbar
  $\Longleftrightarrow$ $f^{-1}(B)\in \CF, \forall B\in \mathfrak{S}$
  \begin{proof}
    \begin{enumerate}[I.]
     \item Notwendigkeit: klar
     \item Hinlänglichkeit: $\tilde{\mathfrak{e}}:= {B\in \mathfrak{E}: f^{-1}(B)\in
      \CF}$ ("`gute"' Mengen
      z.\,z.: $\tilde{\mathfrak{E}}=\mathfrak{E}$
      \begin{enumerate}[1)]
       \item $\mathfrak{S}\subseteq\tilde{\mathfrak{E}}$
       \item
	 \begin{enumerate}[a)]
	  \item $E \in \tilde{\mathfrak{E}}: f^{-1}(E)=\Omega\in \CF$
	  \item $B \in \tilde{\mathfrak{E}}\Rightarrow B^{\complement}\in
	   \tilde{\mathfrak{E}}: f^{-1}(B^{\complement})=(\underbrace{f^{-1}(B)}_{\in \CF})^{\complement}
	   \in \CF$
	  \item $B_{n}\in \tilde{\mathfrak{E}},
	   n\ge1 \Rightarrow \bigcup_{n=1}^{\infty}
	   B_{n}\in\tilde{\mathfrak{E}}:f^{-1}(\bigcup_{n=1}^{\infty}B_{n})=\bigcup_{n=1}^{\infty}
	   f^{-1}(\underbrace{B_{n}}_{\in \CF})\in \CF \Rightarrow \bigcup_{n=1}^{\infty}
	   B_{n}\in \tilde{\mathfrak{E}}$
	   $\Rightarrow$ $\tilde{\mathfrak{E}} \sigma$-Algebra
	   $\xrightarrow{1)} \mathfrak{e}=\sigma(\mathfrak{S})\subseteq \tilde{\mathfrak{E}} (\subseteq \mathfrak{E})$
	 \end{enumerate}
      \end{enumerate}
    \end{enumerate}
  \end{proof}
 \item Anwendung auf den Fall ($E,\mathfrak{E})=(\R, B(\R)$)
  $f\colon(\Omega,\CF)\rightarrow(\R,B(\R))$ messbar
  $\Leftrightarrow {f<r}:=f^{-1}((-\infty,r))\in \CF$
  \begin{proof}
    Folgerung zu A.16:
    $B(\R)=\sigma({(-\infty,r): r\in \Q (oder \R)})\forall r\in \Q (bzw.
    r\in \R)$
  \end{proof}
 \item $(E,\rho)$ metrischer Raum,
  $\mathfrak{E}:=B(E) \sigma$-Algebra der Borelmengen
  $f\colon(\Omega,\CF) \longrightarrow$(E,B(E)) messbar
  $\Leftrightarrow$ $f^{-1}(G) \in \CF, \forall G\subseteq E$ offen
  Anwendung von 4
 \item ($E_{1},\rho_{1}$), ($E_{2}, \rho_{2})$ metrische Räume
  $f\colon(E_{1},B(E_{1}))\rightarrow(E_{2},B(E_{2}))$ messbar
  $\Longleftarrow$ $f\colon E_{1}\rightarrow E_{2}$ stetig
  \begin{proof}
    Es gilt: $f\colon E_{1}\rightarrow E_{2}$ stetig
    $\Leftrightarrow$ $f^{-1}(G)$ offen in $E_{1}, \forall G\in
    E_{2}$ offen
  \end{proof}
\end{enumerate}

\chapter{Messbar erweiterte reelle Funktionen}
$\hat{\R}:=\R\cup\{-\infty,\infty\}=[-\infty,\infty]$

Rechenregeln:
\begin{flalign}
 (\pm \infty)+(\pm\infty)=\pm\infty\\
 x+(\pm\infty)=(\pm\infty)+x=\pm\infty, x \in \R\\
 x-(\pm\infty)=\mp\infty, x\in \R\\
 (\pm\infty)(\pm\infty)=+\infty\\
 (\pm\infty)(\mp\infty)=(\mp\infty)(\pm\infty)=-\infty\\
 x(\pm\infty)=(\pm\infty)x=\pm\infty, x>0\\
 x(\pm\infty)=(\pm\infty)x=\pm\infty, x<0\\
 0\,(\pm\infty)=0
\end{flalign}

Verboten:
\begin{enumerate}
 \item $(\pm\infty)-(\pm\infty)$
 \item $(\pm\infty)+(\mp\infty)$
\end{enumerate}

\begin{gather*}
  h(x)=
     \begin{cases}
       1 & x=+\infty\\
       \frac{x}{1+|x|}& x\in\R\\
       -1& x=-\infty
     \end{cases}\\
  h:\hat{\R}\rightarrow[-1,+1]
\end{gather*}
%}

$\hat{\rho}(x,y)=|h(x)-h(y)|, x,y\in\hat{\R}$
\begin{enumerate}[{Aussage}~1]
 \item ($\hat{\R}, \hat{\rho})$ metrischer Raum (Übungsaufgabe)
 \item G$\subset\R$, 
  G offen bzgl. $\hat{\rho} \Leftrightarrow $ G offen bzgl.
  $\rho(x,y)=|x-y|, x,y\in\R$ (Übungsaufgabe-- Anwendung nächste
  Übungsserie)
  \begin{bemerk}
    $\R$ offen in $\hat{\R}$ bgzl. $\hat{\rho}$
  \end{bemerk}
\end{enumerate}

\begin{defini}
  \begin{enumerate}[(i)]
   \item Eine Funktion $f\colon\Omega\rightarrow\hat{\R}$ heißt
    messbar erweitert reell, falls $f\colon (\Omega, \CF)
    \rightarrow (\hat{\R},B(\hat{\R})$ messbar ist
   \item $f$ messbar erweitert reell,
    $f(\Omega)=\{f(\omega)\colon \omega\in\Omega\}\subseteq\R$. Dann heißt
    f messbar reell auf $(\Omega,\CF)$.
  \end{enumerate}
\end{defini}

\begin{satz}
  \begin{enumerate}[(i)]
   \item $B(\R)=B(\hat{\R})\cap\R$ (Spur von $B(\hat{\R})$ auf $\R$)
   \item $B(\hat{\R})=\sigma({[-\infty,r): r\in\Q (r\in\R)})$
   \item $f\colon\Omega\rightarrow\R$

    $f$ ist messbar reell auf $(\Omega,\CF)\Leftrightarrow
    f\colon(\Omega,\CF)\rightarrow(\R,B(\R))$ messbar
  \end{enumerate}
  \begin{proof}
    \begin{enumerate}[(i)]
     \item Anwendung von Ü-Aufgabe~1.3 $\R$ offen in $\hat{\R}$ bzgl. $\hat{\rho}$
     \item $\mathfrak{E} := \sigma\big(\{[-\infty,r]: r\in\Q\}\big)$
      \begin{enumerate}[1)]
       \item $\mathfrak{E} \subseteq$ B($\hat{\R}$), d [-$\infty$,r) offen in $\hat{\R}$
       \item
	\begin{enumerate}
	 \item $\{-\infty\}, \{+\infty\}, \{-\infty, +\infty\}, \R\quad\in \mathfrak{E}$
	 \item $A\in B(\hat{\R}):A\cap\R\in B(\R)$ (wegen (i))
	  $=\sigma_{\R}({(-\infty,r):r\in\Q})\subseteq \mathfrak{E}\cap\R\subseteq \mathfrak{E}$
	  $A\cap{-\infty,+\infty}\in \mathfrak{E}
	  \longrightarrow
	  A=(A\cap\R)\cup(A\cap{-\infty,+\infty})\in \mathfrak{E}$, d.\,h.
	  $B(\hat{\R})\subseteq \mathfrak{E}$
	\end{enumerate}
      \end{enumerate}
     \item $f\colon\Omega\rightarrow\R$
      \begin{enumerate}[I.]
       \item $f$ sei messbar reell

	z.\,z.: $f\colon(\Omega,\CF)\rightarrow(\R,B(\R))$ messbar Sei
	$B\in B(\R): f^{-1}(B)$ ($B(\R)=B(\hat{\R})\cap\R\subseteq
	B(\hat{\R})$), da $B \in B(\hat{\R})$ und wegen der Eigenschaft,
	dass $f$ messbar reell
       \item Es sei $f\colon(\Omega,\CF)\rightarrow(\R,B(\R))$ messbar

	z.\,z.: $f\colon(\Omega,\CF)\rightarrow(\hat{\R},B(\hat{\R}))$ messbar

	$B\in B(\hat{\R})\colon f^{-1}(B\cap\R)\in \CF$ wegen $B\cap\R\in B(\R)$
	(vgl. (i)) und die vorrausges. Messbarkeit
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{satz}

Kurzbezeichnung: $f$ messbar (auf $(\Omega, \CF)$)

Eigenschaften messbarer Funktionen: $(\Omega, \CF)$ sei fest gewählt
\begin{enumerate}[E1]
 \item $f$ und $g$ messbar, $f(\omega)$ und $g(\omega)$ sind keine Unendlichkeiten
  verschiedenen Vorzeichens $\forall\omega\in\Omega$
  \begin{gather*}
    (f+g)(\omega):=f(\omega)+g(\omega), \omega\in\Omega \Rightarrow f+g\;
       \text{messbar}
  \end{gather*}
  \begin{proof}
    \{f+g < x\}= $\bigcup_{r\in\Q}\{f<r\}\cap\{g<x-r\}\in \CF =
    \{\omega\in\Omega:f(\omega)+g(\omega)<x\}$
    Anwendung von Eigenschaft 2.4, von Satz 3.1(ii)
  \end{proof}
 \item $f$ messbar,
  $a\in\R\Rightarrow (af)(\omega):=af(\omega)$, $\omega\in\Omega$, $af$
  messbar
  \begin{proof}
    \begin{enumerate}[1)]
     \item $a=0$: klar
     \item $a>0$: $\{af<x\}=\{f<\frac{x}{a}\}\in \CF, a\in\R$
     \item $a>0$: $\{af<x\}=\{f>\frac{x}{a}\}=\{f\le\frac{x}{a}\}^{\complement}\in
      \CF$ (da $f$ messbar)
    \end{enumerate}
  \end{proof}
 \item $f,g$ messbar, $(f\vee g)(\omega)=\max\{f(\omega),g(\omega)\}$,
  $(f\wedge g)(\omega)=\min\{f(\omega),g(\omega)\}$,
  $\omega\in\Omega \Rightarrow f\vee g, f\wedge g$ messbar
  \begin{proof}
    $\{f\vee g<r\}=\{f<r\}\cup\{g<r\}\in \CF, r\in\R$

    $\{f\wedge g<r\}=\{f<r\}\cap\{g<r\}\in \CF, r\in\R$
  \end{proof}
 \item $f$ messbar, $f^{+}:=f\vee0:=\max\{f,0\}$ positiver Anteil von $f$
  $f^{-}:=\max\{-f,0\}$ negativer Anteil von $f$
  $\Rightarrow f^{+},f^{-}$ messbare Funktionen
 \item $f$ messbar, $|f|(\omega):=|f(\omega)|, \omega\in\Omega$
  $\Rightarrow|f|$ messbar
  \begin{proof}
    $|f|=f^{+}+f^{-}$
  \end{proof}
 \item $f,g$ messbar, $(f\cdot g)(\omega) :=
  f(\omega)\cdot g(\omega)$, $\omega\in\Omega \Rightarrow f\cdot
  g$ messbar
  \begin{proof}
    \begin{gather}
      f\cdot g=f^{+}\cdot g^{+}-f^{+}\cdot g^{-}-f^{-}\cdot g^{+}+f^{-}\cdot g^{-}
    \end{gather}
    g.\,z.\,z.: $f\cdot g$ für $f,g\ge0$

    $\{f\cdot g < r\}=\bigcap_{n=1}^{\infty}\{f\cdot g\le r-\frac{1}{n}\}$
    deshalb g.\,z.\,z.: $\{f\cdot g\le r\}\in \CF$,$\forall r\in\R$

    $\{f\cdot g\le r\}=
    \bigcap_{n=1}^{\infty}\bigcup_{k=0}^{\infty}\{f\frac{k}{n}\le r\}
    \cap\{\frac{k}{n}\le g <\frac{k+1}{n}\}\in \CF$
    $\Rightarrow \{f\cdot g\le r\}\in \CF, \forall r\in\R$
  \end{proof}
 \item $f_{n}$ messbar, $n\ge1$ $\Rightarrow\sup f_{n}, \inf f_{n}$ messbar
  $(\sup f_{n})(\omega):=\sup f_{n}(\omega), \omega\in\Omega$\\
  $(\inf f_{n})(\omega):=\inf f_{n}(\omega), \omega\in\Omega$
  \begin{proof}
    $\{\sup f_{n} <r\}=\bigcap_{n=1}^{\infty}\underbrace{\{f_{n}<r\}}_{\in \CF}\in \CF, r\in\R$
    $\inf f_{n} = -\sup(-f_{n})$
  \end{proof}
 \item $f_{n}$ messbar, $n\ge1$, $\limsup_{n\rightarrow\infty}f_{n}:=
  \inf_{n}(\sup_{k\ge n}f_{k})$
