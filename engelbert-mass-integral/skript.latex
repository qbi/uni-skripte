\begin{defini}
  Ein Mengensystem $\mathcal{K}\subseteq \mathfrak{P}(\Omega)$ heißt kompakte Klasse, falls
  für beliebige Folgen $K_{n}\in \mathcal{K}$ mit
  $\bigcap_{n=1}^{\infty}K_{n}=\emptyset$ bereits ein $N$ existiert, so dass
  $\bigcap_{n=1}^{N}K_{n}=\emptyset$
\end{defini}

\begin{bsp}
  $(E,\rho)$ metr. Raum, $\mathcal{K} := \{K\subseteq E\colon K \text{kompakt}\}$
  $\Rightarrow$ $\mathcal{K}$ kompakte Klasse
\end{bsp}

\begin{lemma}
  Sei $m$ ein endlicher Inhalt auf einer Algebra $\mathcal{A}$

  $m$ Prämaß $\Leftrightarrow$ $m$ ist stetig in $\emptyset$

  \begin{proof}
    \begin{description}
     \item[$\Rightarrow$] Beweis wie früher für Maße
     \item[$\Leftarrow$] $A_{n}\in \mathcal{A}, n\geq$ paarw. disj.,
      A=$\bigcup_{n=1}^{\infty}A_{n}\in \mathcal{A}$

      z.\,z.: m(A) = $\sum_{n=1}^{\infty}m(A_{n})$

      m(A) = m($\bigcup_{n=1}^{\infty}A_{n}) =
      m(\underbrace{\bigcup_{k=1}^{n}A_{k}}_{\in \mathcal{A}} \cup
      \underbrace{(A\setminus\bigcup_{k=1}^{\infty}A_{k})}_{\in \mathcal{A}}) =^{m
      Inhalt} m(\bigcup_{k=1}^{n}A_{k}) +
      m(\underbrace{A\setminus\bigcup_{k=1}^{n}A_{k}}_{=B_{n}}) =
      \sum_{k=1}^{n}m(A_{k})+m(B_{k}), B_{n}\in \mathcal{A}, B_{n+1}\subseteq
      B_{n}, \bigcap_{n=1}^{\infty}B_{n}=\emptyset \Rightarrow
      m(B_{n})\xrightarrow{n\rightarrow\infty}0$ (m stetig in $\emptyset$
      auf $\mathcal{A}$)
      $\xrightarrow{n\rightarrow\infty}\sum_{k=1}^{\infty}m(A_{k})+0
      \rightarrow $ Beh.
    \end{description}
  \end{proof}
\end{lemma}

\begin{satz}\label{satz:2}
  m endlicher Inhalt auf $(\Omega,\mathcal{A})$, $\mathcal{A}$ Algebra. Es sei folgende
  Bedingung erfüllt:
  \begin{gather}
    \forall A\in \mathcal{A}, \epsilon>0 \exists A'\in \mathcal{A}, K\in \mathcal{K}: A'\subseteq K
       \subseteq A \wedge m(A)-m(A')<\epsilon
  \end{gather}
  Dabei ist $\mathcal{K}\subseteq \mathfrak{P}(\Omega)$ eine kompakte Klasse.

  $\Rightarrow$ Dann ist m ein Prämaß

  \begin{proof}
    zeigen: m isst stetig in $\emptyset$ auf $\mathcal{A}$

    $A_{n}\in \mathcal{A}, A_{n+1}\infty A_{n}, n\geq1, \bigcap_{n=1}^{\infty}A_{n}=\emptyset$

    z.\,z.: $\lim_{n\rightarrow\infty}m(A_{n})=0$

    Benutzer Satz\ref{satz:2} $\epsilon>0$ exist. $K_{n}\in \mathcal{K}, A_{n}'\in
    \mathcal{A}$ mit $A_{n}'\subseteq K_{n}\subseteq A_{n}, m(A_{n})-m(A_{n}') <
    \frac{\epsilon}{2^{n}}$

    $\bigcap_{n=1}^{\infty}K_{n}\subseteq\bigcap_{n=1}^{\infty}A_{n}=\emptyset
    \Rightarrow \bigcap_{n=1}^{\infty}K_{n}=\emptyset \Rightarrow \exists N:
    \bigcap_{n=1}^{N}K_{n}=\emptyset \Rightarrow \bigcap_{n=1}^{N}A_{n}'=\emptyset$

    $A_{N}=A_{N}\setminus\bigcap_{n=1}^{N}A_{n}' =
    A_{n}\cap\bigcup_{n=1}^{N}(A_{n}')^{\complement} =
    \bigcup_{n=1}^{N}(A_{N}\cap(A_{n}')^{\complement})\subseteq
    \bigcup_{n=1}^{N}(A_{n}\setminus A_{n}')$

    $\Rightarrow m(A_{N}) \leq m(\bigcup_{n=1}^{N}(A_{n}\setminus
    A_{n}')\leq \sum_{n=1}^{N}\underbrace{m(A_{n}\setminus
    A_{n}')}_{=m(A_{n}) -m(A_{n}')}\leq\sum_{n=1}^{N}\frac\epsilon{2^{n}}<\epsilon$

    $\Rightarrow \lim_{N\rightarrow\infty} m(A_{N}) <\epsilon
    \rightarrow$ Beh.
  \end{proof}
\end{satz}

\begin{bemerk}
  \begin{enumerate}
   \item Falls $\mathcal{K}\subseteq\mathcal{A}$ $\rightarrow$ möglich: $A'=K$
   \item Satz \ref{satz:2} gilt entsprechend für Halbalgebren $\mathcal{C}$
    (anstelle von $\mathcal{A}$). Aussage: $m$ $\sigma$-additiv auf $\mathcal{C}$ (siehe Neven
    I.6.2)
  \end{enumerate}
\end{bemerk}

\chapter{Maße auf der reellen Achse}
Betrachen Maße m auf dem messbaren Raum $(\R, \mathcal{B}(\R))$

\begin{defini}
  Ein Maß m auf $(\R , \mathcal{B}(\R))$ heißt lokal endlich, falls für alle
  $x\in\R$ eine offene Menge $G_{x}$ existiert, für die gilt $x\in G_{x}$ und $m(G_{x})<+\infty$.
\end{defini}

\begin{bemerk}
  \begin{enumerate}
   \item $m$ lokal endlich $\Rightarrow$ $m([-n,n])<+\infty$, $\forall$
    $n\geq 1$ (folgt aus Überdeckungssatz)
   \item $m$ lokal endlich $\Rightarrow$ $m$ ist $\sigma$-endlich (Umkehrung
    geht nicht!)
  \end{enumerate}
\end{bemerk}

\begin{satz}
  Sei m ein lokal endliches Maß auf $(\R, \mathcal{B}(\R))$.

  Dann existiert eine Funktion $\CF_{m}:\R\rightarrow\R$ mit folgenden
  Eigenschaften:
  \begin{enumerate}
   \item $\CF_{m}$ ist monoton wachsend
   \item $\CF_{m}$ ist stetig von links
   \item $m([a,b)) = \CF_{m}(b)-\CF_{m}(a), a,b\in\R, a\leq b$
  \end{enumerate}

  \begin{proof}
    \begin{gather*}
      \CF_{m}(x) =
         \begin{cases}
           m([0,x))& x\geq0\\
           -m([x,0))& x<0
         \end{cases}
    \end{gather*}
  \end{proof}
\end{satz}

\begin{satz}
  Es sei $\CF:\R\rightarrow\R$ monoton wachsend und stetig von links. Dann
  existiert ein eindeutig bestimmtes Maß $m$ auf $(\R, \mathcal{B}(\R))$, so dass sich
  \begin{gather}
    m([a,b))=\CF(b)-\CF(a),\quad a,b\in\R, a\leq b
  \end{gather}

  Das Maß m ist dann lokal endlich (und damit auch $\sigma$-endlich).

  \begin{proof}
    $\mathcal{C}=\{[a,b)\cap \R\colon -\infty\leq a\leq b \leq+\infty\}$ Halbalgebra,
    $x\in\R: x^{(N)}:=(x\wedge N)\cup(-N), \CF_{N}(x):= \CF(x^{(N)}),x\in\R$
    $\CF_{N}$ monoton wachsend, linksstetig,
    $-\infty < \CF_{N}(+\infty):=\lim_{x-^\infty} \CF_{N}(x) (=\CF(N))<\infty$
    $\infty > \CF_{N}(-\infty) := \lim_{x-v-\infty} \CF_{N}(x) (=\CF(-N))>-\infty$

    $m_{N}([a,b)\cap\R):=\CF_{N}(b) - \CF_{N}(a), [a,b)\cap\R\in \mathcal{C}$

    \begin{enumerate}
     \item $m_{N}(\emptyset)=0$
     \item $m_{N}$ ist additiv auf $\mathcal{C}$ (leicht zu überprüfen)
     \item $m_{N}(\R)=\CF({N})-\CF(-N)<\infty$
    \end{enumerate}

    Nach Satz 6.1.: Es existiert ein eindeutig bestimmter Inhalt
    (bezeichnet durch $m_{N}$) auf $\mathcal{A} = \alpha(\mathcal{C})$, welcher unser
    vorgegebenes $m_{N}$ fortsetzt. $m_{N}(\R) < \infty$

    Anwendung von Satz 6.2.: $m_{N}$ ist sogar ein Prämaß auf $\mathcal{A}$

    $\mathcal{K}:=\{K\subseteq\R: K~\text{kompakt}\}$, überprüfen (*)
    $\epsilon>0, A\in \mathcal{A} $ fest forgegeben,
    A=$\bigcup_{i=1}^{n}[a_{i},b_{i}), [a_{1},b_{1}),\ldots,[a_{n},b_{n})
    paarw. disj.$ (Lemma 6.1)\\
    $K_{\delta}:=\bigcup_{i=1}^{n}[a_{i}^{(N)}, b_{i}^{(N)}-\delta]\subseteq
    A, K_{\delta}$ kompakt
    $A_{\delta}':=\bigcup_{n=1}^{n}[a_{i}^{(N)},b_{i}(N)-\delta]\subseteq
    K_{\delta}, A_{\delta}' \mathcal{A}$
    $m_{N}(A)=\sum_{i=1}^{n}m_{N}([a_{i},b_{i}))=\sum_{i=1}^{n}
    [\CF_{N}(b_{i})-\CF_{N}(a_{i})]=\sum_{i=1}^n[\CF(b_{i}^{(N)})-\CF(a_{i}^{(N)})]$

    $m_{N}(A_{\delta}')=\sum_{i=1}^{n}[\CF(b_{i}^{(N)})-\CF(a_{i}^{(N)})]$

    $\Rightarrow$ $m_{N}(A)-m_{N}(A_{\delta}')=\sum_{i=1}^{n}\underbrace{[
    \CF(b_{i}^{(N)})-\CF(b_{i}^{(N)}-\delta)]}_{\leq\frac\epsilon{n}
    \text{für $\delta$ hinreichend klein (Stetigkeit von links von $\CF$!)}}$

    Aufgrund des Fortsetzungssatz existiert eine Fortsetzung zu einem
    endlichen Maß $m_{N}$ auf $\mathcal{B}(\R) =\sigma(\mathcal{A})$.\\
    Es gilt: $m_{N}\leq m_{N+1}(A), A\in \mathcal{B}(\R)$.\\
    Setzen: $\mu_{N}(A)=m_{N+1}(A\cap[-N,N]), A\in \mathcal{B}(\R) \Rightarrow
    \mu_{N}$ Maß auf $\mathcal{B}(\R)$, $\mu_{N}$ endlich\\
    $\mu_{N}(A)\leq m_{N+1}(A), A\in \mathcal{B}(\R)$\\
    Beh.: $\mu_{N}=m_{N}$ auf $\mathcal{B}(\R)$:
    $\mu_{N}([a,b)\cap\R)=m_{N+1}([a,b)\cap[-N,N)) = m_{N}([a,b)\cap\R)
    \Rightarrow \mu_{N}=m_{N} auf \mathcal{C} \Rightarrow^{Eindeutigkeitssatz für
    endl. Maße} \mu_{N}=m_{N} auf \mathcal{B}(\R)$

    aus Beispiel 1.8: m(A) = $\lim_{N\rightarrow\infty}m_{N}(A), A\in \mathcal{B}(\R)
    \Rightarrow m Maß$
    m([a,b))=$\lim_{N\rightarrow\infty}m_{N}([a,b)) =
    \lim_{N\rightarrow\infty}[\CF_{N}(b)-\CF_{N}(a)]=\CF(b)-\CF(a)$

    Eindeutigkeit von m: Seien m, m' entsprechende Maße,
    $\mathcal{C}'=\{[a,b)\colon a,b\in\R, a\leq b\}$ $\Rightarrow$ $m=m'$ auf $\mathcal{C}'$, $\mathcal{C}'$
    $\pi$-System, $\sigma(\mathcal{C}')=\mathcal{B}(\R)$, $m,m'$ $\sigma$-endlich auf $\mathcal{C}'$
    $\Rightarrow^{Satz 2.2}$ $m=m'$ auf $\mathcal{B}(\R)$
  \end{proof}
\end{satz}

\begin{defini}
  \begin{enumerate}
   \item $\CF:\R\rightarrow \R$, monoton wachsend und linksstetig, heißt
    maßerzeugende Funktion
   \item sei $m$ lokal endliches Maß. Dann heißt $\CF_{m}$ (aus Satz 1)
    $m$-erzeugend
   \item Eine maßerzeugende Funktion $\CF$ heißt Verteilungsfunktion, falls
    zusätzlich $\lim_{x\to-\infty}\CF(x)=0, \lim_{x\to\infty}\CF(x)=1$
  \end{enumerate}
