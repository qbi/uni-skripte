\section{Eindeutigkeitssatz für Maße}
$(\Omega, \CF)$ messbarere Raum, $m_{1}, m_{2}$ Maße auf $(\Omega, \CF)$

\underline{Problem:} $\mathfrak{S}\subseteq \CF$ Mengensystem, $m_{1}(A)=m_{2}(A), \forall A\in \mathfrak{S}
\xrightarrow{?} m_{1}=m_{2}$ (auf $\CF$)

\begin{defini}
  Ein Maß $m$ auf $(\Omega, \CF)$ heißt endlich, falls
  $m(\Omega)<+\infty$ (und somit $m(A)<+\infty, \forall A\in \CF$) gilt.
\end{defini}

\begin{satz}
  Es seien $m_{1}, m_{2}$ \emph{endliche} Maße auf $(\Omega, \CF),
  \mathfrak{S}\subseteq 2^{\Omega}$ mit folgenden Eigenschaften:
  \begin{itemize}
   \item $\mathfrak{S}$ $\pi$-System
   \item $\Omega\in \mathfrak{S}$
   \item $\sigma(\mathfrak{S})=\CF$ \,\,($\mathfrak{S}$ ist Erzeugendensystem
    von $\CF$)
   \item $m_{1}(A)=m_{2}(A), A\in \mathfrak{S}$
  \end{itemize}

  Dann folgt: $m_{1}=m_{2}$ (auf $\CF$)

  \begin{proof}
    $\mathcal{D}=\{A\in \CF: m_{1}(A)=m_{2}(A)\}$
    \begin{enumerate}[a)]
     \item $\mathfrak{S}\in \mathcal{D}$
     \item $\mathcal{D}$ ist ein Dynkin-System:
      \begin{enumerate}[(i)]
       \item $\Omega\in \mathfrak{S}\subseteq \mathcal{D} \Rightarrow \Omega\in \mathcal{D}$
       \item $A\in \mathcal{D} \Rightarrow A^{\complement}\in \mathcal{D}:
	m_{1}(A)=m_{1}(\Omega)-m_{1}(A) = m_{2}(\Omega)-m_{2}(A)=m_{2}(A^{\complement})$
       \item $A_{k}\in \mathcal{D}, k\geq1, $ paarw. disj. $\Rightarrow$
	$m_{1}(\bigcup_{k=1}^\infty
	m_{1}(A_{k})=\sum_{k=1}^{\infty}m_{2}(A_{k})
	=m_{2}(\bigcup_{k=1}^{\infty}A_{k}) \Rightarrow
	\bigcup_{k=1}^{\infty}A_{k}\in \mathcal{D}$

	$\xrightarrow{S. I.5.2.} \sigma(\mathfrak{S})\in \mathcal{D} \Rightarrow \CF\subseteq \mathcal{D}$
      \end{enumerate}
    \end{enumerate}
  \end{proof}
\end{satz}

\begin{defini}
  \begin{enumerate}[(i)]
   \item $\mathfrak{S}\subseteq 2^{\Omega}, m$ Maß auf $(\Omega, \CF)$, m heißt
    $\sigma$-endlich \emph{auf} $\mathfrak{S}$, falls $\exists \Omega_{n}\in \mathfrak{S}$ mit
    $\Omega_{n}\subseteq\Omega_{n+1}, n\geq1$ und
    $\Omega=\bigcup_{n=1}^{\infty}\Omega_{n}$, sowie $m(\Omega_{n})<+\infty$
   \item $m$ heißt $\sigma$-endl., falls $m$ $\sigma$-endl auf $\CF$ ist
  \end{enumerate}
\end{defini}

\begin{satz}
  Seien $m_{1},m_{2}$ Maße auf $(\Omega, \CF)$, $\mathfrak{S}\subseteq$ $2^{\Omega}$
  mit folgenden Vorraussetzungen:
  \begin{enumerate}[1)]
   \item $\mathfrak{S}$ $\pi$-System
   \item $m_{1}, m_{2}$ sind $\sigma$-endl. \emph{auf $\mathfrak{S}$}
   \item $\sigma(\mathfrak{S})=\CF$
   \item $m_{1}(A)=m_{2}(A), A\in \mathfrak{S}$
  \end{enumerate}

  Dann gilt: $m_{1}=m_{2}$ (auf $\CF$)

  % 30.4.
  \begin{proof}
    $\exists \Omega_{i}^{(n)}\in \mathfrak{S},
    \Omega_{i}^{(n)}\uparrow_{n\rightarrow\infty} \Omega,
    \mathfrak{M}_{i}(\Omega_{i}^{(n)})<+\infty, \forall n\geq1, i=1,2$

    $\Omega^{(n)}:=\Omega_{1}^{(n)}\cap\Omega_{2}\in \mathfrak{S},
    \Omega^{(n)}\uparrow_{n\rightarrow\infty}\Omega,
    m_{1}(\Omega^{(n)})<+\infty, m_{2}(\Omega^{(n)})<+\infty$

    $m_{1}^{(n)},m_{2}^{(n)}$ endliche Maße auf $(\Omega,\CF)$, $\mathfrak{S}' :=
    \mathfrak{S}\cup\{\Omega\}$

    $m_{1},m_{2},\mathfrak{S}'$ erfüllen Vorraussetzung von Satz 1 $\Rightarrow$
    $m_{1}^{(n)}=m_{2}^{(n)}$ auf $\CF$ für alle $n$
    $\Rightarrow m_{1}(A\cap\Omega^{(n)})=m_{2}(A\cap\Omega^{(n)}), A\in
    \CF$\\
    $\downarrow n\rightarrow \infty$
    $\downarrow n\rightarrow \infty$ (Stetigkeit von unten von $m_{1},
    m_{2}$)\\
    $m_{1}(A)=m_{2}(A), A\in \CF$
  \end{proof}
\end{satz}

