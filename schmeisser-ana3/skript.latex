\chapter{Vektoranalysis, Integralsätze}
\section{Vektorfelder}
\subsection{Differentialoperatoren-Rechenregeln}

\begin{defini}
  $\Omega\subseteq\R^{n}$ offen, $x=(x_{1},\ldots,x_{n})\in\Omega$,
  $\{\vec{e_{1}}, \ldots,\vec{e_{n}} \}$ kanonische Basis,
  $u:\Omega\rightarrow\R$ $u$ ist einmal stetig differenzierbar $u\in
  C^{1}(\Omega)$, u ist ein \highl{skalares Feld}, $\vec{v} = v_{1}
  (\times) \vec{e_{1}} + \ldots+ \vec{v_{n}} (\times) \vec{e_{n}} =
  (v_{1}(x), \ldots, v_{n}(x))$ oder
  $\begin{bmatrix}v_{1}(x)\\\vdots\\v_{n}(x)\end{bmatrix}, v_{j}\in
  C^{1}(\Omega), j=1,\ldots,n$ $C^{1}$-Vektorfeld

  \begin{enumerate}
   \item
    \begin{align*}
      \grad u &= \nabla u = \partial_{1}u\vec{e_{1}}
         +\ldots+\partial_{n}u\vec{e_{n}}&&\text{wobei }\partial_{j} =
         \frac{\partial}{\partial x_{j}}\\
      &= \begin{bmatrix}\partial_{1}u\\\vdots\\\partial_{n}u\end{bmatrix}
         &\text{Gradient von }u
    \end{align*}
   \item
    \begin{gather*}
      \Div \vec{v} := \partial_{1}v_{1} +
         \partial_{2}v_{2}+\ldots+\partial_{n}v_{n} (=\nabla \vec{v})
    \end{gather*}
    Divergent von $\vec{v}$.
   \item $n=3$
    \begin{align*}
      \rot \vec{v} &= (\partial_{2}v_{3}-\partial_{3}v_{2})\vec{e_{1}} +
         (\partial_{3}v_{1}-\partial_{1}v_{3})\vec{e_{2}} +
         (\partial_{1}v_{2}-\partial_{2}v_{1})\vec{e_{3}}\\
      (&= \nabla \times \vec{v} =
         \begin{vmatrix}
           \vec{e_{1}}&\partial_{1}& v_{1}\\
           \vec{e_{2}}&\partial_{2}& v_{2}\\
           \vec{e_{3}}&\partial_{3}& v_{3}
           \end{vmatrix})
    \end{align*}
    Rotation von $\vec{v}$
  \end{enumerate}
\end{defini}

\begin{bemerk}
  Gilt für $\vec{v}$ $\Div \vec{v} = 0$ auf $\Omega$, dann nennt man
  $\vec{v}$ ein \highl{quellenfreies Vektorfeld}. Ist $\rot \vec{v}=0$
  auf $\Omega$, so nennt man es ein \highl{wirbelfreies Vektorfeld}.
\end{bemerk}

\begin{defini}
  Für $u\in C^{2}(\Omega)$ und $\vec{v}$ ein $C^{2}$-Vektorfeld auf
  $\Omega\subseteq\R^{3}$ bezeichnet
  \begin{enumerate}
   \item
    \begin{gather*}
      \Delta u := \sum_{j=1}^{n}\partial_{j}^{2}u = \sum_{j=1}^{n}
         \frac{\partial^{2}u}{\partial u_{j}^{2}}
    \end{gather*}
    den \highl{Laplace-Operator}.
   \item $\Delta\vec{v} := \Delta v_{1}\vec{e_{1}} + \Delta
    v_{2}\vec{e_{2}} + \Delta v_{3} \vec{e_{3}}$
  \end{enumerate}
\end{defini}

\begin{satz}
  $\Omega\subseteq\R^{n}$ offen, $u\in C^{2}(\Omega)$ und
  $\vec{v}$-$C^{2}$-Vektorfeld.
