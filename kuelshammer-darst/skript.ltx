%satz 1.5
\begin{satz}[lifting idempotents]\label{satz:15}
  Suppose that $I$ is a idempotent ideal of an $F$"~Algebra $A$ and let
  $\epsilon$ be an idempotent in $A/_{I}$. Then there is an idempotent $e$ in
  $A$ such that $\epsilon=e+I$.
  \begin{proof}
    Let $n\in\N$ such that $I^{n}=0$ and let $a\in A$ such that
    $\epsilon=a+I$. Then
    \begin{align*}
      1 &= (1-a+a)^{2n}= \sum_{k=0}^{2n} \binom{2n}{k} (1-a)^{k} a^{2n-k}
    \end{align*}
    Let $e\colon= \sum_{k=0}^{n} \binom{2n}{k} (1-a)^{k} a^{2n-k}$, so that
    $1-e= \sum_{l=n+1}^{2n} \binom{2n}{l} (1-a)^{l} a^{2n-l}$. Then $a(1-a)+I=
    \epsilon(1-\epsilon)=0$, so $a(1-a)\in I$ and $e(1-e)= \sum_{k=0}^{n}
    \sum_{l=n+1}^{2n} \binom{2n}{k} \binom{2n}{l} (1-a)^{k}
    \underbrace{a^{2n-k} (1-a)^{l}}_{\in I^{n}=0} a^{2n-l}=0$. Thus $e=e^{2}$.
    Moreover, $e+I= \sum_{k=0}^{n} \binom{2n}{k} \underbrace{(1-a)^{k}
    a^{2n-k}}_{\in I \text{ for } k\neq 0}+I=
    a^{2n}+I=\epsilon^{2n}=\epsilon$.
  \end{proof}
\end{satz}

\begin{bemerkON}
  Let $e\in A$ be en idempotent. Then $eAe$ is an $F$"~algebra with identity
  element $e$, so it's \emph{not} a subalgebra. Moreover, \[\Rad(eAe)=
  e\Rad(A)e;\] for $e\Rad(A)e$ is certainly a nilpotent ideal of $eAe$, so
  $e\Rad(A)e\subseteq\Rad(eAe)=I$. On the other hand, $(AIA)^{n}= AIAI\dotso
  IA= AeIeAeI\dotso eIeA= AeI^{n}eA=0$ for some $n\in\N$. Thus $AIA$ is a
  nilpotent ideal of $A$, so $i\subseteq AIA\subseteq\Rad(A)$ and
  $I=eIe\subseteq e\Rad(A)e$.
\end{bemerkON}

%def 1.6
\begin{defini}
  Idempotents $e,f$ in an $F$"~algebra $A$ such that $ef=0=fe$ are called
  \highl{orthogonal}.
\end{defini}

\begin{bemerkON}
  In this case, $(e+f)^{2}=e^{2}+ef+fe+f^{2}= e+f$, so $e+f$ is an idempotent.
\end{bemerkON}

\begin{bsp}
  For $n\in\N, \begin{pmatrix}1 & 0 & \dotso & 0\\
		 0\\
		 \vdots\\
		 0\end{pmatrix}, \begin{pmatrix}0 & 0 & \dotso & 0\\
				   0 & 1 & 0 & 0\\
				   \vdots\\
				   0\end{pmatrix}$ etc. are pairwise
  orthogonal idempotents in $F^{n\times n}$, with sum $1_{n}$.
\end{bsp}

%satz 1.6
\begin{satz}[Orthogonalizing idempotents]\label{satz:16}
  Let $I$ be a nilpotent ideal of an $F$"~algebra $A$, and let $e_{1},\dotsc,
  e_{n}$ be idempotents in $A$ such that $e_{1}+\dotsb+ e_{n}+I=1+I$ and
  $e_{i}e_{j}+I=0$ for all $i\neq j$. Then $u\colon= e_{1}+\dotsb+ e_{n}\in
  U(A)$, and $u^{-1}e_{1},\dotsc, u^{-1}e_{n}$ are pairwise orthogonal
  idempotents in $A$ such that $u^{-1}e_{1}+\dotsb+ u^{-1}e_{n}=1$ and
  $u^{-1}e_{i}+I=e_{i}+I$ for $i=1,\dotsc,n$.
  \begin{proof}
    Let $n\in\N$ such that $I^{n}=0$. Then $x\colon=u-1\in I$, so
    \begin{align*}
      (1+x)(1-x+x^{2}-x^{3}+\dotsb+ (-1)^{n} x^{n}) &= 1+(-1)^{n} x^{n+1} =1\\
      (1-x+x^{2}+\dotsb+ (-1)^{n}x^{n})(1+x) &= 1
    \end{align*}
    Thus $u=1+x\in U(A)$ and
    \begin{align*}
      u^{-1}e_{i}+I &= (1-x+x^{2}+\dotsb+ (-1)^{n}x^{n})e_{i}+I= e_{i}+I\qquad
	 i=1,\dotsc,n\\
      u^{-1}e_{1}+\dotsb+ u^{-1}e_{n} &= u^{-1}(e_{1}+\dotsb+ e_{n})=u^{-1}u=1
    \end{align*}
    It suffices to show that $A=Ae_{1}\oplus\dotso \oplus Ae_{n}$; for then
    \begin{align*}
      u^{-1}e_{i} &= u^{-1}e_{i}\cdot1= \underbrace{u^{-1}e_{i}\cdot
	 u^{-1}e_{1}}_{\in Ae_{1}}+\dotsb+ \underbrace{u^{-1}e_{i}\cdot
	 u^{-1}e_{n}}_{\in Ae_{n}}\qquad i=1,\dotsc,n
    \end{align*}
    Comparing components we see that $u^{-1}e_{i}=u^{-1}e_{i}\cdot
    u^{-1}e_{i}$ and $u^{-1}e_{i}\cdot u^{-1}e_{j}=0$ for all $i\neq j$.

    It remains to show that $A=Ae_{i}\oplus\dotso \oplus Ae_{n}$. If $a\in A$,
    then $a=a1=au^{-1}e_{1}+\dotsb+ au^{-1}e_{n}\in Ae_{1}+\dotsb+ Ae_{n}$.
    Thus $A=Ae_{1}+\dotsb+ Ae_{n}$, and it suffices to show that the sum is
    direct. We assume the contrary and choose a nonempty subset
    $A\subseteq\{1,\dotsc,n\}$ and elements $x_{j}\in Ae_{j}\setminus\{0\}$
    such that $\sum_{j\in J} x_{j}=0$. Let $k\in J$ be arbitrary. Then
    $x_{k}=-\sum_{k\neq j\in J} x_{j}= -\sum_{k\neq j\in J} x_{j} e_{j}$. Thus
    $x_{k}=x_{k}e_{k}= -\sum_{k\neq j\in J} x_{j}e_{j}e_{k}$. Taking the
    difference we obtain that $\sum_{k\neq j\in J} x_{j}(1-e_{j}e_{k})=0$. For
    $k\neq j\in J$ we have $e_{j}e_{k}\in I$, so
    $1=(1-e_{j}e_{k})(1-e_{j}e_{k}+(e_{j}e_{k})^{2}+\dotsb+ (e_{j}e_{k})^{n})=
    (1+e_{j}e_{k}+ (e_{j}e_{k})^{2}+\dotsb+ (e_{j}e_{k})^{n})(1-e_{j}e_{k})$.
    Thus $uj:=1-e_{j}e_{k}\in U(A)$ and $y_{j}\colon=x_{j}u_{j}\neq0$.
    Moreover, $y_{j}=x_{j}u_{j}= x_{j}u_{j}\cdot u^{-1}e_{j}u_{j}\in
    Au^{-1}e_{j}u_{j}$ and $u^{-1}_{j}e_{j}e_{j}+I=e_{j}+I$. Thus the
    idempotents $u^{-1}_{1}e_{1}u_{1},\dotsc, u_{n}^{-1}e_{n}u_{n}$ also
    satisfy the hypothesis of the theorem. So we have idempotents
    $e_{1}',\dotsc,e_{n}'$ in $A$ satisfying the hypothesis of the theorem, a
    subset $J'\subseteq J$ such that $\abs{J'}=\abs{J}-1$ and elements
    $x_{j}'\infty Ae_{j}'\setminus\{0\}$ such that $\sum_{j\in J} x_{j}'=0$.
    Now we continue by induction. At the end we have idempotents
    $e_{1}^{*},\dotsc,e_{n}^{*}$ satisfying the hypothesis of the theorem and
    an element $k\in J$ and an element $x_{l}^{*}\in Ae_{l}^{*}\setminus\{0\}$
    such that $x_{l}^{*}=0$, which is impossible.
    \end{proof}
\end{satz}

%satz 1.7
\begin{satz}[Lifting orthogonal idempotents]
  Let $I$ be a nilpotent ideal of an $F$"~algebra $A$, and let
  $\epsilon_{1},\dotsc,\epsilon_{n}$ be pairwise orthogonal idempotents in
  $A/_{I}$ such that $\epsilon_{1}+\dotsb+\epsilon_{n}=1$. Then there are
  pairwise orthogonal idempotents $e_{1},\dotsc, e_{n}$ in $A$ such that
  $e_{1}+\dotsb+ e_{n}=1$ and $\epsilon_{i}=\epsilon_{i}+I$ for $i=1,\dotsc,n$.
  \begin{proof}
    By \autoref{satz:15}, there are idempotents $f_{1},\dotsc, f_{n}\in A$
    such that $\epsilon_{i}=f_{i}+I$ for $i=1,\dotsc,n$. Then $f_{1}+\dotsb+
    f_{n}+I= \epsilon_{1}+\dotsb+\epsilon_{n}=1_{A}+I$ and
    $f_{i}f_{j}+I=\epsilon_{i}\epsilon_{j}=0+I$ for alle $i\neq j$. By
    \autoref{satz:16}, we have $u\colon= f_{1}+\dotsb+ f_{n}\in U(A)$, and
    $e_{1}=u^{-1}f_{1},\dotsc, e_{n}\colon= u^{-1}f_{n}$ are pairwise
    orthogonal idempotents in $A$ such that $e_{1}+\dotsb+ e_{n}=1$ and
    $e_{i}+I= f_{i}+I=\epsilon_{i}$.
  \end{proof}
\end{satz}

\chapter{Modules}

Let $F$ algebraic closed field and $A$ an $F$"~algebra.

% def. 2.1
\begin{defini}
  An $A$"~module is a finite"=dimensional $F$"~vector spce $M$, together with
  a multiplication $A\times M\rightarrow M$ satisfying:
  \begin{enumerate}
   \item $(a+b)m=am+bm$
   \item a(m+n)=am+an$$
   \item $(ab)m=a(bm)$
   \item $(\alpha 1_{A})m=\alpha m$
  \end{enumerate}
  for $a,b\in A, m,n\in M, \alpha\in F$.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item The \highl{zero module} 0.
   \item The \highl{regular module} $A$.
   \item The \highl{direct product} $M_{1}\times\dotso \times M_{n}$ of
    $A$"~modules $M_{1},\dotsc, M_{n}$, via $a(m_{1},\dotsc, m_{n})\colon=
    (am_{1},\dotsc, am_{n})$ for $a\in A, m_{1}\in M_{1},\dotsc, m_{n}\in
    M_{n}$.
  \end{enumerate}
\end{bsp}

% def 2.2
\begin{defini}
  An $F$"~linear map $f\colon M\rightarrow N$ between $A$"~modules $M,N$ such
  that $f(am)=af(m)$ for $a\in A, m\in M$ ist called an
  $A$"~\highl{homomorphism}. Then $A$"~\highl{monomorphisms}, \dots{},
  $A$"~\highl{automorphisms} are defined as usual.
\end{defini}

\begin{bemerkON}
  \begin{enumerate}
   \item Let $f,g\colon M\rightarrow N$ be $A$"~homomorpisms and $\alpha\in
    F$. Then $f+g\colon M\rightarrow N, m\mapsto f(m)+g(m)$, and $\alpha
    f\colon M\rightarrow N, m\mapsto\alpha f(m)$ are $A$"~homomorpisms. Also,
    the zero map $M\rightarrow N, m\mapsto0$ is an $A$"~homomorpism. Thus
    $\Hom_{A}(M,N)\colon= \{f\colon M\rightarrow N| f \text{ is }
    A\text{-homomorpism}\}$ is a $F$"~subspace of $\Hom_{F}(M,N)$.
   \item Let $L,M,N$ be $A$"~modules and $f\in\Hom_{A}(L,M),
    g\in\Hom_{A}(M,N)$. Then $g\circ f\in\Hom_{A}(L,N)$. Moreover,
    $g\circ(f_{1}+f_{2})= (g\circ f_{1})+g\circ f_{2})$ and
    $(g_{1}+g_{2})\circ f= (g_{1}\circ f)+(g_{2}\circ f)$ and $\alpha(g\circ
    f)= (\alpha g)\circ f=g\circ(\alpha f)$ for $f, f_{1},
    f_{2}\in\Hom_{A}(L,M), g,g_{1}, g_{2}\in\Hom_{A}(M,N), \alpha \in F$.
    Also $f\colon M\rightarrow M, m\mapsto m$ is an $A$"~automorphism. Thus
    $E\colon=\End_{A}(M)\colon= \Hom_{A}(M,M)$ is an $F$"~algebra, the
    \highl{endomorphism algebra} of $M$. Finally, $M$ becomes an $E$"~module
    via $f\cdot m\colon=f(m)$ for $f\in E, m\in M$.
   \item $\Aut_{A}(M)\colon=U(\End_{A}(M))$ is called the \highl{automorphism
    group} of $M$.
   \item We call $A$"~modules $M,N$ \highl{isomorphic} and write $M~_{A}N$
    \todo{verbessern} or $M~N$ if there exists an $A$"~isomorphism $g\colon
    M\rightarrow N$. Then $~_{A}$ is an equivalnce relation.
  \end{enumerate}
\end{bemerkON}

% def 2.3
\begin{defini}
  A \highl{submodule} of an $A$"~module $M$ is a subspace $N\subseteq M$ such
  that $an\in N$ for $a\in A, n\in N$. We write $N\geq M$.
\end{defini}

\begin{bsp}
  \begin{enumerate}
   \item 0 and $M$ are always submodules of $M$.
   \item If $N, N'\leq M$, then $N\cap N', N+N'\leq M$.
   \item Let $M,M'$ be $A$"~modules, let $f\in\Hom_{A}(M,M')$ and let $N\geq
    M, N'\geq M'$. Then $f(N)\geq M', f^{-1}(N')\geq M$, in particular,
    $\ker(f)=f^{-1}(0)=\{m\in M\colon f(m)=0\}\leq M$ and
    $\Im(f)=f(M)=\{f(m)\colon m\in M\}\leq M'$.
  \end{enumerate}
\end{bsp}


